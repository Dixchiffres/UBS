[1mdiff --git a/node_modules/mime/README.md b/node_modules/mime/README.md[m
[1mnew file mode 100644[m
[1mindex 0000000..506fbe5[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/README.md[m
[36m@@ -0,0 +1,90 @@[m
[32m+[m[32m# mime[m
[32m+[m
[32m+[m[32mComprehensive MIME type mapping API based on mime-db module.[m
[32m+[m
[32m+[m[32m## Install[m
[32m+[m
[32m+[m[32mInstall with [npm](http://github.com/isaacs/npm):[m
[32m+[m
[32m+[m[32m    npm install mime[m
[32m+[m
[32m+[m[32m## Contributing / Testing[m
[32m+[m
[32m+[m[32m    npm run test[m
[32m+[m
[32m+[m[32m## Command Line[m
[32m+[m
[32m+[m[32m    mime [path_string][m
[32m+[m
[32m+[m[32mE.g.[m
[32m+[m
[32m+[m[32m    > mime scripts/jquery.js[m
[32m+[m[32m    application/javascript[m
[32m+[m
[32m+[m[32m## API - Queries[m
[32m+[m
[32m+[m[32m### mime.lookup(path)[m
[32m+[m[32mGet the mime type associated with a file, if no mime type is found `application/octet-stream` is returned. Performs a case-insensitive lookup using the extension in `path` (the substring after the last '/' or '.').  E.g.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar mime = require('mime');[m
[32m+[m
[32m+[m[32mmime.lookup('/path/to/file.txt');         // => 'text/plain'[m
[32m+[m[32mmime.lookup('file.txt');                  // => 'text/plain'[m
[32m+[m[32mmime.lookup('.TXT');                      // => 'text/plain'[m
[32m+[m[32mmime.lookup('htm');                       // => 'text/html'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.default_type[m
[32m+[m[32mSets the mime type returned when `mime.lookup` fails to find the extension searched for. (Default is `application/octet-stream`.)[m
[32m+[m
[32m+[m[32m### mime.extension(type)[m
[32m+[m[32mGet the default extension for `type`[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.extension('text/html');                 // => 'html'[m
[32m+[m[32mmime.extension('application/octet-stream');  // => 'bin'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.charsets.lookup()[m
[32m+[m
[32m+[m[32mMap mime-type to charset[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.charsets.lookup('text/plain');        // => 'UTF-8'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m(The logic for charset lookups is pretty rudimentary.  Feel free to suggest improvements.)[m
[32m+[m
[32m+[m[32m## API - Defining Custom Types[m
[32m+[m
[32m+[m[32mCustom type mappings can be added on a per-project basis via the following APIs.[m
[32m+[m
[32m+[m[32m### mime.define()[m
[32m+[m
[32m+[m[32mAdd custom mime/extension mappings[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.define({[m
[32m+[m[32m    'text/x-some-format': ['x-sf', 'x-sft', 'x-sfml'],[m
[32m+[m[32m    'application/x-my-type': ['x-mt', 'x-mtt'],[m
[32m+[m[32m    // etc ...[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mmime.lookup('x-sft');                 // => 'text/x-some-format'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mThe first entry in the extensions array is returned by `mime.extension()`. E.g.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.extension('text/x-some-format'); // => 'x-sf'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.load(filepath)[m
[32m+[m
[32m+[m[32mLoad mappings from an Apache ".types" format file[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.load('./my_project.types');[m
[32m+[m[32m```[m
[32m+[m[32mThe .types file format is simple -  See the `types` dir for examples.[m
[1mdiff --git a/node_modules/mime/cli.js b/node_modules/mime/cli.js[m
[1mnew file mode 100644[m
[1mindex 0000000..20b1ffe[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/cli.js[m
[36m@@ -0,0 +1,8 @@[m
[32m+[m[32m#!/usr/bin/env node[m
[32m+[m
[32m+[m[32mvar mime = require('./mime.js');[m
[32m+[m[32mvar file = process.argv[2];[m
[32m+[m[32mvar type = mime.lookup(file);[m
[32m+[m
[32m+[m[32mprocess.stdout.write(type + '\n');[m
[32m+[m
[1mdiff --git a/node_modules/mime/mime.js b/node_modules/mime/mime.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d7efbde[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/mime.js[m
[36m@@ -0,0 +1,108 @@[m
[32m+[m[32mvar path = require('path');[m
[32m+[m[32mvar fs = require('fs');[m
[32m+[m
[32m+[m[32mfunction Mime() {[m
[32m+[m[32m  // Map of extension -> mime type[m
[32m+[m[32m  this.types = Object.create(null);[m
[32m+[m
[32m+[m[32m  // Map of mime type -> extension[m
[32m+[m[32m  this.extensions = Object.create(null);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Define mimetype -> extension mappings.  Each key is a mime-type that maps[m
[32m+[m[32m * to an array of extensions associated with the type.  The first extension is[m
[32m+[m[32m * used as the default extension for the type.[m
[32m+[m[32m *[m
[32m+[m[32m * e.g. mime.define({'audio/ogg', ['oga', 'ogg', 'spx']});[m
[32m+[m[32m *[m
[32m+[m[32m * @param map (Object) type definitions[m
[32m+[m[32m */[m
[32m+[m[32mMime.prototype.define = function (map) {[m
[32m+[m[32m  for (var type in map) {[m
[32m+[m[32m    var exts = map[type];[m
[32m+[m[32m    for (var i = 0; i < exts.length; i++) {[m
[32m+[m[32m      if (process.env.DEBUG_MIME && this.types[exts[i]]) {[m
[32m+[m[32m        console.warn((this._loading || "define()").replace(/.*\//, ''), 'changes "' + exts[i] + '" extension type from ' +[m
[32m+[m[32m          this.types[exts[i]] + ' to ' + type);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.types[exts[i]] = type;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Default extension is the first one we encounter[m
[32m+[m[32m    if (!this.extensions[type]) {[m
[32m+[m[32m      this.extensions[type] = exts[0];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Load an Apache2-style ".types" file[m
[32m+[m[32m *[m
[32m+[m[32m * This may be called multiple times (it's expected).  Where files declare[m
[32m+[m[32m * overlapping types/extensions, the last file wins.[m
[32m+[m[32m *[m
[32m+[m[32m * @param file (String) path of file to load.[m
[32m+[m[32m */[m
[32m+[m[32mMime.prototype.load = function(file) {[m
[32m+[m[32m  this._loading = file;[m
[32m+[m[32m  // Read file and split into lines[m
[32m+[m[32m  var map = {},[m
[32m+[m[32m      content = fs.readFileSync(file, 'ascii'),[m
[32m+[m[32m      lines = content.split(/[\r\n]+/);[m
[32m+[m
[32m+[m[32m  lines.forEach(function(line) {[m
[32m+[m[32m    // Clean up whitespace/comments, and split into fields[m
[32m+[m[32m    var fields = line.replace(/\s*#.*|^\s*|\s*$/g, '').split(/\s+/);[m
[32m+[m[32m    map[fields.shift()] = fields;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  this.define(map);[m
[32m+[m
[32m+[m[32m  this._loading = null;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Lookup a mime type based on extension[m
[32m+[m[32m */[m
[32m+[m[32mMime.prototype.lookup = function(path, fallback) {[m
[32m+[m[32m  var ext = path.replace(/^.*[\.\/\\]/, '').toLowerCase();[m
[32m+[m
[32m+[m[32m  return this.types[ext] || fallback || this.default_type;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return file extension associated with a mime type[m
[32m+[m[32m */[m
[32m+[m[32mMime.prototype.extension = function(mimeType) {[m
[32m+[m[32m  var type = mimeType.match(/^\s*([^;\s]*)(?:;|\s|$)/)[1].toLowerCase();[m
[32m+[m[32m  return this.extensions[type];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Default instance[m
[32m+[m[32mvar mime = new Mime();[m
[32m+[m
[32m+[m[32m// Define built-in types[m
[32m+[m[32mmime.define(require('./types.json'));[m
[32m+[m
[32m+[m[32m// Default type[m
[32m+[m[32mmime.default_type = mime.lookup('bin');[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Additional API specific to the default instance[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mmime.Mime = Mime;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Lookup a charset based on mime type.[m
[32m+[m[32m */[m
[32m+[m[32mmime.charsets = {[m
[32m+[m[32m  lookup: function(mimeType, fallback) {[m
[32m+[m[32m    // Assume text types are utf8[m
[32m+[m[32m    return (/^text\/|^application\/(javascript|json)/).test(mimeType) ? 'UTF-8' : fallback;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = mime;[m
