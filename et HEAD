[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar bodyParser = require('body-parser')[m
[32m+[m[32mvar EventEmitter = require('events').EventEmitter;[m
[32m+[m[32mvar mixin = require('merge-descriptors');[m
[32m+[m[32mvar proto = require('./application');[m
[32m+[m[32mvar Route = require('./router/route');[m
[32m+[m[32mvar Router = require('./router');[m
[32m+[m[32mvar req = require('./request');[m
[32m+[m[32mvar res = require('./response');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose `createApplication()`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = createApplication;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an express application.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createApplication() {[m
[32m+[m[32m  var app = function(req, res, next) {[m
[32m+[m[32m    app.handle(req, res, next);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  mixin(app, EventEmitter.prototype, false);[m
[32m+[m[32m  mixin(app, proto, false);[m
[32m+[m
[32m+[m[32m  // expose the prototype that will get set on requests[m
[32m+[m[32m  app.request = Object.create(req, {[m
[32m+[m[32m    app: { configurable: true, enumerable: true, writable: true, value: app }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  // expose the prototype that will get set on responses[m
[32m+[m[32m  app.response = Object.create(res, {[m
[32m+[m[32m    app: { configurable: true, enumerable: true, writable: true, value: app }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  app.init();[m
[32m+[m[32m  return app;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose the prototypes.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.application = proto;[m
[32m+[m[32mexports.request = req;[m
[32m+[m[32mexports.response = res;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose constructors.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.Route = Route;[m
[32m+[m[32mexports.Router = Router;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose middleware[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.json = bodyParser.json[m
[32m+[m[32mexports.query = require('./middleware/query');[m
[32m+[m[32mexports.raw = bodyParser.raw[m
[32m+[m[32mexports.static = require('serve-static');[m
[32m+[m[32mexports.text = bodyParser.text[m
[32m+[m[32mexports.urlencoded = bodyParser.urlencoded[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Replace removed middleware with an appropriate error message.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar removedMiddlewares = [[m
[32m+[m[32m  'bodyParser',[m
[32m+[m[32m  'compress',[m
[32m+[m[32m  'cookieSession',[m
[32m+[m[32m  'session',[m
[32m+[m[32m  'logger',[m
[32m+[m[32m  'cookieParser',[m
[32m+[m[32m  'favicon',[m
[32m+[m[32m  'responseTime',[m
[32m+[m[32m  'errorHandler',[m
[32m+[m[32m  'timeout',[m
[32m+[m[32m  'methodOverride',[m
[32m+[m[32m  'vhost',[m
[32m+[m[32m  'csrf',[m
[32m+[m[32m  'directory',[m
[32m+[m[32m  'limit',[m
[32m+[m[32m  'multipart',[m
[32m+[m[32m  'staticCache'[m
[32m+[m[32m][m
[32m+[m
[32m+[m[32mremovedMiddlewares.forEach(function (name) {[m
[32m+[m[32m  Object.defineProperty(exports, name, {[m
[32m+[m[32m    get: function () {[m
[32m+[m[32m      throw new Error('Most middleware (like ' + name + ') is no longer bundled with Express and must be installed separately. Please see https://github.com/senchalabs/connect#middleware.');[m
[32m+[m[32m    },[m
[32m+[m[32m    configurable: true[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
