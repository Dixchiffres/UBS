[32m+[m
[32m+[m[32m[yarn]: https://yarnpkg.com/[m
[1mdiff --git a/node_modules/toidentifier/index.js b/node_modules/toidentifier/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..9295d02[m
[1m--- /dev/null[m
[1m+++ b/node_modules/toidentifier/index.js[m
[36m@@ -0,0 +1,32 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * toidentifier[m
[32m+[m[32m * Copyright(c) 2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = toIdentifier[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Trasform the given string into a JavaScript identifier[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction toIdentifier (str) {[m
[32m+[m[32m  return str[m
[32m+[m[32m    .split(' ')[m
[32m+[m[32m    .map(function (token) {[m
[32m+[m[32m      return token.slice(0, 1).toUpperCase() + token.slice(1)[m
[32m+[m[32m    })[m
[32m+[m[32m    .join('')[m
[32m+[m[32m    .replace(/[^ _0-9a-z]/gi, '')[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/toidentifier/package.json b/node_modules/toidentifier/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..42db1a6[m
[1m--- /dev/null[m
[1m+++ b/node_modules/toidentifier/package.json[m
[36m@@ -0,0 +1,38 @@[m
