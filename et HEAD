[32m+[m[32m(function(f){if(typeof exports==="object"&&typeof module!=="undefined"){module.exports=f()}else if(typeof define==="function"&&define.amd){define([],f)}else{var g;if(typeof window!=="undefined"){g=window}else if(typeof global!=="undefined"){g=global}else if(typeof self!=="undefined"){g=self}else{g=this}g.Qs = f()}})(function(){var define,module,exports;return (function(){function r(e,n,t){function o(i,f){if(!n[i]){if(!e[i]){var c="function"==typeof require&&require;if(!f&&c)return c(i,!0);if(u)return u(i,!0);var a=new Error("Cannot find module '"+i+"'");throw a.code="MODULE_NOT_FOUND",a}var p=n[i]={exports:{}};e[i][0].call(p.exports,function(r){var n=e[i][1][r];return o(n||r)},p,p.exports,r,e,n,t)}return n[i].exports}for(var u="function"==typeof require&&require,i=0;i<t.length;i++)o(t[i]);return o}return r})()({1:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar replace = String.prototype.replace;[m
[32m+[m[32mvar percentTwenties = /%20/g;[m
[32m+[m
[32m+[m[32mvar Format = {[m
[32m+[m[32m    RFC1738: 'RFC1738',[m
[32m+[m[32m    RFC3986: 'RFC3986'[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    'default': Format.RFC3986,[m
[32m+[m[32m    formatters: {[m
[32m+[m[32m        RFC1738: function (value) {[m
[32m+[m[32m            return replace.call(value, percentTwenties, '+');[m
[32m+[m[32m        },[m
[32m+[m[32m        RFC3986: function (value) {[m
[32m+[m[32m            return String(value);[m
[32m+[m[32m        }[m
[32m+[m[32m    },[m
[32m+[m[32m    RFC1738: Format.RFC1738,[m
[32m+[m[32m    RFC3986: Format.RFC3986[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{}],2:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar stringify = require('./stringify');[m
[32m+[m[32mvar parse = require('./parse');[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    formats: formats,[m
[32m+[m[32m    parse: parse,[m
[32m+[m[32m    stringify: stringify[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"./formats":1,"./parse":3,"./stringify":4}],3:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m[32mvar isArray = Array.isArray;[m
[32m+[m
[32m+[m[32mvar defaults = {[m
[32m+[m[32m    allowDots: false,[m
[32m+[m[32m    allowPrototypes: false,[m
[32m+[m[32m    allowSparse: false,[m
[32m+[m[32m    arrayLimit: 20,[m
[32m+[m[32m    charset: 'utf-8',[m
[32m+[m[32m    charsetSentinel: false,[m
[32m+[m[32m    comma: false,[m
[32m+[m[32m    decoder: utils.decode,[m
[32m+[m[32m    delimiter: '&',[m
[32m+[m[32m    depth: 5,[m
[32m+[m[32m    ignoreQueryPrefix: false,[m
[32m+[m[32m    interpretNumericEntities: false,[m
[32m+[m[32m    parameterLimit: 1000,[m
[32m+[m[32m    parseArrays: true,[m
[32m+[m[32m    plainObjects: false,[m
[32m+[m[32m    strictNullHandling: false[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar interpretNumericEntities = function (str) {[m
[32m+[m[32m    return str.replace(/&#(\d+);/g, function ($0, numberStr) {[m
[32m+[m[32m        return String.fromCharCode(parseInt(numberStr, 10));[m
[32m+[m[32m    });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseArrayValue = function (val, options) {[m
[32m+[m[32m    if (val && typeof val === 'string' && options.comma && val.indexOf(',') > -1) {[m
[32m+[m[32m        return val.split(',');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return val;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// This is what browsers will submit when the ✓ character occurs in an[m
[32m+[m[32m// application/x-www-form-urlencoded body and the encoding of the page containing[m
[32m+[m[32m// the form is iso-8859-1, or when the submitted form has an accept-charset[m
[32m+[m[32m// attribute of iso-8859-1. Presumably also with other charsets that do not contain[m
[32m+[m[32m// the ✓ character, such as us-ascii.[m
[32m+[m[32mvar isoSentinel = 'utf8=%26%2310003%3B'; // encodeURIComponent('&#10003;')[m
[32m+[m
[32m+[m[32m// These are the percent-encoded utf-8 octets representing a checkmark, indicating that the request actually is utf-8 encoded.[m
[32m+[m[32mvar charsetSentinel = 'utf8=%E2%9C%93'; // encodeURIComponent('✓')[m
[32m+[m
[32m+[m[32mvar parseValues = function parseQueryStringValues(str, options) {[m
[32m+[m[32m    var obj = {};[m
[32m+[m[32m    var cleanStr = options.ignoreQueryPrefix ? str.replace(/^\?/, '') : str;[m
[32m+[m[32m    var limit = options.parameterLimit === Infinity ? undefined : options.parameterLimit;[m
[32m+[m[32m    var parts = cleanStr.split(options.delimiter, limit);[m
[32m+[m[32m    var skipIndex = -1; // Keep track of where the utf8 sentinel was found[m
[32m+[m[32m    var i;[m
[32m+[m
[32m+[m[32m    var charset = options.charset;[m
[32m+[m[32m    if (options.charsetSentinel) {[m
[32m+[m[32m        for (i = 0; i < parts.length; ++i) {[m
[32m+[m[32m            if (parts[i].indexOf('utf8=') === 0) {[m
[32m+[m[32m                if (parts[i] === charsetSentinel) {[m
[32m+[m[32m                    charset = 'utf-8';[m
[32m+[m[32m                } else if (parts[i] === isoSentinel) {[m
[32m+[m[32m                    charset = 'iso-8859-1';[m
[32m+[m[32m                }[m
[32m+[m[32m                skipIndex = i;[m
[32m+[m[32m                i = parts.length; // The eslint settings do not allow break;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (i = 0; i < parts.length; ++i) {[m
[32m+[m[32m        if (i === skipIndex) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m[32m        var part = parts[i];[m
[32m+[m
[32m+[m[32m        var bracketEqualsPos = part.indexOf(']=');[m
[32m+[m[32m        var pos = bracketEqualsPos === -1 ? part.indexOf('=') : bracketEqualsPos + 1;[m
[32m+[m
[32m+[m[32m        var key, val;[m
[32m+[m[32m        if (pos === -1) {[m
[32m+[m[32m            key = options.decoder(part, defaults.decoder, charset, 'key');[m
[32m+[m[32m            val = options.strictNullHandling ? null : '';[m
[32m+[m[32m        } else {[m
[32m+[m[32m            key = options.decoder(part.slice(0, pos), defaults.decoder, charset, 'key');[m
[32m+[m[32m            val = utils.maybeMap([m
[32m+[m[32m                parseArrayValue(part.slice(pos + 1), options),[m
[32m+[m[32m                function (encodedVal) {[m
[32m+[m[32m                    return options.decoder(encodedVal, defaults.decoder, charset, 'value');[m
[32m+[m[32m                }[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (val && options.interpretNumericEntities && charset === 'iso-8859-1') {[m
[32m+[m[32m            val = interpretNumericEntities(val);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (part.indexOf('[]=') > -1) {[m
[32m+[m[32m            val = isArray(val) ? [val] : val;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (has.call(obj, key)) {[m
[32m+[m[32m            obj[key] = utils.combine(obj[key], val);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            obj[key] = val;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseObject = function (chain, val, options, valuesParsed) {[m
[32m+[m[32m    var leaf = valuesParsed ? val : parseArrayValue(val, options);[m
[32m+[m
[32m+[m[32m    for (var i = chain.length - 1; i >= 0; --i) {[m
[32m+[m[32m        var obj;[m
[32m+[m[32m        var root = chain[i];[m
[32m+[m
[32m+[m[32m        if (root === '[]' && options.parseArrays) {[m
[32m+[m[32m            obj = [].concat(leaf);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            obj = options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m            var cleanRoot = root.charAt(0) === '[' && root.charAt(root.length - 1) === ']' ? root.slice(1, -1) : root;[m
[32m+[m[32m            var index = parseInt(cleanRoot, 10);[m
[32m+[m[32m            if (!options.parseArrays && cleanRoot === '') {[m
[32m+[m[32m                obj = { 0: leaf };[m
[32m+[m[32m            } else if ([m
[32m+[m[32m                !isNaN(index)[m
[32m+[m[32m                && root !== cleanRoot[m
[32m+[m[32m                && String(index) === cleanRoot[m
[32m+[m[32m                && index >= 0[m
[32m+[m[32m                && (options.parseArrays && index <= options.arrayLimit)[m
[32m+[m[32m            ) {[m
[32m+[m[32m                obj = [];[m
[32m+[m[32m                obj[index] = leaf;[m
[32m+[m[32m            } else if (cleanRoot !== '__proto__') {[m
[32m+[m[32m                obj[cleanRoot] = leaf;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        leaf = obj;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return leaf;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseKeys = function parseQueryStringKeys(givenKey, val, options, valuesParsed) {[m
[32m+[m[32m    if (!givenKey) {[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Transform dot notation to bracket notation[m
[32m+[m[32m    var key = options.allowDots ? givenKey.replace(/\.([^.[]+)/g, '[$1]') : givenKey;[m
[32m+[m
[32m+[m[32m    // The regex chunks[m
[32m+[m
[32m+[m[32m    var brackets = /(\[[^[\]]*])/;[m
[32m+[m[32m    var child = /(\[[^[\]]*])/g;[m
[32m+[m
[32m+[m[32m    // Get the parent[m
[32m+[m
[32m+[m[32m    var segment = options.depth > 0 && brackets.exec(key);[m
[32m+[m[32m    var parent = segment ? key.slice(0, segment.index) : key;[m
[32m+[m
[32m+[m[32m    // Stash the parent if it exists[m
[32m+[m
[32m+[m[32m    var keys = [];[m
[32m+[m[32m    if (parent) {[m
[32m+[m[32m        // If we aren't using plain objects, optionally prefix keys that would overwrite object prototype properties[m
[32m+[m[32m        if (!options.plainObjects && has.call(Object.prototype, parent)) {[m
[32m+[m[32m            if (!options.allowPrototypes) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        keys.push(parent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Loop through children appending to the array until we hit depth[m
[32m+[m
[32m+[m[32m    var i = 0;[m
[32m+[m[32m    while (options.depth > 0 && (segment = child.exec(key)) !== null && i < options.depth) {[m
[32m+[m[32m        i += 1;[m
[32m+[m[32m        if (!options.plainObjects && has.call(Object.prototype, segment[1].slice(1, -1))) {[m
[32m+[m[32m            if (!options.allowPrototypes) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        keys.push(segment[1]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // If there's a remainder, just add whatever is left[m
[32m+[m
[32m+[m[32m    if (segment) {[m
[32m+[m[32m        keys.push('[' + key.slice(segment.index) + ']');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return parseObject(keys, val, options, valuesParsed);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar normalizeParseOptions = function normalizeParseOptions(opts) {[m
[32m+[m[32m    if (!opts) {[m
[32m+[m[32m        return defaults;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (opts.decoder !== null && opts.decoder !== undefined && typeof opts.decoder !== 'function') {[m
[32m+[m[32m        throw new TypeError('Decoder has to be a function.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof opts.charset !== 'undefined' && opts.charset !== 'utf-8' && opts.charset !== 'iso-8859-1') {[m
[32m+[m[32m        throw new TypeError('The charset option must be either utf-8, iso-8859-1, or undefined');[m
[32m+[m[32m    }[m
[32m+[m[32m    var charset = typeof opts.charset === 'undefined' ? defaults.charset : opts.charset;[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m        allowDots: typeof opts.allowDots === 'undefined' ? defaults.allowDots : !!opts.allowDots,[m
[32m+[m[32m        allowPrototypes: typeof opts.allowPrototypes === 'boolean' ? opts.allowPrototypes : defaults.allowPrototypes,[m
[32m+[m[32m        allowSparse: typeof opts.allowSparse === 'boolean' ? opts.allowSparse : defaults.allowSparse,[m
[32m+[m[32m        arrayLimit: typeof opts.arrayLimit === 'number' ? opts.arrayLimit : defaults.arrayLimit,[m
[32m+[m[32m        charset: charset,[m
[32m+[m[32m        charsetSentinel: typeof opts.charsetSentinel === 'boolean' ? opts.charsetSentinel : defaults.charsetSentinel,[m
[32m+[m[32m        comma: typeof opts.comma === 'boolean' ? opts.comma : defaults.comma,[m
[32m+[m[32m        decoder: typeof opts.decoder === 'function' ? opts.decoder : defaults.decoder,[m
[32m+[m[32m        delimiter: typeof opts.delimiter === 'string' || utils.isRegExp(opts.delimiter) ? opts.delimiter : defaults.delimiter,[m
[32m+[m[32m        // eslint-disable-next-line no-implicit-coercion, no-extra-parens[m
[32m+[m[32m        depth: (typeof opts.depth === 'number' || opts.depth === false) ? +opts.depth : defaults.depth,[m
[32m+[m[32m        ignoreQueryPrefix: opts.ignoreQueryPrefix === true,[m
[32m+[m[32m        interpretNumericEntities: typeof opts.interpretNumericEntities === 'boolean' ? opts.interpretNumericEntities : defaults.interpretNumericEntities,[m
[32m+[m[32m        parameterLimit: typeof opts.parameterLimit === 'number' ? opts.parameterLimit : defaults.parameterLimit,[m
[32m+[m[32m        parseArrays: opts.parseArrays !== false,[m
[32m+[m[32m        plainObjects: typeof opts.plainObjects === 'boolean' ? opts.plainObjects : defaults.plainObjects,[m
[32m+[m[32m        strictNullHandling: typeof opts.strictNullHandling === 'boolean' ? opts.strictNullHandling : defaults.strictNullHandling[m
[32m+[m[32m    };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function (str, opts) {[m
[32m+[m[32m    var options = normalizeParseOptions(opts);[m
[32m+[m
[32m+[m[32m    if (str === '' || str === null || typeof str === 'undefined') {[m
[32m+[m[32m        return options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var tempObj = typeof str === 'string' ? parseValues(str, options) : str;[m
[32m+[m[32m    var obj = options.plainObjects ? Object.create(null) : {};[m
[32m+[m
[32m+[m[32m    // Iterate over the keys and setup the new object[m
[32m+[m
[32m+[m[32m    var keys = Object.keys(tempObj);[m
[32m+[m[32m    for (var i = 0; i < keys.length; ++i) {[m
[32m+[m[32m        var key = keys[i];[m
[32m+[m[32m        var newObj = parseKeys(key, tempObj[key], options, typeof str === 'string');[m
[32m+[m[32m        obj = utils.merge(obj, newObj, options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (options.allowSparse === true) {[m
[32m+[m[32m        return obj;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return utils.compact(obj);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"./utils":5}],4:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar getSideChannel = require('side-channel');[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m
[32m+[m[32mvar arrayPrefixGenerators = {[m
[32m+[m[32m    brackets: function brackets(prefix) {[m
[32m+[m[32m        return prefix + '[]';[m
[32m+[m[32m    },[m
[32m+[m[32m    comma: 'comma',[m
[32m+[m[32m    indices: function indices(prefix, key) {[m
[32m+[m[32m        return prefix + '[' + key + ']';[m
[32m+[m[32m    },[m
[32m+[m[32m    repeat: function repeat(prefix) {[m
[32m+[m[32m        return prefix;[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isArray = Array.isArray;[m
[32m+[m[32mvar split = String.prototype.split;[m
[32m+[m[32mvar push = Array.prototype.push;[m
[32m+[m[32mvar pushToArray = function (arr, valueOrArray) {[m
[32m+[m[32m    push.apply(arr, isArray(valueOrArray) ? valueOrArray : [valueOrArray]);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar toISO = Date.prototype.toISOString;[m
[32m+[m
[32m+[m[32mvar defaultFormat = formats['default'];[m
[32m+[m[32mvar defaults = {[m
[32m+[m[32m    addQueryPrefix: false,[m
[32m+[m[32m    allowDots: false,[m
[32m+[m[32m    charset: 'utf-8',[m
[32m+[m[32m    charsetSentinel: false,[m
[32m+[m[32m    delimiter: '&',[m
[32m+[m[32m    encode: true,[m
[32m+[m[32m    encoder: utils.encode,[m
[32m+[m[32m    encodeValuesOnly: false,[m
[32m+[m[32m    format: defaultFormat,[m
[32m+[m[32m    formatter: formats.formatters[defaultFormat],[m
[32m+[m[32m    // deprecated[m
[32m+[m[32m    indices: false,[m
[32m+[m[32m    serializeDate: function serializeDate(date) {[m
[32m+[m[32m        return toISO.call(date);[m
[32m+[m[32m    },[m
[32m+[m[32m    skipNulls: false,[m
[32m+[m[32m    strictNullHandling: false[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isNonNullishPrimitive = function isNonNullishPrimitive(v) {[m
[32m+[m[32m    return typeof v === 'string'[m
[32m+[m[32m        || typeof v === 'number'[m
[32m+[m[32m        || typeof v === 'boolean'[m
[32m+[m[32m        || typeof v === 'symbol'[m
[32m+[m[32m        || typeof v === 'bigint';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar sentinel = {};[m
[32m+[m
[32m+[m[32mvar stringify = function stringify([m
[32m+[m[32m    object,[m
[32m+[m[32m    prefix,[m
[32m+[m[32m    generateArrayPrefix,[m
[32m+[m[32m    commaRoundTrip,[m
[32m+[m[32m    strictNullHandling,[m
[32m+[m[32m    skipNulls,[m
[32m+[m[32m    encoder,[m
[32m+[m[32m    filter,[m
[32m+[m[32m    sort,[m
[32m+[m[32m    allowDots,[m
[32m+[m[32m    serializeDate,[m
[32m+[m[32m    format,[m
[32m+[m[32m    formatter,[m
[32m+[m[32m    encodeValuesOnly,[m
[32m+[m[32m    charset,[m
[32m+[m[32m    sideChannel[m
[32m+[m[32m) {[m
[32m+[m[32m    var obj = object;[m
[32m+[m
[32m+[m[32m    var tmpSc = sideChannel;[m
[32m+[m[32m    var step = 0;[m
[32m+[m[32m    var findFlag = false;[m
[32m+[m[32m    while ((tmpSc = tmpSc.get(sentinel)) !== void undefined && !findFlag) {[m
[32m+[m[32m        // Where object last appeared in the ref tree[m
[32m+[m[32m        var pos = tmpSc.get(object);[m
[32m+[m[32m        step += 1;[m
[32m+[m[32m        if (typeof pos !== 'undefined') {[m
[32m+[m[32m            if (pos === step) {[m
[32m+[m[32m                throw new RangeError('Cyclic object value');[m
[32m+[m[32m            } else {[m
[32m+[m[32m                findFlag = true; // Break while[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        if (typeof tmpSc.get(sentinel) === 'undefined') {[m
[32m+[m[32m            step = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof filter === 'function') {[m
[32m+[m[32m        obj = filter(prefix, obj);[m
[32m+[m[32m    } else if (obj instanceof Date) {[m
[32m+[m[32m        obj = serializeDate(obj);[m
[32m+[m[32m    } else if (generateArrayPrefix === 'comma' && isArray(obj)) {[m
[32m+[m[32m        obj = utils.maybeMap(obj, function (value) {[m
[32m+[m[32m            if (value instanceof Date) {[m
[32m+[m[32m                return serializeDate(value);[m
[32m+[m[32m            }[m
[32m+[m[32m            return value;[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (obj === null) {[m
[32m+[m[32m        if (strictNullHandling) {[m
[32m+[m[32m            return encoder && !encodeValuesOnly ? encoder(prefix, defaults.encoder, charset, 'key', format) : prefix;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        obj = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (isNonNullishPrimitive(obj) || utils.isBuffer(obj)) {[m
[32m+[m[32m        if (encoder) {[m
[32m+[m[32m            var keyValue = encodeValuesOnly ? prefix : encoder(prefix, defaults.encoder, charset, 'key', format);[m
[32m+[m[32m            if (generateArrayPrefix === 'comma' && encodeValuesOnly) {[m
[32m+[m[32m                var valuesArray = split.call(String(obj), ',');[m
[32m+[m[32m                var valuesJoined = '';[m
[32m+[m[32m                for (var i = 0; i < valuesArray.length; ++i) {[m
[32m+[m[32m                    valuesJoined += (i === 0 ? '' : ',') + formatter(encoder(valuesArray[i], defaults.encoder, charset, 'value', format));[m
[32m+[m[32m                }[m
[32m+[m[32m                return [formatter(keyValue) + (commaRoundTrip && isArray(obj) && valuesArray.length === 1 ? '[]' : '') + '=' + valuesJoined];[m
[32m+[m[32m            }[m
[32m+[m[32m            return [formatter(keyValue) + '=' + formatter(encoder(obj, defaults.encoder, charset, 'value', format))];[m
[32m+[m[32m        }[m
[32m+[m[32m        return [formatter(prefix) + '=' + formatter(String(obj))];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var values = [];[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'undefined') {[m
[32m+[m[32m        return values;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var objKeys;[m
[32m+[m[32m    if (generateArrayPrefix === 'comma' && isArray(obj)) {[m
[32m+[m[32m        // we need to join elements in[m
[32m+[m[32m        objKeys = [{ value: obj.length > 0 ? obj.join(',') || null : void undefined }];[m
[32m+[m[32m    } else if (isArray(filter)) {[m
[32m+[m[32m        objKeys = filter;[m
[32m+[m[32m    } else {[m
[32m+[m[32m        var keys = Object.keys(obj);[m
[32m+[m[32m        objKeys = sort ? keys.sort(sort) : keys;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var adjustedPrefix = commaRoundTrip && isArray(obj) && obj.length === 1 ? prefix + '[]' : prefix;[m
[32m+[m
[32m+[m[32m    for (var j = 0; j < objKeys.length; ++j) {[m
[32m+[m[32m        var key = objKeys[j];[m
[32m+[m[32m        var value = typeof key === 'object' && typeof key.value !== 'undefined' ? key.value : obj[key];[m
[32m+[m
[32m+[m[32m        if (skipNulls && value === null) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var keyPrefix = isArray(obj)[m
[32m+[m[32m            ? typeof generateArrayPrefix === 'function' ? generateArrayPrefix(adjustedPrefix, key) : adjustedPrefix[m
[32m+[m[32m            : adjustedPrefix + (allowDots ? '.' + key : '[' + key + ']');[m
[32m+[m
[32m+[m[32m        sideChannel.set(object, step);[m
[32m+[m[32m        var valueSideChannel = getSideChannel();[m
[32m+[m[32m        valueSideChannel.set(sentinel, sideChannel);[m
[32m+[m[32m        pushToArray(values, stringify([m
[32m+[m[32m            value,[m
[32m+[m[32m            keyPrefix,[m
[32m+[m[32m            generateArrayPrefix,[m
[32m+[m[32m            commaRoundTrip,[m
[32m+[m[32m            strictNullHandling,[m
[32m+[m[32m            skipNulls,[m
[32m+[m[32m            encoder,[m
[32m+[m[32m            filter,[m
[32m+[m[32m            sort,[m
[32m+[m[32m            allowDots,[m
[32m+[m[32m            serializeDate,[m
[32m+[m[32m            format,[m
[32m+[m[32m            formatter,[m
[32m+[m[32m            encodeValuesOnly,[m
[32m+[m[32m            charset,[m
[32m+[m[32m            valueSideChannel[m
[32m+[m[32m        ));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return values;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar normalizeStringifyOptions = function normalizeStringifyOptions(opts) {[m
[32m+[m[32m    if (!opts) {[m
[32m+[m[32m        return defaults;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (opts.encoder !== null && typeof opts.encoder !== 'undefined' && typeof opts.encoder !== 'function') {[m
[32m+[m[32m        throw new TypeError('Encoder has to be a function.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var charset = opts.charset || defaults.charset;[m
[32m+[m[32m    if (typeof opts.charset !== 'undefined' && opts.charset !== 'utf-8' && opts.charset !== 'iso-8859-1') {[m
[32m+[m[32m        throw new TypeError('The charset option must be either utf-8, iso-8859-1, or undefined');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var format = formats['default'];[m
[32m+[m[32m    if (typeof opts.format !== 'undefined') {[m
[32m+[m[32m        if (!has.call(formats.formatters, opts.format)) {[m
[32m+[m[32m            throw new TypeError('Unknown format option provided.');[m
[32m+[m[32m        }[m
[32m+[m[32m        format = opts.format;[m
[32m+[m[32m    }[m
[32m+[m[32m    var formatter = formats.formatters[format];[m
[32m+[m
[32m+[m[32m    var filter = defaults.filter;[m
[32m+[m[32m    if (typeof opts.filter === 'function' || isArray(opts.filter)) {[m
[32m+[m[32m        filter = opts.filter;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m        addQueryPrefix: typeof opts.addQueryPrefix === 'boolean' ? opts.addQueryPrefix : defaults.addQueryPrefix,[m
[32m+[m[32m        allowDots: typeof opts.allowDots === 'undefined' ? defaults.allowDots : !!opts.allowDots,[m
[32m+[m[32m        charset: charset,[m
[32m+[m[32m        charsetSentinel: typeof opts.charsetSentinel === 'boolean' ? opts.charsetSentinel : defaults.charsetSentinel,[m
[32m+[m[32m        delimiter: typeof opts.delimiter === 'undefined' ? defaults.delimiter : opts.delimiter,[m
[32m+[m[32m        encode: typeof opts.encode === 'boolean' ? opts.encode : defaults.encode,[m
[32m+[m[32m        encoder: typeof opts.encoder === 'function' ? opts.encoder : defaults.encoder,[m
[32m+[m[32m        encodeValuesOnly: typeof opts.encodeValuesOnly === 'boolean' ? opts.encodeValuesOnly : defaults.encodeValuesOnly,[m
[32m+[m[32m        filter: filter,[m
[32m+[m[32m        format: format,[m
[32m+[m[32m        formatter: formatter,[m
[32m+[m[32m        serializeDate: typeof opts.serializeDate === 'function' ? opts.serializeDate : defaults.serializeDate,[m
[32m+[m[32m        skipNulls: typeof opts.skipNulls === 'boolean' ? opts.skipNulls : defaults.skipNulls,[m
[32m+[m[32m        sort: typeof opts.sort === 'function' ? opts.sort : null,[m
[32m+[m[32m        strictNullHandling: typeof opts.strictNullHandling === 'boolean' ? opts.strictNullHandling : defaults.strictNullHandling[m
[32m+[m[32m    };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function (object, opts) {[m
[32m+[m[32m    var obj = object;[m
[32m+[m[32m    var options = normalizeStringifyOptions(opts);[m
[32m+[m
[32m+[m[32m    var objKeys;[m
[32m+[m[32m    var filter;[m
[32m+[m
[32m+[m[32m    if (typeof options.filter === 'function') {[m
[32m+[m[32m        filter = options.filter;[m
[32m+[m[32m        obj = filter('', obj);[m
[32m+[m[32m    } else if (isArray(options.filter)) {[m
[32m+[m[32m        filter = options.filter;[m
[32m+[m[32m        objKeys = filter;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var keys = [];[m
[32m+[m
[32m+[m[32m    if (typeof obj !== 'object' || obj === null) {[m
[32m+[m[32m        return '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var arrayFormat;[m
[32m+[m[32m    if (opts && opts.arrayFormat in arrayPrefixGenerators) {[m
[32m+[m[32m        arrayFormat = opts.arrayFormat;[m
[32m+[m[32m    } else if (opts && 'indices' in opts) {[m
[32m+[m[32m        arrayFormat = opts.indices ? 'indices' : 'repeat';[m
[32m+[m[32m    } else {[m
[32m+[m[32m        arrayFormat = 'indices';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var generateArrayPrefix = arrayPrefixGenerators[arrayFormat];[m
[32m+[m[32m    if (opts && 'commaRoundTrip' in opts && typeof opts.commaRoundTrip !== 'boolean') {[m
[32m+[m[32m        throw new TypeError('`commaRoundTrip` must be a boolean, or absent');[m
[32m+[m[32m    }[m
[32m+[m[32m    var commaRoundTrip = generateArrayPrefix === 'comma' && opts && opts.commaRoundTrip;[m
[32m+[m
[32m+[m[32m    if (!objKeys) {[m
[32m+[m[32m        objKeys = Object.keys(obj);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (options.sort) {[m
[32m+[m[32m        objKeys.sort(options.sort);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var sideChannel = getSideChannel();[m
[32m+[m[32m    for (var i = 0; i < objKeys.length; ++i) {[m
[32m+[m[32m        var key = objKeys[i];[m
[32m+[m
[32m+[m[32m        if (options.skipNulls && obj[key] === null) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m[32m        pushToArray(keys, stringify([m
[32m+[m[32m            obj[key],[m
[32m+[m[32m            key,[m
[32m+[m[32m            generateArrayPrefix,[m
[32m+[m[32m            commaRoundTrip,[m
[32m+[m[32m            options.strictNullHandling,[m
[32m+[m[32m            options.skipNulls,[m
[32m+[m[32m            options.encode ? options.encoder : null,[m
[32m+[m[32m            options.filter,[m
[32m+[m[32m            options.sort,[m
[32m+[m[32m            options.allowDots,[m
[32m+[m[32m            options.serializeDate,[m
[32m+[m[32m            options.format,[m
[32m+[m[32m            options.formatter,[m
[32m+[m[32m            options.encodeValuesOnly,[m
[32m+[m[32m            options.charset,[m
[32m+[m[32m            sideChannel[m
[32m+[m[32m        ));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var joined = keys.join(options.delimiter);[m
[32m+[m[32m    var prefix = options.addQueryPrefix === true ? '?' : '';[m
[32m+[m
[32m+[m[32m    if (options.charsetSentinel) {[m
[32m+[m[32m        if (options.charset === 'iso-8859-1') {[m
[32m+[m[32m            // encodeURIComponent('&#10003;'), the "numeric entity" representation of a checkmark[m
[32m+[m[32m            prefix += 'utf8=%26%2310003%3B&';[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // encodeURIComponent('✓')[m
[32m+[m[32m            prefix += 'utf8=%E2%9C%93&';[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return joined.length > 0 ? prefix + joined : '';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"./formats":1,"./utils":5,"side-channel":16}],5:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m[32mvar isArray = Array.isArray;[m
[32m+[m
[32m+[m[32mvar hexTable = (function () {[m
[32m+[m[32m    var array = [];[m
[32m+[m[32m    for (var i = 0; i < 256; ++i) {[m
[32m+[m[32m        array.push('%' + ((i < 16 ? '0' : '') + i.toString(16)).toUpperCase());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return array;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32mvar compactQueue = function compactQueue(queue) {[m
[32m+[m[32m    while (queue.length > 1) {[m
[32m+[m[32m        var item = queue.pop();[m
[32m+[m[32m        var obj = item.obj[item.prop];[m
[32m+[m
[32m+[m[32m        if (isArray(obj)) {[m
[32m+[m[32m            var compacted = [];[m
[32m+[m
[32m+[m[32m            for (var j = 0; j < obj.length; ++j) {[m
[32m+[m[32m                if (typeof obj[j] !== 'undefined') {[m
[32m+[m[32m                    compacted.push(obj[j]);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            item.obj[item.prop] = compacted;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar arrayToObject = function arrayToObject(source, options) {[m
[32m+[m[32m    var obj = options && options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m    for (var i = 0; i < source.length; ++i) {[m
[32m+[m[32m        if (typeof source[i] !== 'undefined') {[m
[32m+[m[32m            obj[i] = source[i];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar merge = function merge(target, source, options) {[m
[32m+[m[32m    /* eslint no-param-reassign: 0 */[m
[32m+[m[32m    if (!source) {[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof source !== 'object') {[m
[32m+[m[32m        if (isArray(target)) {[m
[32m+[m[32m            target.push(source);[m
[32m+[m[32m        } else if (target && typeof target === 'object') {[m
[32m+[m[32m            if ((options && (options.plainObjects || options.allowPrototypes)) || !has.call(Object.prototype, source)) {[m
[32m+[m[32m                target[source] = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return [target, source];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!target || typeof target !== 'object') {[m
[32m+[m[32m        return [target].concat(source);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var mergeTarget = target;[m
[32m+[m[32m    if (isArray(target) && !isArray(source)) {[m
[32m+[m[32m        mergeTarget = arrayToObject(target, options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (isArray(target) && isArray(source)) {[m
[32m+[m[32m        source.forEach(function (item, i) {[m
[32m+[m[32m            if (has.call(target, i)) {[m
[32m+[m[32m                var targetItem = target[i];[m
[32m+[m[32m                if (targetItem && typeof targetItem === 'object' && item && typeof item === 'object') {[m
[32m+[m[32m                    target[i] = merge(targetItem, item, options);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    target.push(item);[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                target[i] = item;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        var value = source[key];[m
[32m+[m
[32m+[m[32m        if (has.call(acc, key)) {[m
[32m+[m[32m            acc[key] = merge(acc[key], value, options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            acc[key] = value;[m
[32m+[m[32m        }[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, mergeTarget);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar assign = function assignSingleSource(target, source) {[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        acc[key] = source[key];[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, target);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar decode = function (str, decoder, charset) {[m
[32m+[m[32m    var strWithoutPlus = str.replace(/\+/g, ' ');[m
[32m+[m[32m    if (charset === 'iso-8859-1') {[m
[32m+[m[32m        // unescape never throws, no try...catch needed:[m
[32m+[m[32m        return strWithoutPlus.replace(/%[0-9a-f]{2}/gi, unescape);[m
[32m+[m[32m    }[m
[32m+[m[32m    // utf-8[m
[32m+[m[32m    try {[m
[32m+[m[32m        return decodeURIComponent(strWithoutPlus);[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m        return strWithoutPlus;[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar encode = function encode(str, defaultEncoder, charset, kind, format) {[m
[32m+[m[32m    // This code was originally written by Brian White (mscdex) for the io.js core querystring library.[m
[32m+[m[32m    // It has been adapted here for stricter adherence to RFC 3986[m
[32m+[m[32m    if (str.length === 0) {[m
[32m+[m[32m        return str;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var string = str;[m
[32m+[m[32m    if (typeof str === 'symbol') {[m
[32m+[m[32m        string = Symbol.prototype.toString.call(str);[m
[32m+[m[32m    } else if (typeof str !== 'string') {[m
[32m+[m[32m        string = String(str);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (charset === 'iso-8859-1') {[m
[32m+[m[32m        return escape(string).replace(/%u[0-9a-f]{4}/gi, function ($0) {[m
[32m+[m[32m            return '%26%23' + parseInt($0.slice(2), 16) + '%3B';[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var out = '';[m
[32m+[m[32m    for (var i = 0; i < string.length; ++i) {[m
[32m+[m[32m        var c = string.charCodeAt(i);[m
[32m+[m
[32m+[m[32m        if ([m
[32m+[m[32m            c === 0x2D // -[m
[32m+[m[32m            || c === 0x2E // .[m
[32m+[m[32m            || c === 0x5F // _[m
[32m+[m[32m            || c === 0x7E // ~[m
[32m+[m[32m            || (c >= 0x30 && c <= 0x39) // 0-9[m
[32m+[m[32m            || (c >= 0x41 && c <= 0x5A) // a-z[m
[32m+[m[32m            || (c >= 0x61 && c <= 0x7A) // A-Z[m
[32m+[m[32m            || (format === formats.RFC1738 && (c === 0x28 || c === 0x29)) // ( )[m
[32m+[m[32m        ) {[m
[32m+[m[32m            out += string.charAt(i);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x80) {[m
[32m+[m[32m            out = out + hexTable[c];[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x800) {[m
[32m+[m[32m            out = out + (hexTable[0xC0 | (c >> 6)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0xD800 || c >= 0xE000) {[m
[32m+[m[32m            out = out + (hexTable[0xE0 | (c >> 12)] + hexTable[0x80 | ((c >> 6) & 0x3F)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        i += 1;[m
[32m+[m[32m        c = 0x10000 + (((c & 0x3FF) << 10) | (string.charCodeAt(i) & 0x3FF));[m
[32m+[m[32m        /* eslint operator-linebreak: [2, "before"] */[m
[32m+[m[32m        out += hexTable[0xF0 | (c >> 18)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 12) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 6) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | (c & 0x3F)];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return out;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar compact = function compact(value) {[m
[32m+[m[32m    var queue = [{ obj: { o: value }, prop: 'o' }];[m
[32m+[m[32m    var refs = [];[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < queue.length; ++i) {[m
[32m+[m[32m        var item = queue[i];[m
[32m+[m[32m        var obj = item.obj[item.prop];[m
[32m+[m
[32m+[m[32m        var keys = Object.keys(obj);[m
[32m+[m[32m        for (var j = 0; j < keys.length; ++j) {[m
[32m+[m[32m            var key = keys[j];[m
[32m+[m[32m            var val = obj[key];[m
[32m+[m[32m            if (typeof val === 'object' && val !== null && refs.indexOf(val) === -1) {[m
[32m+[m[32m                queue.push({ obj: obj, prop: key });[m
[32m+[m[32m                refs.push(val);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    compactQueue(queue);[m
[32m+[m
[32m+[m[32m    return value;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isRegExp = function isRegExp(obj) {[m
[32m+[m[32m    return Object.prototype.toString.call(obj) === '[object RegExp]';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isBuffer = function isBuffer(obj) {[m
[32m+[m[32m    if (!obj || typeof obj !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return !!(obj.constructor && obj.constructor.isBuffer && obj.constructor.isBuffer(obj));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar combine = function combine(a, b) {[m
[32m+[m[32m    return [].concat(a, b);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar maybeMap = function maybeMap(val, fn) {[m
[32m+[m[32m    if (isArray(val)) {[m
[32m+[m[32m        var mapped = [];[m
[32m+[m[32m        for (var i = 0; i < val.length; i += 1) {[m
[32m+[m[32m            mapped.push(fn(val[i]));[m
[32m+[m[32m        }[m
[32m+[m[32m        return mapped;[m
[32m+[m[32m    }[m
[32m+[m[32m    return fn(val);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    arrayToObject: arrayToObject,[m
[32m+[m[32m    assign: assign,[m
[32m+[m[32m    combine: combine,[m
[32m+[m[32m    compact: compact,[m
[32m+[m[32m    decode: decode,[m
[32m+[m[32m    encode: encode,[m
[32m+[m[32m    isBuffer: isBuffer,[m
[32m+[m[32m    isRegExp: isRegExp,[m
[32m+[m[32m    maybeMap: maybeMap,[m
[32m+[m[32m    merge: merge[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"./formats":1}],6:[function(require,module,exports){[m
[32m+[m
[32m+[m[32m},{}],7:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m
[32m+[m[32mvar callBind = require('./');[m
[32m+[m
[32m+[m[32mvar $indexOf = callBind(GetIntrinsic('String.prototype.indexOf'));[m
[32m+[m
[32m+[m[32mmodule.exports = function callBoundIntrinsic(name, allowMissing) {[m
[32m+[m	[32mvar intrinsic = GetIntrinsic(name, !!allowMissing);[m
[32m+[m	[32mif (typeof intrinsic === 'function' && $indexOf(name, '.prototype.') > -1) {[m
[32m+[m		[32mreturn callBind(intrinsic);[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn intrinsic;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"./":8,"get-intrinsic":11}],8:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m
[32m+[m[32mvar $apply = GetIntrinsic('%Function.prototype.apply%');[m
[32m+[m[32mvar $call = GetIntrinsic('%Function.prototype.call%');[m
[32m+[m[32mvar $reflectApply = GetIntrinsic('%Reflect.apply%', true) || bind.call($call, $apply);[m
[32m+[m
[32m+[m[32mvar $gOPD = GetIntrinsic('%Object.getOwnPropertyDescriptor%', true);[m
[32m+[m[32mvar $defineProperty = GetIntrinsic('%Object.defineProperty%', true);[m
[32m+[m[32mvar $max = GetIntrinsic('%Math.max%');[m
[32m+[m
[32m+[m[32mif ($defineProperty) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32m$defineProperty({}, 'a', { value: 1 });[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32m// IE 8 has a broken defineProperty[m
[32m+[m		[32m$defineProperty = null;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = function callBind(originalFunction) {[m
[32m+[m	[32mvar func = $reflectApply(bind, $call, arguments);[m
[32m+[m	[32mif ($gOPD && $defineProperty) {[m
[32m+[m		[32mvar desc = $gOPD(func, 'length');[m
[32m+[m		[32mif (desc.configurable) {[m
[32m+[m			[32m// original length, plus the receiver, minus any additional arguments (after the receiver)[m
[32m+[m			[32m$defineProperty([m
[32m+[m				[32mfunc,[m
[32m+[m				[32m'length',[m
[32m+[m				[32m{ value: 1 + $max(0, originalFunction.length - (arguments.length - 1)) }[m
[32m+[m			[32m);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn func;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar applyBind = function applyBind() {[m
[32m+[m	[32mreturn $reflectApply(bind, $apply, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mif ($defineProperty) {[m
[32m+[m	[32m$defineProperty(module.exports, 'apply', { value: applyBind });[m
[32m+[m[32m} else {[m
[32m+[m	[32mmodule.exports.apply = applyBind;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m},{"function-bind":10,"get-intrinsic":11}],9:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/* eslint no-invalid-this: 1 */[m
[32m+[m
[32m+[m[32mvar ERROR_MESSAGE = 'Function.prototype.bind called on incompatible ';[m
[32m+[m[32mvar slice = Array.prototype.slice;[m
[32m+[m[32mvar toStr = Object.prototype.toString;[m
[32m+[m[32mvar funcType = '[object Function]';[m
[32m+[m
[32m+[m[32mmodule.exports = function bind(that) {[m
[32m+[m[32m    var target = this;[m
[32m+[m[32m    if (typeof target !== 'function' || toStr.call(target) !== funcType) {[m
[32m+[m[32m        throw new TypeError(ERROR_MESSAGE + target);[m
[32m+[m[32m    }[m
[32m+[m[32m    var args = slice.call(arguments, 1);[m
[32m+[m
[32m+[m[32m    var bound;[m
[32m+[m[32m    var binder = function () {[m
[32m+[m[32m        if (this instanceof bound) {[m
[32m+[m[32m            var result = target.apply([m
[32m+[m[32m                this,[m
[32m+[m[32m                args.concat(slice.call(arguments))[m
[32m+[m[32m            );[m
[32m+[m[32m            if (Object(result) === result) {[m
[32m+[m[32m                return result;[m
[32m+[m[32m            }[m
[32m+[m[32m            return this;[m
[32m+[m[32m        } else {[m
