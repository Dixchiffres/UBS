[32m+[m[32m    "index.js"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "scripts": {[m
[32m+[m[32m    "lint": "eslint .",[m
[32m+[m[32m    "test": "mocha --reporter spec --bail --check-leaks test/",[m
[32m+[m[32m    "test-ci": "nyc --reporter=lcovonly --reporter=text npm test",[m
[32m+[m[32m    "test-cov": "nyc --reporter=html --reporter=text npm test"[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
