[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32m'in loose mode, obj still adds property 2'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mdefineDataProperty(obj, 'added', 'added value 3', false, false, true, true);[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mgetOwnPropertyDescriptors(obj),[m
