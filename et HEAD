[32m+[m[32mdeclare function hasProto(): boolean;[m
[32m+[m
[32m+[m[32mexport = hasProto;[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/has-proto/index.js b/node_modules/has-proto/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d3c8a0a[m
[1m--- /dev/null[m
[1m+++ b/node_modules/has-proto/index.js[m
[36m@@ -0,0 +1,15 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = {[m
[32m+[m	[32m__proto__: null,[m
[32m+[m	[32mfoo: {}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $Object = Object;[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mmodule.exports = function hasProto() {[m
[32m+[m	[32m// @ts-expect-error: TS errors on an inherited property for some reason[m
[32m+[m	[32mreturn { __proto__: test }.foo === test.foo[m
[32m+[m		[32m&& !(test instanceof $Object);[m
[32m+[m[32m};[m
