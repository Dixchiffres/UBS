[32m+[m[32m# Security Policies and Procedures[m
[32m+[m
[32m+[m[32m## Reporting a Bug[m
[32m+[m
[32m+[m[32mThe `raw-body` team and community take all security bugs seriously. Thank you[m
[32m+[m[32mfor improving the security of Express. We appreciate your efforts and[m
[32m+[m[32mresponsible disclosure and will make every effort to acknowledge your[m
[32m+[m[32mcontributions.[m
[32m+[m
[32m+[m[32mReport security bugs by emailing the current owners of `raw-body`. This information[m
[32m+[m[32mcan be found in the npm registry using the command `npm owner ls raw-body`.[m
[32m+[m[32mIf unsure or unable to get the information from the above, open an issue[m
[32m+[m[32min the [project issue tracker](https://github.com/stream-utils/raw-body/issues)[m
[32m+[m[32masking for the current contact information.[m
[32m+[m
[32m+[m[32mTo ensure the timely response to your report, please ensure that the entirety[m
[32m+[m[32mof the report is contained within the email body and not solely behind a web[m
[32m+[m[32mlink or an attachment.[m
[32m+[m
[32m+[m[32mAt least one owner will acknowledge your email within 48 hours, and will send a[m
[32m+[m[32mmore detailed response within 48 hours indicating the next steps in handling[m
[32m+[m[32myour report. After the initial reply to your report, the owners will[m
[32m+[m[32mendeavor to keep you informed of the progress towards a fix and full[m
[32m+[m[32mannouncement, and may ask for additional information or guidance.[m
[1mdiff --git a/node_modules/raw-body/index.d.ts b/node_modules/raw-body/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..dcbbebd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/raw-body/index.d.ts[m
[36m@@ -0,0 +1,87 @@[m
[32m+[m[32mimport { Readable } from 'stream';[m
[32m+[m
[32m+[m[32mdeclare namespace getRawBody {[m
[32m+[m[32m  export type Encoding = string | true;[m
[32m+[m
[32m+[m[32m  export interface Options {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The expected length of the stream.[m
[32m+[m[32m     */[m
[32m+[m[32m    length?: number | string | null;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The byte limit of the body. This is the number of bytes or any string[m
[32m+[m[32m     * format supported by `bytes`, for example `1000`, `'500kb'` or `'3mb'`.[m
[32m+[m[32m     */[m
[32m+[m[32m    limit?: number | string | null;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The encoding to use to decode the body into a string. By default, a[m
[32m+[m[32m     * `Buffer` instance will be returned when no encoding is specified. Most[m
[32m+[m[32m     * likely, you want `utf-8`, so setting encoding to `true` will decode as[m
[32m+[m[32m     * `utf-8`. You can use any type of encoding supported by `iconv-lite`.[m
[32m+[m[32m     */[m
[32m+[m[32m    encoding?: Encoding | null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  export interface RawBodyError extends Error {[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The limit in bytes.[m
[32m+[m[32m     */[m
[32m+[m[32m    limit?: number;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The expected length of the stream.[m
[32m+[m[32m     */[m
[32m+[m[32m    length?: number;[m
[32m+[m[32m    expected?: number;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The received bytes.[m
[32m+[m[32m     */[m
[32m+[m[32m    received?: number;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The encoding.[m
[32m+[m[32m     */[m
[32m+[m[32m    encoding?: string;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The corresponding status code for the error.[m
[32m+[m[32m     */[m
[32m+[m[32m    status: number;[m
[32m+[m[32m    statusCode: number;[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The error type.[m
[32m+[m[32m     */[m
[32m+[m[32m    type: string;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Gets the entire buffer of a stream either as a `Buffer` or a string.[m
[32m+[m[32m * Validates the stream's length against an expected length and maximum[m
[32m+[m[32m * limit. Ideal for parsing request bodies.[m
[32m+[m[32m */[m
[32m+[m[32mdeclare function getRawBody([m
[32m+[m[32m  stream: Readable,[m
[32m+[m[32m  callback: (err: getRawBody.RawBodyError, body: Buffer) => void[m
[32m+[m[32m): void;[m
[32m+[m
[32m+[m[32mdeclare function getRawBody([m
[32m+[m[32m  stream: Readable,[m
[32m+[m[32m  options: (getRawBody.Options & { encoding: getRawBody.Encoding }) | getRawBody.Encoding,[m
[32m+[m[32m  callback: (err: getRawBody.RawBodyError, body: string) => void[m
[32m+[m[32m): void;[m
[32m+[m
[32m+[m[32mdeclare function getRawBody([m
[32m+[m[32m  stream: Readable,[m
[32m+[m[32m  options: getRawBody.Options,[m
[32m+[m[32m  callback: (err: getRawBody.RawBodyError, body: Buffer) => void[m
[32m+[m[32m): void;[m
[32m+[m
[32m+[m[32mdeclare function getRawBody([m
[32m+[m[32m  stream: Readable,[m
[32m+[m[32m  options: (getRawBody.Options & { encoding: getRawBody.Encoding }) | getRawBody.Encoding[m
[32m+[m[32m): Promise<string>;[m
[32m+[m
[32m+[m[32mdeclare function getRawBody([m
[32m+[m[32m  stream: Readable,[m
[32m+[m[32m  options?: getRawBody.Options[m
[32m+[m[32m): Promise<Buffer>;[m
[32m+[m
[32m+[m[32mexport = getRawBody;[m
[1mdiff --git a/node_modules/raw-body/index.js b/node_modules/raw-body/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..9cdcd12[m
[1m--- /dev/null[m
[1m+++ b/node_modules/raw-body/index.js[m
[36m@@ -0,0 +1,336 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * raw-body[m
[32m+[m[32m * Copyright(c) 2013-2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2014-2022 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar asyncHooks = tryRequireAsyncHooks()[m
[32m+[m[32mvar bytes = require('bytes')[m
[32m+[m[32mvar createError = require('http-errors')[m
[32m+[m[32mvar iconv = require('iconv-lite')[m
[32m+[m[32mvar unpipe = require('unpipe')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = getRawBody[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar ICONV_ENCODING_MESSAGE_REGEXP = /^Encoding not recognized: /[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the decoder for a given encoding.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} encoding[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getDecoder (encoding) {[m
[32m+[m[32m  if (!encoding) return null[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    return iconv.getDecoder(encoding)[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    // error getting decoder[m
[32m+[m[32m    if (!ICONV_ENCODING_MESSAGE_REGEXP.test(e.message)) throw e[m
[32m+[m
[32m+[m[32m    // the encoding was not found[m
[32m+[m[32m    throw createError(415, 'specified encoding unsupported', {[m
[32m+[m[32m      encoding: encoding,[m
[32m+[m[32m      type: 'encoding.unsupported'[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the raw body of a stream (typically HTTP).[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} stream[m
[32m+[m[32m * @param {object|string|function} [options][m
[32m+[m[32m * @param {function} [callback][m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getRawBody (stream, options, callback) {[m
[32m+[m[32m  var done = callback[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  // light validation[m
[32m+[m[32m  if (stream === undefined) {[m
[32m+[m[32m    throw new TypeError('argument stream is required')[m
[32m+[m[32m  } else if (typeof stream !== 'object' || stream === null || typeof stream.on !== 'function') {[m
[32m+[m[32m    throw new TypeError('argument stream must be a stream')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (options === true || typeof options === 'string') {[m
[32m+[m[32m    // short cut for encoding[m
[32m+[m[32m    opts = {[m
[32m+[m[32m      encoding: options[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    done = options[m
[32m+[m[32m    opts = {}[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // validate callback is a function, if provided[m
[32m+[m[32m  if (done !== undefined && typeof done !== 'function') {[m
[32m+[m[32m    throw new TypeError('argument callback must be a function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // require the callback without promises[m
[32m+[m[32m  if (!done && !global.Promise) {[m
[32m+[m[32m    throw new TypeError('argument callback is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // get encoding[m
[32m+[m[32m  var encoding = opts.encoding !== true[m
[32m+[m[32m    ? opts.encoding[m
[32m+[m[32m    : 'utf-8'[m
[32m+[m
[32m+[m[32m  // convert the limit to an integer[m
[32m+[m[32m  var limit = bytes.parse(opts.limit)[m
[32m+[m
[32m+[m[32m  // convert the expected length to an integer[m
[32m+[m[32m  var length = opts.length != null && !isNaN(opts.length)[m
[32m+[m[32m    ? parseInt(opts.length, 10)[m
[32m+[m[32m    : null[m
[32m+[m
[32m+[m[32m  if (done) {[m
[32m+[m[32m    // classic callback style[m
[32m+[m[32m    return readStream(stream, encoding, length, limit, wrap(done))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return new Promise(function executor (resolve, reject) {[m
[32m+[m[32m    readStream(stream, encoding, length, limit, function onRead (err, buf) {[m
[32m+[m[32m      if (err) return reject(err)[m
[32m+[m[32m      resolve(buf)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Halt a stream.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} stream[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction halt (stream) {[m
[32m+[m[32m  // unpipe everything from the stream[m
[32m+[m[32m  unpipe(stream)[m
[32m+[m
[32m+[m[32m  // pause stream[m
[32m+[m[32m  if (typeof stream.pause === 'function') {[m
[32m+[m[32m    stream.pause()[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Read the data from the stream.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} stream[m
[32m+[m[32m * @param {string} encoding[m
[32m+[m[32m * @param {number} length[m
[32m+[m[32m * @param {number} limit[m
[32m+[m[32m * @param {function} callback[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction readStream (stream, encoding, length, limit, callback) {[m
[32m+[m[32m  var complete = false[m
[32m+[m[32m  var sync = true[m
[32m+[m
[32m+[m[32m  // check the length and limit options.[m
[32m+[m[32m  // note: we intentionally leave the stream paused,[m
[32m+[m[32m  // so users should handle the stream themselves.[m
[32m+[m[32m  if (limit !== null && length !== null && length > limit) {[m
[32m+[m[32m    return done(createError(413, 'request entity too large', {[m
[32m+[m[32m      expected: length,[m
[32m+[m[32m      length: length,[m
[32m+[m[32m      limit: limit,[m
[32m+[m[32m      type: 'entity.too.large'[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // streams1: assert request encoding is buffer.[m
[32m+[m[32m  // streams2+: assert the stream encoding is buffer.[m
[32m+[m[32m  //   stream._decoder: streams1[m
[32m+[m[32m  //   state.encoding: streams2[m
[32m+[m[32m  //   state.decoder: streams2, specifically < 0.10.6[m
[32m+[m[32m  var state = stream._readableState[m
[32m+[m[32m  if (stream._decoder || (state && (state.encoding || state.decoder))) {[m
[32m+[m[32m    // developer error[m
[32m+[m[32m    return done(createError(500, 'stream encoding should not be set', {[m
[32m+[m[32m      type: 'stream.encoding.set'[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof stream.readable !== 'undefined' && !stream.readable) {[m
[32m+[m[32m    return done(createError(500, 'stream is not readable', {[m
[32m+[m[32m      type: 'stream.not.readable'[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var received = 0[m
[32m+[m[32m  var decoder[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    decoder = getDecoder(encoding)[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return done(err)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var buffer = decoder[m
[32m+[m[32m    ? ''[m
[32m+[m[32m    : [][m
[32m+[m
[32m+[m[32m  // attach listeners[m
[32m+[m[32m  stream.on('aborted', onAborted)[m
[32m+[m[32m  stream.on('close', cleanup)[m
[32m+[m[32m  stream.on('data', onData)[m
[32m+[m[32m  stream.on('end', onEnd)[m
[32m+[m[32m  stream.on('error', onEnd)[m
[32m+[m
[32m+[m[32m  // mark sync section complete[m
[32m+[m[32m  sync = false[m
[32m+[m
[32m+[m[32m  function done () {[m
[32m+[m[32m    var args = new Array(arguments.length)[m
[32m+[m
[32m+[m[32m    // copy arguments[m
[32m+[m[32m    for (var i = 0; i < args.length; i++) {[m
[32m+[m[32m      args[i] = arguments[i][m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // mark complete[m
[32m+[m[32m    complete = true[m
[32m+[m
[32m+[m[32m    if (sync) {[m
[32m+[m[32m      process.nextTick(invokeCallback)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      invokeCallback()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function invokeCallback () {[m
[32m+[m[32m      cleanup()[m
[32m+[m
[32m+[m[32m      if (args[0]) {[m
[32m+[m[32m        // halt the stream on error[m
[32m+[m[32m        halt(stream)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      callback.apply(null, args)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function onAborted () {[m
[32m+[m[32m    if (complete) return[m
[32m+[m
[32m+[m[32m    done(createError(400, 'request aborted', {[m
[32m+[m[32m      code: 'ECONNABORTED',[m
[32m+[m[32m      expected: length,[m
[32m+[m[32m      length: length,[m
[32m+[m[32m      received: received,[m
[32m+[m[32m      type: 'request.aborted'[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function onData (chunk) {[m
[32m+[m[32m    if (complete) return[m
[32m+[m
[32m+[m[32m    received += chunk.length[m
[32m+[m
[32m+[m[32m    if (limit !== null && received > limit) {[m
[32m+[m[32m      done(createError(413, 'request entity too large', {[m
[32m+[m[32m        limit: limit,[m
[32m+[m[32m        received: received,[m
[32m+[m[32m        type: 'entity.too.large'[m
[32m+[m[32m      }))[m
[32m+[m[32m    } else if (decoder) {[m
[32m+[m[32m      buffer += decoder.write(chunk)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      buffer.push(chunk)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function onEnd (err) {[m
[32m+[m[32m    if (complete) return[m
[32m+[m[32m    if (err) return done(err)[m
[32m+[m
[32m+[m[32m    if (length !== null && received !== length) {[m
[32m+[m[32m      done(createError(400, 'request size did not match content length', {[m
[32m+[m[32m        expected: length,[m
[32m+[m[32m        length: length,[m
[32m+[m[32m        received: received,[m
[32m+[m[32m        type: 'request.size.invalid'[m
[32m+[m[32m      }))[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var string = decoder[m
[32m+[m[32m        ? buffer + (decoder.end() || '')[m
[32m+[m[32m        : Buffer.concat(buffer)[m
[32m+[m[32m      done(null, string)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function cleanup () {[m
[32m+[m[32m    buffer = null[m
[32m+[m
[32m+[m[32m    stream.removeListener('aborted', onAborted)[m
[32m+[m[32m    stream.removeListener('data', onData)[m
[32m+[m[32m    stream.removeListener('end', onEnd)[m
[32m+[m[32m    stream.removeListener('error', onEnd)[m
[32m+[m[32m    stream.removeListener('close', cleanup)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Try to require async_hooks[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction tryRequireAsyncHooks () {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return require('async_hooks')[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return {}[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Wrap function with async resource, if possible.[m
[32m+[m[32m * AsyncResource.bind static method backported.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction wrap (fn) {[m
[32m+[m[32m  var res[m
[32m+[m
[32m+[m[32m  // create anonymous resource[m
[32m+[m[32m  if (asyncHooks.AsyncResource) {[m
[32m+[m[32m    res = new asyncHooks.AsyncResource(fn.name || 'bound-anonymous-fn')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // incompatible node.js[m
[32m+[m[32m  if (!res || !res.runInAsyncScope) {[m
[32m+[m[32m    return fn[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // return bound function[m
[32m+[m[32m  return res.runInAsyncScope.bind(res, fn, null)[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/raw-body/package.json b/node_modules/raw-body/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..aabb1c3[m
[1m--- /dev/null[m
[1m+++ b/node_modules/raw-body/package.json[m
[36m@@ -0,0 +1,49 @@[m
