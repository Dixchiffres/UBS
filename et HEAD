[32m+[m[32m# forwarded[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Node.js Version][node-version-image]][node-version-url][m
[32m+[m[32m[![Build Status][ci-image]][ci-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mParse HTTP X-Forwarded-For header[m
[32m+[m
[32m+[m[32m## Installation[m
[32m+[m
[32m+[m[32mThis is a [Node.js](https://nodejs.org/en/) module available through the[m
[32m+[m[32m[npm registry](https://www.npmjs.com/). Installation is done using the[m
[32m+[m[32m[`npm install` command](https://docs.npmjs.com/getting-started/installing-npm-packages-locally):[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install forwarded[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar forwarded = require('forwarded')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### forwarded(req)[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar addresses = forwarded(req)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mParse the `X-Forwarded-For` header from the request. Returns an array[m
[32m+[m[32mof the addresses, including the socket address for the `req`, in reverse[m
[32m+[m[32morder (i.e. index `0` is the socket address and the last index is the[m
[32m+[m[32mfurthest address, typically the end-user).[m
[32m+[m
[32m+[m[32m## Testing[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm test[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[MIT](LICENSE)[m
[32m+[m
[32m+[m[32m[ci-image]: https://badgen.net/github/checks/jshttp/forwarded/master?label=ci[m
[32m+[m[32m[ci-url]: https://github.com/jshttp/forwarded/actions?query=workflow%3Aci[m
[32m+[m[32m[npm-image]: https://img.shields.io/npm/v/forwarded.svg[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/forwarded[m
[32m+[m[32m[node-version-image]: https://img.shields.io/node/v/forwarded.svg[m
[32m+[m[32m[node-version-url]: https://nodejs.org/en/download/[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/jshttp/forwarded/master.svg[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/jshttp/forwarded?branch=master[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/forwarded.svg[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/forwarded[m
[1mdiff --git a/node_modules/forwarded/index.js b/node_modules/forwarded/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b2b6bdd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/forwarded/index.js[m
[36m@@ -0,0 +1,90 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * forwarded[m
[32m+[m[32m * Copyright(c) 2014-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = forwarded[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get all addresses in the request, using the `X-Forwarded-For` header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @return {array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction forwarded (req) {[m
[32m+[m[32m  if (!req) {[m
[32m+[m[32m    throw new TypeError('argument req is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // simple header parsing[m
[32m+[m[32m  var proxyAddrs = parse(req.headers['x-forwarded-for'] || '')[m
[32m+[m[32m  var socketAddr = getSocketAddr(req)[m
[32m+[m[32m  var addrs = [socketAddr].concat(proxyAddrs)[m
[32m+[m
[32m+[m[32m  // return all addresses[m
[32m+[m[32m  return addrs[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the socket address for a request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getSocketAddr (req) {[m
[32m+[m[32m  return req.socket[m
[32m+[m[32m    ? req.socket.remoteAddress[m
[32m+[m[32m    : req.connection.remoteAddress[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the X-Forwarded-For header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} header[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse (header) {[m
[32m+[m[32m  var end = header.length[m
[32m+[m[32m  var list = [][m
[32m+[m[32m  var start = header.length[m
[32m+[m
[32m+[m[32m  // gather addresses, backwards[m
[32m+[m[32m  for (var i = header.length - 1; i >= 0; i--) {[m
[32m+[m[32m    switch (header.charCodeAt(i)) {[m
[32m+[m[32m      case 0x20: /*   */[m
[32m+[m[32m        if (start === end) {[m
[32m+[m[32m          start = end = i[m
[32m+[m[32m        }[m
[32m+[m[32m        break[m
[32m+[m[32m      case 0x2c: /* , */[m
[32m+[m[32m        if (start !== end) {[m
[32m+[m[32m          list.push(header.substring(start, end))[m
[32m+[m[32m        }[m
[32m+[m[32m        start = end = i[m
[32m+[m[32m        break[m
[32m+[m[32m      default:[m
[32m+[m[32m        start = i[m
[32m+[m[32m        break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // final address[m
[32m+[m[32m  if (start !== end) {[m
[32m+[m[32m    list.push(header.substring(start, end))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return list[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/forwarded/package.json b/node_modules/forwarded/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..bf9c7d6[m
