[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar preferredCharsets = require('./lib/charset')[m
[32m+[m[32mvar preferredEncodings = require('./lib/encoding')[m
[32m+[m[32mvar preferredLanguages = require('./lib/language')[m
[32m+[m[32mvar preferredMediaTypes = require('./lib/mediaType')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = Negotiator;[m
[32m+[m[32mmodule.exports.Negotiator = Negotiator;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a Negotiator instance from a request.[m
[32m+[m[32m * @param {object} request[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction Negotiator(request) {[m
[32m+[m[32m  if (!(this instanceof Negotiator)) {[m
[32m+[m[32m    return new Negotiator(request);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.request = request;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mNegotiator.prototype.charset = function charset(available) {[m
[32m+[m[32m  var set = this.charsets(available);[m
[32m+[m[32m  return set && set[0];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.charsets = function charsets(available) {[m
[32m+[m[32m  return preferredCharsets(this.request.headers['accept-charset'], available);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.encoding = function encoding(available) {[m
[32m+[m[32m  var set = this.encodings(available);[m
[32m+[m[32m  return set && set[0];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.encodings = function encodings(available) {[m
[32m+[m[32m  return preferredEncodings(this.request.headers['accept-encoding'], available);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.language = function language(available) {[m
[32m+[m[32m  var set = this.languages(available);[m
[32m+[m[32m  return set && set[0];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.languages = function languages(available) {[m
[32m+[m[32m  return preferredLanguages(this.request.headers['accept-language'], available);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.mediaType = function mediaType(available) {[m
[32m+[m[32m  var set = this.mediaTypes(available);[m
[32m+[m[32m  return set && set[0];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mNegotiator.prototype.mediaTypes = function mediaTypes(available) {[m
[32m+[m[32m  return preferredMediaTypes(this.request.headers.accept, available);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Backwards compatibility[m
[32m+[m[32mNegotiator.prototype.preferredCharset = Negotiator.prototype.charset;[m
[32m+[m[32mNegotiator.prototype.preferredCharsets = Negotiator.prototype.charsets;[m
[32m+[m[32mNegotiator.prototype.preferredEncoding = Negotiator.prototype.encoding;[m
[32m+[m[32mNegotiator.prototype.preferredEncodings = Negotiator.prototype.encodings;[m
[32m+[m[32mNegotiator.prototype.preferredLanguage = Negotiator.prototype.language;[m
[32m+[m[32mNegotiator.prototype.preferredLanguages = Negotiator.prototype.languages;[m
[32m+[m[32mNegotiator.prototype.preferredMediaType = Negotiator.prototype.mediaType;[m
[32m+[m[32mNegotiator.prototype.preferredMediaTypes = Negotiator.prototype.mediaTypes;[m
[1mdiff --git a/node_modules/negotiator/lib/charset.js b/node_modules/negotiator/lib/charset.js[m
[1mnew file mode 100644[m
[1mindex 0000000..cdd0148[m
[1m--- /dev/null[m
[1m+++ b/node_modules/negotiator/lib/charset.js[m
[36m@@ -0,0 +1,169 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * negotiator[m
[32m+[m[32m * Copyright(c) 2012 Isaac Z. Schlueter[m
[32m+[m[32m * Copyright(c) 2014 Federico Romero[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = preferredCharsets;[m
[32m+[m[32mmodule.exports.preferredCharsets = preferredCharsets;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar simpleCharsetRegExp = /^\s*([^\s;]+)\s*(?:;(.*))?$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the Accept-Charset header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseAcceptCharset(accept) {[m
[32m+[m[32m  var accepts = accept.split(',');[m
[32m+[m
[32m+[m[32m  for (var i = 0, j = 0; i < accepts.length; i++) {[m
[32m+[m[32m    var charset = parseCharset(accepts[i].trim(), i);[m
[32m+[m
[32m+[m[32m    if (charset) {[m
[32m+[m[32m      accepts[j++] = charset;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim accepts[m
[32m+[m[32m  accepts.length = j;[m
[32m+[m
[32m+[m[32m  return accepts;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a charset from the Accept-Charset header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseCharset(str, i) {[m
[32m+[m[32m  var match = simpleCharsetRegExp.exec(str);[m
[32m+[m[32m  if (!match) return null;[m
[32m+[m
[32m+[m[32m  var charset = match[1];[m
[32m+[m[32m  var q = 1;[m
[32m+[m[32m  if (match[2]) {[m
[32m+[m[32m    var params = match[2].split(';')[m
[32m+[m[32m    for (var j = 0; j < params.length; j++) {[m
[32m+[m[32m      var p = params[j].trim().split('=');[m
[32m+[m[32m      if (p[0] === 'q') {[m
[32m+[m[32m        q = parseFloat(p[1]);[m
[32m+[m[32m        break;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    charset: charset,[m
[32m+[m[32m    q: q,[m
[32m+[m[32m    i: i[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the priority of a charset.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getCharsetPriority(charset, accepted, index) {[m
[32m+[m[32m  var priority = {o: -1, q: 0, s: 0};[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < accepted.length; i++) {[m
[32m+[m[32m    var spec = specify(charset, accepted[i], index);[m
[32m+[m
[32m+[m[32m    if (spec && (priority.s - spec.s || priority.q - spec.q || priority.o - spec.o) < 0) {[m
[32m+[m[32m      priority = spec;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return priority;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the specificity of the charset.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction specify(charset, spec, index) {[m
[32m+[m[32m  var s = 0;[m
[32m+[m[32m  if(spec.charset.toLowerCase() === charset.toLowerCase()){[m
[32m+[m[32m    s |= 1;[m
[32m+[m[32m  } else if (spec.charset !== '*' ) {[m
[32m+[m[32m    return null[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    i: index,[m
[32m+[m[32m    o: spec.i,[m
[32m+[m[32m    q: spec.q,[m
[32m+[m[32m    s: s[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the preferred charsets from an Accept-Charset header.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction preferredCharsets(accept, provided) {[m
[32m+[m[32m  // RFC 2616 sec 14.2: no header = *[m
[32m+[m[32m  var accepts = parseAcceptCharset(accept === undefined ? '*' : accept || '');[m
[32m+[m
[32m+[m[32m  if (!provided) {[m
[32m+[m[32m    // sorted list of all charsets[m
[32m+[m[32m    return accepts[m
[32m+[m[32m      .filter(isQuality)[m
[32m+[m[32m      .sort(compareSpecs)[m
[32m+[m[32m      .map(getFullCharset);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var priorities = provided.map(function getPriority(type, index) {[m
[32m+[m[32m    return getCharsetPriority(type, accepts, index);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // sorted list of accepted charsets[m
[32m+[m[32m  return priorities.filter(isQuality).sort(compareSpecs).map(function getCharset(priority) {[m
[32m+[m[32m    return provided[priorities.indexOf(priority)];[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compare two specs.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction compareSpecs(a, b) {[m
[32m+[m[32m  return (b.q - a.q) || (b.s - a.s) || (a.o - b.o) || (a.i - b.i) || 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get full charset string.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getFullCharset(spec) {[m
[32m+[m[32m  return spec.charset;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if a spec has any quality.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isQuality(spec) {[m
[32m+[m[32m  return spec.q > 0;[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/negotiator/lib/encoding.js b/node_modules/negotiator/lib/encoding.js[m
[1mnew file mode 100644[m
[1mindex 0000000..8432cd7[m
[1m--- /dev/null[m
[1m+++ b/node_modules/negotiator/lib/encoding.js[m
[36m@@ -0,0 +1,184 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * negotiator[m
[32m+[m[32m * Copyright(c) 2012 Isaac Z. Schlueter[m
[32m+[m[32m * Copyright(c) 2014 Federico Romero[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = preferredEncodings;[m
[32m+[m[32mmodule.exports.preferredEncodings = preferredEncodings;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar simpleEncodingRegExp = /^\s*([^\s;]+)\s*(?:;(.*))?$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the Accept-Encoding header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseAcceptEncoding(accept) {[m
[32m+[m[32m  var accepts = accept.split(',');[m
[32m+[m[32m  var hasIdentity = false;[m
[32m+[m[32m  var minQuality = 1;[m
[32m+[m
[32m+[m[32m  for (var i = 0, j = 0; i < accepts.length; i++) {[m
[32m+[m[32m    var encoding = parseEncoding(accepts[i].trim(), i);[m
[32m+[m
[32m+[m[32m    if (encoding) {[m
[32m+[m[32m      accepts[j++] = encoding;[m
[32m+[m[32m      hasIdentity = hasIdentity || specify('identity', encoding);[m
[32m+[m[32m      minQuality = Math.min(minQuality, encoding.q || 1);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!hasIdentity) {[m
[32m+[m[32m    /*[m
[32m+[m[32m     * If identity doesn't explicitly appear in the accept-encoding header,[m
[32m+[m[32m     * it's added to the list of acceptable encoding with the lowest q[m
[32m+[m[32m     */[m
[32m+[m[32m    accepts[j++] = {[m
[32m+[m[32m      encoding: 'identity',[m
[32m+[m[32m      q: minQuality,[m
[32m+[m[32m      i: i[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim accepts[m
[32m+[m[32m  accepts.length = j;[m
[32m+[m
[32m+[m[32m  return accepts;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse an encoding from the Accept-Encoding header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseEncoding(str, i) {[m
[32m+[m[32m  var match = simpleEncodingRegExp.exec(str);[m
[32m+[m[32m  if (!match) return null;[m
[32m+[m
[32m+[m[32m  var encoding = match[1];[m
[32m+[m[32m  var q = 1;[m
[32m+[m[32m  if (match[2]) {[m
[32m+[m[32m    var params = match[2].split(';');[m
[32m+[m[32m    for (var j = 0; j < params.length; j++) {[m
[32m+[m[32m      var p = params[j].trim().split('=');[m
[32m+[m[32m      if (p[0] === 'q') {[m
[32m+[m[32m        q = parseFloat(p[1]);[m
[32m+[m[32m        break;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    encoding: encoding,[m
[32m+[m[32m    q: q,[m
[32m+[m[32m    i: i[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the priority of an encoding.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getEncodingPriority(encoding, accepted, index) {[m
[32m+[m[32m  var priority = {o: -1, q: 0, s: 0};[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < accepted.length; i++) {[m
[32m+[m[32m    var spec = specify(encoding, accepted[i], index);[m
[32m+[m
[32m+[m[32m    if (spec && (priority.s - spec.s || priority.q - spec.q || priority.o - spec.o) < 0) {[m
[32m+[m[32m      priority = spec;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return priority;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the specificity of the encoding.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction specify(encoding, spec, index) {[m
[32m+[m[32m  var s = 0;[m
[32m+[m[32m  if(spec.encoding.toLowerCase() === encoding.toLowerCase()){[m
[32m+[m[32m    s |= 1;[m
[32m+[m[32m  } else if (spec.encoding !== '*' ) {[m
[32m+[m[32m    return null[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    i: index,[m
[32m+[m[32m    o: spec.i,[m
[32m+[m[32m    q: spec.q,[m
[32m+[m[32m    s: s[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the preferred encodings from an Accept-Encoding header.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction preferredEncodings(accept, provided) {[m
[32m+[m[32m  var accepts = parseAcceptEncoding(accept || '');[m
[32m+[m
[32m+[m[32m  if (!provided) {[m
[32m+[m[32m    // sorted list of all encodings[m
[32m+[m[32m    return accepts[m
[32m+[m[32m      .filter(isQuality)[m
[32m+[m[32m      .sort(compareSpecs)[m
[32m+[m[32m      .map(getFullEncoding);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var priorities = provided.map(function getPriority(type, index) {[m
[32m+[m[32m    return getEncodingPriority(type, accepts, index);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // sorted list of accepted encodings[m
[32m+[m[32m  return priorities.filter(isQuality).sort(compareSpecs).map(function getEncoding(priority) {[m
[32m+[m[32m    return provided[priorities.indexOf(priority)];[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compare two specs.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction compareSpecs(a, b) {[m
[32m+[m[32m  return (b.q - a.q) || (b.s - a.s) || (a.o - b.o) || (a.i - b.i) || 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get full encoding string.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getFullEncoding(spec) {[m
[32m+[m[32m  return spec.encoding;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if a spec has any quality.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isQuality(spec) {[m
[32m+[m[32m  return spec.q > 0;[m
[32m+[m[32m}[m
