[1mdiff --git a/node_modules/es-define-property/index.d.ts b/node_modules/es-define-property/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..6012247[m
[1m--- /dev/null[m
[1m+++ b/node_modules/es-define-property/index.d.ts[m
[36m@@ -0,0 +1,3 @@[m
[32m+[m[32mdeclare const defineProperty: false | typeof Object.defineProperty;[m
[32m+[m
[32m+[m[32mexport = defineProperty;[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/es-define-property/index.js b/node_modules/es-define-property/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f32737d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/es-define-property/index.js[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mvar $defineProperty = GetIntrinsic('%Object.defineProperty%', true) || false;[m
[32m+[m[32mif ($defineProperty) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32m$defineProperty({}, 'a', { value: 1 });[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32m// IE 8 has a broken defineProperty[m
[32m+[m		[32m$defineProperty = false;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = $defineProperty;[m
