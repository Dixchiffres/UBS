[1mdiff --git a/node_modules/http-errors/index.js b/node_modules/http-errors/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c425f1e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/http-errors/index.js[m
[36m@@ -0,0 +1,289 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * http-errors[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar deprecate = require('depd')('http-errors')[m
[32m+[m[32mvar setPrototypeOf = require('setprototypeof')[m
[32m+[m[32mvar statuses = require('statuses')[m
[32m+[m[32mvar inherits = require('inherits')[m
[32m+[m[32mvar toIdentifier = require('toidentifier')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = createError[m
[32m+[m[32mmodule.exports.HttpError = createHttpErrorConstructor()[m
[32m+[m[32mmodule.exports.isHttpError = createIsHttpErrorFunction(module.exports.HttpError)[m
[32m+[m
[32m+[m[32m// Populate exports for all constructors[m
[32m+[m[32mpopulateConstructorExports(module.exports, statuses.codes, module.exports.HttpError)[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the code class of a status code.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction codeClass (status) {[m
[32m+[m[32m  return Number(String(status).charAt(0) + '00')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a new HTTP Error.[m
[32m+[m[32m *[m
[32m+[m[32m * @returns {Error}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createError () {[m
[32m+[m[32m  // so much arity going on ~_~[m
[32m+[m[32m  var err[m
[32m+[m[32m  var msg[m
[32m+[m[32m  var status = 500[m
[32m+[m[32m  var props = {}[m
[32m+[m[32m  for (var i = 0; i < arguments.length; i++) {[m
[32m+[m[32m    var arg = arguments[i][m
[32m+[m[32m    var type = typeof arg[m
[32m+[m[32m    if (type === 'object' && arg instanceof Error) {[m
[32m+[m[32m      err = arg[m
[32m+[m[32m      status = err.status || err.statusCode || status[m
[32m+[m[32m    } else if (type === 'number' && i === 0) {[m
[32m+[m[32m      status = arg[m
[32m+[m[32m    } else if (type === 'string') {[m
[32m+[m[32m      msg = arg[m
[32m+[m[32m    } else if (type === 'object') {[m
[32m+[m[32m      props = arg[m
[32m+[m[32m    } else {[m
[32m+[m[32m      throw new TypeError('argument #' + (i + 1) + ' unsupported type ' + type)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof status === 'number' && (status < 400 || status >= 600)) {[m
[32m+[m[32m    deprecate('non-error status code; use only 4xx or 5xx status codes')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof status !== 'number' ||[m
[32m+[m[32m    (!statuses.message[status] && (status < 400 || status >= 600))) {[m
[32m+[m[32m    status = 500[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // constructor[m
[32m+[m[32m  var HttpError = createError[status] || createError[codeClass(status)][m
[32m+[m
[32m+[m[32m  if (!err) {[m
[32m+[m[32m    // create error[m
[32m+[m[32m    err = HttpError[m
[32m+[m[32m      ? new HttpError(msg)[m
[32m+[m[32m      : new Error(msg || statuses.message[status])[m
[32m+[m[32m    Error.captureStackTrace(err, createError)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!HttpError || !(err instanceof HttpError) || err.status !== status) {[m
[32m+[m[32m    // add properties to generic error[m
[32m+[m[32m    err.expose = status < 500[m
[32m+[m[32m    err.status = err.statusCode = status[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var key in props) {[m
[32m+[m[32m    if (key !== 'status' && key !== 'statusCode') {[m
[32m+[m[32m      err[key] = props[key][m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return err[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create HTTP error abstract base class.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createHttpErrorConstructor () {[m
[32m+[m[32m  function HttpError () {[m
[32m+[m[32m    throw new TypeError('cannot construct abstract class')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  inherits(HttpError, Error)[m
[32m+[m
[32m+[m[32m  return HttpError[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a constructor for a client error.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createClientErrorConstructor (HttpError, name, code) {[m
[32m+[m[32m  var className = toClassName(name)[m
[32m+[m
[32m+[m[32m  function ClientError (message) {[m
[32m+[m[32m    // create the error object[m
[32m+[m[32m    var msg = message != null ? message : statuses.message[code][m
[32m+[m[32m    var err = new Error(msg)[m
[32m+[m
[32m+[m[32m    // capture a stack trace to the construction point[m
[32m+[m[32m    Error.captureStackTrace(err, ClientError)[m
[32m+[m
[32m+[m[32m    // adjust the [[Prototype]][m
[32m+[m[32m    setPrototypeOf(err, ClientError.prototype)[m
[32m+[m
[32m+[m[32m    // redefine the error message[m
[32m+[m[32m    Object.defineProperty(err, 'message', {[m
[32m+[m[32m      enumerable: true,[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      value: msg,[m
[32m+[m[32m      writable: true[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    // redefine the error name[m
[32m+[m[32m    Object.defineProperty(err, 'name', {[m
[32m+[m[32m      enumerable: false,[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      value: className,[m
[32m+[m[32m      writable: true[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    return err[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  inherits(ClientError, HttpError)[m
[32m+[m[32m  nameFunc(ClientError, className)[m
[32m+[m
[32m+[m[32m  ClientError.prototype.status = code[m
[32m+[m[32m  ClientError.prototype.statusCode = code[m
[32m+[m[32m  ClientError.prototype.expose = true[m
[32m+[m
[32m+[m[32m  return ClientError[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create function to test is a value is a HttpError.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createIsHttpErrorFunction (HttpError) {[m
[32m+[m[32m  return function isHttpError (val) {[m
[32m+[m[32m    if (!val || typeof val !== 'object') {[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (val instanceof HttpError) {[m
[32m+[m[32m      return true[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return val instanceof Error &&[m
[32m+[m[32m      typeof val.expose === 'boolean' &&[m
[32m+[m[32m      typeof val.statusCode === 'number' && val.status === val.statusCode[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a constructor for a server error.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createServerErrorConstructor (HttpError, name, code) {[m
[32m+[m[32m  var className = toClassName(name)[m
[32m+[m
[32m+[m[32m  function ServerError (message) {[m
[32m+[m[32m    // create the error object[m
[32m+[m[32m    var msg = message != null ? message : statuses.message[code][m
[32m+[m[32m    var err = new Error(msg)[m
[32m+[m
[32m+[m[32m    // capture a stack trace to the construction point[m
[32m+[m[32m    Error.captureStackTrace(err, ServerError)[m
[32m+[m
[32m+[m[32m    // adjust the [[Prototype]][m
[32m+[m[32m    setPrototypeOf(err, ServerError.prototype)[m
[32m+[m
[32m+[m[32m    // redefine the error message[m
[32m+[m[32m    Object.defineProperty(err, 'message', {[m
[32m+[m[32m      enumerable: true,[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      value: msg,[m
[32m+[m[32m      writable: true[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    // redefine the error name[m
[32m+[m[32m    Object.defineProperty(err, 'name', {[m
[32m+[m[32m      enumerable: false,[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      value: className,[m
[32m+[m[32m      writable: true[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    return err[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  inherits(ServerError, HttpError)[m
[32m+[m[32m  nameFunc(ServerError, className)[m
[32m+[m
[32m+[m[32m  ServerError.prototype.status = code[m
[32m+[m[32m  ServerError.prototype.statusCode = code[m
[32m+[m[32m  ServerError.prototype.expose = false[m
[32m+[m
[32m+[m[32m  return ServerError[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set the name of a function, if possible.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction nameFunc (func, name) {[m
[32m+[m[32m  var desc = Object.getOwnPropertyDescriptor(func, 'name')[m
[32m+[m
[32m+[m[32m  if (desc && desc.configurable) {[m
[32m+[m[32m    desc.value = name[m
[32m+[m[32m    Object.defineProperty(func, 'name', desc)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Populate the exports object with constructors for every error class.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction populateConstructorExports (exports, codes, HttpError) {[m
[32m+[m[32m  codes.forEach(function forEachCode (code) {[m
[32m+[m[32m    var CodeError[m
[32m+[m[32m    var name = toIdentifier(statuses.message[code])[m
[32m+[m
[32m+[m[32m    switch (codeClass(code)) {[m
[32m+[m[32m      case 400:[m
[32m+[m[32m        CodeError = createClientErrorConstructor(HttpError, name, code)[m
[32m+[m[32m        break[m
[32m+[m[32m      case 500:[m
[32m+[m[32m        CodeError = createServerErrorConstructor(HttpError, name, code)[m
[32m+[m[32m        break[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (CodeError) {[m
[32m+[m[32m      // export the constructor[m
[32m+[m[32m      exports[code] = CodeError[m
[32m+[m[32m      exports[name] = CodeError[m
[32m+[m[32m    }[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get a class name from a name identifier.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction toClassName (name) {[m
[32m+[m[32m  return name.substr(-5) !== 'Error'[m
[32m+[m[32m    ? name + 'Error'[m
[32m+[m[32m    : name[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/http-errors/package.json b/node_modules/http-errors/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..4cb6d7e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/http-errors/package.json[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32m{[m
[32m+[m[32m  "name": "http-errors",[m
[32m+[m[32m  "description": "Create HTTP error objects",[m
