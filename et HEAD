[1mdiff --git a/node_modules/function-bind/implementation.js b/node_modules/function-bind/implementation.js[m
[1mnew file mode 100644[m
[1mindex 0000000..fd4384c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/function-bind/implementation.js[m
[36m@@ -0,0 +1,84 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/* eslint no-invalid-this: 1 */[m
[32m+[m
[32m+[m[32mvar ERROR_MESSAGE = 'Function.prototype.bind called on incompatible ';[m
[32m+[m[32mvar toStr = Object.prototype.toString;[m
[32m+[m[32mvar max = Math.max;[m
[32m+[m[32mvar funcType = '[object Function]';[m
[32m+[m
[32m+[m[32mvar concatty = function concatty(a, b) {[m
[32m+[m[32m    var arr = [];[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < a.length; i += 1) {[m
[32m+[m[32m        arr[i] = a[i];[m
[32m+[m[32m    }[m
[32m+[m[32m    for (var j = 0; j < b.length; j += 1) {[m
[32m+[m[32m        arr[j + a.length] = b[j];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return arr;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar slicy = function slicy(arrLike, offset) {[m
[32m+[m[32m    var arr = [];[m
[32m+[m[32m    for (var i = offset || 0, j = 0; i < arrLike.length; i += 1, j += 1) {[m
[32m+[m[32m        arr[j] = arrLike[i];[m
[32m+[m[32m    }[m
[32m+[m[32m    return arr;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar joiny = function (arr, joiner) {[m
[32m+[m[32m    var str = '';[m
[32m+[m[32m    for (var i = 0; i < arr.length; i += 1) {[m
[32m+[m[32m        str += arr[i];[m
[32m+[m[32m        if (i + 1 < arr.length) {[m
[32m+[m[32m            str += joiner;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return str;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function bind(that) {[m
[32m+[m[32m    var target = this;[m
[32m+[m[32m    if (typeof target !== 'function' || toStr.apply(target) !== funcType) {[m
[32m+[m[32m        throw new TypeError(ERROR_MESSAGE + target);[m
[32m+[m[32m    }[m
[32m+[m[32m    var args = slicy(arguments, 1);[m
[32m+[m
[32m+[m[32m    var bound;[m
[32m+[m[32m    var binder = function () {[m
[32m+[m[32m        if (this instanceof bound) {[m
[32m+[m[32m            var result = target.apply([m
[32m+[m[32m                this,[m
[32m+[m[32m                concatty(args, arguments)[m
[32m+[m[32m            );[m
[32m+[m[32m            if (Object(result) === result) {[m
[32m+[m[32m                return result;[m
[32m+[m[32m            }[m
[32m+[m[32m            return this;[m
[32m+[m[32m        }[m
[32m+[m[32m        return target.apply([m
[32m+[m[32m            that,[m
[32m+[m[32m            concatty(args, arguments)[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var boundLength = max(0, target.length - args.length);[m
[32m+[m[32m    var boundArgs = [];[m
[32m+[m[32m    for (var i = 0; i < boundLength; i++) {[m
[32m+[m[32m        boundArgs[i] = '$' + i;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    bound = Function('binder', 'return function (' + joiny(boundArgs, ',') + '){ return binder.apply(this,arguments); }')(binder);[m
[32m+[m
[32m+[m[32m    if (target.prototype) {[m
[32m+[m[32m        var Empty = function Empty() {};[m
[32m+[m[32m        Empty.prototype = target.prototype;[m
[32m+[m[32m        bound.prototype = new Empty();[m
[32m+[m[32m        Empty.prototype = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return bound;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/function-bind/index.js b/node_modules/function-bind/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..3bb6b96[m
[1m--- /dev/null[m
[1m+++ b/node_modules/function-bind/index.js[m
[36m@@ -0,0 +1,5 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar implementation = require('./implementation');[m
[32m+[m
[32m+[m[32mmodule.exports = Function.prototype.bind || implementation;[m
