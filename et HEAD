[32m+[m[32m[actions-image]: https://img.shields.io/endpoint?url=https://github-actions-badge-u3jn4tfpocch.runkit.sh/ljharb/call-bind[m
[32m+[m[32m[actions-url]: https://github.com/ljharb/call-bind/actions[m
[1mdiff --git a/node_modules/call-bind/callBound.js b/node_modules/call-bind/callBound.js[m
[1mnew file mode 100644[m
[1mindex 0000000..8374adf[m
[1m--- /dev/null[m
[1m+++ b/node_modules/call-bind/callBound.js[m
[36m@@ -0,0 +1,15 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m
[32m+[m[32mvar callBind = require('./');[m
[32m+[m
[32m+[m[32mvar $indexOf = callBind(GetIntrinsic('String.prototype.indexOf'));[m
[32m+[m
[32m+[m[32mmodule.exports = function callBoundIntrinsic(name, allowMissing) {[m
[32m+[m	[32mvar intrinsic = GetIntrinsic(name, !!allowMissing);[m
[32m+[m	[32mif (typeof intrinsic === 'function' && $indexOf(name, '.prototype.') > -1) {[m
[32m+[m		[32mreturn callBind(intrinsic);[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn intrinsic;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/call-bind/index.js b/node_modules/call-bind/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..01c5b3d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/call-bind/index.js[m
[36m@@ -0,0 +1,35 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m[32mvar setFunctionLength = require('set-function-length');[m
[32m+[m
[32m+[m[32mvar $TypeError = require('es-errors/type');[m
[32m+[m[32mvar $apply = GetIntrinsic('%Function.prototype.apply%');[m
[32m+[m[32mvar $call = GetIntrinsic('%Function.prototype.call%');[m
[32m+[m[32mvar $reflectApply = GetIntrinsic('%Reflect.apply%', true) || bind.call($call, $apply);[m
[32m+[m
[32m+[m[32mvar $defineProperty = require('es-define-property');[m
[32m+[m[32mvar $max = GetIntrinsic('%Math.max%');[m
[32m+[m
[32m+[m[32mmodule.exports = function callBind(originalFunction) {[m
[32m+[m	[32mif (typeof originalFunction !== 'function') {[m
[32m+[m		[32mthrow new $TypeError('a function is required');[m
[32m+[m	[32m}[m
[32m+[m	[32mvar func = $reflectApply(bind, $call, arguments);[m
[32m+[m	[32mreturn setFunctionLength([m
[32m+[m		[32mfunc,[m
[32m+[m		[32m1 + $max(0, originalFunction.length - (arguments.length - 1)),[m
[32m+[m		[32mtrue[m
[32m+[m	[32m);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar applyBind = function applyBind() {[m
[32m+[m	[32mreturn $reflectApply(bind, $apply, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mif ($defineProperty) {[m
[32m+[m	[32m$defineProperty(module.exports, 'apply', { value: applyBind });[m
[32m+[m[32m} else {[m
[32m+[m	[32mmodule.exports.apply = applyBind;[m
[32m+[m[32m}[m
