[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('../');[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar forEach = require('for-each');[m
[32m+[m[32mvar debug = require('object-inspect');[m
[32m+[m[32mvar generatorFns = require('make-generator-function')();[m
[32m+[m[32mvar asyncFns = require('make-async-function').list();[m
[32m+[m[32mvar asyncGenFns = require('make-async-generator-function')();[m
[32m+[m[32mvar mockProperty = require('mock-property');[m
[32m+[m
[32m+[m[32mvar callBound = require('call-bind/callBound');[m
[32m+[m[32mvar v = require('es-value-fixtures');[m
[32m+[m[32mvar $gOPD = require('gopd');[m
[32m+[m[32mvar DefinePropertyOrThrow = require('es-abstract/2021/DefinePropertyOrThrow');[m
[32m+[m
[32m+[m[32mvar $isProto = callBound('%Object.prototype.isPrototypeOf%');[m
[32m+[m
[32m+[m[32mtest('export', function (t) {[m
[32m+[m	[32mt.equal(typeof GetIntrinsic, 'function', 'it is a function');[m
[32m+[m	[32mt.equal(GetIntrinsic.length, 2, 'function has length of 2');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('throws', function (t) {[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('not an intrinsic'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'nonexistent intrinsic throws a syntax error'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic(''); },[m
[32m+[m		[32mTypeError,[m
[32m+[m		[32m'empty string intrinsic throws a type error'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('.'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'"just a dot" intrinsic throws a syntax error'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('%String'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'Leading % without trailing % throws a syntax error'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('String%'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'Trailing % without leading % throws a syntax error'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic("String['prototype]"); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'Dynamic property access is disallowed for intrinsics (unterminated string)'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('%Proxy.prototype.undefined%'); },[m
[32m+[m		[32mTypeError,[m
[32m+[m		[32m"Throws when middle part doesn't exist (%Proxy.prototype.undefined%)"[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('%Array.prototype%garbage%'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'Throws with extra percent signs'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { GetIntrinsic('%Array.prototype%push%'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'Throws with extra percent signs, even on an existing intrinsic'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mforEach(v.nonStrings, function (nonString) {[m
[32m+[m		[32mt['throws']([m
[32m+[m			[32mfunction () { GetIntrinsic(nonString); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32mdebug(nonString) + ' is not a String'[m
[32m+[m		[32m);[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mforEach(v.nonBooleans, function (nonBoolean) {[m
[32m+[m		[32mt['throws']([m
[32m+[m			[32mfunction () { GetIntrinsic('%', nonBoolean); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32mdebug(nonBoolean) + ' is not a Boolean'[m
[32m+[m		[32m);[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mforEach([[m
[32m+[m		[32m'toString',[m
[32m+[m		[32m'propertyIsEnumerable',[m
[32m+[m		[32m'hasOwnProperty'[m
[32m+[m	[32m], function (objectProtoMember) {[m
[32m+[m		[32mt['throws']([m
[32m+[m			[32mfunction () { GetIntrinsic(objectProtoMember); },[m
[32m+[m			[32mSyntaxError,[m
[32m+[m			[32mdebug(objectProtoMember) + ' is not an intrinsic'[m
[32m+[m		[32m);[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('base intrinsics', function (t) {[m
[32m+[m	[32mt.equal(GetIntrinsic('%Object%'), Object, '%Object% yields Object');[m
[32m+[m	[32mt.equal(GetIntrinsic('Object'), Object, 'Object yields Object');[m
[32m+[m	[32mt.equal(GetIntrinsic('%Array%'), Array, '%Array% yields Array');[m
[32m+[m	[32mt.equal(GetIntrinsic('Array'), Array, 'Array yields Array');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('dotted paths', function (t) {[m
[32m+[m	[32mt.equal(GetIntrinsic('%Object.prototype.toString%'), Object.prototype.toString, '%Object.prototype.toString% yields Object.prototype.toString');[m
[32m+[m	[32mt.equal(GetIntrinsic('Object.prototype.toString'), Object.prototype.toString, 'Object.prototype.toString yields Object.prototype.toString');[m
[32m+[m	[32mt.equal(GetIntrinsic('%Array.prototype.push%'), Array.prototype.push, '%Array.prototype.push% yields Array.prototype.push');[m
[32m+[m	[32mt.equal(GetIntrinsic('Array.prototype.push'), Array.prototype.push, 'Array.prototype.push yields Array.prototype.push');[m
[32m+[m
[32m+[m	[32mtest('underscore paths are aliases for dotted paths', { skip: !Object.isFrozen || Object.isFrozen(Object.prototype) }, function (st) {[m
[32m+[m		[32mvar original = GetIntrinsic('%ObjProto_toString%');[m
[32m+[m
[32m+[m		[32mforEach([[m
[32m+[m			[32m'%Object.prototype.toString%',[m
[32m+[m			[32m'Object.prototype.toString',[m
[32m+[m			[32m'%ObjectPrototype.toString%',[m
[32m+[m			[32m'ObjectPrototype.toString',[m
[32m+[m			[32m'%ObjProto_toString%',[m
[32m+[m			[32m'ObjProto_toString'[m
[32m+[m		[32m], function (name) {[m
[32m+[m			[32mDefinePropertyOrThrow(Object.prototype, 'toString', {[m
[32m+[m				[32m'[[Value]]': function toString() {[m
[32m+[m					[32mreturn original.apply(this, arguments);[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m			[32mst.equal(GetIntrinsic(name), original, name + ' yields original Object.prototype.toString');[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mDefinePropertyOrThrow(Object.prototype, 'toString', { '[[Value]]': original });[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest('dotted paths cache', { skip: !Object.isFrozen || Object.isFrozen(Object.prototype) }, function (st) {[m
[32m+[m		[32mvar original = GetIntrinsic('%Object.prototype.propertyIsEnumerable%');[m
[32m+[m
[32m+[m		[32mforEach([[m
[32m+[m			[32m'%Object.prototype.propertyIsEnumerable%',[m
[32m+[m			[32m'Object.prototype.propertyIsEnumerable',[m
[32m+[m			[32m'%ObjectPrototype.propertyIsEnumerable%',[m
[32m+[m			[32m'ObjectPrototype.propertyIsEnumerable'[m
[32m+[m		[32m], function (name) {[m
[32m+[m			[32mvar restore = mockProperty(Object.prototype, 'propertyIsEnumerable', {[m
[32m+[m				[32mvalue: function propertyIsEnumerable() {[m
[32m+[m					[32mreturn original.apply(this, arguments);[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m			[32mst.equal(GetIntrinsic(name), original, name + ' yields cached Object.prototype.propertyIsEnumerable');[m
[32m+[m
[32m+[m			[32mrestore();[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest('dotted path reports correct error', function (st) {[m
[32m+[m		[32mst['throws'](function () {[m
[32m+[m			[32mGetIntrinsic('%NonExistentIntrinsic.prototype.property%');[m
[32m+[m		[32m}, /%NonExistentIntrinsic%/, 'The base intrinsic of %NonExistentIntrinsic.prototype.property% is %NonExistentIntrinsic%');[m
[32m+[m
[32m+[m		[32mst['throws'](function () {[m
[32m+[m			[32mGetIntrinsic('%NonExistentIntrinsicPrototype.property%');[m
[32m+[m		[32m}, /%NonExistentIntrinsicPrototype%/, 'The base intrinsic of %NonExistentIntrinsicPrototype.property% is %NonExistentIntrinsicPrototype%');[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('accessors', { skip: !$gOPD || typeof Map !== 'function' }, function (t) {[m
[32m+[m	[32mvar actual = $gOPD(Map.prototype, 'size');[m
[32m+[m	[32mt.ok(actual, 'Map.prototype.size has a descriptor');[m
[32m+[m	[32mt.equal(typeof actual.get, 'function', 'Map.prototype.size has a getter function');[m
[32m+[m	[32mt.equal(GetIntrinsic('%Map.prototype.size%'), actual.get, '%Map.prototype.size% yields the getter for it');[m
[32m+[m	[32mt.equal(GetIntrinsic('Map.prototype.size'), actual.get, 'Map.prototype.size yields the getter for it');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('generator functions', { skip: !generatorFns.length }, function (t) {[m
[32m+[m	[32mvar $GeneratorFunction = GetIntrinsic('%GeneratorFunction%');[m
[32m+[m	[32mvar $GeneratorFunctionPrototype = GetIntrinsic('%Generator%');[m
[32m+[m	[32mvar $GeneratorPrototype = GetIntrinsic('%GeneratorPrototype%');[m
[32m+[m
[32m+[m	[32mforEach(generatorFns, function (genFn) {[m
[32m+[m		[32mvar fnName = genFn.name;[m
[32m+[m		[32mfnName = fnName ? "'" + fnName + "'" : 'genFn';[m
[32m+[m
[32m+[m		[32mt.ok(genFn instanceof $GeneratorFunction, fnName + ' instanceof %GeneratorFunction%');[m
[32m+[m		[32mt.ok($isProto($GeneratorFunctionPrototype, genFn), '%Generator% is prototype of ' + fnName);[m
[32m+[m		[32mt.ok($isProto($GeneratorPrototype, genFn.prototype), '%GeneratorPrototype% is prototype of ' + fnName + '.prototype');[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('async functions', { skip: !asyncFns.length }, function (t) {[m
[32m+[m	[32mvar $AsyncFunction = GetIntrinsic('%AsyncFunction%');[m
[32m+[m	[32mvar $AsyncFunctionPrototype = GetIntrinsic('%AsyncFunctionPrototype%');[m
[32m+[m
[32m+[m	[32mforEach(asyncFns, function (asyncFn) {[m
[32m+[m		[32mvar fnName = asyncFn.name;[m
[32m+[m		[32mfnName = fnName ? "'" + fnName + "'" : 'asyncFn';[m
[32m+[m
[32m+[m		[32mt.ok(asyncFn instanceof $AsyncFunction, fnName + ' instanceof %AsyncFunction%');[m
[32m+[m		[32mt.ok($isProto($AsyncFunctionPrototype, asyncFn), '%AsyncFunctionPrototype% is prototype of ' + fnName);[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('async generator functions', { skip: asyncGenFns.length === 0 }, function (t) {[m
[32m+[m	[32mvar $AsyncGeneratorFunction = GetIntrinsic('%AsyncGeneratorFunction%');[m
[32m+[m	[32mvar $AsyncGeneratorFunctionPrototype = GetIntrinsic('%AsyncGenerator%');[m
[32m+[m	[32mvar $AsyncGeneratorPrototype = GetIntrinsic('%AsyncGeneratorPrototype%');[m
[32m+[m
[32m+[m	[32mforEach(asyncGenFns, function (asyncGenFn) {[m
[32m+[m		[32mvar fnName = asyncGenFn.name;[m
[32m+[m		[32mfnName = fnName ? "'" + fnName + "'" : 'asyncGenFn';[m
[32m+[m
[32m+[m		[32mt.ok(asyncGenFn instanceof $AsyncGeneratorFunction, fnName + ' instanceof %AsyncGeneratorFunction%');[m
[32m+[m		[32mt.ok($isProto($AsyncGeneratorFunctionPrototype, asyncGenFn), '%AsyncGenerator% is prototype of ' + fnName);[m
[32m+[m		[32mt.ok($isProto($AsyncGeneratorPrototype, asyncGenFn.prototype), '%AsyncGeneratorPrototype% is prototype of ' + fnName + '.prototype');[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('%ThrowTypeError%', function (t) {[m
[32m+[m	[32mvar $ThrowTypeError = GetIntrinsic('%ThrowTypeError%');[m
[32m+[m
[32m+[m	[32mt.equal(typeof $ThrowTypeError, 'function', 'is a function');[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32m$ThrowTypeError,[m
[32m+[m		[32mTypeError,[m
