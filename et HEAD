[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32m// Export Node.js internal encodings.[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    // Encodings[m
[32m+[m[32m    utf8:   { type: "_internal", bomAware: true},[m
[32m+[m[32m    cesu8:  { type: "_internal", bomAware: true},[m
[32m+[m[32m    unicode11utf8: "utf8",[m
[32m+[m
[32m+[m[32m    ucs2:   { type: "_internal", bomAware: true},[m
[32m+[m[32m    utf16le: "ucs2",[m
[32m+[m
[32m+[m[32m    binary: { type: "_internal" },[m
[32m+[m[32m    base64: { type: "_internal" },[m
[32m+[m[32m    hex:    { type: "_internal" },[m
[32m+[m
[32m+[m[32m    // Codec.[m
[32m+[m[32m    _internal: InternalCodec,[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m
[32m+[m[32mfunction InternalCodec(codecOptions, iconv) {[m
[32m+[m[32m    this.enc = codecOptions.encodingName;[m
[32m+[m[32m    this.bomAware = codecOptions.bomAware;[m
[32m+[m
[32m+[m[32m    if (this.enc === "base64")[m
[32m+[m[32m        this.encoder = InternalEncoderBase64;[m
[32m+[m[32m    else if (this.enc === "cesu8") {[m
[32m+[m[32m        this.enc = "utf8"; // Use utf8 for decoding.[m
[32m+[m[32m        this.encoder = InternalEncoderCesu8;[m
[32m+[m
[32m+[m[32m        // Add decoder for versions of Node not supporting CESU-8[m
[32m+[m[32m        if (Buffer.from('eda0bdedb2a9', 'hex').toString() !== '💩') {[m
[32m+[m[32m            this.decoder = InternalDecoderCesu8;[m
[32m+[m[32m            this.defaultCharUnicode = iconv.defaultCharUnicode;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalCodec.prototype.encoder = InternalEncoder;[m
[32m+[m[32mInternalCodec.prototype.decoder = InternalDecoder;[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m
[32m+[m[32m// We use node.js internal decoder. Its signature is the same as ours.[m
[32m+[m[32mvar StringDecoder = require('string_decoder').StringDecoder;[m
[32m+[m
[32m+[m[32mif (!StringDecoder.prototype.end) // Node v0.8 doesn't have this method.[m
[32m+[m[32m    StringDecoder.prototype.end = function() {};[m
[32m+[m
[32m+[m
[32m+[m[32mfunction InternalDecoder(options, codec) {[m
[32m+[m[32m    StringDecoder.call(this, codec.enc);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalDecoder.prototype = StringDecoder.prototype;[m
[32m+[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m[32m// Encoder is mostly trivial[m
[32m+[m
[32m+[m[32mfunction InternalEncoder(options, codec) {[m
[32m+[m[32m    this.enc = codec.enc;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoder.prototype.write = function(str) {[m
[32m+[m[32m    return Buffer.from(str, this.enc);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoder.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m[32m// Except base64 encoder, which must keep its state.[m
[32m+[m
[32m+[m[32mfunction InternalEncoderBase64(options, codec) {[m
[32m+[m[32m    this.prevStr = '';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoderBase64.prototype.write = function(str) {[m
[32m+[m[32m    str = this.prevStr + str;[m
[32m+[m[32m    var completeQuads = str.length - (str.length % 4);[m
[32m+[m[32m    this.prevStr = str.slice(completeQuads);[m
[32m+[m[32m    str = str.slice(0, completeQuads);[m
[32m+[m
[32m+[m[32m    return Buffer.from(str, "base64");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoderBase64.prototype.end = function() {[m
[32m+[m[32m    return Buffer.from(this.prevStr, "base64");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m[32m// CESU-8 encoder is also special.[m
[32m+[m
[32m+[m[32mfunction InternalEncoderCesu8(options, codec) {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoderCesu8.prototype.write = function(str) {[m
[32m+[m[32m    var buf = Buffer.alloc(str.length * 3), bufIdx = 0;[m
[32m+[m[32m    for (var i = 0; i < str.length; i++) {[m
[32m+[m[32m        var charCode = str.charCodeAt(i);[m
[32m+[m[32m        // Naive implementation, but it works because CESU-8 is especially easy[m
[32m+[m[32m        // to convert from UTF-16 (which all JS strings are encoded in).[m
[32m+[m[32m        if (charCode < 0x80)[m
[32m+[m[32m            buf[bufIdx++] = charCode;[m
[32m+[m[32m        else if (charCode < 0x800) {[m
[32m+[m[32m            buf[bufIdx++] = 0xC0 + (charCode >>> 6);[m
[32m+[m[32m            buf[bufIdx++] = 0x80 + (charCode & 0x3f);[m
[32m+[m[32m        }[m
[32m+[m[32m        else { // charCode will always be < 0x10000 in javascript.[m
[32m+[m[32m            buf[bufIdx++] = 0xE0 + (charCode >>> 12);[m
[32m+[m[32m            buf[bufIdx++] = 0x80 + ((charCode >>> 6) & 0x3f);[m
[32m+[m[32m            buf[bufIdx++] = 0x80 + (charCode & 0x3f);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return buf.slice(0, bufIdx);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalEncoderCesu8.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m[32m// CESU-8 decoder is not implemented in Node v4.0+[m
[32m+[m
[32m+[m[32mfunction InternalDecoderCesu8(options, codec) {[m
[32m+[m[32m    this.acc = 0;[m
[32m+[m[32m    this.contBytes = 0;[m
[32m+[m[32m    this.accBytes = 0;[m
[32m+[m[32m    this.defaultCharUnicode = codec.defaultCharUnicode;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalDecoderCesu8.prototype.write = function(buf) {[m
[32m+[m[32m    var acc = this.acc, contBytes = this.contBytes, accBytes = this.accBytes,[m[41m [m
[32m+[m[32m        res = '';[m
[32m+[m[32m    for (var i = 0; i < buf.length; i++) {[m
[32m+[m[32m        var curByte = buf[i];[m
[32m+[m[32m        if ((curByte & 0xC0) !== 0x80) { // Leading byte[m
[32m+[m[32m            if (contBytes > 0) { // Previous code is invalid[m
[32m+[m[32m                res += this.defaultCharUnicode;[m
[32m+[m[32m                contBytes = 0;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (curByte < 0x80) { // Single-byte code[m
[32m+[m[32m                res += String.fromCharCode(curByte);[m
[32m+[m[32m            } else if (curByte < 0xE0) { // Two-byte code[m
[32m+[m[32m                acc = curByte & 0x1F;[m
[32m+[m[32m                contBytes = 1; accBytes = 1;[m
[32m+[m[32m            } else if (curByte < 0xF0) { // Three-byte code[m
[32m+[m[32m                acc = curByte & 0x0F;[m
[32m+[m[32m                contBytes = 2; accBytes = 1;[m
[32m+[m[32m            } else { // Four or more are not supported for CESU-8.[m
[32m+[m[32m                res += this.defaultCharUnicode;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else { // Continuation byte[m
[32m+[m[32m            if (contBytes > 0) { // We're waiting for it.[m
[32m+[m[32m                acc = (acc << 6) | (curByte & 0x3f);[m
[32m+[m[32m                contBytes--; accBytes++;[m
[32m+[m[32m                if (contBytes === 0) {[m
[32m+[m[32m                    // Check for overlong encoding, but support Modified UTF-8 (encoding NULL as C0 80)[m
[32m+[m[32m                    if (accBytes === 2 && acc < 0x80 && acc > 0)[m
[32m+[m[32m                        res += this.defaultCharUnicode;[m
[32m+[m[32m                    else if (accBytes === 3 && acc < 0x800)[m
[32m+[m[32m                        res += this.defaultCharUnicode;[m
[32m+[m[32m                    else[m
[32m+[m[32m                        // Actually add character.[m
[32m+[m[32m                        res += String.fromCharCode(acc);[m
[32m+[m[32m                }[m
[32m+[m[32m            } else { // Unexpected continuation byte[m
[32m+[m[32m                res += this.defaultCharUnicode;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    this.acc = acc; this.contBytes = contBytes; this.accBytes = accBytes;[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mInternalDecoderCesu8.prototype.end = function() {[m
[32m+[m[32m    var res = 0;[m
[32m+[m[32m    if (this.contBytes > 0)[m
[32m+[m[32m        res += this.defaultCharUnicode;[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/iconv-lite/encodings/sbcs-codec.js b/node_modules/iconv-lite/encodings/sbcs-codec.js[m
[1mnew file mode 100644[m
[1mindex 0000000..abac5ff[m
[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/encodings/sbcs-codec.js[m
[36m@@ -0,0 +1,72 @@[m
[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32m// Single-byte codec. Needs a 'chars' string parameter that contains 256 or 128 chars that[m
[32m+[m[32m// correspond to encoded bytes (if 128 - then lower half is ASCII).[m[41m [m
[32m+[m
[32m+[m[32mexports._sbcs = SBCSCodec;[m
[32m+[m[32mfunction SBCSCodec(codecOptions, iconv) {[m
[32m+[m[32m    if (!codecOptions)[m
[32m+[m[32m        throw new Error("SBCS codec is called without the data.")[m
[32m+[m[41m    [m
[32m+[m[32m    // Prepare char buffer for decoding.[m
[32m+[m[32m    if (!codecOptions.chars || (codecOptions.chars.length !== 128 && codecOptions.chars.length !== 256))[m
[32m+[m[32m        throw new Error("Encoding '"+codecOptions.type+"' has incorrect 'chars' (must be of len 128 or 256)");[m
[32m+[m[41m    [m
[32m+[m[32m    if (codecOptions.chars.length === 128) {[m
[32m+[m[32m        var asciiString = "";[m
[32m+[m[32m        for (var i = 0; i < 128; i++)[m
[32m+[m[32m            asciiString += String.fromCharCode(i);[m
[32m+[m[32m        codecOptions.chars = asciiString + codecOptions.chars;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.decodeBuf = Buffer.from(codecOptions.chars, 'ucs2');[m
[32m+[m[41m    [m
[32m+[m[32m    // Encoding buffer.[m
[32m+[m[32m    var encodeBuf = Buffer.alloc(65536, iconv.defaultCharSingleByte.charCodeAt(0));[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < codecOptions.chars.length; i++)[m
[32m+[m[32m        encodeBuf[codecOptions.chars.charCodeAt(i)] = i;[m
[32m+[m
[32m+[m[32m    this.encodeBuf = encodeBuf;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSBCSCodec.prototype.encoder = SBCSEncoder;[m
[32m+[m[32mSBCSCodec.prototype.decoder = SBCSDecoder;[m
[32m+[m
[32m+[m
[32m+[m[32mfunction SBCSEncoder(options, codec) {[m
[32m+[m[32m    this.encodeBuf = codec.encodeBuf;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSBCSEncoder.prototype.write = function(str) {[m
[32m+[m[32m    var buf = Buffer.alloc(str.length);[m
[32m+[m[32m    for (var i = 0; i < str.length; i++)[m
[32m+[m[32m        buf[i] = this.encodeBuf[str.charCodeAt(i)];[m
[32m+[m[41m    [m
[32m+[m[32m    return buf;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSBCSEncoder.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction SBCSDecoder(options, codec) {[m
[32m+[m[32m    this.decodeBuf = codec.decodeBuf;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSBCSDecoder.prototype.write = function(buf) {[m
[32m+[m[32m    // Strings are immutable in JS -> we use ucs2 buffer to speed up computations.[m
[32m+[m[32m    var decodeBuf = this.decodeBuf;[m
[32m+[m[32m    var newBuf = Buffer.alloc(buf.length*2);[m
[32m+[m[32m    var idx1 = 0, idx2 = 0;[m
[32m+[m[32m    for (var i = 0; i < buf.length; i++) {[m
[32m+[m[32m        idx1 = buf[i]*2; idx2 = i*2;[m
[32m+[m[32m        newBuf[idx2] = decodeBuf[idx1];[m
[32m+[m[32m        newBuf[idx2+1] = decodeBuf[idx1+1];[m
[32m+[m[32m    }[m
[32m+[m[32m    return newBuf.toString('ucs2');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSBCSDecoder.prototype.end = function() {[m
[32m+[m[32m}[m
