[32m+[m[32m  "description": "Sign and unsign cookies",[m
[32m+[m[32m  "keywords": ["cookie", "sign", "unsign"],[m
[32m+[m[32m  "author": "TJ Holowaychuk <tj@learnboost.com>",[m
[32m+[m[32m  "license": "MIT",[m
[32m+[m[32m  "repository": { "type": "git", "url": "https://github.com/visionmedia/node-cookie-signature.git"},[m
[32m+[m[32m  "dependencies": {},[m
