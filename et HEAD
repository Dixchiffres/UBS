[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32m// Description of supported double byte encodings and aliases.[m
[32m+[m[32m// Tables are not require()-d until they are needed to speed up library load.[m
[32m+[m[32m// require()-s are direct to support Browserify.[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[41m    [m
[32m+[m[32m    // == Japanese/ShiftJIS ====================================================[m
[32m+[m[32m    // All japanese encodings are based on JIS X set of standards:[m
[32m+[m[32m    // JIS X 0201 - Single-byte encoding of ASCII + ¥ + Kana chars at 0xA1-0xDF.[m
[32m+[m[32m    // JIS X 0208 - Main set of 6879 characters, placed in 94x94 plane, to be encoded by 2 bytes.[m[41m [m
[32m+[m[32m    //              Has several variations in 1978, 1983, 1990 and 1997.[m
[32m+[m[32m    // JIS X 0212 - Supplementary plane of 6067 chars in 94x94 plane. 1990. Effectively dead.[m
[32m+[m[32m    // JIS X 0213 - Extension and modern replacement of 0208 and 0212. Total chars: 11233.[m
[32m+[m[32m    //              2 planes, first is superset of 0208, second - revised 0212.[m
[32m+[m[32m    //              Introduced in 2000, revised 2004. Some characters are in Unicode Plane 2 (0x2xxxx)[m
[32m+[m
[32m+[m[32m    // Byte encodings are:[m
[32m+[m[32m    //  * Shift_JIS: Compatible with 0201, uses not defined chars in top half as lead bytes for double-byte[m
[32m+[m[32m    //               encoding of 0208. Lead byte ranges: 0x81-0x9F, 0xE0-0xEF; Trail byte ranges: 0x40-0x7E, 0x80-0x9E, 0x9F-0xFC.[m
[32m+[m[32m    //               Windows CP932 is a superset of Shift_JIS. Some companies added more chars, notably KDDI.[m
[32m+[m[32m    //  * EUC-JP:    Up to 3 bytes per character. Used mostly on *nixes.[m
[32m+[m[32m    //               0x00-0x7F       - lower part of 0201[m
[32m+[m[32m    //               0x8E, 0xA1-0xDF - upper part of 0201[m
[32m+[m[32m    //               (0xA1-0xFE)x2   - 0208 plane (94x94).[m
[32m+[m[32m    //               0x8F, (0xA1-0xFE)x2 - 0212 plane (94x94).[m
[32m+[m[32m    //  * JIS X 208: 7-bit, direct encoding of 0208. Byte ranges: 0x21-0x7E (94 values). Uncommon.[m
[32m+[m[32m    //               Used as-is in ISO2022 family.[m
[32m+[m[32m    //  * ISO2022-JP: Stateful encoding, with escape sequences to switch between ASCII,[m[41m [m
[32m+[m[32m    //                0201-1976 Roman, 0208-1978, 0208-1983.[m
[32m+[m[32m    //  * ISO2022-JP-1: Adds esc seq for 0212-1990.[m
[32m+[m[32m    //  * ISO2022-JP-2: Adds esc seq for GB2313-1980, KSX1001-1992, ISO8859-1, ISO8859-7.[m
[32m+[m[32m    //  * ISO2022-JP-3: Adds esc seq for 0201-1976 Kana set, 0213-2000 Planes 1, 2.[m
[32m+[m[32m    //  * ISO2022-JP-2004: Adds 0213-2004 Plane 1.[m
[32m+[m[32m    //[m
[32m+[m[32m    // After JIS X 0213 appeared, Shift_JIS-2004, EUC-JISX0213 and ISO2022-JP-2004 followed, with just changing the planes.[m
[32m+[m[32m    //[m
[32m+[m[32m    // Overall, it seems that it's a mess :( http://www8.plala.or.jp/tkubota1/unicode-symbols-map2.html[m
[32m+[m
[32m+[m[32m    'shiftjis': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/shiftjis.json') },[m
[32m+[m[32m        encodeAdd: {'\u00a5': 0x5C, '\u203E': 0x7E},[m
[32m+[m[32m        encodeSkipVals: [{from: 0xED40, to: 0xF940}],[m
[32m+[m[32m    },[m
[32m+[m[32m    'csshiftjis': 'shiftjis',[m
[32m+[m[32m    'mskanji': 'shiftjis',[m
[32m+[m[32m    'sjis': 'shiftjis',[m
[32m+[m[32m    'windows31j': 'shiftjis',[m
[32m+[m[32m    'ms31j': 'shiftjis',[m
[32m+[m[32m    'xsjis': 'shiftjis',[m
[32m+[m[32m    'windows932': 'shiftjis',[m
[32m+[m[32m    'ms932': 'shiftjis',[m
[32m+[m[32m    '932': 'shiftjis',[m
[32m+[m[32m    'cp932': 'shiftjis',[m
[32m+[m
[32m+[m[32m    'eucjp': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/eucjp.json') },[m
[32m+[m[32m        encodeAdd: {'\u00a5': 0x5C, '\u203E': 0x7E},[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    // TODO: KDDI extension to Shift_JIS[m
[32m+[m[32m    // TODO: IBM CCSID 942 = CP932, but F0-F9 custom chars and other char changes.[m
[32m+[m[32m    // TODO: IBM CCSID 943 = Shift_JIS = CP932 with original Shift_JIS lower 128 chars.[m
[32m+[m
[32m+[m
[32m+[m[32m    // == Chinese/GBK ==========================================================[m
[32m+[m[32m    // http://en.wikipedia.org/wiki/GBK[m
[32m+[m[32m    // We mostly implement W3C recommendation: https://www.w3.org/TR/encoding/#gbk-encoder[m
[32m+[m
[32m+[m[32m    // Oldest GB2312 (1981, ~7600 chars) is a subset of CP936[m
[32m+[m[32m    'gb2312': 'cp936',[m
[32m+[m[32m    'gb231280': 'cp936',[m
[32m+[m[32m    'gb23121980': 'cp936',[m
[32m+[m[32m    'csgb2312': 'cp936',[m
[32m+[m[32m    'csiso58gb231280': 'cp936',[m
[32m+[m[32m    'euccn': 'cp936',[m
[32m+[m
[32m+[m[32m    // Microsoft's CP936 is a subset and approximation of GBK.[m
[32m+[m[32m    'windows936': 'cp936',[m
[32m+[m[32m    'ms936': 'cp936',[m
[32m+[m[32m    '936': 'cp936',[m
[32m+[m[32m    'cp936': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp936.json') },[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    // GBK (~22000 chars) is an extension of CP936 that added user-mapped chars and some other.[m
[32m+[m[32m    'gbk': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp936.json').concat(require('./tables/gbk-added.json')) },[m
[32m+[m[32m    },[m
[32m+[m[32m    'xgbk': 'gbk',[m
[32m+[m[32m    'isoir58': 'gbk',[m
[32m+[m
[32m+[m[32m    // GB18030 is an algorithmic extension of GBK.[m
[32m+[m[32m    // Main source: https://www.w3.org/TR/encoding/#gbk-encoder[m
[32m+[m[32m    // http://icu-project.org/docs/papers/gb18030.html[m
[32m+[m[32m    // http://source.icu-project.org/repos/icu/data/trunk/charset/data/xml/gb-18030-2000.xml[m
[32m+[m[32m    // http://www.khngai.com/chinese/charmap/tblgbk.php?page=0[m
[32m+[m[32m    'gb18030': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp936.json').concat(require('./tables/gbk-added.json')) },[m
[32m+[m[32m        gb18030: function() { return require('./tables/gb18030-ranges.json') },[m
[32m+[m[32m        encodeSkipVals: [0x80],[m
[32m+[m[32m        encodeAdd: {'€': 0xA2E3},[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    'chinese': 'gb18030',[m
[32m+[m
[32m+[m
[32m+[m[32m    // == Korean ===============================================================[m
[32m+[m[32m    // EUC-KR, KS_C_5601 and KS X 1001 are exactly the same.[m
[32m+[m[32m    'windows949': 'cp949',[m
[32m+[m[32m    'ms949': 'cp949',[m
[32m+[m[32m    '949': 'cp949',[m
[32m+[m[32m    'cp949': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp949.json') },[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    'cseuckr': 'cp949',[m
[32m+[m[32m    'csksc56011987': 'cp949',[m
[32m+[m[32m    'euckr': 'cp949',[m
[32m+[m[32m    'isoir149': 'cp949',[m
[32m+[m[32m    'korean': 'cp949',[m
[32m+[m[32m    'ksc56011987': 'cp949',[m
[32m+[m[32m    'ksc56011989': 'cp949',[m
[32m+[m[32m    'ksc5601': 'cp949',[m
[32m+[m
[32m+[m
[32m+[m[32m    // == Big5/Taiwan/Hong Kong ================================================[m
[32m+[m[32m    // There are lots of tables for Big5 and cp950. Please see the following links for history:[m
[32m+[m[32m    // http://moztw.org/docs/big5/  http://www.haible.de/bruno/charsets/conversion-tables/Big5.html[m
[32m+[m[32m    // Variations, in roughly number of defined chars:[m
[32m+[m[32m    //  * Windows CP 950: Microsoft variant of Big5. Canonical: http://www.unicode.org/Public/MAPPINGS/VENDORS/MICSFT/WINDOWS/CP950.TXT[m
[32m+[m[32m    //  * Windows CP 951: Microsoft variant of Big5-HKSCS-2001. Seems to be never public. http://me.abelcheung.org/articles/research/what-is-cp951/[m
[32m+[m[32m    //  * Big5-2003 (Taiwan standard) almost superset of cp950.[m
[32m+[m[32m    //  * Unicode-at-on (UAO) / Mozilla 1.8. Falling out of use on the Web. Not supported by other browsers.[m
[32m+[m[32m    //  * Big5-HKSCS (-2001, -2004, -2008). Hong Kong standard.[m[41m [m
[32m+[m[32m    //    many unicode code points moved from PUA to Supplementary plane (U+2XXXX) over the years.[m
[32m+[m[32m    //    Plus, it has 4 combining sequences.[m
[32m+[m[32m    //    Seems that Mozilla refused to support it for 10 yrs. https://bugzilla.mozilla.org/show_bug.cgi?id=162431 https://bugzilla.mozilla.org/show_bug.cgi?id=310299[m
[32m+[m[32m    //    because big5-hkscs is the only encoding to include astral characters in non-algorithmic way.[m
[32m+[m[32m    //    Implementations are not consistent within browsers; sometimes labeled as just big5.[m
[32m+[m[32m    //    MS Internet Explorer switches from big5 to big5-hkscs when a patch applied.[m
[32m+[m[32m    //    Great discussion & recap of what's going on https://bugzilla.mozilla.org/show_bug.cgi?id=912470#c31[m
[32m+[m[32m    //    In the encoder, it might make sense to support encoding old PUA mappings to Big5 bytes seq-s.[m
[32m+[m[32m    //    Official spec: http://www.ogcio.gov.hk/en/business/tech_promotion/ccli/terms/doc/2003cmp_2008.txt[m
[32m+[m[32m    //                   http://www.ogcio.gov.hk/tc/business/tech_promotion/ccli/terms/doc/hkscs-2008-big5-iso.txt[m
[32m+[m[32m    //[m[41m [m
[32m+[m[32m    // Current understanding of how to deal with Big5(-HKSCS) is in the Encoding Standard, http://encoding.spec.whatwg.org/#big5-encoder[m
[32m+[m[32m    // Unicode mapping (http://www.unicode.org/Public/MAPPINGS/OBSOLETE/EASTASIA/OTHER/BIG5.TXT) is said to be wrong.[m
[32m+[m
[32m+[m[32m    'windows950': 'cp950',[m
[32m+[m[32m    'ms950': 'cp950',[m
[32m+[m[32m    '950': 'cp950',[m
[32m+[m[32m    'cp950': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp950.json') },[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    // Big5 has many variations and is an extension of cp950. We use Encoding Standard's as a consensus.[m
[32m+[m[32m    'big5': 'big5hkscs',[m
[32m+[m[32m    'big5hkscs': {[m
[32m+[m[32m        type: '_dbcs',[m
[32m+[m[32m        table: function() { return require('./tables/cp950.json').concat(require('./tables/big5-added.json')) },[m
[32m+[m[32m        encodeSkipVals: [0xa2cc],[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    'cnbig5': 'big5hkscs',[m
[32m+[m[32m    'csbig5': 'big5hkscs',[m
[32m+[m[32m    'xxbig5': 'big5hkscs',[m
[32m+[m[32m};[m
