[32m+[m[32m# media-typer[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Node.js Version][node-version-image]][node-version-url][m
[32m+[m[32m[![Build Status][travis-image]][travis-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mSimple RFC 6838 media type parser[m
[32m+[m
[32m+[m[32m## Installation[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install media-typer[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar typer = require('media-typer')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### typer.parse(string)[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar obj = typer.parse('image/svg+xml; charset=utf-8')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mParse a media type string. This will return an object with the following[m
[32m+[m[32mproperties (examples are shown for the string `'image/svg+xml; charset=utf-8'`):[m
[32m+[m
[32m+[m[32m - `type`: The type of the media type (always lower case). Example: `'image'`[m
[32m+[m
[32m+[m[32m - `subtype`: The subtype of the media type (always lower case). Example: `'svg'`[m
[32m+[m
[32m+[m[32m - `suffix`: The suffix of the media type (always lower case). Example: `'xml'`[m
[32m+[m
[32m+[m[32m - `parameters`: An object of the parameters in the media type (name of parameter always lower case). Example: `{charset: 'utf-8'}`[m
[32m+[m
[32m+[m[32m### typer.parse(req)[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar obj = typer.parse(req)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mParse the `content-type` header from the given `req`. Short-cut for[m
[32m+[m[32m`typer.parse(req.headers['content-type'])`.[m
[32m+[m
[32m+[m[32m### typer.parse(res)[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar obj = typer.parse(res)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mParse the `content-type` header set on the given `res`. Short-cut for[m
[32m+[m[32m`typer.parse(res.getHeader('content-type'))`.[m
[32m+[m
[32m+[m[32m### typer.format(obj)[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar obj = typer.format({type: 'image', subtype: 'svg', suffix: 'xml'})[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mFormat an object into a media type string. This will return a string of the[m
[32m+[m[32mmime type for the given object. For the properties of the object, see the[m
[32m+[m[32mdocumentation for `typer.parse(string)`.[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[MIT](LICENSE)[m
[32m+[m
[32m+[m[32m[npm-image]: https://img.shields.io/npm/v/media-typer.svg?style=flat[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/media-typer[m
[32m+[m[32m[node-version-image]: https://img.shields.io/badge/node.js-%3E%3D_0.6-brightgreen.svg?style=flat[m
[32m+[m[32m[node-version-url]: http://nodejs.org/download/[m
[32m+[m[32m[travis-image]: https://img.shields.io/travis/jshttp/media-typer.svg?style=flat[m
[32m+[m[32m[travis-url]: https://travis-ci.org/jshttp/media-typer[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/jshttp/media-typer.svg?style=flat[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/jshttp/media-typer[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/media-typer.svg?style=flat[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/media-typer[m
[1mdiff --git a/node_modules/media-typer/index.js b/node_modules/media-typer/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..07f7295[m
[1m--- /dev/null[m
[1m+++ b/node_modules/media-typer/index.js[m
[36m@@ -0,0 +1,270 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * media-typer[m
[32m+[m[32m * Copyright(c) 2014 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match *( ";" parameter ) in RFC 2616 sec 3.7[m
[32m+[m[32m *[m
[32m+[m[32m * parameter     = token "=" ( token | quoted-string )[m
[32m+[m[32m * token         = 1*<any CHAR except CTLs or separators>[m
[32m+[m[32m * separators    = "(" | ")" | "<" | ">" | "@"[m
[32m+[m[32m *               | "," | ";" | ":" | "\" | <">[m
[32m+[m[32m *               | "/" | "[" | "]" | "?" | "="[m
[32m+[m[32m *               | "{" | "}" | SP | HT[m
[32m+[m[32m * quoted-string = ( <"> *(qdtext | quoted-pair ) <"> )[m
[32m+[m[32m * qdtext        = <any TEXT except <">>[m
[32m+[m[32m * quoted-pair   = "\" CHAR[m
[32m+[m[32m * CHAR          = <any US-ASCII character (octets 0 - 127)>[m
[32m+[m[32m * TEXT          = <any OCTET except CTLs, but including LWS>[m
[32m+[m[32m * LWS           = [CRLF] 1*( SP | HT )[m
[32m+[m[32m * CRLF          = CR LF[m
[32m+[m[32m * CR            = <US-ASCII CR, carriage return (13)>[m
[32m+[m[32m * LF            = <US-ASCII LF, linefeed (10)>[m
[32m+[m[32m * SP            = <US-ASCII SP, space (32)>[m
[32m+[m[32m * SHT           = <US-ASCII HT, horizontal-tab (9)>[m
[32m+[m[32m * CTL           = <any US-ASCII control character (octets 0 - 31) and DEL (127)>[m
[32m+[m[32m * OCTET         = <any 8-bit sequence of data>[m
[32m+[m[32m */[m
[32m+[m[32mvar paramRegExp = /; *([!#$%&'\*\+\-\.0-9A-Z\^_`a-z\|~]+) *= *("(?:[ !\u0023-\u005b\u005d-\u007e\u0080-\u00ff]|\\[\u0020-\u007e])*"|[!#$%&'\*\+\-\.0-9A-Z\^_`a-z\|~]+) */g;[m
[32m+[m[32mvar textRegExp = /^[\u0020-\u007e\u0080-\u00ff]+$/[m
[32m+[m[32mvar tokenRegExp = /^[!#$%&'\*\+\-\.0-9A-Z\^_`a-z\|~]+$/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match quoted-pair in RFC 2616[m
[32m+[m[32m *[m
[32m+[m[32m * quoted-pair = "\" CHAR[m
[32m+[m[32m * CHAR        = <any US-ASCII character (octets 0 - 127)>[m
[32m+[m[32m */[m
[32m+[m[32mvar qescRegExp = /\\([\u0000-\u007f])/g;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match chars that must be quoted-pair in RFC 2616[m
[32m+[m[32m */[m
[32m+[m[32mvar quoteRegExp = /([\\"])/g;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match type in RFC 6838[m
[32m+[m[32m *[m
[32m+[m[32m * type-name = restricted-name[m
[32m+[m[32m * subtype-name = restricted-name[m
[32m+[m[32m * restricted-name = restricted-name-first *126restricted-name-chars[m
[32m+[m[32m * restricted-name-first  = ALPHA / DIGIT[m
[32m+[m[32m * restricted-name-chars  = ALPHA / DIGIT / "!" / "#" /[m
[32m+[m[32m *                          "$" / "&" / "-" / "^" / "_"[m
[32m+[m[32m * restricted-name-chars =/ "." ; Characters before first dot always[m
[32m+[m[32m *                              ; specify a facet name[m
[32m+[m[32m * restricted-name-chars =/ "+" ; Characters after last plus always[m
[32m+[m[32m *                              ; specify a structured syntax suffix[m
[32m+[m[32m * ALPHA =  %x41-5A / %x61-7A   ; A-Z / a-z[m
[32m+[m[32m * DIGIT =  %x30-39             ; 0-9[m
[32m+[m[32m */[m
[32m+[m[32mvar subtypeNameRegExp = /^[A-Za-z0-9][A-Za-z0-9!#$&^_.-]{0,126}$/[m
[32m+[m[32mvar typeNameRegExp = /^[A-Za-z0-9][A-Za-z0-9!#$&^_-]{0,126}$/[m
[32m+[m[32mvar typeRegExp = /^ *([A-Za-z0-9][A-Za-z0-9!#$&^_-]{0,126})\/([A-Za-z0-9][A-Za-z0-9!#$&^_.+-]{0,126}) *$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.format = format[m
[32m+[m[32mexports.parse = parse[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format object to media type.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} obj[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction format(obj) {[m
[32m+[m[32m  if (!obj || typeof obj !== 'object') {[m
[32m+[m[32m    throw new TypeError('argument obj is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var parameters = obj.parameters[m
[32m+[m[32m  var subtype = obj.subtype[m
[32m+[m[32m  var suffix = obj.suffix[m
[32m+[m[32m  var type = obj.type[m
[32m+[m
[32m+[m[32m  if (!type || !typeNameRegExp.test(type)) {[m
[32m+[m[32m    throw new TypeError('invalid type')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!subtype || !subtypeNameRegExp.test(subtype)) {[m
[32m+[m[32m    throw new TypeError('invalid subtype')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // format as type/subtype[m
[32m+[m[32m  var string = type + '/' + subtype[m
[32m+[m
[32m+[m[32m  // append +suffix[m
[32m+[m[32m  if (suffix) {[m
[32m+[m[32m    if (!typeNameRegExp.test(suffix)) {[m
[32m+[m[32m      throw new TypeError('invalid suffix')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    string += '+' + suffix[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // append parameters[m
[32m+[m[32m  if (parameters && typeof parameters === 'object') {[m
[32m+[m[32m    var param[m
[32m+[m[32m    var params = Object.keys(parameters).sort()[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < params.length; i++) {[m
[32m+[m[32m      param = params[i][m
[32m+[m
[32m+[m[32m      if (!tokenRegExp.test(param)) {[m
[32m+[m[32m        throw new TypeError('invalid parameter name')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      string += '; ' + param + '=' + qstring(parameters[param])[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return string[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse media type to object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|object} string[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse(string) {[m
[32m+[m[32m  if (!string) {[m
[32m+[m[32m    throw new TypeError('argument string is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // support req/res-like objects as argument[m
[32m+[m[32m  if (typeof string === 'object') {[m
[32m+[m[32m    string = getcontenttype(string)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof string !== 'string') {[m
[32m+[m[32m    throw new TypeError('argument string is required to be a string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var index = string.indexOf(';')[m
[32m+[m[32m  var type = index !== -1[m
[32m+[m[32m    ? string.substr(0, index)[m
[32m+[m[32m    : string[m
[32m+[m
[32m+[m[32m  var key[m
[32m+[m[32m  var match[m
[32m+[m[32m  var obj = splitType(type)[m
[32m+[m[32m  var params = {}[m
[32m+[m[32m  var value[m
[32m+[m
[32m+[m[32m  paramRegExp.lastIndex = index[m
[32m+[m
[32m+[m[32m  while (match = paramRegExp.exec(string)) {[m
[32m+[m[32m    if (match.index !== index) {[m
[32m+[m[32m      throw new TypeError('invalid parameter format')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    index += match[0].length[m
[32m+[m[32m    key = match[1].toLowerCase()[m
[32m+[m[32m    value = match[2][m
[32m+[m
[32m+[m[32m    if (value[0] === '"') {[m
[32m+[m[32m      // remove quotes and escapes[m
[32m+[m[32m      value = value[m
[32m+[m[32m        .substr(1, value.length - 2)[m
[32m+[m[32m        .replace(qescRegExp, '$1')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    params[key] = value[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (index !== -1 && index !== string.length) {[m
[32m+[m[32m    throw new TypeError('invalid parameter format')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  obj.parameters = params[m
[32m+[m
[32m+[m[32m  return obj[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get content-type from req/res objects.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object}[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getcontenttype(obj) {[m
[32m+[m[32m  if (typeof obj.getHeader === 'function') {[m
[32m+[m[32m    // res-like[m
[32m+[m[32m    return obj.getHeader('content-type')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof obj.headers === 'object') {[m
[32m+[m[32m    // req-like[m
[32m+[m[32m    return obj.headers && obj.headers['content-type'][m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Quote a string if necessary.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction qstring(val) {[m
[32m+[m[32m  var str = String(val)[m
[32m+[m
[32m+[m[32m  // no need to quote tokens[m
[32m+[m[32m  if (tokenRegExp.test(str)) {[m
[32m+[m[32m    return str[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (str.length > 0 && !textRegExp.test(str)) {[m
[32m+[m[32m    throw new TypeError('invalid parameter value')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return '"' + str.replace(quoteRegExp, '\\$1') + '"'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Simply "type/subtype+siffx" into parts.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} string[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction splitType(string) {[m
[32m+[m[32m  var match = typeRegExp.exec(string.toLowerCase())[m
[32m+[m
[32m+[m[32m  if (!match) {[m
[32m+[m[32m    throw new TypeError('invalid media type')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var type = match[1][m
[32m+[m[32m  var subtype = match[2][m
[32m+[m[32m  var suffix[m
[32m+[m
[32m+[m[32m  // suffix after last +[m
[32m+[m[32m  var index = subtype.lastIndexOf('+')[m
[32m+[m[32m  if (index !== -1) {[m
[32m+[m[32m    suffix = subtype.substr(index + 1)[m
[32m+[m[32m    subtype = subtype.substr(0, index)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var obj = {[m
[32m+[m[32m    type: type,[m
[32m+[m[32m    subtype: subtype,[m
[32m+[m[32m    suffix: suffix[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return obj[m
[32m+[m[32m}[m
