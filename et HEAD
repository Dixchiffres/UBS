[32m+[m[32mdeclare module "ipaddr.js" {[m
[32m+[m[32m    type IPv4Range = 'unicast' | 'unspecified' | 'broadcast' | 'multicast' | 'linkLocal' | 'loopback' | 'carrierGradeNat' | 'private' | 'reserved';[m
[32m+[m[32m    type IPv6Range = 'unicast' | 'unspecified' | 'linkLocal' | 'multicast' | 'loopback' | 'uniqueLocal' | 'ipv4Mapped' | 'rfc6145' | 'rfc6052' | '6to4' | 'teredo' | 'reserved';[m
[32m+[m
[32m+[m[32m    interface RangeList<T> {[m
[32m+[m[32m        [name: string]: [T, number] | [T, number][];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Common methods/properties for IPv4 and IPv6 classes.[m
[32m+[m[32m    class IP {[m
[32m+[m[32m        prefixLengthFromSubnetMask(): number | null;[m
[32m+[m[32m        toByteArray(): number[];[m
[32m+[m[32m        toNormalizedString(): string;[m
[32m+[m[32m        toString(): string;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    namespace Address {[m
[32m+[m[32m        export function isValid(addr: string): boolean;[m
[32m+[m[32m        export function fromByteArray(bytes: number[]): IPv4 | IPv6;[m
[32m+[m[32m        export function parse(addr: string): IPv4 | IPv6;[m
[32m+[m[32m        export function parseCIDR(mask: string): [IPv4 | IPv6, number];[m
[32m+[m[32m        export function process(addr: string): IPv4 | IPv6;[m
[32m+[m[32m        export function subnetMatch(addr: IPv4, rangeList: RangeList<IPv4>, defaultName?: string): string;[m
[32m+[m[32m        export function subnetMatch(addr: IPv6, rangeList: RangeList<IPv6>, defaultName?: string): string;[m
[32m+[m
[32m+[m[32m        export class IPv4 extends IP {[m
[32m+[m[32m            static broadcastAddressFromCIDR(addr: string): IPv4;[m
[32m+[m[32m            static isIPv4(addr: string): boolean;[m
[32m+[m[32m            static isValidFourPartDecimal(addr: string): boolean;[m
[32m+[m[32m            static isValid(addr: string): boolean;[m
[32m+[m[32m            static networkAddressFromCIDR(addr: string): IPv4;[m
[32m+[m[32m            static parse(addr: string): IPv4;[m
[32m+[m[32m            static parseCIDR(addr: string): [IPv4, number];[m
[32m+[m[32m            static subnetMaskFromPrefixLength(prefix: number): IPv4;[m
[32m+[m[32m            constructor(octets: number[]);[m
[32m+[m[32m            octets: number[][m
[32m+[m
[32m+[m[32m            kind(): 'ipv4';[m
[32m+[m[32m            match(addr: IPv4, bits: number): boolean;[m
[32m+[m[32m            match(mask: [IPv4, number]): boolean;[m
[32m+[m[32m            range(): IPv4Range;[m
[32m+[m[32m            subnetMatch(rangeList: RangeList<IPv4>, defaultName?: string): string;[m
[32m+[m[32m            toIPv4MappedAddress(): IPv6;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        export class IPv6 extends IP {[m
[32m+[m[32m            static broadcastAddressFromCIDR(addr: string): IPv6;[m
[32m+[m[32m            static isIPv6(addr: string): boolean;[m
[32m+[m[32m            static isValid(addr: string): boolean;[m
[32m+[m[32m            static parse(addr: string): IPv6;[m
[32m+[m[32m            static parseCIDR(addr: string): [IPv6, number];[m
[32m+[m[32m            static subnetMaskFromPrefixLength(prefix: number): IPv6;[m
[32m+[m[32m            constructor(parts: number[]);[m
[32m+[m[32m            parts: number[][m
[32m+[m[32m            zoneId?: string[m
[32m+[m
[32m+[m[32m            isIPv4MappedAddress(): boolean;[m
[32m+[m[32m            kind(): 'ipv6';[m
[32m+[m[32m            match(addr: IPv6, bits: number): boolean;[m
[32m+[m[32m            match(mask: [IPv6, number]): boolean;[m
[32m+[m[32m            range(): IPv6Range;[m
[32m+[m[32m            subnetMatch(rangeList: RangeList<IPv6>, defaultName?: string): string;[m
[32m+[m[32m            toIPv4Address(): IPv4;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    export = Address;[m
[32m+[m[32m}[m
