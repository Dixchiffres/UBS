[32m+[m			[32m},[m
[32m+[m			[32m'existing value on sealed object has expected descriptor'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mdefineDataProperty(sealed, 'existing', 'new value');[m
[32m+[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(sealed, 'existing'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: false,[m
[32m+[m				[32menumerable: true,[m
[32m+[m				[32mvalue: 'new value',[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'existing value on sealed object has changed descriptor'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst['throws']([m
[32m+[m			[32mfunction () { defineDataProperty(sealed, 'new', 'new property'); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32m'sealed object can not add a new property'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.test('nonextensible object, ES5+', { skip: !hasPropertyDescriptors }, function (st) {[m
[32m+[m		[32mvar nonExt = Object.preventExtensions({ existing: true });[m
[32m+[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(nonExt, 'existing'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: true,[m
[32m+[m				[32menumerable: true,[m
[32m+[m				[32mvalue: true,[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'existing value on non-extensible object has expected descriptor'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mdefineDataProperty(nonExt, 'existing', 'new value', true);[m
[32m+[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(nonExt, 'existing'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: true,[m
[32m+[m				[32menumerable: false,[m
[32m+[m				[32mvalue: 'new value',[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'existing value on non-extensible object has changed descriptor'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst['throws']([m
[32m+[m			[32mfunction () { defineDataProperty(nonExt, 'new', 'new property'); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32m'non-extensible object can not add a new property'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
