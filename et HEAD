[32m+[m[32m        document.getElementById('login-form').addEventListener('submit', function (event) {[m
[32m+[m[32m            event.preventDefault(); // Empêche le rechargement de la page[m
[32m+[m
[32m+[m[32m            // Récupération des valeurs des champs du formulaire[m
[32m+[m[32m            const username = document.getElementById('username').value.trim();[m
[32m+[m[32m            const password = document.getElementById('password').value.trim();[m
[32m+[m
[32m+[m[32m            // Identifiants corrects[m
[32m+[m[32m            const validUsername = 'S09458930';[m
[32m+[m[32m            const validPassword = 'Ingrid0305';[m
[32m+[m
[32m+[m[32m            // Vérification[m
[32m+[m[32m            if (username === validUsername && password === validPassword) {[m
[32m+[m[32m                localStorage.setItem('username', username); // Sauvegarde l'utilisateur dans localStorage[m
[32m+[m[32m                alert('Connexion réussie !');[m
[32m+[m[32m                // Redirection vers la page demandée après connexion réussie[m
[32m+[m[32m                const redirectUrl = new URLSearchParams(window.location.search).get('redirect') || 'comptes.html';[m
[32m+[m[32m                window.location.href = redirectUrl;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                alert('Identifiant ou mot de passe incorrect.');[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    </script>[m
