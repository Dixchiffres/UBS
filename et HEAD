[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar Buffer = require('safe-buffer').Buffer[m
[32m+[m[32mvar contentDisposition = require('content-disposition');[m
[32m+[m[32mvar contentType = require('content-type');[m
[32m+[m[32mvar deprecate = require('depd')('express');[m
[32m+[m[32mvar flatten = require('array-flatten');[m
[32m+[m[32mvar mime = require('send').mime;[m
[32m+[m[32mvar etag = require('etag');[m
[32m+[m[32mvar proxyaddr = require('proxy-addr');[m
[32m+[m[32mvar qs = require('qs');[m
[32m+[m[32mvar querystring = require('querystring');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return strong ETag for `body`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Buffer} body[m
[32m+[m[32m * @param {String} [encoding][m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.etag = createETagGenerator({ weak: false })[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return weak ETag for `body`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Buffer} body[m
[32m+[m[32m * @param {String} [encoding][m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.wetag = createETagGenerator({ weak: true })[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if `path` looks absolute.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} path[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.isAbsolute = function(path){[m
[32m+[m[32m  if ('/' === path[0]) return true;[m
[32m+[m[32m  if (':' === path[1] && ('\\' === path[2] || '/' === path[2])) return true; // Windows device path[m
[32m+[m[32m  if ('\\\\' === path.substring(0, 2)) return true; // Microsoft Azure absolute path[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Flatten the given `arr`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Array} arr[m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.flatten = deprecate.function(flatten,[m
[32m+[m[32m  'utils.flatten: use array-flatten npm module instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Normalize the given `type`, for example "html" becomes "text/html".[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} type[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.normalizeType = function(type){[m
[32m+[m[32m  return ~type.indexOf('/')[m
[32m+[m[32m    ? acceptParams(type)[m
[32m+[m[32m    : { value: mime.lookup(type), params: {} };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Normalize `types`, for example "html" becomes "text/html".[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Array} types[m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.normalizeTypes = function(types){[m
[32m+[m[32m  var ret = [];[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < types.length; ++i) {[m
[32m+[m[32m    ret.push(exports.normalizeType(types[i]));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return ret;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Generate Content-Disposition header appropriate for the filename.[m
[32m+[m[32m * non-ascii filenames are urlencoded and a filename* parameter is added[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} filename[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.contentDisposition = deprecate.function(contentDisposition,[m
[32m+[m[32m  'utils.contentDisposition: use content-disposition npm module instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse accept params `str` returning an[m
[32m+[m[32m * object with `.value`, `.quality` and `.params`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} str[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction acceptParams (str) {[m
[32m+[m[32m  var parts = str.split(/ *; */);[m
[32m+[m[32m  var ret = { value: parts[0], quality: 1, params: {} }[m
[32m+[m
[32m+[m[32m  for (var i = 1; i < parts.length; ++i) {[m
[32m+[m[32m    var pms = parts[i].split(/ *= */);[m
[32m+[m[32m    if ('q' === pms[0]) {[m
[32m+[m[32m      ret.quality = parseFloat(pms[1]);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      ret.params[pms[0]] = pms[1];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return ret;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compile "etag" value to function.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {Boolean|String|Function} val[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.compileETag = function(val) {[m
[32m+[m[32m  var fn;[m
[32m+[m
[32m+[m[32m  if (typeof val === 'function') {[m
[32m+[m[32m    return val;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  switch (val) {[m
[32m+[m[32m    case true:[m
[32m+[m[32m    case 'weak':[m
[32m+[m[32m      fn = exports.wetag;[m
[32m+[m[32m      break;[m
[32m+[m[32m    case false:[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'strong':[m
[32m+[m[32m      fn = exports.etag;[m
[32m+[m[32m      break;[m
[32m+[m[32m    default:[m
[32m+[m[32m      throw new TypeError('unknown value for etag function: ' + val);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return fn;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compile "query parser" value to function.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {String|Function} val[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.compileQueryParser = function compileQueryParser(val) {[m
[32m+[m[32m  var fn;[m
[32m+[m
[32m+[m[32m  if (typeof val === 'function') {[m
[32m+[m[32m    return val;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  switch (val) {[m
[32m+[m[32m    case true:[m
[32m+[m[32m    case 'simple':[m
[32m+[m[32m      fn = querystring.parse;[m
[32m+[m[32m      break;[m
[32m+[m[32m    case false:[m
[32m+[m[32m      fn = newObject;[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'extended':[m
[32m+[m[32m      fn = parseExtendedQueryString;[m
[32m+[m[32m      break;[m
[32m+[m[32m    default:[m
[32m+[m[32m      throw new TypeError('unknown value for query parser function: ' + val);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return fn;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compile "proxy trust" value to function.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {Boolean|String|Number|Array|Function} val[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.compileTrust = function(val) {[m
[32m+[m[32m  if (typeof val === 'function') return val;[m
[32m+[m
[32m+[m[32m  if (val === true) {[m
[32m+[m[32m    // Support plain true/false[m
[32m+[m[32m    return function(){ return true };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof val === 'number') {[m
[32m+[m[32m    // Support trusting hop count[m
[32m+[m[32m    return function(a, i){ return i < val };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof val === 'string') {[m
[32m+[m[32m    // Support comma-separated values[m
[32m+[m[32m    val = val.split(',')[m
[32m+[m[32m      .map(function (v) { return v.trim() })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return proxyaddr.compile(val || []);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set the charset in a given Content-Type string.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} type[m
[32m+[m[32m * @param {String} charset[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.setCharset = function setCharset(type, charset) {[m
[32m+[m[32m  if (!type || !charset) {[m
[32m+[m[32m    return type;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // parse type[m
[32m+[m[32m  var parsed = contentType.parse(type);[m
[32m+[m
[32m+[m[32m  // set charset[m
[32m+[m[32m  parsed.parameters.charset = charset;[m
[32m+[m
[32m+[m[32m  // format type[m
[32m+[m[32m  return contentType.format(parsed);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an ETag generator function, generating ETags with[m
[32m+[m[32m * the given options.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createETagGenerator (options) {[m
[32m+[m[32m  return function generateETag (body, encoding) {[m
[32m+[m[32m    var buf = !Buffer.isBuffer(body)[m
[32m+[m[32m      ? Buffer.from(body, encoding)[m
[32m+[m[32m      : body[m
[32m+[m
[32m+[m[32m    return etag(buf, options)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse an extended query string with qs.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} str[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseExtendedQueryString(str) {[m
[32m+[m[32m  return qs.parse(str, {[m
[32m+[m[32m    allowPrototypes: true[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return new empty object.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction newObject() {[m
[32m+[m[32m  return {};[m
[32m+[m[32m}[m
