[32m+[m[32m(function() {[m
[32m+[m[32m  var expandIPv6, ipaddr, ipv4Part, ipv4Regexes, ipv6Part, ipv6Regexes, matchCIDR, root, zoneIndex;[m
[32m+[m
[32m+[m[32m  ipaddr = {};[m
[32m+[m
[32m+[m[32m  root = this;[m
[32m+[m
[32m+[m[32m  if ((typeof module !== "undefined" && module !== null) && module.exports) {[m
[32m+[m[32m    module.exports = ipaddr;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    root['ipaddr'] = ipaddr;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  matchCIDR = function(first, second, partSize, cidrBits) {[m
[32m+[m[32m    var part, shift;[m
[32m+[m[32m    if (first.length !== second.length) {[m
[32m+[m[32m      throw new Error("ipaddr: cannot match CIDR for objects with different lengths");[m
[32m+[m[32m    }[m
[32m+[m[32m    part = 0;[m
[32m+[m[32m    while (cidrBits > 0) {[m
[32m+[m[32m      shift = partSize - cidrBits;[m
[32m+[m[32m      if (shift < 0) {[m
[32m+[m[32m        shift = 0;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (first[part] >> shift !== second[part] >> shift) {[m
[32m+[m[32m        return false;[m
[32m+[m[32m      }[m
[32m+[m[32m      cidrBits -= partSize;[m
[32m+[m[32m      part += 1;[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.subnetMatch = function(address, rangeList, defaultName) {[m
[32m+[m[32m    var k, len, rangeName, rangeSubnets, subnet;[m
[32m+[m[32m    if (defaultName == null) {[m
[32m+[m[32m      defaultName = 'unicast';[m
[32m+[m[32m    }[m
[32m+[m[32m    for (rangeName in rangeList) {[m
[32m+[m[32m      rangeSubnets = rangeList[rangeName];[m
[32m+[m[32m      if (rangeSubnets[0] && !(rangeSubnets[0] instanceof Array)) {[m
[32m+[m[32m        rangeSubnets = [rangeSubnets];[m
[32m+[m[32m      }[m
[32m+[m[32m      for (k = 0, len = rangeSubnets.length; k < len; k++) {[m
[32m+[m[32m        subnet = rangeSubnets[k];[m
[32m+[m[32m        if (address.kind() === subnet[0].kind()) {[m
[32m+[m[32m          if (address.match.apply(address, subnet)) {[m
[32m+[m[32m            return rangeName;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return defaultName;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4 = (function() {[m
[32m+[m[32m    function IPv4(octets) {[m
[32m+[m[32m      var k, len, octet;[m
[32m+[m[32m      if (octets.length !== 4) {[m
[32m+[m[32m        throw new Error("ipaddr: ipv4 octet count should be 4");[m
[32m+[m[32m      }[m
[32m+[m[32m      for (k = 0, len = octets.length; k < len; k++) {[m
[32m+[m[32m        octet = octets[k];[m
[32m+[m[32m        if (!((0 <= octet && octet <= 255))) {[m
[32m+[m[32m          throw new Error("ipaddr: ipv4 octet should fit in 8 bits");[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      this.octets = octets;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IPv4.prototype.kind = function() {[m
[32m+[m[32m      return 'ipv4';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.toString = function() {[m
[32m+[m[32m      return this.octets.join(".");[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.toNormalizedString = function() {[m
[32m+[m[32m      return this.toString();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.toByteArray = function() {[m
[32m+[m[32m      return this.octets.slice(0);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.match = function(other, cidrRange) {[m
[32m+[m[32m      var ref;[m
[32m+[m[32m      if (cidrRange === void 0) {[m
[32m+[m[32m        ref = other, other = ref[0], cidrRange = ref[1];[m
[32m+[m[32m      }[m
[32m+[m[32m      if (other.kind() !== 'ipv4') {[m
[32m+[m[32m        throw new Error("ipaddr: cannot match ipv4 address with non-ipv4 one");[m
[32m+[m[32m      }[m
[32m+[m[32m      return matchCIDR(this.octets, other.octets, 8, cidrRange);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.SpecialRanges = {[m
[32m+[m[32m      unspecified: [[new IPv4([0, 0, 0, 0]), 8]],[m
[32m+[m[32m      broadcast: [[new IPv4([255, 255, 255, 255]), 32]],[m
[32m+[m[32m      multicast: [[new IPv4([224, 0, 0, 0]), 4]],[m
[32m+[m[32m      linkLocal: [[new IPv4([169, 254, 0, 0]), 16]],[m
[32m+[m[32m      loopback: [[new IPv4([127, 0, 0, 0]), 8]],[m
[32m+[m[32m      carrierGradeNat: [[new IPv4([100, 64, 0, 0]), 10]],[m
[32m+[m[32m      "private": [[new IPv4([10, 0, 0, 0]), 8], [new IPv4([172, 16, 0, 0]), 12], [new IPv4([192, 168, 0, 0]), 16]],[m
[32m+[m[32m      reserved: [[new IPv4([192, 0, 0, 0]), 24], [new IPv4([192, 0, 2, 0]), 24], [new IPv4([192, 88, 99, 0]), 24], [new IPv4([198, 51, 100, 0]), 24], [new IPv4([203, 0, 113, 0]), 24], [new IPv4([240, 0, 0, 0]), 4]][m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.range = function() {[m
[32m+[m[32m      return ipaddr.subnetMatch(this, this.SpecialRanges);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.toIPv4MappedAddress = function() {[m
[32m+[m[32m      return ipaddr.IPv6.parse("::ffff:" + (this.toString()));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv4.prototype.prefixLengthFromSubnetMask = function() {[m
[32m+[m[32m      var cidr, i, k, octet, stop, zeros, zerotable;[m
[32m+[m[32m      zerotable = {[m
[32m+[m[32m        0: 8,[m
[32m+[m[32m        128: 7,[m
[32m+[m[32m        192: 6,[m
[32m+[m[32m        224: 5,[m
[32m+[m[32m        240: 4,[m
[32m+[m[32m        248: 3,[m
[32m+[m[32m        252: 2,[m
[32m+[m[32m        254: 1,[m
[32m+[m[32m        255: 0[m
[32m+[m[32m      };[m
[32m+[m[32m      cidr = 0;[m
[32m+[m[32m      stop = false;[m
[32m+[m[32m      for (i = k = 3; k >= 0; i = k += -1) {[m
[32m+[m[32m        octet = this.octets[i];[m
[32m+[m[32m        if (octet in zerotable) {[m
[32m+[m[32m          zeros = zerotable[octet];[m
[32m+[m[32m          if (stop && zeros !== 0) {[m
[32m+[m[32m            return null;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (zeros !== 8) {[m
[32m+[m[32m            stop = true;[m
[32m+[m[32m          }[m
[32m+[m[32m          cidr += zeros;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          return null;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      return 32 - cidr;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return IPv4;[m
[32m+[m
[32m+[m[32m  })();[m
[32m+[m
[32m+[m[32m  ipv4Part = "(0?\\d+|0x[a-f0-9]+)";[m
[32m+[m
[32m+[m[32m  ipv4Regexes = {[m
[32m+[m[32m    fourOctet: new RegExp("^" + ipv4Part + "\\." + ipv4Part + "\\." + ipv4Part + "\\." + ipv4Part + "$", 'i'),[m
[32m+[m[32m    longValue: new RegExp("^" + ipv4Part + "$", 'i')[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.parser = function(string) {[m
[32m+[m[32m    var match, parseIntAuto, part, shift, value;[m
[32m+[m[32m    parseIntAuto = function(string) {[m
[32m+[m[32m      if (string[0] === "0" && string[1] !== "x") {[m
[32m+[m[32m        return parseInt(string, 8);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return parseInt(string);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m    if (match = string.match(ipv4Regexes.fourOctet)) {[m
[32m+[m[32m      return (function() {[m
[32m+[m[32m        var k, len, ref, results;[m
[32m+[m[32m        ref = match.slice(1, 6);[m
[32m+[m[32m        results = [];[m
[32m+[m[32m        for (k = 0, len = ref.length; k < len; k++) {[m
[32m+[m[32m          part = ref[k];[m
[32m+[m[32m          results.push(parseIntAuto(part));[m
[32m+[m[32m        }[m
[32m+[m[32m        return results;[m
[32m+[m[32m      })();[m
[32m+[m[32m    } else if (match = string.match(ipv4Regexes.longValue)) {[m
[32m+[m[32m      value = parseIntAuto(match[1]);[m
[32m+[m[32m      if (value > 0xffffffff || value < 0) {[m
[32m+[m[32m        throw new Error("ipaddr: address outside defined range");[m
[32m+[m[32m      }[m
[32m+[m[32m      return ((function() {[m
[32m+[m[32m        var k, results;[m
[32m+[m[32m        results = [];[m
[32m+[m[32m        for (shift = k = 0; k <= 24; shift = k += 8) {[m
[32m+[m[32m          results.push((value >> shift) & 0xff);[m
[32m+[m[32m        }[m
[32m+[m[32m        return results;[m
[32m+[m[32m      })()).reverse();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv6 = (function() {[m
[32m+[m[32m    function IPv6(parts, zoneId) {[m
[32m+[m[32m      var i, k, l, len, part, ref;[m
[32m+[m[32m      if (parts.length === 16) {[m
[32m+[m[32m        this.parts = [];[m
[32m+[m[32m        for (i = k = 0; k <= 14; i = k += 2) {[m
[32m+[m[32m          this.parts.push((parts[i] << 8) | parts[i + 1]);[m
[32m+[m[32m        }[m
[32m+[m[32m      } else if (parts.length === 8) {[m
[32m+[m[32m        this.parts = parts;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error("ipaddr: ipv6 part count should be 8 or 16");[m
[32m+[m[32m      }[m
[32m+[m[32m      ref = this.parts;[m
[32m+[m[32m      for (l = 0, len = ref.length; l < len; l++) {[m
[32m+[m[32m        part = ref[l];[m
[32m+[m[32m        if (!((0 <= part && part <= 0xffff))) {[m
[32m+[m[32m          throw new Error("ipaddr: ipv6 part should fit in 16 bits");[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      if (zoneId) {[m
[32m+[m[32m        this.zoneId = zoneId;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IPv6.prototype.kind = function() {[m
[32m+[m[32m      return 'ipv6';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toString = function() {[m
[32m+[m[32m      return this.toNormalizedString().replace(/((^|:)(0(:|$))+)/, '::');[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toRFC5952String = function() {[m
[32m+[m[32m      var bestMatchIndex, bestMatchLength, match, regex, string;[m
[32m+[m[32m      regex = /((^|:)(0(:|$)){2,})/g;[m
[32m+[m[32m      string = this.toNormalizedString();[m
[32m+[m[32m      bestMatchIndex = 0;[m
[32m+[m[32m      bestMatchLength = -1;[m
[32m+[m[32m      while ((match = regex.exec(string))) {[m
[32m+[m[32m        if (match[0].length > bestMatchLength) {[m
[32m+[m[32m          bestMatchIndex = match.index;[m
[32m+[m[32m          bestMatchLength = match[0].length;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      if (bestMatchLength < 0) {[m
[32m+[m[32m        return string;[m
[32m+[m[32m      }[m
[32m+[m[32m      return string.substring(0, bestMatchIndex) + '::' + string.substring(bestMatchIndex + bestMatchLength);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toByteArray = function() {[m
[32m+[m[32m      var bytes, k, len, part, ref;[m
[32m+[m[32m      bytes = [];[m
[32m+[m[32m      ref = this.parts;[m
[32m+[m[32m      for (k = 0, len = ref.length; k < len; k++) {[m
[32m+[m[32m        part = ref[k];[m
[32m+[m[32m        bytes.push(part >> 8);[m
[32m+[m[32m        bytes.push(part & 0xff);[m
[32m+[m[32m      }[m
[32m+[m[32m      return bytes;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toNormalizedString = function() {[m
[32m+[m[32m      var addr, part, suffix;[m
[32m+[m[32m      addr = ((function() {[m
[32m+[m[32m        var k, len, ref, results;[m
[32m+[m[32m        ref = this.parts;[m
[32m+[m[32m        results = [];[m
[32m+[m[32m        for (k = 0, len = ref.length; k < len; k++) {[m
[32m+[m[32m          part = ref[k];[m
[32m+[m[32m          results.push(part.toString(16));[m
[32m+[m[32m        }[m
[32m+[m[32m        return results;[m
[32m+[m[32m      }).call(this)).join(":");[m
[32m+[m[32m      suffix = '';[m
[32m+[m[32m      if (this.zoneId) {[m
[32m+[m[32m        suffix = '%' + this.zoneId;[m
[32m+[m[32m      }[m
[32m+[m[32m      return addr + suffix;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toFixedLengthString = function() {[m
[32m+[m[32m      var addr, part, suffix;[m
[32m+[m[32m      addr = ((function() {[m
[32m+[m[32m        var k, len, ref, results;[m
[32m+[m[32m        ref = this.parts;[m
[32m+[m[32m        results = [];[m
[32m+[m[32m        for (k = 0, len = ref.length; k < len; k++) {[m
[32m+[m[32m          part = ref[k];[m
[32m+[m[32m          results.push(part.toString(16).padStart(4, '0'));[m
[32m+[m[32m        }[m
[32m+[m[32m        return results;[m
[32m+[m[32m      }).call(this)).join(":");[m
[32m+[m[32m      suffix = '';[m
[32m+[m[32m      if (this.zoneId) {[m
[32m+[m[32m        suffix = '%' + this.zoneId;[m
[32m+[m[32m      }[m
[32m+[m[32m      return addr + suffix;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.match = function(other, cidrRange) {[m
[32m+[m[32m      var ref;[m
[32m+[m[32m      if (cidrRange === void 0) {[m
[32m+[m[32m        ref = other, other = ref[0], cidrRange = ref[1];[m
[32m+[m[32m      }[m
[32m+[m[32m      if (other.kind() !== 'ipv6') {[m
[32m+[m[32m        throw new Error("ipaddr: cannot match ipv6 address with non-ipv6 one");[m
[32m+[m[32m      }[m
[32m+[m[32m      return matchCIDR(this.parts, other.parts, 16, cidrRange);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.SpecialRanges = {[m
[32m+[m[32m      unspecified: [new IPv6([0, 0, 0, 0, 0, 0, 0, 0]), 128],[m
[32m+[m[32m      linkLocal: [new IPv6([0xfe80, 0, 0, 0, 0, 0, 0, 0]), 10],[m
[32m+[m[32m      multicast: [new IPv6([0xff00, 0, 0, 0, 0, 0, 0, 0]), 8],[m
[32m+[m[32m      loopback: [new IPv6([0, 0, 0, 0, 0, 0, 0, 1]), 128],[m
[32m+[m[32m      uniqueLocal: [new IPv6([0xfc00, 0, 0, 0, 0, 0, 0, 0]), 7],[m
[32m+[m[32m      ipv4Mapped: [new IPv6([0, 0, 0, 0, 0, 0xffff, 0, 0]), 96],[m
[32m+[m[32m      rfc6145: [new IPv6([0, 0, 0, 0, 0xffff, 0, 0, 0]), 96],[m
[32m+[m[32m      rfc6052: [new IPv6([0x64, 0xff9b, 0, 0, 0, 0, 0, 0]), 96],[m
[32m+[m[32m      '6to4': [new IPv6([0x2002, 0, 0, 0, 0, 0, 0, 0]), 16],[m
[32m+[m[32m      teredo: [new IPv6([0x2001, 0, 0, 0, 0, 0, 0, 0]), 32],[m
[32m+[m[32m      reserved: [[new IPv6([0x2001, 0xdb8, 0, 0, 0, 0, 0, 0]), 32]][m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.range = function() {[m
[32m+[m[32m      return ipaddr.subnetMatch(this, this.SpecialRanges);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.isIPv4MappedAddress = function() {[m
[32m+[m[32m      return this.range() === 'ipv4Mapped';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.toIPv4Address = function() {[m
[32m+[m[32m      var high, low, ref;[m
[32m+[m[32m      if (!this.isIPv4MappedAddress()) {[m
[32m+[m[32m        throw new Error("ipaddr: trying to convert a generic ipv6 address to ipv4");[m
[32m+[m[32m      }[m
[32m+[m[32m      ref = this.parts.slice(-2), high = ref[0], low = ref[1];[m
[32m+[m[32m      return new ipaddr.IPv4([high >> 8, high & 0xff, low >> 8, low & 0xff]);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    IPv6.prototype.prefixLengthFromSubnetMask = function() {[m
[32m+[m[32m      var cidr, i, k, part, stop, zeros, zerotable;[m
[32m+[m[32m      zerotable = {[m
[32m+[m[32m        0: 16,[m
[32m+[m[32m        32768: 15,[m
[32m+[m[32m        49152: 14,[m
[32m+[m[32m        57344: 13,[m
[32m+[m[32m        61440: 12,[m
[32m+[m[32m        63488: 11,[m
[32m+[m[32m        64512: 10,[m
[32m+[m[32m        65024: 9,[m
[32m+[m[32m        65280: 8,[m
[32m+[m[32m        65408: 7,[m
[32m+[m[32m        65472: 6,[m
[32m+[m[32m        65504: 5,[m
[32m+[m[32m        65520: 4,[m
[32m+[m[32m        65528: 3,[m
[32m+[m[32m        65532: 2,[m
[32m+[m[32m        65534: 1,[m
[32m+[m[32m        65535: 0[m
[32m+[m[32m      };[m
[32m+[m[32m      cidr = 0;[m
[32m+[m[32m      stop = false;[m
[32m+[m[32m      for (i = k = 7; k >= 0; i = k += -1) {[m
[32m+[m[32m        part = this.parts[i];[m
[32m+[m[32m        if (part in zerotable) {[m
[32m+[m[32m          zeros = zerotable[part];[m
[32m+[m[32m          if (stop && zeros !== 0) {[m
[32m+[m[32m            return null;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (zeros !== 16) {[m
[32m+[m[32m            stop = true;[m
[32m+[m[32m          }[m
[32m+[m[32m          cidr += zeros;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          return null;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      return 128 - cidr;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return IPv6;[m
[32m+[m
[32m+[m[32m  })();[m
[32m+[m
[32m+[m[32m  ipv6Part = "(?:[0-9a-f]+::?)+";[m
[32m+[m
[32m+[m[32m  zoneIndex = "%[0-9a-z]{1,}";[m
[32m+[m
[32m+[m[32m  ipv6Regexes = {[m
[32m+[m[32m    zoneIndex: new RegExp(zoneIndex, 'i'),[m
[32m+[m[32m    "native": new RegExp("^(::)?(" + ipv6Part + ")?([0-9a-f]+)?(::)?(" + zoneIndex + ")?$", 'i'),[m
[32m+[m[32m    transitional: new RegExp(("^((?:" + ipv6Part + ")|(?:::)(?:" + ipv6Part + ")?)") + (ipv4Part + "\\." + ipv4Part + "\\." + ipv4Part + "\\." + ipv4Part) + ("(" + zoneIndex + ")?$"), 'i')[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  expandIPv6 = function(string, parts) {[m
[32m+[m[32m    var colonCount, lastColon, part, replacement, replacementCount, zoneId;[m
[32m+[m[32m    if (string.indexOf('::') !== string.lastIndexOf('::')) {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[32m    zoneId = (string.match(ipv6Regexes['zoneIndex']) || [])[0];[m
[32m+[m[32m    if (zoneId) {[m
[32m+[m[32m      zoneId = zoneId.substring(1);[m
[32m+[m[32m      string = string.replace(/%.+$/, '');[m
[32m+[m[32m    }[m
[32m+[m[32m    colonCount = 0;[m
[32m+[m[32m    lastColon = -1;[m
[32m+[m[32m    while ((lastColon = string.indexOf(':', lastColon + 1)) >= 0) {[m
[32m+[m[32m      colonCount++;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (string.substr(0, 2) === '::') {[m
[32m+[m[32m      colonCount--;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (string.substr(-2, 2) === '::') {[m
[32m+[m[32m      colonCount--;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (colonCount > parts) {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[32m    replacementCount = parts - colonCount;[m
[32m+[m[32m    replacement = ':';[m
[32m+[m[32m    while (replacementCount--) {[m
[32m+[m[32m      replacement += '0:';[m
[32m+[m[32m    }[m
[32m+[m[32m    string = string.replace('::', replacement);[m
[32m+[m[32m    if (string[0] === ':') {[m
[32m+[m[32m      string = string.slice(1);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (string[string.length - 1] === ':') {[m
[32m+[m[32m      string = string.slice(0, -1);[m
[32m+[m[32m    }[m
[32m+[m[32m    parts = (function() {[m
[32m+[m[32m      var k, len, ref, results;[m
[32m+[m[32m      ref = string.split(":");[m
[32m+[m[32m      results = [];[m
[32m+[m[32m      for (k = 0, len = ref.length; k < len; k++) {[m
[32m+[m[32m        part = ref[k];[m
[32m+[m[32m        results.push(parseInt(part, 16));[m
[32m+[m[32m      }[m
[32m+[m[32m      return results;[m
[32m+[m[32m    })();[m
[32m+[m[32m    return {[m
[32m+[m[32m      parts: parts,[m
[32m+[m[32m      zoneId: zoneId[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv6.parser = function(string) {[m
[32m+[m[32m    var addr, k, len, match, octet, octets, zoneId;[m
[32m+[m[32m    if (ipv6Regexes['native'].test(string)) {[m
[32m+[m[32m      return expandIPv6(string, 8);[m
[32m+[m[32m    } else if (match = string.match(ipv6Regexes['transitional'])) {[m
[32m+[m[32m      zoneId = match[6] || '';[m
[32m+[m[32m      addr = expandIPv6(match[1].slice(0, -1) + zoneId, 6);[m
[32m+[m[32m      if (addr.parts) {[m
[32m+[m[32m        octets = [parseInt(match[2]), parseInt(match[3]), parseInt(match[4]), parseInt(match[5])];[m
[32m+[m[32m        for (k = 0, len = octets.length; k < len; k++) {[m
[32m+[m[32m          octet = octets[k];[m
[32m+[m[32m          if (!((0 <= octet && octet <= 255))) {[m
[32m+[m[32m            return null;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        addr.parts.push(octets[0] << 8 | octets[1]);[m
[32m+[m[32m        addr.parts.push(octets[2] << 8 | octets[3]);[m
[32m+[m[32m        return {[m
[32m+[m[32m          parts: addr.parts,[m
[32m+[m[32m          zoneId: addr.zoneId[m
[32m+[m[32m        };[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return null;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.isIPv4 = ipaddr.IPv6.isIPv6 = function(string) {[m
[32m+[m[32m    return this.parser(string) !== null;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.isValid = function(string) {[m
[32m+[m[32m    var e;[m
[32m+[m[32m    try {[m
[32m+[m[32m      new this(this.parser(string));[m
[32m+[m[32m      return true;[m
[32m+[m[32m    } catch (error1) {[m
[32m+[m[32m      e = error1;[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.isValidFourPartDecimal = function(string) {[m
[32m+[m[32m    if (ipaddr.IPv4.isValid(string) && string.match(/^(0|[1-9]\d*)(\.(0|[1-9]\d*)){3}$/)) {[m
[32m+[m[32m      return true;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv6.isValid = function(string) {[m
[32m+[m[32m    var addr, e;[m
[32m+[m[32m    if (typeof string === "string" && string.indexOf(":") === -1) {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m      addr = this.parser(string);[m
[32m+[m[32m      new this(addr.parts, addr.zoneId);[m
[32m+[m[32m      return true;[m
[32m+[m[32m    } catch (error1) {[m
[32m+[m[32m      e = error1;[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.parse = function(string) {[m
[32m+[m[32m    var parts;[m
[32m+[m[32m    parts = this.parser(string);[m
[32m+[m[32m    if (parts === null) {[m
[32m+[m[32m      throw new Error("ipaddr: string is not formatted like ip address");[m
[32m+[m[32m    }[m
[32m+[m[32m    return new this(parts);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv6.parse = function(string) {[m
[32m+[m[32m    var addr;[m
[32m+[m[32m    addr = this.parser(string);[m
[32m+[m[32m    if (addr.parts === null) {[m
[32m+[m[32m      throw new Error("ipaddr: string is not formatted like ip address");[m
[32m+[m[32m    }[m
[32m+[m[32m    return new this(addr.parts, addr.zoneId);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.parseCIDR = function(string) {[m
[32m+[m[32m    var maskLength, match, parsed;[m
[32m+[m[32m    if (match = string.match(/^(.+)\/(\d+)$/)) {[m
[32m+[m[32m      maskLength = parseInt(match[2]);[m
[32m+[m[32m      if (maskLength >= 0 && maskLength <= 32) {[m
[32m+[m[32m        parsed = [this.parse(match[1]), maskLength];[m
[32m+[m[32m        Object.defineProperty(parsed, 'toString', {[m
[32m+[m[32m          value: function() {[m
[32m+[m[32m            return this.join('/');[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m        return parsed;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    throw new Error("ipaddr: string is not formatted like an IPv4 CIDR range");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.subnetMaskFromPrefixLength = function(prefix) {[m
[32m+[m[32m    var filledOctetCount, j, octets;[m
[32m+[m[32m    prefix = parseInt(prefix);[m
[32m+[m[32m    if (prefix < 0 || prefix > 32) {[m
[32m+[m[32m      throw new Error('ipaddr: invalid IPv4 prefix length');[m
[32m+[m[32m    }[m
[32m+[m[32m    octets = [0, 0, 0, 0];[m
[32m+[m[32m    j = 0;[m
[32m+[m[32m    filledOctetCount = Math.floor(prefix / 8);[m
[32m+[m[32m    while (j < filledOctetCount) {[m
[32m+[m[32m      octets[j] = 255;[m
[32m+[m[32m      j++;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (filledOctetCount < 4) {[m
[32m+[m[32m      octets[filledOctetCount] = Math.pow(2, prefix % 8) - 1 << 8 - (prefix % 8);[m
[32m+[m[32m    }[m
[32m+[m[32m    return new this(octets);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.broadcastAddressFromCIDR = function(string) {[m
[32m+[m[32m    var cidr, error, i, ipInterfaceOctets, octets, subnetMaskOctets;[m
[32m+[m[32m    try {[m
[32m+[m[32m      cidr = this.parseCIDR(string);[m
[32m+[m[32m      ipInterfaceOctets = cidr[0].toByteArray();[m
[32m+[m[32m      subnetMaskOctets = this.subnetMaskFromPrefixLength(cidr[1]).toByteArray();[m
[32m+[m[32m      octets = [];[m
[32m+[m[32m      i = 0;[m
[32m+[m[32m      while (i < 4) {[m
[32m+[m[32m        octets.push(parseInt(ipInterfaceOctets[i], 10) | parseInt(subnetMaskOctets[i], 10) ^ 255);[m
[32m+[m[32m        i++;[m
[32m+[m[32m      }[m
[32m+[m[32m      return new this(octets);[m
[32m+[m[32m    } catch (error1) {[m
[32m+[m[32m      error = error1;[m
[32m+[m[32m      throw new Error('ipaddr: the address does not have IPv4 CIDR format');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv4.networkAddressFromCIDR = function(string) {[m
[32m+[m[32m    var cidr, error, i, ipInterfaceOctets, octets, subnetMaskOctets;[m
[32m+[m[32m    try {[m
[32m+[m[32m      cidr = this.parseCIDR(string);[m
[32m+[m[32m      ipInterfaceOctets = cidr[0].toByteArray();[m
[32m+[m[32m      subnetMaskOctets = this.subnetMaskFromPrefixLength(cidr[1]).toByteArray();[m
[32m+[m[32m      octets = [];[m
[32m+[m[32m      i = 0;[m
[32m+[m[32m      while (i < 4) {[m
[32m+[m[32m        octets.push(parseInt(ipInterfaceOctets[i], 10) & parseInt(subnetMaskOctets[i], 10));[m
[32m+[m[32m        i++;[m
[32m+[m[32m      }[m
[32m+[m[32m      return new this(octets);[m
[32m+[m[32m    } catch (error1) {[m
[32m+[m[32m      error = error1;[m
[32m+[m[32m      throw new Error('ipaddr: the address does not have IPv4 CIDR format');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.IPv6.parseCIDR = function(string) {[m
[32m+[m[32m    var maskLength, match, parsed;[m
[32m+[m[32m    if (match = string.match(/^(.+)\/(\d+)$/)) {[m
[32m+[m[32m      maskLength = parseInt(match[2]);[m
[32m+[m[32m      if (maskLength >= 0 && maskLength <= 128) {[m
[32m+[m[32m        parsed = [this.parse(match[1]), maskLength];[m
[32m+[m[32m        Object.defineProperty(parsed, 'toString', {[m
[32m+[m[32m          value: function() {[m
[32m+[m[32m            return this.join('/');[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m        return parsed;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    throw new Error("ipaddr: string is not formatted like an IPv6 CIDR range");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.isValid = function(string) {[m
[32m+[m[32m    return ipaddr.IPv6.isValid(string) || ipaddr.IPv4.isValid(string);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.parse = function(string) {[m
[32m+[m[32m    if (ipaddr.IPv6.isValid(string)) {[m
[32m+[m[32m      return ipaddr.IPv6.parse(string);[m
[32m+[m[32m    } else if (ipaddr.IPv4.isValid(string)) {[m
[32m+[m[32m      return ipaddr.IPv4.parse(string);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      throw new Error("ipaddr: the address has neither IPv6 nor IPv4 format");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.parseCIDR = function(string) {[m
[32m+[m[32m    var e;[m
[32m+[m[32m    try {[m
[32m+[m[32m      return ipaddr.IPv6.parseCIDR(string);[m
[32m+[m[32m    } catch (error1) {[m
[32m+[m[32m      e = error1;[m
[32m+[m[32m      try {[m
[32m+[m[32m        return ipaddr.IPv4.parseCIDR(string);[m
[32m+[m[32m      } catch (error1) {[m
[32m+[m[32m        e = error1;[m
[32m+[m[32m        throw new Error("ipaddr: the address has neither IPv6 nor IPv4 CIDR format");[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.fromByteArray = function(bytes) {[m
[32m+[m[32m    var length;[m
[32m+[m[32m    length = bytes.length;[m
[32m+[m[32m    if (length === 4) {[m
[32m+[m[32m      return new ipaddr.IPv4(bytes);[m
[32m+[m[32m    } else if (length === 16) {[m
[32m+[m[32m      return new ipaddr.IPv6(bytes);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      throw new Error("ipaddr: the binary input is neither an IPv6 nor IPv4 address");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ipaddr.process = function(string) {[m
[32m+[m[32m    var addr;[m
[32m+[m[32m    addr = this.parse(string);[m
[32m+[m[32m    if (addr.kind() === 'ipv6' && addr.isIPv4MappedAddress()) {[m
[32m+[m[32m      return addr.toIPv4Address();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return addr;[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m}).call(this);[m
