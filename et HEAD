[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar bytes = require('bytes')[m
[32m+[m[32mvar contentType = require('content-type')[m
[32m+[m[32mvar debug = require('debug')('body-parser:text')[m
[32m+[m[32mvar read = require('../read')[m
[32m+[m[32mvar typeis = require('type-is')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = text[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a middleware to parse text bodies.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction text (options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  var defaultCharset = opts.defaultCharset || 'utf-8'[m
[32m+[m[32m  var inflate = opts.inflate !== false[m
[32m+[m[32m  var limit = typeof opts.limit !== 'number'[m
[32m+[m[32m    ? bytes.parse(opts.limit || '100kb')[m
[32m+[m[32m    : opts.limit[m
[32m+[m[32m  var type = opts.type || 'text/plain'[m
[32m+[m[32m  var verify = opts.verify || false[m
[32m+[m
[32m+[m[32m  if (verify !== false && typeof verify !== 'function') {[m
[32m+[m[32m    throw new TypeError('option verify must be function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create the appropriate type checking function[m
[32m+[m[32m  var shouldParse = typeof type !== 'function'[m
[32m+[m[32m    ? typeChecker(type)[m
[32m+[m[32m    : type[m
[32m+[m
[32m+[m[32m  function parse (buf) {[m
[32m+[m[32m    return buf[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function textParser (req, res, next) {[m
[32m+[m[32m    if (req._body) {[m
[32m+[m[32m      debug('body already parsed')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    req.body = req.body || {}[m
[32m+[m
[32m+[m[32m    // skip requests without bodies[m
[32m+[m[32m    if (!typeis.hasBody(req)) {[m
[32m+[m[32m      debug('skip empty body')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('content-type %j', req.headers['content-type'])[m
[32m+[m
[32m+[m[32m    // determine if request should be parsed[m
[32m+[m[32m    if (!shouldParse(req)) {[m
[32m+[m[32m      debug('skip parsing')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // get charset[m
[32m+[m[32m    var charset = getCharset(req) || defaultCharset[m
[32m+[m
[32m+[m[32m    // read[m
[32m+[m[32m    read(req, res, next, parse, debug, {[m
[32m+[m[32m      encoding: charset,[m
[32m+[m[32m      inflate: inflate,[m
[32m+[m[32m      limit: limit,[m
[32m+[m[32m      verify: verify[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the charset of a request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getCharset (req) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return (contentType.parse(req).parameters.charset || '').toLowerCase()[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the simple type checker.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction typeChecker (type) {[m
[32m+[m[32m  return function checkType (req) {[m
[32m+[m[32m    return Boolean(typeis(req, type))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
