[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/README.md[m
[36m@@ -0,0 +1,156 @@[m
[32m+[m[32m## Pure JS character encoding conversion [![Build Status](https://travis-ci.org/ashtuchkin/iconv-lite.svg?branch=master)](https://travis-ci.org/ashtuchkin/iconv-lite)[m
[32m+[m
[32m+[m[32m * Doesn't need native code compilation. Works on Windows and in sandboxed environments like [Cloud9](http://c9.io).[m
[32m+[m[32m * Used in popular projects like [Express.js (body_parser)](https://github.com/expressjs/body-parser),[m[41m [m
[32m+[m[32m   [Grunt](http://gruntjs.com/), [Nodemailer](http://www.nodemailer.com/), [Yeoman](http://yeoman.io/) and others.[m
[32m+[m[32m * Faster than [node-iconv](https://github.com/bnoordhuis/node-iconv) (see below for performance comparison).[m
[32m+[m[32m * Intuitive encode/decode API[m
[32m+[m[32m * Streaming support for Node v0.10+[m
[32m+[m[32m * [Deprecated] Can extend Node.js primitives (buffers, streams) to support all iconv-lite encodings.[m
[32m+[m[32m * In-browser usage via [Browserify](https://github.com/substack/node-browserify) (~180k gzip compressed with Buffer shim included).[m
[32m+[m[32m * Typescript [type definition file](https://github.com/ashtuchkin/iconv-lite/blob/master/lib/index.d.ts) included.[m
[32m+[m[32m * React Native is supported (need to explicitly `npm install` two more modules: `buffer` and `stream`).[m
[32m+[m[32m * License: MIT.[m
[32m+[m
[32m+[m[32m[![NPM Stats](https://nodei.co/npm/iconv-lite.png?downloads=true&downloadRank=true)](https://npmjs.org/packages/iconv-lite/)[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m[32m### Basic API[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar iconv = require('iconv-lite');[m
[32m+[m
[32m+[m[32m// Convert from an encoded buffer to js string.[m
[32m+[m[32mstr = iconv.decode(Buffer.from([0x68, 0x65, 0x6c, 0x6c, 0x6f]), 'win1251');[m
[32m+[m
[32m+[m[32m// Convert from js string to an encoded buffer.[m
[32m+[m[32mbuf = iconv.encode("Sample input string", 'win1251');[m
[32m+[m
[32m+[m[32m// Check if encoding is supported[m
[32m+[m[32miconv.encodingExists("us-ascii")[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Streaming API (Node v0.10+)[m
[32m+[m[32m```javascript[m
[32m+[m
[32m+[m[32m// Decode stream (from binary stream to js strings)[m
[32m+[m[32mhttp.createServer(function(req, res) {[m
[32m+[m[32m    var converterStream = iconv.decodeStream('win1251');[m
[32m+[m[32m    req.pipe(converterStream);[m
[32m+[m
[32m+[m[32m    converterStream.on('data', function(str) {[m
[32m+[m[32m        console.log(str); // Do something with decoded strings, chunk-by-chunk.[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Convert encoding streaming example[m
[32m+[m[32mfs.createReadStream('file-in-win1251.txt')[m
[32m+[m[32m    .pipe(iconv.decodeStream('win1251'))[m
[32m+[m[32m    .pipe(iconv.encodeStream('ucs2'))[m
[32m+[m[32m    .pipe(fs.createWriteStream('file-in-ucs2.txt'));[m
[32m+[m
[32m+[m[32m// Sugar: all encode/decode streams have .collect(cb) method to accumulate data.[m
[32m+[m[32mhttp.createServer(function(req, res) {[m
[32m+[m[32m    req.pipe(iconv.decodeStream('win1251')).collect(function(err, body) {[m
[32m+[m[32m        assert(typeof body == 'string');[m
[32m+[m[32m        console.log(body); // full request body string[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### [Deprecated] Extend Node.js own encodings[m
[32m+[m[32m> NOTE: This doesn't work on latest Node versions. See [details](https://github.com/ashtuchkin/iconv-lite/wiki/Node-v4-compatibility).[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32m// After this call all Node basic primitives will understand iconv-lite encodings.[m
[32m+[m[32miconv.extendNodeEncodings();[m
[32m+[m
[32m+[m[32m// Examples:[m
[32m+[m[32mbuf = new Buffer(str, 'win1251');[m
[32m+[m[32mbuf.write(str, 'gbk');[m
[32m+[m[32mstr = buf.toString('latin1');[m
[32m+[m[32massert(Buffer.isEncoding('iso-8859-15'));[m
[32m+[m[32mBuffer.byteLength(str, 'us-ascii');[m
[32m+[m
[32m+[m[32mhttp.createServer(function(req, res) {[m
[32m+[m[32m    req.setEncoding('big5');[m
[32m+[m[32m    req.collect(function(err, body) {[m
[32m+[m[32m        console.log(body);[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfs.createReadStream("file.txt", "shift_jis");[m
[32m+[m
[32m+[m[32m// External modules are also supported (if they use Node primitives, which they probably do).[m
[32m+[m[32mrequest = require('request');[m
[32m+[m[32mrequest({[m
[32m+[m[32m    url: "http://github.com/",[m[41m [m
[32m+[m[32m    encoding: "cp932"[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// To remove extensions[m
[32m+[m[32miconv.undoExtendNodeEncodings();[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Supported encodings[m
[32m+[m
[32m+[m[32m *  All node.js native encodings: utf8, ucs2 / utf16-le, ascii, binary, base64, hex.[m
[32m+[m[32m *  Additional unicode encodings: utf16, utf16-be, utf-7, utf-7-imap.[m
[32m+[m[32m *  All widespread singlebyte encodings: Windows 125x family, ISO-8859 family,[m[41m [m
[32m+[m[32m    IBM/DOS codepages, Macintosh family, KOI8 family, all others supported by iconv library.[m[41m [m
[32m+[m[32m    Aliases like 'latin1', 'us-ascii' also supported.[m
[32m+[m[32m *  All widespread multibyte encodings: CP932, CP936, CP949, CP950, GB2312, GBK, GB18030, Big5, Shift_JIS, EUC-JP.[m
[32m+[m
[32m+[m[32mSee [all supported encodings on wiki](https://github.com/ashtuchkin/iconv-lite/wiki/Supported-Encodings).[m
[32m+[m
[32m+[m[32mMost singlebyte encodings are generated automatically from [node-iconv](https://github.com/bnoordhuis/node-iconv). Thank you Ben Noordhuis and libiconv authors![m
[32m+[m
[32m+[m[32mMultibyte encodings are generated from [Unicode.org mappings](http://www.unicode.org/Public/MAPPINGS/) and [WHATWG Encoding Standard mappings](http://encoding.spec.whatwg.org/). Thank you, respective authors![m
[32m+[m
[32m+[m
[32m+[m[32m## Encoding/decoding speed[m
[32m+[m
[32m+[m[32mComparison with node-iconv module (1000x256kb, on MacBook Pro, Core i5/2.6 GHz, Node v0.12.0).[m[41m [m
[32m+[m[32mNote: your results may vary, so please always check on your hardware.[m
[32m+[m
[32m+[m[32m    operation             iconv@2.1.4   iconv-lite@0.4.7[m
[32m+[m[32m    ----------------------------------------------------------[m
[32m+[m[32m    encode('win1251')     ~96 Mb/s      ~320 Mb/s[m
[32m+[m[32m    decode('win1251')     ~95 Mb/s      ~246 Mb/s[m
[32m+[m
[32m+[m[32m## BOM handling[m
[32m+[m
[32m+[m[32m * Decoding: BOM is stripped by default, unless overridden by passing `stripBOM: false` in options[m
[32m+[m[32m   (f.ex. `iconv.decode(buf, enc, {stripBOM: false})`).[m
[32m+[m[32m   A callback might also be given as a `stripBOM` parameter - it'll be called if BOM character was actually found.[m
[32m+[m[32m * If you want to detect UTF-8 BOM when decoding other encodings, use [node-autodetect-decoder-stream](https://github.com/danielgindi/node-autodetect-decoder-stream) module.[m
[32m+[m[32m * Encoding: No BOM added, unless overridden by `addBOM: true` option.[m
[32m+[m
[32m+[m[32m## UTF-16 Encodings[m
[32m+[m
[32m+[m[32mThis library supports UTF-16LE, UTF-16BE and UTF-16 encodings. First two are straightforward, but UTF-16 is trying to be[m
[32m+[m[32msmart about endianness in the following ways:[m
[32m+[m[32m * Decoding: uses BOM and 'spaces heuristic' to determine input endianness. Default is UTF-16LE, but can be[m[41m [m
[32m+[m[32m   overridden with `defaultEncoding: 'utf-16be'` option. Strips BOM unless `stripBOM: false`.[m
[32m+[m[32m * Encoding: uses UTF-16LE and writes BOM by default. Use `addBOM: false` to override.[m
[32m+[m
[32m+[m[32m## Other notes[m
[32m+[m
[32m+[m[32mWhen decoding, be sure to supply a Buffer to decode() method, otherwise [bad things usually happen](https://github.com/ashtuchkin/iconv-lite/wiki/Use-Buffers-when-decoding).[m[41m  [m
[32m+[m[32mUntranslatable characters are set to � or ?. No transliteration is currently supported.[m[41m  [m
[32m+[m[32mNode versions 0.10.31 and 0.11.13 are buggy, don't use them (see #65, #77).[m[41m  [m
[32m+[m
[32m+[m[32m## Testing[m
[32m+[m
[32m+[m[32m```bash[m
[32m+[m[32m$ git clone git@github.com:ashtuchkin/iconv-lite.git[m
[32m+[m[32m$ cd iconv-lite[m
[32m+[m[32m$ npm install[m
[32m+[m[32m$ npm test[m
[32m+[m[41m    [m
[32m+[m[32m$ # To view performance:[m
[32m+[m[32m$ node test/performance.js[m
[32m+[m
[32m+[m[32m$ # To view test coverage:[m
[32m+[m[32m$ npm run coverage[m
[32m+[m[32m$ open coverage/lcov-report/index.html[m
[32m+[m[32m```[m
[1mdiff --git a/node_modules/iconv-lite/encodings/dbcs-codec.js b/node_modules/iconv-lite/encodings/dbcs-codec.js[m
[1mnew file mode 100644[m
[1mindex 0000000..1fe3e16[m
[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/encodings/dbcs-codec.js[m
[36m@@ -0,0 +1,555 @@[m
[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32m// Multibyte codec. In this scheme, a character is represented by 1 or more bytes.[m
[32m+[m[32m// Our codec supports UTF-16 surrogates, extensions for GB18030 and unicode sequences.[m
[32m+[m[32m// To save memory and loading time, we read table files only when requested.[m
[32m+[m
[32m+[m[32mexports._dbcs = DBCSCodec;[m
[32m+[m
[32m+[m[32mvar UNASSIGNED = -1,[m
[32m+[m[32m    GB18030_CODE = -2,[m
[32m+[m[32m    SEQ_START  = -10,[m
[32m+[m[32m    NODE_START = -1000,[m
[32m+[m[32m    UNASSIGNED_NODE = new Array(0x100),[m
[32m+[m[32m    DEF_CHAR = -1;[m
[32m+[m
[32m+[m[32mfor (var i = 0; i < 0x100; i++)[m
[32m+[m[32m    UNASSIGNED_NODE[i] = UNASSIGNED;[m
[32m+[m
[32m+[m
[32m+[m[32m// Class DBCSCodec reads and initializes mapping tables.[m
[32m+[m[32mfunction DBCSCodec(codecOptions, iconv) {[m
[32m+[m[32m    this.encodingName = codecOptions.encodingName;[m
[32m+[m[32m    if (!codecOptions)[m
[32m+[m[32m        throw new Error("DBCS codec is called without the data.")[m
[32m+[m[32m    if (!codecOptions.table)[m
[32m+[m[32m        throw new Error("Encoding '" + this.encodingName + "' has no data.");[m
[32m+[m
[32m+[m[32m    // Load tables.[m
[32m+[m[32m    var mappingTable = codecOptions.table();[m
[32m+[m
[32m+[m
[32m+[m[32m    // Decode tables: MBCS -> Unicode.[m
[32m+[m
[32m+[m[32m    // decodeTables is a trie, encoded as an array of arrays of integers. Internal arrays are trie nodes and all have len = 256.[m
[32m+[m[32m    // Trie root is decodeTables[0].[m
[32m+[m[32m    // Values: >=  0 -> unicode character code. can be > 0xFFFF[m
[32m+[m[32m    //         == UNASSIGNED -> unknown/unassigned sequence.[m
[32m+[m[32m    //         == GB18030_CODE -> this is the end of a GB18030 4-byte sequence.[m
[32m+[m[32m    //         <= NODE_START -> index of the next node in our trie to process next byte.[m
[32m+[m[32m    //         <= SEQ_START  -> index of the start of a character code sequence, in decodeTableSeq.[m
[32m+[m[32m    this.decodeTables = [];[m
[32m+[m[32m    this.decodeTables[0] = UNASSIGNED_NODE.slice(0); // Create root node.[m
[32m+[m
[32m+[m[32m    // Sometimes a MBCS char corresponds to a sequence of unicode chars. We store them as arrays of integers here.[m[41m [m
[32m+[m[32m    this.decodeTableSeq = [];[m
[32m+[m
[32m+[m[32m    // Actual mapping tables consist of chunks. Use them to fill up decode tables.[m
[32m+[m[32m    for (var i = 0; i < mappingTable.length; i++)[m
[32m+[m[32m        this._addDecodeChunk(mappingTable[i]);[m
[32m+[m
[32m+[m[32m    this.defaultCharUnicode = iconv.defaultCharUnicode;[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    // Encode tables: Unicode -> DBCS.[m
[32m+[m
[32m+[m[32m    // `encodeTable` is array mapping from unicode char to encoded char. All its values are integers for performance.[m
[32m+[m[32m    // Because it can be sparse, it is represented as array of buckets by 256 chars each. Bucket can be null.[m
[32m+[m[32m    // Values: >=  0 -> it is a normal char. Write the value (if <=256 then 1 byte, if <=65536 then 2 bytes, etc.).[m
[32m+[m[32m    //         == UNASSIGNED -> no conversion found. Output a default char.[m
[32m+[m[32m    //         <= SEQ_START  -> it's an index in encodeTableSeq, see below. The character starts a sequence.[m
[32m+[m[32m    this.encodeTable = [];[m
[32m+[m[41m    [m
[32m+[m[32m    // `encodeTableSeq` is used when a sequence of unicode characters is encoded as a single code. We use a tree of[m
[32m+[m[32m    // objects where keys correspond to characters in sequence and leafs are the encoded dbcs values. A special DEF_CHAR key[m
[32m+[m[32m    // means end of sequence (needed when one sequence is a strict subsequence of another).[m
[32m+[m[32m    // Objects are kept separately from encodeTable to increase performance.[m
[32m+[m[32m    this.encodeTableSeq = [];[m
[32m+[m
[32m+[m[32m    // Some chars can be decoded, but need not be encoded.[m
[32m+[m[32m    var skipEncodeChars = {};[m
[32m+[m[32m    if (codecOptions.encodeSkipVals)[m
[32m+[m[32m        for (var i = 0; i < codecOptions.encodeSkipVals.length; i++) {[m
[32m+[m[32m            var val = codecOptions.encodeSkipVals[i];[m
[32m+[m[32m            if (typeof val === 'number')[m
[32m+[m[32m                skipEncodeChars[val] = true;[m
[32m+[m[32m            else[m
[32m+[m[32m                for (var j = val.from; j <= val.to; j++)[m
[32m+[m[32m                    skipEncodeChars[j] = true;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    // Use decode trie to recursively fill out encode tables.[m
[32m+[m[32m    this._fillEncodeTable(0, 0, skipEncodeChars);[m
[32m+[m
[32m+[m[32m    // Add more encoding pairs when needed.[m
[32m+[m[32m    if (codecOptions.encodeAdd) {[m
[32m+[m[32m        for (var uChar in codecOptions.encodeAdd)[m
[32m+[m[32m            if (Object.prototype.hasOwnProperty.call(codecOptions.encodeAdd, uChar))[m
[32m+[m[32m                this._setEncodeChar(uChar.charCodeAt(0), codecOptions.encodeAdd[uChar]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.defCharSB  = this.encodeTable[0][iconv.defaultCharSingleByte.charCodeAt(0)];[m
[32m+[m[32m    if (this.defCharSB === UNASSIGNED) this.defCharSB = this.encodeTable[0]['?'];[m
[32m+[m[32m    if (this.defCharSB === UNASSIGNED) this.defCharSB = "?".charCodeAt(0);[m
[32m+[m
[32m+[m
[32m+[m[32m    // Load & create GB18030 tables when needed.[m
[32m+[m[32m    if (typeof codecOptions.gb18030 === 'function') {[m
[32m+[m[32m        this.gb18030 = codecOptions.gb18030(); // Load GB18030 ranges.[m
[32m+[m
[32m+[m[32m        // Add GB18030 decode tables.[m
[32m+[m[32m        var thirdByteNodeIdx = this.decodeTables.length;[m
[32m+[m[32m        var thirdByteNode = this.decodeTables[thirdByteNodeIdx] = UNASSIGNED_NODE.slice(0);[m
[32m+[m
[32m+[m[32m        var fourthByteNodeIdx = this.decodeTables.length;[m
[32m+[m[32m        var fourthByteNode = this.decodeTables[fourthByteNodeIdx] = UNASSIGNED_NODE.slice(0);[m
[32m+[m
[32m+[m[32m        for (var i = 0x81; i <= 0xFE; i++) {[m
[32m+[m[32m            var secondByteNodeIdx = NODE_START - this.decodeTables[0][i];[m
[32m+[m[32m            var secondByteNode = this.decodeTables[secondByteNodeIdx];[m
[32m+[m[32m            for (var j = 0x30; j <= 0x39; j++)[m
[32m+[m[32m                secondByteNode[j] = NODE_START - thirdByteNodeIdx;[m
[32m+[m[32m        }[m
[32m+[m[32m        for (var i = 0x81; i <= 0xFE; i++)[m
[32m+[m[32m            thirdByteNode[i] = NODE_START - fourthByteNodeIdx;[m
[32m+[m[32m        for (var i = 0x30; i <= 0x39; i++)[m
[32m+[m[32m            fourthByteNode[i] = GB18030_CODE[m
[32m+[m[32m    }[m[41m        [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSCodec.prototype.encoder = DBCSEncoder;[m
[32m+[m[32mDBCSCodec.prototype.decoder = DBCSDecoder;[m
[32m+[m
[32m+[m[32m// Decoder helpers[m
[32m+[m[32mDBCSCodec.prototype._getDecodeTrieNode = function(addr) {[m
[32m+[m[32m    var bytes = [];[m
[32m+[m[32m    for (; addr > 0; addr >>= 8)[m
[32m+[m[32m        bytes.push(addr & 0xFF);[m
[32m+[m[32m    if (bytes.length == 0)[m
[32m+[m[32m        bytes.push(0);[m
[32m+[m
[32m+[m[32m    var node = this.decodeTables[0];[m
[32m+[m[32m    for (var i = bytes.length-1; i > 0; i--) { // Traverse nodes deeper into the trie.[m
[32m+[m[32m        var val = node[bytes[i]];[m
[32m+[m
[32m+[m[32m        if (val == UNASSIGNED) { // Create new node.[m
[32m+[m[32m            node[bytes[i]] = NODE_START - this.decodeTables.length;[m
[32m+[m[32m            this.decodeTables.push(node = UNASSIGNED_NODE.slice(0));[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (val <= NODE_START) { // Existing node.[m
[32m+[m[32m            node = this.decodeTables[NODE_START - val];[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m            throw new Error("Overwrite byte in " + this.encodingName + ", addr: " + addr.toString(16));[m
[32m+[m[32m    }[m
[32m+[m[32m    return node;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mDBCSCodec.prototype._addDecodeChunk = function(chunk) {[m
[32m+[m[32m    // First element of chunk is the hex mbcs code where we start.[m
[32m+[m[32m    var curAddr = parseInt(chunk[0], 16);[m
[32m+[m
[32m+[m[32m    // Choose the decoding node where we'll write our chars.[m
[32m+[m[32m    var writeTable = this._getDecodeTrieNode(curAddr);[m
[32m+[m[32m    curAddr = curAddr & 0xFF;[m
[32m+[m
[32m+[m[32m    // Write all other elements of the chunk to the table.[m
[32m+[m[32m    for (var k = 1; k < chunk.length; k++) {[m
[32m+[m[32m        var part = chunk[k];[m
[32m+[m[32m        if (typeof part === "string") { // String, write as-is.[m
[32m+[m[32m            for (var l = 0; l < part.length;) {[m
[32m+[m[32m                var code = part.charCodeAt(l++);[m
[32m+[m[32m                if (0xD800 <= code && code < 0xDC00) { // Decode surrogate[m
[32m+[m[32m                    var codeTrail = part.charCodeAt(l++);[m
[32m+[m[32m                    if (0xDC00 <= codeTrail && codeTrail < 0xE000)[m
[32m+[m[32m                        writeTable[curAddr++] = 0x10000 + (code - 0xD800) * 0x400 + (codeTrail - 0xDC00);[m
[32m+[m[32m                    else[m
[32m+[m[32m                        throw new Error("Incorrect surrogate pair in "  + this.encodingName + " at chunk " + chunk[0]);[m
[32m+[m[32m                }[m
[32m+[m[32m                else if (0x0FF0 < code && code <= 0x0FFF) { // Character sequence (our own encoding used)[m
[32m+[m[32m                    var len = 0xFFF - code + 2;[m
[32m+[m[32m                    var seq = [];[m
[32m+[m[32m                    for (var m = 0; m < len; m++)[m
[32m+[m[32m                        seq.push(part.charCodeAt(l++)); // Simple variation: don't support surrogates or subsequences in seq.[m
[32m+[m
[32m+[m[32m                    writeTable[curAddr++] = SEQ_START - this.decodeTableSeq.length;[m
[32m+[m[32m                    this.decodeTableSeq.push(seq);[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                    writeTable[curAddr++] = code; // Basic char[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m[41m [m
[32m+[m[32m        else if (typeof part === "number") { // Integer, meaning increasing sequence starting with prev character.[m
[32m+[m[32m            var charCode = writeTable[curAddr - 1] + 1;[m
[32m+[m[32m            for (var l = 0; l < part; l++)[m
[32m+[m[32m                writeTable[curAddr++] = charCode++;[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m            throw new Error("Incorrect type '" + typeof part + "' given in "  + this.encodingName + " at chunk " + chunk[0]);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (curAddr > 0xFF)[m
[32m+[m[32m        throw new Error("Incorrect chunk in "  + this.encodingName + " at addr " + chunk[0] + ": too long" + curAddr);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Encoder helpers[m
[32m+[m[32mDBCSCodec.prototype._getEncodeBucket = function(uCode) {[m
[32m+[m[32m    var high = uCode >> 8; // This could be > 0xFF because of astral characters.[m
[32m+[m[32m    if (this.encodeTable[high] === undefined)[m
[32m+[m[32m        this.encodeTable[high] = UNASSIGNED_NODE.slice(0); // Create bucket on demand.[m
[32m+[m[32m    return this.encodeTable[high];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSCodec.prototype._setEncodeChar = function(uCode, dbcsCode) {[m
[32m+[m[32m    var bucket = this._getEncodeBucket(uCode);[m
[32m+[m[32m    var low = uCode & 0xFF;[m
[32m+[m[32m    if (bucket[low] <= SEQ_START)[m
[32m+[m[32m        this.encodeTableSeq[SEQ_START-bucket[low]][DEF_CHAR] = dbcsCode; // There's already a sequence, set a single-char subsequence of it.[m
[32m+[m[32m    else if (bucket[low] == UNASSIGNED)[m
[32m+[m[32m        bucket[low] = dbcsCode;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSCodec.prototype._setEncodeSequence = function(seq, dbcsCode) {[m
[32m+[m[41m    [m
[32m+[m[32m    // Get the root of character tree according to first character of the sequence.[m
[32m+[m[32m    var uCode = seq[0];[m
[32m+[m[32m    var bucket = this._getEncodeBucket(uCode);[m
[32m+[m[32m    var low = uCode & 0xFF;[m
[32m+[m
[32m+[m[32m    var node;[m
[32m+[m[32m    if (bucket[low] <= SEQ_START) {[m
[32m+[m[32m        // There's already a sequence with  - use it.[m
[32m+[m[32m        node = this.encodeTableSeq[SEQ_START-bucket[low]];[m
[32m+[m[32m    }[m
[32m+[m[32m    else {[m
[32m+[m[32m        // There was no sequence object - allocate a new one.[m
[32m+[m[32m        node = {};[m
[32m+[m[32m        if (bucket[low] !== UNASSIGNED) node[DEF_CHAR] = bucket[low]; // If a char was set before - make it a single-char subsequence.[m
[32m+[m[32m        bucket[low] = SEQ_START - this.encodeTableSeq.length;[m
[32m+[m[32m        this.encodeTableSeq.push(node);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Traverse the character tree, allocating new nodes as needed.[m
[32m+[m[32m    for (var j = 1; j < seq.length-1; j++) {[m
[32m+[m[32m        var oldVal = node[uCode];[m
[32m+[m[32m        if (typeof oldVal === 'object')[m
[32m+[m[32m            node = oldVal;[m
[32m+[m[32m        else {[m
[32m+[m[32m            node = node[uCode] = {}[m
[32m+[m[32m            if (oldVal !== undefined)[m
[32m+[m[32m                node[DEF_CHAR] = oldVal[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Set the leaf to given dbcsCode.[m
[32m+[m[32m    uCode = seq[seq.length-1];[m
[32m+[m[32m    node[uCode] = dbcsCode;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSCodec.prototype._fillEncodeTable = function(nodeIdx, prefix, skipEncodeChars) {[m
[32m+[m[32m    var node = this.decodeTables[nodeIdx];[m
[32m+[m[32m    for (var i = 0; i < 0x100; i++) {[m
[32m+[m[32m        var uCode = node[i];[m
[32m+[m[32m        var mbCode = prefix + i;[m
[32m+[m[32m        if (skipEncodeChars[mbCode])[m
[32m+[m[32m            continue;[m
[32m+[m
[32m+[m[32m        if (uCode >= 0)[m
[32m+[m[32m            this._setEncodeChar(uCode, mbCode);[m
[32m+[m[32m        else if (uCode <= NODE_START)[m
[32m+[m[32m            this._fillEncodeTable(NODE_START - uCode, mbCode << 8, skipEncodeChars);[m
[32m+[m[32m        else if (uCode <= SEQ_START)[m
[32m+[m[32m            this._setEncodeSequence(this.decodeTableSeq[SEQ_START - uCode], mbCode);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// == Encoder ==================================================================[m
[32m+[m
[32m+[m[32mfunction DBCSEncoder(options, codec) {[m
[32m+[m[32m    // Encoder state[m
[32m+[m[32m    this.leadSurrogate = -1;[m
[32m+[m[32m    this.seqObj = undefined;[m
[32m+[m[41m    [m
[32m+[m[32m    // Static data[m
[32m+[m[32m    this.encodeTable = codec.encodeTable;[m
[32m+[m[32m    this.encodeTableSeq = codec.encodeTableSeq;[m
[32m+[m[32m    this.defaultCharSingleByte = codec.defCharSB;[m
[32m+[m[32m    this.gb18030 = codec.gb18030;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSEncoder.prototype.write = function(str) {[m
[32m+[m[32m    var newBuf = Buffer.alloc(str.length * (this.gb18030 ? 4 : 3)),[m
[32m+[m[32m        leadSurrogate = this.leadSurrogate,[m
[32m+[m[32m        seqObj = this.seqObj, nextChar = -1,[m
[32m+[m[32m        i = 0, j = 0;[m
[32m+[m
[32m+[m[32m    while (true) {[m
[32m+[m[32m        // 0. Get next character.[m
[32m+[m[32m        if (nextChar === -1) {[m
[32m+[m[32m            if (i == str.length) break;[m
[32m+[m[32m            var uCode = str.charCodeAt(i++);[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            var uCode = nextChar;[m
[32m+[m[32m            nextChar = -1;[m[41m    [m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // 1. Handle surrogates.[m
[32m+[m[32m        if (0xD800 <= uCode && uCode < 0xE000) { // Char is one of surrogates.[m
[32m+[m[32m            if (uCode < 0xDC00) { // We've got lead surrogate.[m
[32m+[m[32m                if (leadSurrogate === -1) {[m
[32m+[m[32m                    leadSurrogate = uCode;[m
[32m+[m[32m                    continue;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    leadSurrogate = uCode;[m
[32m+[m[32m                    // Double lead surrogate found.[m
[32m+[m[32m                    uCode = UNASSIGNED;[m
[32m+[m[32m                }[m
[32m+[m[32m            } else { // We've got trail surrogate.[m
[32m+[m[32m                if (leadSurrogate !== -1) {[m
[32m+[m[32m                    uCode = 0x10000 + (leadSurrogate - 0xD800) * 0x400 + (uCode - 0xDC00);[m
[32m+[m[32m                    leadSurrogate = -1;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // Incomplete surrogate pair - only trail surrogate found.[m
[32m+[m[32m                    uCode = UNASSIGNED;[m
[32m+[m[32m                }[m
[32m+[m[41m                [m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (leadSurrogate !== -1) {[m
[32m+[m[32m            // Incomplete surrogate pair - only lead surrogate found.[m
[32m+[m[32m            nextChar = uCode; uCode = UNASSIGNED; // Write an error, then current char.[m
[32m+[m[32m            leadSurrogate = -1;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // 2. Convert uCode character.[m
[32m+[m[32m        var dbcsCode = UNASSIGNED;[m
[32m+[m[32m        if (seqObj !== undefined && uCode != UNASSIGNED) { // We are in the middle of the sequence[m
[32m+[m[32m            var resCode = seqObj[uCode];[m
[32m+[m[32m            if (typeof resCode === 'object') { // Sequence continues.[m
[32m+[m[32m                seqObj = resCode;[m
[32m+[m[32m                continue;[m
[32m+[m
[32m+[m[32m            } else if (typeof resCode == 'number') { // Sequence finished. Write it.[m
[32m+[m[32m                dbcsCode = resCode;[m
[32m+[m
[32m+[m[32m            } else if (resCode == undefined) { // Current character is not part of the sequence.[m
[32m+[m
[32m+[m[32m                // Try default character for this sequence[m
[32m+[m[32m                resCode = seqObj[DEF_CHAR];[m
[32m+[m[32m                if (resCode !== undefined) {[m
[32m+[m[32m                    dbcsCode = resCode; // Found. Write it.[m
[32m+[m[32m                    nextChar = uCode; // Current character will be written too in the next iteration.[m
[32m+[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // TODO: What if we have no default? (resCode == undefined)[m
[32m+[m[32m                    // Then, we should write first char of the sequence as-is and try the rest recursively.[m
[32m+[m[32m                    // Didn't do it for now because no encoding has this situation yet.[m
[32m+[m[32m                    // Currently, just skip the sequence and write current char.[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            seqObj = undefined;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (uCode >= 0) {  // Regular character[m
[32m+[m[32m            var subtable = this.encodeTable[uCode >> 8];[m
[32m+[m[32m            if (subtable !== undefined)[m
[32m+[m[32m                dbcsCode = subtable[uCode & 0xFF];[m
[32m+[m[41m            [m
[32m+[m[32m            if (dbcsCode <= SEQ_START) { // Sequence start[m
[32m+[m[32m                seqObj = this.encodeTableSeq[SEQ_START-dbcsCode];[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (dbcsCode == UNASSIGNED && this.gb18030) {[m
[32m+[m[32m                // Use GB18030 algorithm to find character(s) to write.[m
[32m+[m[32m                var idx = findIdx(this.gb18030.uChars, uCode);[m
[32m+[m[32m                if (idx != -1) {[m
[32m+[m[32m                    var dbcsCode = this.gb18030.gbChars[idx] + (uCode - this.gb18030.uChars[idx]);[m
[32m+[m[32m                    newBuf[j++] = 0x81 + Math.floor(dbcsCode / 12600); dbcsCode = dbcsCode % 12600;[m
[32m+[m[32m                    newBuf[j++] = 0x30 + Math.floor(dbcsCode / 1260); dbcsCode = dbcsCode % 1260;[m
[32m+[m[32m                    newBuf[j++] = 0x81 + Math.floor(dbcsCode / 10); dbcsCode = dbcsCode % 10;[m
[32m+[m[32m                    newBuf[j++] = 0x30 + dbcsCode;[m
[32m+[m[32m                    continue;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // 3. Write dbcsCode character.[m
[32m+[m[32m        if (dbcsCode === UNASSIGNED)[m
[32m+[m[32m            dbcsCode = this.defaultCharSingleByte;[m
[32m+[m[41m        [m
[32m+[m[32m        if (dbcsCode < 0x100) {[m
[32m+[m[32m            newBuf[j++] = dbcsCode;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (dbcsCode < 0x10000) {[m
[32m+[m[32m            newBuf[j++] = dbcsCode >> 8;   // high byte[m
[32m+[m[32m            newBuf[j++] = dbcsCode & 0xFF; // low byte[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            newBuf[j++] = dbcsCode >> 16;[m
[32m+[m[32m            newBuf[j++] = (dbcsCode >> 8) & 0xFF;[m
[32m+[m[32m            newBuf[j++] = dbcsCode & 0xFF;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.seqObj = seqObj;[m
[32m+[m[32m    this.leadSurrogate = leadSurrogate;[m
[32m+[m[32m    return newBuf.slice(0, j);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSEncoder.prototype.end = function() {[m
[32m+[m[32m    if (this.leadSurrogate === -1 && this.seqObj === undefined)[m
[32m+[m[32m        return; // All clean. Most often case.[m
[32m+[m
[32m+[m[32m    var newBuf = Buffer.alloc(10), j = 0;[m
[32m+[m
[32m+[m[32m    if (this.seqObj) { // We're in the sequence.[m
[32m+[m[32m        var dbcsCode = this.seqObj[DEF_CHAR];[m
[32m+[m[32m        if (dbcsCode !== undefined) { // Write beginning of the sequence.[m
[32m+[m[32m            if (dbcsCode < 0x100) {[m
[32m+[m[32m                newBuf[j++] = dbcsCode;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                newBuf[j++] = dbcsCode >> 8;   // high byte[m
[32m+[m[32m                newBuf[j++] = dbcsCode & 0xFF; // low byte[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // See todo above.[m
[32m+[m[32m        }[m
[32m+[m[32m        this.seqObj = undefined;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.leadSurrogate !== -1) {[m
[32m+[m[32m        // Incomplete surrogate pair - only lead surrogate found.[m
[32m+[m[32m        newBuf[j++] = this.defaultCharSingleByte;[m
[32m+[m[32m        this.leadSurrogate = -1;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    return newBuf.slice(0, j);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Export for testing[m
[32m+[m[32mDBCSEncoder.prototype.findIdx = findIdx;[m
[32m+[m
[32m+[m
[32m+[m[32m// == Decoder ==================================================================[m
[32m+[m
[32m+[m[32mfunction DBCSDecoder(options, codec) {[m
[32m+[m[32m    // Decoder state[m
[32m+[m[32m    this.nodeIdx = 0;[m
[32m+[m[32m    this.prevBuf = Buffer.alloc(0);[m
[32m+[m
[32m+[m[32m    // Static data[m
[32m+[m[32m    this.decodeTables = codec.decodeTables;[m
[32m+[m[32m    this.decodeTableSeq = codec.decodeTableSeq;[m
[32m+[m[32m    this.defaultCharUnicode = codec.defaultCharUnicode;[m
[32m+[m[32m    this.gb18030 = codec.gb18030;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSDecoder.prototype.write = function(buf) {[m
[32m+[m[32m    var newBuf = Buffer.alloc(buf.length*2),[m
[32m+[m[32m        nodeIdx = this.nodeIdx,[m[41m [m
[32m+[m[32m        prevBuf = this.prevBuf, prevBufOffset = this.prevBuf.length,[m
[32m+[m[32m        seqStart = -this.prevBuf.length, // idx of the start of current parsed sequence.[m
[32m+[m[32m        uCode;[m
[32m+[m
[32m+[m[32m    if (prevBufOffset > 0) // Make prev buf overlap a little to make it easier to slice later.[m
[32m+[m[32m        prevBuf = Buffer.concat([prevBuf, buf.slice(0, 10)]);[m
[32m+[m[41m    [m
[32m+[m[32m    for (var i = 0, j = 0; i < buf.length; i++) {[m
[32m+[m[32m        var curByte = (i >= 0) ? buf[i] : prevBuf[i + prevBufOffset];[m
[32m+[m
[32m+[m[32m        // Lookup in current trie node.[m
[32m+[m[32m        var uCode = this.decodeTables[nodeIdx][curByte];[m
[32m+[m
[32m+[m[32m        if (uCode >= 0) {[m[41m [m
[32m+[m[32m            // Normal character, just use it.[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (uCode === UNASSIGNED) { // Unknown char.[m
[32m+[m[32m            // TODO: Callback with seq.[m
[32m+[m[32m            //var curSeq = (seqStart >= 0) ? buf.slice(seqStart, i+1) : prevBuf.slice(seqStart + prevBufOffset, i+1 + prevBufOffset);[m
[32m+[m[32m            i = seqStart; // Try to parse again, after skipping first byte of the sequence ('i' will be incremented by 'for' cycle).[m
[32m+[m[32m            uCode = this.defaultCharUnicode.charCodeAt(0);[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (uCode === GB18030_CODE) {[m
[32m+[m[32m            var curSeq = (seqStart >= 0) ? buf.slice(seqStart, i+1) : prevBuf.slice(seqStart + prevBufOffset, i+1 + prevBufOffset);[m
[32m+[m[32m            var ptr = (curSeq[0]-0x81)*12600 + (curSeq[1]-0x30)*1260 + (curSeq[2]-0x81)*10 + (curSeq[3]-0x30);[m
[32m+[m[32m            var idx = findIdx(this.gb18030.gbChars, ptr);[m
[32m+[m[32m            uCode = this.gb18030.uChars[idx] + ptr - this.gb18030.gbChars[idx];[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (uCode <= NODE_START) { // Go to next trie node.[m
[32m+[m[32m            nodeIdx = NODE_START - uCode;[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (uCode <= SEQ_START) { // Output a sequence of chars.[m
[32m+[m[32m            var seq = this.decodeTableSeq[SEQ_START - uCode];[m
[32m+[m[32m            for (var k = 0; k < seq.length - 1; k++) {[m
[32m+[m[32m                uCode = seq[k];[m
[32m+[m[32m                newBuf[j++] = uCode & 0xFF;[m
[32m+[m[32m                newBuf[j++] = uCode >> 8;[m
[32m+[m[32m            }[m
[32m+[m[32m            uCode = seq[seq.length-1];[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m            throw new Error("iconv-lite internal error: invalid decoding table value " + uCode + " at " + nodeIdx + "/" + curByte);[m
[32m+[m
[32m+[m[32m        // Write the character to buffer, handling higher planes using surrogate pair.[m
[32m+[m[32m        if (uCode > 0xFFFF) {[m[41m [m
[32m+[m[32m            uCode -= 0x10000;[m
[32m+[m[32m            var uCodeLead = 0xD800 + Math.floor(uCode / 0x400);[m
[32m+[m[32m            newBuf[j++] = uCodeLead & 0xFF;[m
[32m+[m[32m            newBuf[j++] = uCodeLead >> 8;[m
[32m+[m
[32m+[m[32m            uCode = 0xDC00 + uCode % 0x400;[m
[32m+[m[32m        }[m
[32m+[m[32m        newBuf[j++] = uCode & 0xFF;[m
[32m+[m[32m        newBuf[j++] = uCode >> 8;[m
[32m+[m
[32m+[m[32m        // Reset trie node.[m
[32m+[m[32m        nodeIdx = 0; seqStart = i+1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.nodeIdx = nodeIdx;[m
[32m+[m[32m    this.prevBuf = (seqStart >= 0) ? buf.slice(seqStart) : prevBuf.slice(seqStart + prevBufOffset);[m
[32m+[m[32m    return newBuf.slice(0, j).toString('ucs2');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDBCSDecoder.prototype.end = function() {[m
[32m+[m[32m    var ret = '';[m
[32m+[m
[32m+[m[32m    // Try to parse all remaining chars.[m
[32m+[m[32m    while (this.prevBuf.length > 0) {[m
[32m+[m[32m        // Skip 1 character in the buffer.[m
[32m+[m[32m        ret += this.defaultCharUnicode;[m
[32m+[m[32m        var buf = this.prevBuf.slice(1);[m
[32m+[m
[32m+[m[32m        // Parse remaining as usual.[m
[32m+[m[32m        this.prevBuf = Buffer.alloc(0);[m
[32m+[m[32m        this.nodeIdx = 0;[m
[32m+[m[32m        if (buf.length > 0)[m
[32m+[m[32m            ret += this.write(buf);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.nodeIdx = 0;[m
[32m+[m[32m    return ret;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Binary search for GB18030. Returns largest i such that table[i] <= val.[m
[32m+[m[32mfunction findIdx(table, val) {[m
[32m+[m[32m    if (table[0] > val)[m
[32m+[m[32m        return -1;[m
[32m+[m
[32m+[m[32m    var l = 0, r = table.length;[m
[32m+[m[32m    while (l < r-1) { // always table[l] <= val < table[r][m
[32m+[m[32m        var mid = l + Math.floor((r-l+1)/2);[m
[32m+[m[32m        if (table[mid] <= val)[m
[32m+[m[32m            l = mid;[m
[32m+[m[32m        else[m
[32m+[m[32m            r = mid;[m
[32m+[m[32m    }[m
[32m+[m[32m    return l;[m
[32m+[m[32m}[m
[32m+[m
