[32m+[m[32m# unpipe[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Node.js Version][node-image]][node-url][m
[32m+[m[32m[![Build Status][travis-image]][travis-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mUnpipe a stream from all destinations.[m
[32m+[m
[32m+[m[32m## Installation[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install unpipe[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar unpipe = require('unpipe')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### unpipe(stream)[m
[32m+[m
[32m+[m[32mUnpipes all destinations from a given stream. With stream 2+, this is[m
[32m+[m[32mequivalent to `stream.unpipe()`. When used with streams 1 style streams[m
[32m+[m[32m(typically Node.js 0.8 and below), this module attempts to undo the[m
[32m+[m[32mactions done in `stream.pipe(dest)`.[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[MIT](LICENSE)[m
[32m+[m
[32m+[m[32m[npm-image]: https://img.shields.io/npm/v/unpipe.svg[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/unpipe[m
[32m+[m[32m[node-image]: https://img.shields.io/node/v/unpipe.svg[m
[32m+[m[32m[node-url]: http://nodejs.org/download/[m
[32m+[m[32m[travis-image]: https://img.shields.io/travis/stream-utils/unpipe.svg[m
[32m+[m[32m[travis-url]: https://travis-ci.org/stream-utils/unpipe[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/stream-utils/unpipe.svg[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/stream-utils/unpipe?branch=master[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/unpipe.svg[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/unpipe[m
[1mdiff --git a/node_modules/unpipe/index.js b/node_modules/unpipe/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..15c3d97[m
[1m--- /dev/null[m
[1m+++ b/node_modules/unpipe/index.js[m
[36m@@ -0,0 +1,69 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * unpipe[m
[32m+[m[32m * Copyright(c) 2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = unpipe[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if there are Node.js pipe-like data listeners.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction hasPipeDataListeners(stream) {[m
[32m+[m[32m  var listeners = stream.listeners('data')[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < listeners.length; i++) {[m
[32m+[m[32m    if (listeners[i].name === 'ondata') {[m
[32m+[m[32m      return true[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return false[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Unpipe a stream from all destinations.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} stream[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction unpipe(stream) {[m
[32m+[m[32m  if (!stream) {[m
[32m+[m[32m    throw new TypeError('argument stream is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof stream.unpipe === 'function') {[m
[32m+[m[32m    // new-style[m
[32m+[m[32m    stream.unpipe()[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Node.js 0.8 hack[m
[32m+[m[32m  if (!hasPipeDataListeners(stream)) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var listener[m
[32m+[m[32m  var listeners = stream.listeners('close')[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < listeners.length; i++) {[m
[32m+[m[32m    listener = listeners[i][m
[32m+[m
[32m+[m[32m    if (listener.name !== 'cleanup' && listener.name !== 'onclose') {[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // invoke the listener[m
[32m+[m[32m    listener.call(stream)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/unpipe/package.json b/node_modules/unpipe/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..a2b7358[m
[1m--- /dev/null[m
[1m+++ b/node_modules/unpipe/package.json[m
[36m@@ -0,0 +1,27 @@[m
[32m+[m[32m{[m
