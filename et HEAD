[32m+[m[32m    "test-ci": "nyc --reporter=lcov --reporter=text npm test",[m
[32m+[m[32m    "test-cov": "nyc --reporter=html --reporter=text npm test",[m
[32m+[m[32m    "version": "node scripts/version-history.js && git add HISTORY.md"[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
