[32m+[m[32m  },[m
[32m+[m[32m  "keywords": [[m
[32m+[m[32m    "express",[m
[32m+[m[32m    "regexp"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "component": {[m
[32m+[m[32m    "scripts": {[m
[32m+[m[32m      "path-to-regexp": "index.js"[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  "license": "MIT",[m
[32m+[m[32m  "repository": {[m
[32m+[m[32m    "type": "git",[m
[32m+[m[32m    "url": "https://github.com/component/path-to-regexp.git"[m
[32m+[m[32m  },[m
