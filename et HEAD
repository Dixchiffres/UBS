[32m+[m[32m    "README.md",[m
[32m+[m[32m    "index.js"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "engines": {[m
[32m+[m[32m    "node": ">= 0.6"[m
[32m+[m[32m  },[m
