[32m+[m[32mtest('truly nameless function', { skip: !arrow || !functionsHaveConfigurableNames }, function (t) {[m
[32m+[m[32m    function f() {}[m
[32m+[m[32m    Object.defineProperty(f, 'name', { value: false });[m
[32m+[m[32m    t.equal(f.name, false);[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(f),[m
[32m+[m[32m        '[Function: f]',[m
[32m+[m[32m        'named function with falsy `.name` does not hide its original name'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    function g() {}[m
[32m+[m[32m    Object.defineProperty(g, 'name', { value: true });[m
[32m+[m[32m    t.equal(g.name, true);[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(g),[m
[32m+[m[32m        '[Function: true]',[m
[32m+[m[32m        'named function with truthy `.name` hides its original name'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    var anon = function () {}; // eslint-disable-line func-style[m
[32m+[m[32m    Object.defineProperty(anon, 'name', { value: null });[m
[32m+[m[32m    t.equal(anon.name, null);[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(anon),[m
[32m+[m[32m        '[Function (anonymous)]',[m
[32m+[m[32m        'anon function with falsy `.name` does not hide its anonymity'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    var anon2 = function () {}; // eslint-disable-line func-style[m
[32m+[m[32m    Object.defineProperty(anon2, 'name', { value: 1 });[m
[32m+[m[32m    t.equal(anon2.name, 1);[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(anon2),[m
[32m+[m[32m        '[Function: 1]',[m
[32m+[m[32m        'anon function with truthy `.name` hides its anonymity'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
