[32m+[m[32mvar inspect = require('../');[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mtest('circular', function (t) {[m
[32m+[m[32m    t.plan(2);[m
[32m+[m[32m    var obj = { a: 1, b: [3, 4] };[m
[32m+[m[32m    obj.c = obj;[m
[32m+[m[32m    t.equal(inspect(obj), '{ a: 1, b: [ 3, 4 ], c: [Circular] }');[m
[32m+[m
[32m+[m[32m    var double = {};[m
[32m+[m[32m    double.a = [double];[m
[32m+[m[32m    double.b = {};[m
[32m+[m[32m    double.b.inner = double.b;[m
[32m+[m[32m    double.b.obj = double;[m
[32m+[m[32m    t.equal(inspect(double), '{ a: [ [Circular] ], b: { inner: [Circular], obj: [Circular] } }');[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/deep.js b/node_modules/object-inspect/test/deep.js[m
