[36m@@ -0,0 +1,34 @@[m
[32m+[m[32m# utils-merge[m
[32m+[m
[32m+[m[32m[![Version](https://img.shields.io/npm/v/utils-merge.svg?label=version)](https://www.npmjs.com/package/utils-merge)[m
[32m+[m[32m[![Build](https://img.shields.io/travis/jaredhanson/utils-merge.svg)](https://travis-ci.org/jaredhanson/utils-merge)[m
[32m+[m[32m[![Quality](https://img.shields.io/codeclimate/github/jaredhanson/utils-merge.svg?label=quality)](https://codeclimate.com/github/jaredhanson/utils-merge)[m
[32m+[m[32m[![Coverage](https://img.shields.io/coveralls/jaredhanson/utils-merge.svg)](https://coveralls.io/r/jaredhanson/utils-merge)[m
[32m+[m[32m[![Dependencies](https://img.shields.io/david/jaredhanson/utils-merge.svg)](https://david-dm.org/jaredhanson/utils-merge)[m
[32m+[m
[32m+[m
[32m+[m[32mMerges the properties from a source object into a destination object.[m
[32m+[m
[32m+[m[32m## Install[m
[32m+[m
[32m+[m[32m```bash[m
[32m+[m[32m$ npm install utils-merge[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar a = { foo: 'bar' }[m
[32m+[m[32m  , b = { bar: 'baz' };[m
[32m+[m
[32m+[m[32mmerge(a, b);[m
[32m+[m[32m// => { foo: 'bar', bar: 'baz' }[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[The MIT License](http://opensource.org/licenses/MIT)[m
[32m+[m
[32m+[m[32mCopyright (c) 2013-2017 Jared Hanson <[http://jaredhanson.net/](http://jaredhanson.net/)>[m
[32m+[m
[32m+[m[32m<a target='_blank' rel='nofollow' href='https://app.codesponsor.io/link/vK9dyjRnnWsMzzJTQ57fRJpH/jaredhanson/utils-merge'>  <img alt='Sponsor' width='888' height='68' src='https://app.codesponsor.io/embed/vK9dyjRnnWsMzzJTQ57fRJpH/jaredhanson/utils-merge.svg' /></a>[m
[1mdiff --git a/node_modules/utils-merge/index.js b/node_modules/utils-merge/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4265c69[m
[1m--- /dev/null[m
[1m+++ b/node_modules/utils-merge/index.js[m
[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Merge object b with object a.[m
[32m+[m[32m *[m
[32m+[m[32m *     var a = { foo: 'bar' }[m
[32m+[m[32m *       , b = { bar: 'baz' };[m
[32m+[m[32m *[m
[32m+[m[32m *     merge(a, b);[m
[32m+[m[32m *     // => { foo: 'bar', bar: 'baz' }[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} a[m
[32m+[m[32m * @param {Object} b[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = function(a, b){[m
[32m+[m[32m  if (a && b) {[m
[32m+[m[32m    for (var key in b) {[m
[32m+[m[32m      a[key] = b[key];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return a;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/utils-merge/package.json b/node_modules/utils-merge/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..e36b078[m
[1m--- /dev/null[m
[1m+++ b/node_modules/utils-merge/package.json[m
[36m@@ -0,0 +1,40 @@[m
[32m+[m[32m{[m
[32m+[m[32m  "name": "utils-merge",[m
[32m+[m[32m  "version": "1.0.1",[m
[32m+[m[32m  "description": "merge() utility function",[m
[32m+[m[32m  "keywords": [[m
[32m+[m[32m    "util"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "author": {[m
[32m+[m[32m    "name": "Jared Hanson",[m
[32m+[m[32m    "email": "jaredhanson@gmail.com",[m
[32m+[m[32m    "url": "http://www.jaredhanson.net/"[m
[32m+[m[32m  },[m
[32m+[m[32m  "repository": {[m
[32m+[m[32m    "type": "git",[m
[32m+[m[32m    "url": "git://github.com/jaredhanson/utils-merge.git"[m
[32m+[m[32m  },[m
[32m+[m[32m  "bugs": {[m
[32m+[m[32m    "url": "http://github.com/jaredhanson/utils-merge/issues"[m
[32m+[m[32m  },[m
[32m+[m[32m  "license": "MIT",[m
