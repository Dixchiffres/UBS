[36m@@ -0,0 +1,252 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m[32mvar isArray = Array.isArray;[m
[32m+[m
[32m+[m[32mvar hexTable = (function () {[m
[32m+[m[32m    var array = [];[m
[32m+[m[32m    for (var i = 0; i < 256; ++i) {[m
[32m+[m[32m        array.push('%' + ((i < 16 ? '0' : '') + i.toString(16)).toUpperCase());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return array;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32mvar compactQueue = function compactQueue(queue) {[m
[32m+[m[32m    while (queue.length > 1) {[m
[32m+[m[32m        var item = queue.pop();[m
[32m+[m[32m        var obj = item.obj[item.prop];[m
[32m+[m
[32m+[m[32m        if (isArray(obj)) {[m
[32m+[m[32m            var compacted = [];[m
[32m+[m
[32m+[m[32m            for (var j = 0; j < obj.length; ++j) {[m
[32m+[m[32m                if (typeof obj[j] !== 'undefined') {[m
[32m+[m[32m                    compacted.push(obj[j]);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            item.obj[item.prop] = compacted;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar arrayToObject = function arrayToObject(source, options) {[m
[32m+[m[32m    var obj = options && options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m    for (var i = 0; i < source.length; ++i) {[m
[32m+[m[32m        if (typeof source[i] !== 'undefined') {[m
[32m+[m[32m            obj[i] = source[i];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar merge = function merge(target, source, options) {[m
[32m+[m[32m    /* eslint no-param-reassign: 0 */[m
[32m+[m[32m    if (!source) {[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof source !== 'object') {[m
[32m+[m[32m        if (isArray(target)) {[m
[32m+[m[32m            target.push(source);[m
[32m+[m[32m        } else if (target && typeof target === 'object') {[m
[32m+[m[32m            if ((options && (options.plainObjects || options.allowPrototypes)) || !has.call(Object.prototype, source)) {[m
[32m+[m[32m                target[source] = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return [target, source];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!target || typeof target !== 'object') {[m
[32m+[m[32m        return [target].concat(source);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var mergeTarget = target;[m
[32m+[m[32m    if (isArray(target) && !isArray(source)) {[m
[32m+[m[32m        mergeTarget = arrayToObject(target, options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (isArray(target) && isArray(source)) {[m
[32m+[m[32m        source.forEach(function (item, i) {[m
[32m+[m[32m            if (has.call(target, i)) {[m
[32m+[m[32m                var targetItem = target[i];[m
[32m+[m[32m                if (targetItem && typeof targetItem === 'object' && item && typeof item === 'object') {[m
[32m+[m[32m                    target[i] = merge(targetItem, item, options);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    target.push(item);[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                target[i] = item;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        var value = source[key];[m
[32m+[m
[32m+[m[32m        if (has.call(acc, key)) {[m
[32m+[m[32m            acc[key] = merge(acc[key], value, options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            acc[key] = value;[m
[32m+[m[32m        }[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, mergeTarget);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar assign = function assignSingleSource(target, source) {[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        acc[key] = source[key];[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, target);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar decode = function (str, decoder, charset) {[m
[32m+[m[32m    var strWithoutPlus = str.replace(/\+/g, ' ');[m
[32m+[m[32m    if (charset === 'iso-8859-1') {[m
[32m+[m[32m        // unescape never throws, no try...catch needed:[m
[32m+[m[32m        return strWithoutPlus.replace(/%[0-9a-f]{2}/gi, unescape);[m
[32m+[m[32m    }[m
[32m+[m[32m    // utf-8[m
[32m+[m[32m    try {[m
[32m+[m[32m        return decodeURIComponent(strWithoutPlus);[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m        return strWithoutPlus;[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar encode = function encode(str, defaultEncoder, charset, kind, format) {[m
[32m+[m[32m    // This code was originally written by Brian White (mscdex) for the io.js core querystring library.[m
[32m+[m[32m    // It has been adapted here for stricter adherence to RFC 3986[m
[32m+[m[32m    if (str.length === 0) {[m
[32m+[m[32m        return str;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var string = str;[m
[32m+[m[32m    if (typeof str === 'symbol') {[m
[32m+[m[32m        string = Symbol.prototype.toString.call(str);[m
[32m+[m[32m    } else if (typeof str !== 'string') {[m
[32m+[m[32m        string = String(str);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (charset === 'iso-8859-1') {[m
[32m+[m[32m        return escape(string).replace(/%u[0-9a-f]{4}/gi, function ($0) {[m
[32m+[m[32m            return '%26%23' + parseInt($0.slice(2), 16) + '%3B';[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var out = '';[m
[32m+[m[32m    for (var i = 0; i < string.length; ++i) {[m
[32m+[m[32m        var c = string.charCodeAt(i);[m
[32m+[m
[32m+[m[32m        if ([m
[32m+[m[32m            c === 0x2D // -[m
[32m+[m[32m            || c === 0x2E // .[m
[32m+[m[32m            || c === 0x5F // _[m
[32m+[m[32m            || c === 0x7E // ~[m
[32m+[m[32m            || (c >= 0x30 && c <= 0x39) // 0-9[m
[32m+[m[32m            || (c >= 0x41 && c <= 0x5A) // a-z[m
[32m+[m[32m            || (c >= 0x61 && c <= 0x7A) // A-Z[m
[32m+[m[32m            || (format === formats.RFC1738 && (c === 0x28 || c === 0x29)) // ( )[m
[32m+[m[32m        ) {[m
[32m+[m[32m            out += string.charAt(i);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x80) {[m
[32m+[m[32m            out = out + hexTable[c];[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x800) {[m
[32m+[m[32m            out = out + (hexTable[0xC0 | (c >> 6)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0xD800 || c >= 0xE000) {[m
[32m+[m[32m            out = out + (hexTable[0xE0 | (c >> 12)] + hexTable[0x80 | ((c >> 6) & 0x3F)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        i += 1;[m
[32m+[m[32m        c = 0x10000 + (((c & 0x3FF) << 10) | (string.charCodeAt(i) & 0x3FF));[m
[32m+[m[32m        /* eslint operator-linebreak: [2, "before"] */[m
[32m+[m[32m        out += hexTable[0xF0 | (c >> 18)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 12) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 6) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | (c & 0x3F)];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return out;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar compact = function compact(value) {[m
[32m+[m[32m    var queue = [{ obj: { o: value }, prop: 'o' }];[m
[32m+[m[32m    var refs = [];[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < queue.length; ++i) {[m
[32m+[m[32m        var item = queue[i];[m
[32m+[m[32m        var obj = item.obj[item.prop];[m
[32m+[m
[32m+[m[32m        var keys = Object.keys(obj);[m
[32m+[m[32m        for (var j = 0; j < keys.length; ++j) {[m
[32m+[m[32m            var key = keys[j];[m
[32m+[m[32m            var val = obj[key];[m
[32m+[m[32m            if (typeof val === 'object' && val !== null && refs.indexOf(val) === -1) {[m
[32m+[m[32m                queue.push({ obj: obj, prop: key });[m
[32m+[m[32m                refs.push(val);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    compactQueue(queue);[m
[32m+[m
[32m+[m[32m    return value;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isRegExp = function isRegExp(obj) {[m
[32m+[m[32m    return Object.prototype.toString.call(obj) === '[object RegExp]';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar isBuffer = function isBuffer(obj) {[m
[32m+[m[32m    if (!obj || typeof obj !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return !!(obj.constructor && obj.constructor.isBuffer && obj.constructor.isBuffer(obj));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar combine = function combine(a, b) {[m
[32m+[m[32m    return [].concat(a, b);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar maybeMap = function maybeMap(val, fn) {[m
[32m+[m[32m    if (isArray(val)) {[m
[32m+[m[32m        var mapped = [];[m
[32m+[m[32m        for (var i = 0; i < val.length; i += 1) {[m
[32m+[m[32m            mapped.push(fn(val[i]));[m
[32m+[m[32m        }[m
[32m+[m[32m        return mapped;[m
[32m+[m[32m    }[m
[32m+[m[32m    return fn(val);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    arrayToObject: arrayToObject,[m
[32m+[m[32m    assign: assign,[m
[32m+[m[32m    combine: combine,[m
[32m+[m[32m    compact: compact,[m
[32m+[m[32m    decode: decode,[m
[32m+[m[32m    encode: encode,[m
[32m+[m[32m    isBuffer: isBuffer,[m
[32m+[m[32m    isRegExp: isRegExp,[m
[32m+[m[32m    maybeMap: maybeMap,[m
[32m+[m[32m    merge: merge[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/qs/package.json b/node_modules/qs/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..2ff42f3[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/package.json[m
[36m@@ -0,0 +1,77 @@[m
