[36m@@ -0,0 +1,10 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/* eslint-env browser */[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m
[32m+[m[32mvar d = document.createElement('div');[m
[32m+[m[32md.setAttribute('id', 'beep');[m
[32m+[m[32md.innerHTML = '<b>wooo</b><i>iiiii</i>';[m
[32m+[m
[32m+[m[32mconsole.log(inspect([d, { a: 3, b: 4, c: [5, 6, [7, [8, [9]]]] }]));[m
[1mdiff --git a/node_modules/object-inspect/index.js b/node_modules/object-inspect/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..eb89faf[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/index.js[m
[36m@@ -0,0 +1,527 @@[m
[32m+[m[32mvar hasMap = typeof Map === 'function' && Map.prototype;[m
[32m+[m[32mvar mapSizeDescriptor = Object.getOwnPropertyDescriptor && hasMap ? Object.getOwnPropertyDescriptor(Map.prototype, 'size') : null;[m
[32m+[m[32mvar mapSize = hasMap && mapSizeDescriptor && typeof mapSizeDescriptor.get === 'function' ? mapSizeDescriptor.get : null;[m
[32m+[m[32mvar mapForEach = hasMap && Map.prototype.forEach;[m
[32m+[m[32mvar hasSet = typeof Set === 'function' && Set.prototype;[m
[32m+[m[32mvar setSizeDescriptor = Object.getOwnPropertyDescriptor && hasSet ? Object.getOwnPropertyDescriptor(Set.prototype, 'size') : null;[m
[32m+[m[32mvar setSize = hasSet && setSizeDescriptor && typeof setSizeDescriptor.get === 'function' ? setSizeDescriptor.get : null;[m
[32m+[m[32mvar setForEach = hasSet && Set.prototype.forEach;[m
[32m+[m[32mvar hasWeakMap = typeof WeakMap === 'function' && WeakMap.prototype;[m
[32m+[m[32mvar weakMapHas = hasWeakMap ? WeakMap.prototype.has : null;[m
[32m+[m[32mvar hasWeakSet = typeof WeakSet === 'function' && WeakSet.prototype;[m
[32m+[m[32mvar weakSetHas = hasWeakSet ? WeakSet.prototype.has : null;[m
[32m+[m[32mvar hasWeakRef = typeof WeakRef === 'function' && WeakRef.prototype;[m
[32m+[m[32mvar weakRefDeref = hasWeakRef ? WeakRef.prototype.deref : null;[m
[32m+[m[32mvar booleanValueOf = Boolean.prototype.valueOf;[m
[32m+[m[32mvar objectToString = Object.prototype.toString;[m
[32m+[m[32mvar functionToString = Function.prototype.toString;[m
[32m+[m[32mvar $match = String.prototype.match;[m
[32m+[m[32mvar $slice = String.prototype.slice;[m
[32m+[m[32mvar $replace = String.prototype.replace;[m
[32m+[m[32mvar $toUpperCase = String.prototype.toUpperCase;[m
[32m+[m[32mvar $toLowerCase = String.prototype.toLowerCase;[m
[32m+[m[32mvar $test = RegExp.prototype.test;[m
[32m+[m[32mvar $concat = Array.prototype.concat;[m
[32m+[m[32mvar $join = Array.prototype.join;[m
[32m+[m[32mvar $arrSlice = Array.prototype.slice;[m
[32m+[m[32mvar $floor = Math.floor;[m
[32m+[m[32mvar bigIntValueOf = typeof BigInt === 'function' ? BigInt.prototype.valueOf : null;[m
[32m+[m[32mvar gOPS = Object.getOwnPropertySymbols;[m
[32m+[m[32mvar symToString = typeof Symbol === 'function' && typeof Symbol.iterator === 'symbol' ? Symbol.prototype.toString : null;[m
[32m+[m[32mvar hasShammedSymbols = typeof Symbol === 'function' && typeof Symbol.iterator === 'object';[m
[32m+[m[32m// ie, `has-tostringtag/shams[m
[32m+[m[32mvar toStringTag = typeof Symbol === 'function' && Symbol.toStringTag && (typeof Symbol.toStringTag === hasShammedSymbols ? 'object' : 'symbol')[m
[32m+[m[32m    ? Symbol.toStringTag[m
[32m+[m[32m    : null;[m
[32m+[m[32mvar isEnumerable = Object.prototype.propertyIsEnumerable;[m
[32m+[m
[32m+[m[32mvar gPO = (typeof Reflect === 'function' ? Reflect.getPrototypeOf : Object.getPrototypeOf) || ([m
[32m+[m[32m    [].__proto__ === Array.prototype // eslint-disable-line no-proto[m
[32m+[m[32m        ? function (O) {[m
[32m+[m[32m            return O.__proto__; // eslint-disable-line no-proto[m
[32m+[m[32m        }[m
[32m+[m[32m        : null[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mfunction addNumericSeparator(num, str) {[m
[32m+[m[32m    if ([m
[32m+[m[32m        num === Infinity[m
[32m+[m[32m        || num === -Infinity[m
[32m+[m[32m        || num !== num[m
[32m+[m[32m        || (num && num > -1000 && num < 1000)[m
[32m+[m[32m        || $test.call(/e/, str)[m
[32m+[m[32m    ) {[m
[32m+[m[32m        return str;[m
[32m+[m[32m    }[m
[32m+[m[32m    var sepRegex = /[0-9](?=(?:[0-9]{3})+(?![0-9]))/g;[m
[32m+[m[32m    if (typeof num === 'number') {[m
[32m+[m[32m        var int = num < 0 ? -$floor(-num) : $floor(num); // trunc(num)[m
[32m+[m[32m        if (int !== num) {[m
[32m+[m[32m            var intStr = String(int);[m
[32m+[m[32m            var dec = $slice.call(str, intStr.length + 1);[m
[32m+[m[32m            return $replace.call(intStr, sepRegex, '$&_') + '.' + $replace.call($replace.call(dec, /([0-9]{3})/g, '$&_'), /_$/, '');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return $replace.call(str, sepRegex, '$&_');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar utilInspect = require('./util.inspect');[m
[32m+[m[32mvar inspectCustom = utilInspect.custom;[m
[32m+[m[32mvar inspectSymbol = isSymbol(inspectCustom) ? inspectCustom : null;[m
[32m+[m
[32m+[m[32mmodule.exports = function inspect_(obj, options, depth, seen) {[m
[32m+[m[32m    var opts = options || {};[m
[32m+[m
[32m+[m[32m    if (has(opts, 'quoteStyle') && (opts.quoteStyle !== 'single' && opts.quoteStyle !== 'double')) {[m
[32m+[m[32m        throw new TypeError('option "quoteStyle" must be "single" or "double"');[m
[32m+[m[32m    }[m
[32m+[m[32m    if ([m
[32m+[m[32m        has(opts, 'maxStringLength') && (typeof opts.maxStringLength === 'number'[m
[32m+[m[32m            ? opts.maxStringLength < 0 && opts.maxStringLength !== Infinity[m
[32m+[m[32m            : opts.maxStringLength !== null[m
[32m+[m[32m        )[m
[32m+[m[32m    ) {[m
[32m+[m[32m        throw new TypeError('option "maxStringLength", if provided, must be a positive integer, Infinity, or `null`');[m
[32m+[m[32m    }[m
[32m+[m[32m    var customInspect = has(opts, 'customInspect') ? opts.customInspect : true;[m
[32m+[m[32m    if (typeof customInspect !== 'boolean' && customInspect !== 'symbol') {[m
[32m+[m[32m        throw new TypeError('option "customInspect", if provided, must be `true`, `false`, or `\'symbol\'`');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ([m
[32m+[m[32m        has(opts, 'indent')[m
[32m+[m[32m        && opts.indent !== null[m
[32m+[m[32m        && opts.indent !== '\t'[m
[32m+[m[32m        && !(parseInt(opts.indent, 10) === opts.indent && opts.indent > 0)[m
[32m+[m[32m    ) {[m
[32m+[m[32m        throw new TypeError('option "indent" must be "\\t", an integer > 0, or `null`');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (has(opts, 'numericSeparator') && typeof opts.numericSeparator !== 'boolean') {[m
[32m+[m[32m        throw new TypeError('option "numericSeparator", if provided, must be `true` or `false`');[m
[32m+[m[32m    }[m
[32m+[m[32m    var numericSeparator = opts.numericSeparator;[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'undefined') {[m
[32m+[m[32m        return 'undefined';[m
[32m+[m[32m    }[m
[32m+[m[32m    if (obj === null) {[m
[32m+[m[32m        return 'null';[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof obj === 'boolean') {[m
[32m+[m[32m        return obj ? 'true' : 'false';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'string') {[m
[32m+[m[32m        return inspectString(obj, opts);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof obj === 'number') {[m
[32m+[m[32m        if (obj === 0) {[m
[32m+[m[32m            return Infinity / obj > 0 ? '0' : '-0';[m
[32m+[m[32m        }[m
[32m+[m[32m        var str = String(obj);[m
[32m+[m[32m        return numericSeparator ? addNumericSeparator(obj, str) : str;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof obj === 'bigint') {[m
[32m+[m[32m        var bigIntStr = String(obj) + 'n';[m
[32m+[m[32m        return numericSeparator ? addNumericSeparator(obj, bigIntStr) : bigIntStr;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var maxDepth = typeof opts.depth === 'undefined' ? 5 : opts.depth;[m
[32m+[m[32m    if (typeof depth === 'undefined') { depth = 0; }[m
[32m+[m[32m    if (depth >= maxDepth && maxDepth > 0 && typeof obj === 'object') {[m
[32m+[m[32m        return isArray(obj) ? '[Array]' : '[Object]';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var indent = getIndent(opts, depth);[m
[32m+[m
[32m+[m[32m    if (typeof seen === 'undefined') {[m
[32m+[m[32m        seen = [];[m
[32m+[m[32m    } else if (indexOf(seen, obj) >= 0) {[m
[32m+[m[32m        return '[Circular]';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function inspect(value, from, noIndent) {[m
[32m+[m[32m        if (from) {[m
[32m+[m[32m            seen = $arrSlice.call(seen);[m
[32m+[m[32m            seen.push(from);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (noIndent) {[m
[32m+[m[32m            var newOpts = {[m
[32m+[m[32m                depth: opts.depth[m
[32m+[m[32m            };[m
[32m+[m[32m            if (has(opts, 'quoteStyle')) {[m
[32m+[m[32m                newOpts.quoteStyle = opts.quoteStyle;[m
[32m+[m[32m            }[m
[32m+[m[32m            return inspect_(value, newOpts, depth + 1, seen);[m
[32m+[m[32m        }[m
[32m+[m[32m        return inspect_(value, opts, depth + 1, seen);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'function' && !isRegExp(obj)) { // in older engines, regexes are callable[m
[32m+[m[32m        var name = nameOf(obj);[m
[32m+[m[32m        var keys = arrObjKeys(obj, inspect);[m
[32m+[m[32m        return '[Function' + (name ? ': ' + name : ' (anonymous)') + ']' + (keys.length > 0 ? ' { ' + $join.call(keys, ', ') + ' }' : '');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isSymbol(obj)) {[m
[32m+[m[32m        var symString = hasShammedSymbols ? $replace.call(String(obj), /^(Symbol\(.*\))_[^)]*$/, '$1') : symToString.call(obj);[m
[32m+[m[32m        return typeof obj === 'object' && !hasShammedSymbols ? markBoxed(symString) : symString;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isElement(obj)) {[m
[32m+[m[32m        var s = '<' + $toLowerCase.call(String(obj.nodeName));[m
[32m+[m[32m        var attrs = obj.attributes || [];[m
[32m+[m[32m        for (var i = 0; i < attrs.length; i++) {[m
[32m+[m[32m            s += ' ' + attrs[i].name + '=' + wrapQuotes(quote(attrs[i].value), 'double', opts);[m
[32m+[m[32m        }[m
[32m+[m[32m        s += '>';[m
[32m+[m[32m        if (obj.childNodes && obj.childNodes.length) { s += '...'; }[m
[32m+[m[32m        s += '</' + $toLowerCase.call(String(obj.nodeName)) + '>';[m
[32m+[m[32m        return s;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isArray(obj)) {[m
[32m+[m[32m        if (obj.length === 0) { return '[]'; }[m
[32m+[m[32m        var xs = arrObjKeys(obj, inspect);[m
[32m+[m[32m        if (indent && !singleLineValues(xs)) {[m
[32m+[m[32m            return '[' + indentedJoin(xs, indent) + ']';[m
[32m+[m[32m        }[m
[32m+[m[32m        return '[ ' + $join.call(xs, ', ') + ' ]';[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isError(obj)) {[m
[32m+[m[32m        var parts = arrObjKeys(obj, inspect);[m
[32m+[m[32m        if (!('cause' in Error.prototype) && 'cause' in obj && !isEnumerable.call(obj, 'cause')) {[m
[32m+[m[32m            return '{ [' + String(obj) + '] ' + $join.call($concat.call('[cause]: ' + inspect(obj.cause), parts), ', ') + ' }';[m
[32m+[m[32m        }[m
[32m+[m[32m        if (parts.length === 0) { return '[' + String(obj) + ']'; }[m
[32m+[m[32m        return '{ [' + String(obj) + '] ' + $join.call(parts, ', ') + ' }';[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof obj === 'object' && customInspect) {[m
[32m+[m[32m        if (inspectSymbol && typeof obj[inspectSymbol] === 'function' && utilInspect) {[m
[32m+[m[32m            return utilInspect(obj, { depth: maxDepth - depth });[m
[32m+[m[32m        } else if (customInspect !== 'symbol' && typeof obj.inspect === 'function') {[m
[32m+[m[32m            return obj.inspect();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isMap(obj)) {[m
[32m+[m[32m        var mapParts = [];[m
[32m+[m[32m        if (mapForEach) {[m
[32m+[m[32m            mapForEach.call(obj, function (value, key) {[m
[32m+[m[32m                mapParts.push(inspect(key, obj, true) + ' => ' + inspect(value, obj));[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m        return collectionOf('Map', mapSize.call(obj), mapParts, indent);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isSet(obj)) {[m
[32m+[m[32m        var setParts = [];[m
[32m+[m[32m        if (setForEach) {[m
[32m+[m[32m            setForEach.call(obj, function (value) {[m
[32m+[m[32m                setParts.push(inspect(value, obj));[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m        return collectionOf('Set', setSize.call(obj), setParts, indent);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isWeakMap(obj)) {[m
[32m+[m[32m        return weakCollectionOf('WeakMap');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isWeakSet(obj)) {[m
[32m+[m[32m        return weakCollectionOf('WeakSet');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isWeakRef(obj)) {[m
[32m+[m[32m        return weakCollectionOf('WeakRef');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isNumber(obj)) {[m
[32m+[m[32m        return markBoxed(inspect(Number(obj)));[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isBigInt(obj)) {[m
[32m+[m[32m        return markBoxed(inspect(bigIntValueOf.call(obj)));[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isBoolean(obj)) {[m
[32m+[m[32m        return markBoxed(booleanValueOf.call(obj));[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isString(obj)) {[m
[32m+[m[32m        return markBoxed(inspect(String(obj)));[m
[32m+[m[32m    }[m
[32m+[m[32m    // note: in IE 8, sometimes `global !== window` but both are the prototypes of each other[m
[32m+[m[32m    /* eslint-env browser */[m
[32m+[m[32m    if (typeof window !== 'undefined' && obj === window) {[m
[32m+[m[32m        return '{ [object Window] }';[m
[32m+[m[32m    }[m
[32m+[m[32m    if ([m
[32m+[m[32m        (typeof globalThis !== 'undefined' && obj === globalThis)[m
[32m+[m[32m        || (typeof global !== 'undefined' && obj === global)[m
[32m+[m[32m    ) {[m
[32m+[m[32m        return '{ [object globalThis] }';[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!isDate(obj) && !isRegExp(obj)) {[m
[32m+[m[32m        var ys = arrObjKeys(obj, inspect);[m
[32m+[m[32m        var isPlainObject = gPO ? gPO(obj) === Object.prototype : obj instanceof Object || obj.constructor === Object;[m
[32m+[m[32m        var protoTag = obj instanceof Object ? '' : 'null prototype';[m
[32m+[m[32m        var stringTag = !isPlainObject && toStringTag && Object(obj) === obj && toStringTag in obj ? $slice.call(toStr(obj), 8, -1) : protoTag ? 'Object' : '';[m
[32m+[m[32m        var constructorTag = isPlainObject || typeof obj.constructor !== 'function' ? '' : obj.constructor.name ? obj.constructor.name + ' ' : '';[m
[32m+[m[32m        var tag = constructorTag + (stringTag || protoTag ? '[' + $join.call($concat.call([], stringTag || [], protoTag || []), ': ') + '] ' : '');[m
[32m+[m[32m        if (ys.length === 0) { return tag + '{}'; }[m
[32m+[m[32m        if (indent) {[m
[32m+[m[32m            return tag + '{' + indentedJoin(ys, indent) + '}';[m
[32m+[m[32m        }[m
[32m+[m[32m        return tag + '{ ' + $join.call(ys, ', ') + ' }';[m
[32m+[m[32m    }[m
[32m+[m[32m    return String(obj);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction wrapQuotes(s, defaultStyle, opts) {[m
[32m+[m[32m    var quoteChar = (opts.quoteStyle || defaultStyle) === 'double' ? '"' : "'";[m
[32m+[m[32m    return quoteChar + s + quoteChar;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction quote(s) {[m
[32m+[m[32m    return $replace.call(String(s), /"/g, '&quot;');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isArray(obj) { return toStr(obj) === '[object Array]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isDate(obj) { return toStr(obj) === '[object Date]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isRegExp(obj) { return toStr(obj) === '[object RegExp]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isError(obj) { return toStr(obj) === '[object Error]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isString(obj) { return toStr(obj) === '[object String]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isNumber(obj) { return toStr(obj) === '[object Number]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m[32mfunction isBoolean(obj) { return toStr(obj) === '[object Boolean]' && (!toStringTag || !(typeof obj === 'object' && toStringTag in obj)); }[m
[32m+[m
[32m+[m[32m// Symbol and BigInt do have Symbol.toStringTag by spec, so that can't be used to eliminate false positives[m
[32m+[m[32mfunction isSymbol(obj) {[m
[32m+[m[32m    if (hasShammedSymbols) {[m
[32m+[m[32m        return obj && typeof obj === 'object' && obj instanceof Symbol;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof obj === 'symbol') {[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!obj || typeof obj !== 'object' || !symToString) {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        symToString.call(obj);[m
[32m+[m[32m        return true;[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isBigInt(obj) {[m
[32m+[m[32m    if (!obj || typeof obj !== 'object' || !bigIntValueOf) {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        bigIntValueOf.call(obj);[m
[32m+[m[32m        return true;[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar hasOwn = Object.prototype.hasOwnProperty || function (key) { return key in this; };[m
[32m+[m[32mfunction has(obj, key) {[m
[32m+[m[32m    return hasOwn.call(obj, key);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction toStr(obj) {[m
[32m+[m[32m    return objectToString.call(obj);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction nameOf(f) {[m
[32m+[m[32m    if (f.name) { return f.name; }[m
[32m+[m[32m    var m = $match.call(functionToString.call(f), /^function\s*([\w$]+)/);[m
[32m+[m[32m    if (m) { return m[1]; }[m
[32m+[m[32m    return null;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction indexOf(xs, x) {[m
[32m+[m[32m    if (xs.indexOf) { return xs.indexOf(x); }[m
[32m+[m[32m    for (var i = 0, l = xs.length; i < l; i++) {[m
[32m+[m[32m        if (xs[i] === x) { return i; }[m
[32m+[m[32m    }[m
[32m+[m[32m    return -1;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isMap(x) {[m
[32m+[m[32m    if (!mapSize || !x || typeof x !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        mapSize.call(x);[m
[32m+[m[32m        try {[m
[32m+[m[32m            setSize.call(x);[m
[32m+[m[32m        } catch (s) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return x instanceof Map; // core-js workaround, pre-v2.5.0[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isWeakMap(x) {[m
[32m+[m[32m    if (!weakMapHas || !x || typeof x !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        weakMapHas.call(x, weakMapHas);[m
[32m+[m[32m        try {[m
[32m+[m[32m            weakSetHas.call(x, weakSetHas);[m
[32m+[m[32m        } catch (s) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return x instanceof WeakMap; // core-js workaround, pre-v2.5.0[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isWeakRef(x) {[m
[32m+[m[32m    if (!weakRefDeref || !x || typeof x !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        weakRefDeref.call(x);[m
[32m+[m[32m        return true;[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isSet(x) {[m
[32m+[m[32m    if (!setSize || !x || typeof x !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        setSize.call(x);[m
[32m+[m[32m        try {[m
[32m+[m[32m            mapSize.call(x);[m
[32m+[m[32m        } catch (m) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return x instanceof Set; // core-js workaround, pre-v2.5.0[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isWeakSet(x) {[m
[32m+[m[32m    if (!weakSetHas || !x || typeof x !== 'object') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m        weakSetHas.call(x, weakSetHas);[m
[32m+[m[32m        try {[m
[32m+[m[32m            weakMapHas.call(x, weakMapHas);[m
[32m+[m[32m        } catch (s) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return x instanceof WeakSet; // core-js workaround, pre-v2.5.0[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isElement(x) {[m
[32m+[m[32m    if (!x || typeof x !== 'object') { return false; }[m
[32m+[m[32m    if (typeof HTMLElement !== 'undefined' && x instanceof HTMLElement) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m[32m    return typeof x.nodeName === 'string' && typeof x.getAttribute === 'function';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction inspectString(str, opts) {[m
[32m+[m[32m    if (str.length > opts.maxStringLength) {[m
[32m+[m[32m        var remaining = str.length - opts.maxStringLength;[m
[32m+[m[32m        var trailer = '... ' + remaining + ' more character' + (remaining > 1 ? 's' : '');[m
[32m+[m[32m        return inspectString($slice.call(str, 0, opts.maxStringLength), opts) + trailer;[m
[32m+[m[32m    }[m
[32m+[m[32m    // eslint-disable-next-line no-control-regex[m
[32m+[m[32m    var s = $replace.call($replace.call(str, /(['\\])/g, '\\$1'), /[\x00-\x1f]/g, lowbyte);[m
[32m+[m[32m    return wrapQuotes(s, 'single', opts);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction lowbyte(c) {[m
[32m+[m[32m    var n = c.charCodeAt(0);[m
[32m+[m[32m    var x = {[m
[32m+[m[32m        8: 'b',[m
[32m+[m[32m        9: 't',[m
[32m+[m[32m        10: 'n',[m
[32m+[m[32m        12: 'f',[m
[32m+[m[32m        13: 'r'[m
[32m+[m[32m    }[n];[m
[32m+[m[32m    if (x) { return '\\' + x; }[m
[32m+[m[32m    return '\\x' + (n < 0x10 ? '0' : '') + $toUpperCase.call(n.toString(16));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction markBoxed(str) {[m
[32m+[m[32m    return 'Object(' + str + ')';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction weakCollectionOf(type) {[m
[32m+[m[32m    return type + ' { ? }';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction collectionOf(type, size, entries, indent) {[m
[32m+[m[32m    var joinedEntries = indent ? indentedJoin(entries, indent) : $join.call(entries, ', ');[m
[32m+[m[32m    return type + ' (' + size + ') {' + joinedEntries + '}';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction singleLineValues(xs) {[m
[32m+[m[32m    for (var i = 0; i < xs.length; i++) {[m
[32m+[m[32m        if (indexOf(xs[i], '\n') >= 0) {[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getIndent(opts, depth) {[m
[32m+[m[32m    var baseIndent;[m
[32m+[m[32m    if (opts.indent === '\t') {[m
[32m+[m[32m        baseIndent = '\t';[m
[32m+[m[32m    } else if (typeof opts.indent === 'number' && opts.indent > 0) {[m
[32m+[m[32m        baseIndent = $join.call(Array(opts.indent + 1), ' ');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        return null;[m
[32m+[m[32m    }[m
[32m+[m[32m    return {[m
[32m+[m[32m        base: baseIndent,[m
[32m+[m[32m        prev: $join.call(Array(depth + 1), baseIndent)[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction indentedJoin(xs, indent) {[m
[32m+[m[32m    if (xs.length === 0) { return ''; }[m
[32m+[m[32m    var lineJoiner = '\n' + indent.prev + indent.base;[m
[32m+[m[32m    return lineJoiner + $join.call(xs, ',' + lineJoiner) + '\n' + indent.prev;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction arrObjKeys(obj, inspect) {[m
[32m+[m[32m    var isArr = isArray(obj);[m
[32m+[m[32m    var xs = [];[m
[32m+[m[32m    if (isArr) {[m
[32m+[m[32m        xs.length = obj.length;[m
[32m+[m[32m        for (var i = 0; i < obj.length; i++) {[m
[32m+[m[32m            xs[i] = has(obj, i) ? inspect(obj[i], obj) : '';[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    var syms = typeof gOPS === 'function' ? gOPS(obj) : [];[m
[32m+[m[32m    var symMap;[m
[32m+[m[32m    if (hasShammedSymbols) {[m
[32m+[m[32m        symMap = {};[m
[32m+[m[32m        for (var k = 0; k < syms.length; k++) {[m
[32m+[m[32m            symMap['$' + syms[k]] = syms[k];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var key in obj) { // eslint-disable-line no-restricted-syntax[m
[32m+[m[32m        if (!has(obj, key)) { continue; } // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        if (isArr && String(Number(key)) === key && key < obj.length) { continue; } // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        if (hasShammedSymbols && symMap['$' + key] instanceof Symbol) {[m
[32m+[m[32m            // this is to prevent shammed Symbols, which are stored as strings, from being included in the string key section[m
[32m+[m[32m            continue; // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        } else if ($test.call(/[^\w$]/, key)) {[m
[32m+[m[32m            xs.push(inspect(key, obj) + ': ' + inspect(obj[key], obj));[m
[32m+[m[32m        } else {[m
[32m+[m[32m            xs.push(key + ': ' + inspect(obj[key], obj));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof gOPS === 'function') {[m
[32m+[m[32m        for (var j = 0; j < syms.length; j++) {[m
[32m+[m[32m            if (isEnumerable.call(obj, syms[j])) {[m
[32m+[m[32m                xs.push('[' + inspect(syms[j]) + ']: ' + inspect(obj[syms[j]], obj));[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return xs;[m
[32m+[m[32m}[m
