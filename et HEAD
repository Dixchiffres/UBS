[36m@@ -0,0 +1,42 @@[m
[32m+[m[32mBrowser-friendly inheritance fully compatible with standard node.js[m
[32m+[m[32m[inherits](http://nodejs.org/api/util.html#util_util_inherits_constructor_superconstructor).[m
[32m+[m
[32m+[m[32mThis package exports standard `inherits` from node.js `util` module in[m
[32m+[m[32mnode environment, but also provides alternative browser-friendly[m
[32m+[m[32mimplementation through [browser[m
[32m+[m[32mfield](https://gist.github.com/shtylman/4339901). Alternative[m
[32m+[m[32mimplementation is a literal copy of standard one located in standalone[m
[32m+[m[32mmodule to avoid requiring of `util`. It also has a shim for old[m
[32m+[m[32mbrowsers with no `Object.create` support.[m
[32m+[m
[32m+[m[32mWhile keeping you sure you are using standard `inherits`[m
[32m+[m[32mimplementation in node.js environment, it allows bundlers such as[m
[32m+[m[32m[browserify](https://github.com/substack/node-browserify) to not[m
[32m+[m[32minclude full `util` package to your client code if all you need is[m
[32m+[m[32mjust `inherits` function. It worth, because browser shim for `util`[m
[32m+[m[32mpackage is large and `inherits` is often the single function you need[m
[32m+[m[32mfrom it.[m
[32m+[m
[32m+[m[32mIt's recommended to use this package instead of[m
[32m+[m[32m`require('util').inherits` for any code that has chances to be used[m
[32m+[m[32mnot only in node.js but in browser too.[m
[32m+[m
[32m+[m[32m## usage[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar inherits = require('inherits');[m
[32m+[m[32m// then use exactly as the standard one[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## note on version ~1.0[m
[32m+[m
[32m+[m[32mVersion ~1.0 had completely different motivation and is not compatible[m
[32m+[m[32mneither with 2.0 nor with standard node.js `inherits`.[m
[32m+[m
[32m+[m[32mIf you are using version ~1.0 and planning to switch to ~2.0, be[m
[32m+[m[32mcareful:[m
[32m+[m
[32m+[m[32m* new version uses `super_` instead of `super` for referencing[m
[32m+[m[32m  superclass[m
[32m+[m[32m* new version overwrites current prototype while old one preserves any[m
[32m+[m[32m  existing fields on it[m
[1mdiff --git a/node_modules/inherits/inherits.js b/node_modules/inherits/inherits.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f71f2d9[m
[1m--- /dev/null[m
[1m+++ b/node_modules/inherits/inherits.js[m
[36m@@ -0,0 +1,9 @@[m
[32m+[m[32mtry {[m
[32m+[m[32m  var util = require('util');[m
[32m+[m[32m  /* istanbul ignore next */[m
[32m+[m[32m  if (typeof util.inherits !== 'function') throw '';[m
[32m+[m[32m  module.exports = util.inherits;[m
[32m+[m[32m} catch (e) {[m
[32m+[m[32m  /* istanbul ignore next */[m
[32m+[m[32m  module.exports = require('./inherits_browser.js');[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/inherits/inherits_browser.js b/node_modules/inherits/inherits_browser.js[m
[1mnew file mode 100644[m
[1mindex 0000000..86bbb3d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/inherits/inherits_browser.js[m
[36m@@ -0,0 +1,27 @@[m
[32m+[m[32mif (typeof Object.create === 'function') {[m
[32m+[m[32m  // implementation from standard node.js 'util' module[m
[32m+[m[32m  module.exports = function inherits(ctor, superCtor) {[m
[32m+[m[32m    if (superCtor) {[m
[32m+[m[32m      ctor.super_ = superCtor[m
[32m+[m[32m      ctor.prototype = Object.create(superCtor.prototype, {[m
[32m+[m[32m        constructor: {[m
[32m+[m[32m          value: ctor,[m
[32m+[m[32m          enumerable: false,[m
[32m+[m[32m          writable: true,[m
[32m+[m[32m          configurable: true[m
[32m+[m[32m        }[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m} else {[m
[32m+[m[32m  // old school shim for old browsers[m
[32m+[m[32m  module.exports = function inherits(ctor, superCtor) {[m
[32m+[m[32m    if (superCtor) {[m
[32m+[m[32m      ctor.super_ = superCtor[m
[32m+[m[32m      var TempCtor = function () {}[m
[32m+[m[32m      TempCtor.prototype = superCtor.prototype[m
[32m+[m[32m      ctor.prototype = new TempCtor()[m
[32m+[m[32m      ctor.prototype.constructor = ctor[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
