[1m+++ b/node_modules/path-to-regexp/Readme.md[m
[36m@@ -0,0 +1,35 @@[m
[32m+[m[32m# Path-to-RegExp[m
[32m+[m
[32m+[m[32mTurn an Express-style path string such as `/user/:name` into a regular expression.[m
[32m+[m
[32m+[m[32m**Note:** This is a legacy branch. You should upgrade to `1.x`.[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar pathToRegexp = require('path-to-regexp');[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### pathToRegexp(path, keys, options)[m
[32m+[m
[32m+[m[32m - **path** A string in the express format, an array of such strings, or a regular expression[m
[32m+[m[32m - **keys** An array to be populated with the keys present in the url.  Once the function completes, this will be an array of strings.[m
[32m+[m[32m - **options**[m
[32m+[m[32m   - **options.sensitive** Defaults to false, set this to true to make routes case sensitive[m
[32m+[m[32m   - **options.strict** Defaults to false, set this to true to make the trailing slash matter.[m
[32m+[m[32m   - **options.end** Defaults to true, set this to false to only match the prefix of the URL.[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar keys = [];[m
[32m+[m[32mvar exp = pathToRegexp('/foo/:bar', keys);[m
[32m+[m[32m//keys = ['bar'][m
[32m+[m[32m//exp = /^\/foo\/(?:([^\/]+?))\/?$/i[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Live Demo[m
[32m+[m
[32m+[m[32mYou can see a live demo of this library in use at [express-route-tester](http://forbeslindesay.github.com/express-route-tester/).[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m  MIT[m
[1mdiff --git a/node_modules/path-to-regexp/index.js b/node_modules/path-to-regexp/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..500d1da[m
[1m--- /dev/null[m
[1m+++ b/node_modules/path-to-regexp/index.js[m
[36m@@ -0,0 +1,129 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose `pathtoRegexp`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = pathtoRegexp;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Match matching groups in a regular expression.[m
[32m+[m[32m */[m
[32m+[m[32mvar MATCHING_GROUP_REGEXP = /\((?!\?)/g;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Normalize the given path string,[m
[32m+[m[32m * returning a regular expression.[m
[32m+[m[32m *[m
[32m+[m[32m * An empty array should be passed,[m
[32m+[m[32m * which will contain the placeholder[m
[32m+[m[32m * key names. For example "/user/:id" will[m
[32m+[m[32m * then contain ["id"].[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {String|RegExp|Array} path[m
[32m+[m[32m * @param  {Array} keys[m
[32m+[m[32m * @param  {Object} options[m
[32m+[m[32m * @return {RegExp}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction pathtoRegexp(path, keys, options) {[m
[32m+[m[32m  options = options || {};[m
[32m+[m[32m  keys = keys || [];[m
[32m+[m[32m  var strict = options.strict;[m
[32m+[m[32m  var end = options.end !== false;[m
[32m+[m[32m  var flags = options.sensitive ? '' : 'i';[m
[32m+[m[32m  var extraOffset = 0;[m
[32m+[m[32m  var keysOffset = keys.length;[m
[32m+[m[32m  var i = 0;[m
[32m+[m[32m  var name = 0;[m
[32m+[m[32m  var m;[m
[32m+[m
[32m+[m[32m  if (path instanceof RegExp) {[m
[32m+[m[32m    while (m = MATCHING_GROUP_REGEXP.exec(path.source)) {[m
[32m+[m[32m      keys.push({[m
[32m+[m[32m        name: name++,[m
[32m+[m[32m        optional: false,[m
[32m+[m[32m        offset: m.index[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return path;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (Array.isArray(path)) {[m
[32m+[m[32m    // Map array parts into regexps and return their source. We also pass[m
[32m+[m[32m    // the same keys and options instance into every generation to get[m
[32m+[m[32m    // consistent matching groups before we join the sources together.[m
[32m+[m[32m    path = path.map(function (value) {[m
[32m+[m[32m      return pathtoRegexp(value, keys, options).source;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return new RegExp('(?:' + path.join('|') + ')', flags);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  path = ('^' + path + (strict ? '' : path[path.length - 1] === '/' ? '?' : '/?'))[m
[32m+[m[32m    .replace(/\/\(/g, '/(?:')[m
[32m+[m[32m    .replace(/([\/\.])/g, '\\$1')[m
[32m+[m[32m    .replace(/(\\\/)?(\\\.)?:(\w+)(\(.*?\))?(\*)?(\?)?/g, function (match, slash, format, key, capture, star, optional, offset) {[m
[32m+[m[32m      slash = slash || '';[m
[32m+[m[32m      format = format || '';[m
[32m+[m[32m      capture = capture || '([^\\/' + format + ']+?)';[m
[32m+[m[32m      optional = optional || '';[m
[32m+[m
[32m+[m[32m      keys.push({[m
[32m+[m[32m        name: key,[m
[32m+[m[32m        optional: !!optional,[m
[32m+[m[32m        offset: offset + extraOffset[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      var result = ''[m
[32m+[m[32m        + (optional ? '' : slash)[m
[32m+[m[32m        + '(?:'[m
[32m+[m[32m        + format + (optional ? slash : '') + capture[m
[32m+[m[32m        + (star ? '((?:[\\/' + format + '].+?)?)' : '')[m
[32m+[m[32m        + ')'[m
[32m+[m[32m        + optional;[m
[32m+[m
[32m+[m[32m      extraOffset += result.length - match.length;[m
[32m+[m
[32m+[m[32m      return result;[m
[32m+[m[32m    })[m
[32m+[m[32m    .replace(/\*/g, function (star, index) {[m
[32m+[m[32m      var len = keys.length[m
[32m+[m
[32m+[m[32m      while (len-- > keysOffset && keys[len].offset > index) {[m
[32m+[m[32m        keys[len].offset += 3; // Replacement length minus asterisk length.[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return '(.*)';[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m  // This is a workaround for handling unnamed matching groups.[m
[32m+[m[32m  while (m = MATCHING_GROUP_REGEXP.exec(path)) {[m
[32m+[m[32m    var escapeCount = 0;[m
[32m+[m[32m    var index = m.index;[m
[32m+[m
[32m+[m[32m    while (path.charAt(--index) === '\\') {[m
[32m+[m[32m      escapeCount++;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // It's possible to escape the bracket.[m
[32m+[m[32m    if (escapeCount % 2 === 1) {[m
[32m+[m[32m      continue;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (keysOffset + i === keys.length || keys[keysOffset + i].offset > m.index) {[m
[32m+[m[32m      keys.splice(keysOffset + i, 0, {[m
[32m+[m[32m        name: name++, // Unnamed matching groups must be consistently linear.[m
[32m+[m[32m        optional: false,[m
[32m+[m[32m        offset: m.index[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    i++;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // If the path is non-ending, match until the end or a slash.[m
[32m+[m[32m  path += (end ? '$' : (path[path.length - 1] === '/' ? '' : '(?=\\/|$)'));[m
[32m+[m
[32m+[m[32m  return new RegExp(path, flags);[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/path-to-regexp/package.json b/node_modules/path-to-regexp/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..d4e51b5[m
[1m--- /dev/null[m
[1m+++ b/node_modules/path-to-regexp/package.json[m
[36m@@ -0,0 +1,30 @@[m
