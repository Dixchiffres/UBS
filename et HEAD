[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32m// Note: UTF16-LE (or UCS2) codec is Node.js native. See encodings/internal.js[m
[32m+[m
[32m+[m[32m// == UTF16-BE codec. ==========================================================[m
[32m+[m
[32m+[m[32mexports.utf16be = Utf16BECodec;[m
[32m+[m[32mfunction Utf16BECodec() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16BECodec.prototype.encoder = Utf16BEEncoder;[m
[32m+[m[32mUtf16BECodec.prototype.decoder = Utf16BEDecoder;[m
[32m+[m[32mUtf16BECodec.prototype.bomAware = true;[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Encoding[m
[32m+[m
[32m+[m[32mfunction Utf16BEEncoder() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16BEEncoder.prototype.write = function(str) {[m
[32m+[m[32m    var buf = Buffer.from(str, 'ucs2');[m
[32m+[m[32m    for (var i = 0; i < buf.length; i += 2) {[m
[32m+[m[32m        var tmp = buf[i]; buf[i] = buf[i+1]; buf[i+1] = tmp;[m
[32m+[m[32m    }[m
[32m+[m[32m    return buf;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16BEEncoder.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Decoding[m
[32m+[m
[32m+[m[32mfunction Utf16BEDecoder() {[m
[32m+[m[32m    this.overflowByte = -1;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16BEDecoder.prototype.write = function(buf) {[m
[32m+[m[32m    if (buf.length == 0)[m
[32m+[m[32m        return '';[m
[32m+[m
[32m+[m[32m    var buf2 = Buffer.alloc(buf.length + 1),[m
[32m+[m[32m        i = 0, j = 0;[m
[32m+[m
[32m+[m[32m    if (this.overflowByte !== -1) {[m
[32m+[m[32m        buf2[0] = buf[0];[m
[32m+[m[32m        buf2[1] = this.overflowByte;[m
[32m+[m[32m        i = 1; j = 2;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (; i < buf.length-1; i += 2, j+= 2) {[m
[32m+[m[32m        buf2[j] = buf[i+1];[m
[32m+[m[32m        buf2[j+1] = buf[i];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.overflowByte = (i == buf.length-1) ? buf[buf.length-1] : -1;[m
[32m+[m
[32m+[m[32m    return buf2.slice(0, j).toString('ucs2');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16BEDecoder.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// == UTF-16 codec =============================================================[m
[32m+[m[32m// Decoder chooses automatically from UTF-16LE and UTF-16BE using BOM and space-based heuristic.[m
[32m+[m[32m// Defaults to UTF-16LE, as it's prevalent and default in Node.[m
[32m+[m[32m// http://en.wikipedia.org/wiki/UTF-16 and http://encoding.spec.whatwg.org/#utf-16le[m
[32m+[m[32m// Decoder default can be changed: iconv.decode(buf, 'utf16', {defaultEncoding: 'utf-16be'});[m
[32m+[m
[32m+[m[32m// Encoder uses UTF-16LE and prepends BOM (which can be overridden with addBOM: false).[m
[32m+[m
[32m+[m[32mexports.utf16 = Utf16Codec;[m
[32m+[m[32mfunction Utf16Codec(codecOptions, iconv) {[m
[32m+[m[32m    this.iconv = iconv;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16Codec.prototype.encoder = Utf16Encoder;[m
[32m+[m[32mUtf16Codec.prototype.decoder = Utf16Decoder;[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Encoding (pass-through)[m
[32m+[m
[32m+[m[32mfunction Utf16Encoder(options, codec) {[m
[32m+[m[32m    options = options || {};[m
[32m+[m[32m    if (options.addBOM === undefined)[m
[32m+[m[32m        options.addBOM = true;[m
[32m+[m[32m    this.encoder = codec.iconv.getEncoder('utf-16le', options);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16Encoder.prototype.write = function(str) {[m
[32m+[m[32m    return this.encoder.write(str);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16Encoder.prototype.end = function() {[m
[32m+[m[32m    return this.encoder.end();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Decoding[m
[32m+[m
[32m+[m[32mfunction Utf16Decoder(options, codec) {[m
[32m+[m[32m    this.decoder = null;[m
[32m+[m[32m    this.initialBytes = [];[m
[32m+[m[32m    this.initialBytesLen = 0;[m
[32m+[m
[32m+[m[32m    this.options = options || {};[m
[32m+[m[32m    this.iconv = codec.iconv;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16Decoder.prototype.write = function(buf) {[m
[32m+[m[32m    if (!this.decoder) {[m
[32m+[m[32m        // Codec is not chosen yet. Accumulate initial bytes.[m
[32m+[m[32m        this.initialBytes.push(buf);[m
[32m+[m[32m        this.initialBytesLen += buf.length;[m
[32m+[m[41m        [m
[32m+[m[32m        if (this.initialBytesLen < 16) // We need more bytes to use space heuristic (see below)[m
[32m+[m[32m            return '';[m
[32m+[m
[32m+[m[32m        // We have enough bytes -> detect endianness.[m
[32m+[m[32m        var buf = Buffer.concat(this.initialBytes),[m
[32m+[m[32m            encoding = detectEncoding(buf, this.options.defaultEncoding);[m
[32m+[m[32m        this.decoder = this.iconv.getDecoder(encoding, this.options);[m
[32m+[m[32m        this.initialBytes.length = this.initialBytesLen = 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return this.decoder.write(buf);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf16Decoder.prototype.end = function() {[m
[32m+[m[32m    if (!this.decoder) {[m
[32m+[m[32m        var buf = Buffer.concat(this.initialBytes),[m
[32m+[m[32m            encoding = detectEncoding(buf, this.options.defaultEncoding);[m
[32m+[m[32m        this.decoder = this.iconv.getDecoder(encoding, this.options);[m
[32m+[m
[32m+[m[32m        var res = this.decoder.write(buf),[m
[32m+[m[32m            trail = this.decoder.end();[m
[32m+[m
[32m+[m[32m        return trail ? (res + trail) : res;[m
[32m+[m[32m    }[m
[32m+[m[32m    return this.decoder.end();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction detectEncoding(buf, defaultEncoding) {[m
[32m+[m[32m    var enc = defaultEncoding || 'utf-16le';[m
[32m+[m
[32m+[m[32m    if (buf.length >= 2) {[m
[32m+[m[32m        // Check BOM.[m
[32m+[m[32m        if (buf[0] == 0xFE && buf[1] == 0xFF) // UTF-16BE BOM[m
[32m+[m[32m            enc = 'utf-16be';[m
[32m+[m[32m        else if (buf[0] == 0xFF && buf[1] == 0xFE) // UTF-16LE BOM[m
[32m+[m[32m            enc = 'utf-16le';[m
[32m+[m[32m        else {[m
[32m+[m[32m            // No BOM found. Try to deduce encoding from initial content.[m
[32m+[m[32m            // Most of the time, the content has ASCII chars (U+00**), but the opposite (U+**00) is uncommon.[m
[32m+[m[32m            // So, we count ASCII as if it was LE or BE, and decide from that.[m
[32m+[m[32m            var asciiCharsLE = 0, asciiCharsBE = 0, // Counts of chars in both positions[m
[32m+[m[32m                _len = Math.min(buf.length - (buf.length % 2), 64); // Len is always even.[m
[32m+[m
[32m+[m[32m            for (var i = 0; i < _len; i += 2) {[m
[32m+[m[32m                if (buf[i] === 0 && buf[i+1] !== 0) asciiCharsBE++;[m
[32m+[m[32m                if (buf[i] !== 0 && buf[i+1] === 0) asciiCharsLE++;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (asciiCharsBE > asciiCharsLE)[m
[32m+[m[32m                enc = 'utf-16be';[m
[32m+[m[32m            else if (asciiCharsBE < asciiCharsLE)[m
[32m+[m[32m                enc = 'utf-16le';[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return enc;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[1mdiff --git a/node_modules/iconv-lite/encodings/utf7.js b/node_modules/iconv-lite/encodings/utf7.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b7631c2[m
[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/encodings/utf7.js[m
[36m@@ -0,0 +1,290 @@[m
[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32m// UTF-7 codec, according to https://tools.ietf.org/html/rfc2152[m
[32m+[m[32m// See also below a UTF-7-IMAP codec, according to http://tools.ietf.org/html/rfc3501#section-5.1.3[m
[32m+[m
[32m+[m[32mexports.utf7 = Utf7Codec;[m
[32m+[m[32mexports.unicode11utf7 = 'utf7'; // Alias UNICODE-1-1-UTF-7[m
[32m+[m[32mfunction Utf7Codec(codecOptions, iconv) {[m
[32m+[m[32m    this.iconv = iconv;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mUtf7Codec.prototype.encoder = Utf7Encoder;[m
[32m+[m[32mUtf7Codec.prototype.decoder = Utf7Decoder;[m
[32m+[m[32mUtf7Codec.prototype.bomAware = true;[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Encoding[m
[32m+[m
[32m+[m[32mvar nonDirectChars = /[^A-Za-z0-9'\(\),-\.\/:\? \n\r\t]+/g;[m
[32m+[m
[32m+[m[32mfunction Utf7Encoder(options, codec) {[m
[32m+[m[32m    this.iconv = codec.iconv;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7Encoder.prototype.write = function(str) {[m
[32m+[m[32m    // Naive implementation.[m
[32m+[m[32m    // Non-direct chars are encoded as "+<base64>-"; single "+" char is encoded as "+-".[m
[32m+[m[32m    return Buffer.from(str.replace(nonDirectChars, function(chunk) {[m
[32m+[m[32m        return "+" + (chunk === '+' ? '' :[m[41m [m
[32m+[m[32m            this.iconv.encode(chunk, 'utf16-be').toString('base64').replace(/=+$/, ''))[m[41m [m
[32m+[m[32m            + "-";[m
[32m+[m[32m    }.bind(this)));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7Encoder.prototype.end = function() {[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Decoding[m
[32m+[m
[32m+[m[32mfunction Utf7Decoder(options, codec) {[m
[32m+[m[32m    this.iconv = codec.iconv;[m
[32m+[m[32m    this.inBase64 = false;[m
[32m+[m[32m    this.base64Accum = '';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar base64Regex = /[A-Za-z0-9\/+]/;[m
[32m+[m[32mvar base64Chars = [];[m
[32m+[m[32mfor (var i = 0; i < 256; i++)[m
[32m+[m[32m    base64Chars[i] = base64Regex.test(String.fromCharCode(i));[m
[32m+[m
[32m+[m[32mvar plusChar = '+'.charCodeAt(0),[m[41m [m
[32m+[m[32m    minusChar = '-'.charCodeAt(0),[m
[32m+[m[32m    andChar = '&'.charCodeAt(0);[m
[32m+[m
[32m+[m[32mUtf7Decoder.prototype.write = function(buf) {[m
[32m+[m[32m    var res = "", lastI = 0,[m
[32m+[m[32m        inBase64 = this.inBase64,[m
[32m+[m[32m        base64Accum = this.base64Accum;[m
[32m+[m
[32m+[m[32m    // The decoder is more involved as we must handle chunks in stream.[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < buf.length; i++) {[m
[32m+[m[32m        if (!inBase64) { // We're in direct mode.[m
[32m+[m[32m            // Write direct chars until '+'[m
[32m+[m[32m            if (buf[i] == plusChar) {[m
[32m+[m[32m                res += this.iconv.decode(buf.slice(lastI, i), "ascii"); // Write direct chars.[m
[32m+[m[32m                lastI = i+1;[m
[32m+[m[32m                inBase64 = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else { // We decode base64.[m
[32m+[m[32m            if (!base64Chars[buf[i]]) { // Base64 ended.[m
[32m+[m[32m                if (i == lastI && buf[i] == minusChar) {// "+-" -> "+"[m
[32m+[m[32m                    res += "+";[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    var b64str = base64Accum + buf.slice(lastI, i).toString();[m
[32m+[m[32m                    res += this.iconv.decode(Buffer.from(b64str, 'base64'), "utf16-be");[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (buf[i] != minusChar) // Minus is absorbed after base64.[m
[32m+[m[32m                    i--;[m
[32m+[m
[32m+[m[32m                lastI = i+1;[m
[32m+[m[32m                inBase64 = false;[m
[32m+[m[32m                base64Accum = '';[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!inBase64) {[m
[32m+[m[32m        res += this.iconv.decode(buf.slice(lastI), "ascii"); // Write direct chars.[m
[32m+[m[32m    } else {[m
[32m+[m[32m        var b64str = base64Accum + buf.slice(lastI).toString();[m
[32m+[m
[32m+[m[32m        var canBeDecoded = b64str.length - (b64str.length % 8); // Minimal chunk: 2 quads -> 2x3 bytes -> 3 chars.[m
[32m+[m[32m        base64Accum = b64str.slice(canBeDecoded); // The rest will be decoded in future.[m
[32m+[m[32m        b64str = b64str.slice(0, canBeDecoded);[m
[32m+[m
[32m+[m[32m        res += this.iconv.decode(Buffer.from(b64str, 'base64'), "utf16-be");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.inBase64 = inBase64;[m
[32m+[m[32m    this.base64Accum = base64Accum;[m
[32m+[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7Decoder.prototype.end = function() {[m
[32m+[m[32m    var res = "";[m
[32m+[m[32m    if (this.inBase64 && this.base64Accum.length > 0)[m
[32m+[m[32m        res = this.iconv.decode(Buffer.from(this.base64Accum, 'base64'), "utf16-be");[m
[32m+[m
[32m+[m[32m    this.inBase64 = false;[m
[32m+[m[32m    this.base64Accum = '';[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// UTF-7-IMAP codec.[m
[32m+[m[32m// RFC3501 Sec. 5.1.3 Modified UTF-7 (http://tools.ietf.org/html/rfc3501#section-5.1.3)[m
[32m+[m[32m// Differences:[m
[32m+[m[32m//  * Base64 part is started by "&" instead of "+"[m
[32m+[m[32m//  * Direct characters are 0x20-0x7E, except "&" (0x26)[m
[32m+[m[32m//  * In Base64, "," is used instead of "/"[m
[32m+[m[32m//  * Base64 must not be used to represent direct characters.[m
[32m+[m[32m//  * No implicit shift back from Base64 (should always end with '-')[m
[32m+[m[32m//  * String must end in non-shifted position.[m
[32m+[m[32m//  * "-&" while in base64 is not allowed.[m
[32m+[m
[32m+[m
[32m+[m[32mexports.utf7imap = Utf7IMAPCodec;[m
[32m+[m[32mfunction Utf7IMAPCodec(codecOptions, iconv) {[m
[32m+[m[32m    this.iconv = iconv;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mUtf7IMAPCodec.prototype.encoder = Utf7IMAPEncoder;[m
[32m+[m[32mUtf7IMAPCodec.prototype.decoder = Utf7IMAPDecoder;[m
[32m+[m[32mUtf7IMAPCodec.prototype.bomAware = true;[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Encoding[m
[32m+[m
[32m+[m[32mfunction Utf7IMAPEncoder(options, codec) {[m
[32m+[m[32m    this.iconv = codec.iconv;[m
[32m+[m[32m    this.inBase64 = false;[m
[32m+[m[32m    this.base64Accum = Buffer.alloc(6);[m
[32m+[m[32m    this.base64AccumIdx = 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7IMAPEncoder.prototype.write = function(str) {[m
[32m+[m[32m    var inBase64 = this.inBase64,[m
[32m+[m[32m        base64Accum = this.base64Accum,[m
[32m+[m[32m        base64AccumIdx = this.base64AccumIdx,[m
[32m+[m[32m        buf = Buffer.alloc(str.length*5 + 10), bufIdx = 0;[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < str.length; i++) {[m
[32m+[m[32m        var uChar = str.charCodeAt(i);[m
[32m+[m[32m        if (0x20 <= uChar && uChar <= 0x7E) { // Direct character or '&'.[m
[32m+[m[32m            if (inBase64) {[m
[32m+[m[32m                if (base64AccumIdx > 0) {[m
[32m+[m[32m                    bufIdx += buf.write(base64Accum.slice(0, base64AccumIdx).toString('base64').replace(/\//g, ',').replace(/=+$/, ''), bufIdx);[m
[32m+[m[32m                    base64AccumIdx = 0;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                buf[bufIdx++] = minusChar; // Write '-', then go to direct mode.[m
[32m+[m[32m                inBase64 = false;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!inBase64) {[m
[32m+[m[32m                buf[bufIdx++] = uChar; // Write direct character[m
[32m+[m
[32m+[m[32m                if (uChar === andChar)  // Ampersand -> '&-'[m
[32m+[m[32m                    buf[bufIdx++] = minusChar;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        } else { // Non-direct character[m
[32m+[m[32m            if (!inBase64) {[m
[32m+[m[32m                buf[bufIdx++] = andChar; // Write '&', then go to base64 mode.[m
[32m+[m[32m                inBase64 = true;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (inBase64) {[m
[32m+[m[32m                base64Accum[base64AccumIdx++] = uChar >> 8;[m
[32m+[m[32m                base64Accum[base64AccumIdx++] = uChar & 0xFF;[m
[32m+[m
[32m+[m[32m                if (base64AccumIdx == base64Accum.length) {[m
[32m+[m[32m                    bufIdx += buf.write(base64Accum.toString('base64').replace(/\//g, ','), bufIdx);[m
[32m+[m[32m                    base64AccumIdx = 0;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.inBase64 = inBase64;[m
[32m+[m[32m    this.base64AccumIdx = base64AccumIdx;[m
[32m+[m
[32m+[m[32m    return buf.slice(0, bufIdx);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7IMAPEncoder.prototype.end = function() {[m
[32m+[m[32m    var buf = Buffer.alloc(10), bufIdx = 0;[m
[32m+[m[32m    if (this.inBase64) {[m
[32m+[m[32m        if (this.base64AccumIdx > 0) {[m
[32m+[m[32m            bufIdx += buf.write(this.base64Accum.slice(0, this.base64AccumIdx).toString('base64').replace(/\//g, ',').replace(/=+$/, ''), bufIdx);[m
[32m+[m[32m            this.base64AccumIdx = 0;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        buf[bufIdx++] = minusChar; // Write '-', then go to direct mode.[m
[32m+[m[32m        this.inBase64 = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return buf.slice(0, bufIdx);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// -- Decoding[m
[32m+[m
[32m+[m[32mfunction Utf7IMAPDecoder(options, codec) {[m
[32m+[m[32m    this.iconv = codec.iconv;[m
[32m+[m[32m    this.inBase64 = false;[m
[32m+[m[32m    this.base64Accum = '';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar base64IMAPChars = base64Chars.slice();[m
[32m+[m[32mbase64IMAPChars[','.charCodeAt(0)] = true;[m
[32m+[m
[32m+[m[32mUtf7IMAPDecoder.prototype.write = function(buf) {[m
[32m+[m[32m    var res = "", lastI = 0,[m
[32m+[m[32m        inBase64 = this.inBase64,[m
[32m+[m[32m        base64Accum = this.base64Accum;[m
[32m+[m
[32m+[m[32m    // The decoder is more involved as we must handle chunks in stream.[m
[32m+[m[32m    // It is forgiving, closer to standard UTF-7 (for example, '-' is optional at the end).[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < buf.length; i++) {[m
[32m+[m[32m        if (!inBase64) { // We're in direct mode.[m
[32m+[m[32m            // Write direct chars until '&'[m
[32m+[m[32m            if (buf[i] == andChar) {[m
[32m+[m[32m                res += this.iconv.decode(buf.slice(lastI, i), "ascii"); // Write direct chars.[m
[32m+[m[32m                lastI = i+1;[m
[32m+[m[32m                inBase64 = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else { // We decode base64.[m
[32m+[m[32m            if (!base64IMAPChars[buf[i]]) { // Base64 ended.[m
[32m+[m[32m                if (i == lastI && buf[i] == minusChar) { // "&-" -> "&"[m
[32m+[m[32m                    res += "&";[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    var b64str = base64Accum + buf.slice(lastI, i).toString().replace(/,/g, '/');[m
[32m+[m[32m                    res += this.iconv.decode(Buffer.from(b64str, 'base64'), "utf16-be");[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (buf[i] != minusChar) // Minus may be absorbed after base64.[m
[32m+[m[32m                    i--;[m
[32m+[m
[32m+[m[32m                lastI = i+1;[m
[32m+[m[32m                inBase64 = false;[m
[32m+[m[32m                base64Accum = '';[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!inBase64) {[m
[32m+[m[32m        res += this.iconv.decode(buf.slice(lastI), "ascii"); // Write direct chars.[m
[32m+[m[32m    } else {[m
[32m+[m[32m        var b64str = base64Accum + buf.slice(lastI).toString().replace(/,/g, '/');[m
[32m+[m
[32m+[m[32m        var canBeDecoded = b64str.length - (b64str.length % 8); // Minimal chunk: 2 quads -> 2x3 bytes -> 3 chars.[m
[32m+[m[32m        base64Accum = b64str.slice(canBeDecoded); // The rest will be decoded in future.[m
[32m+[m[32m        b64str = b64str.slice(0, canBeDecoded);[m
[32m+[m
[32m+[m[32m        res += this.iconv.decode(Buffer.from(b64str, 'base64'), "utf16-be");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.inBase64 = inBase64;[m
[32m+[m[32m    this.base64Accum = base64Accum;[m
[32m+[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mUtf7IMAPDecoder.prototype.end = function() {[m
[32m+[m[32m    var res = "";[m
[32m+[m[32m    if (this.inBase64 && this.base64Accum.length > 0)[m
[32m+[m[32m        res = this.iconv.decode(Buffer.from(this.base64Accum, 'base64'), "utf16-be");[m
[32m+[m
[32m+[m[32m    this.inBase64 = false;[m
[32m+[m[32m    this.base64Accum = '';[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[1mdiff --git a/node_modules/iconv-lite/lib/bom-handling.js b/node_modules/iconv-lite/lib/bom-handling.js[m
[1mnew file mode 100644[m
[1mindex 0000000..1050872[m
[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/lib/bom-handling.js[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32mvar BOMChar = '\uFEFF';[m
[32m+[m
[32m+[m[32mexports.PrependBOM = PrependBOMWrapper[m
[32m+[m[32mfunction PrependBOMWrapper(encoder, options) {[m
[32m+[m[32m    this.encoder = encoder;[m
[32m+[m[32m    this.addBOM = true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mPrependBOMWrapper.prototype.write = function(str) {[m
[32m+[m[32m    if (this.addBOM) {[m
[32m+[m[32m        str = BOMChar + str;[m
[32m+[m[32m        this.addBOM = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return this.encoder.write(str);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mPrependBOMWrapper.prototype.end = function() {[m
[32m+[m[32m    return this.encoder.end();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//------------------------------------------------------------------------------[m
[32m+[m
[32m+[m[32mexports.StripBOM = StripBOMWrapper;[m
[32m+[m[32mfunction StripBOMWrapper(decoder, options) {[m
[32m+[m[32m    this.decoder = decoder;[m
[32m+[m[32m    this.pass = false;[m
[32m+[m[32m    this.options = options || {};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mStripBOMWrapper.prototype.write = function(buf) {[m
[32m+[m[32m    var res = this.decoder.write(buf);[m
[32m+[m[32m    if (this.pass || !res)[m
[32m+[m[32m        return res;[m
[32m+[m
[32m+[m[32m    if (res[0] === BOMChar) {[m
[32m+[m[32m        res = res.slice(1);[m
[32m+[m[32m        if (typeof this.options.stripBOM === 'function')[m
[32m+[m[32m            this.options.stripBOM();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.pass = true;[m
[32m+[m[32m    return res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mStripBOMWrapper.prototype.end = function() {[m
[32m+[m[32m    return this.decoder.end();[m
[32m+[m[32m}[m
[32m+[m
