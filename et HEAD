[32m+[m	[32mtest('has native Symbol support', function (t) {[m
[32m+[m		[32mt.equal(typeof Symbol, 'function');[m
[32m+[m		[32mt.equal(typeof Symbol(), 'symbol');[m
[32m+[m		[32mt.end();[m
[32m+[m	[32m});[m
