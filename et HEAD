[32m+[m[32mtest('defineDataProperty', function (t) {[m
[32m+[m	[32mt.test('argument validation', function (st) {[m
[32m+[m		[32mforEach(v.primitives, function (nonObject) {[m
[32m+[m			[32mst['throws']([m
[32m+[m				[32m// @ts-expect-error[m
[32m+[m				[32mfunction () { defineDataProperty(nonObject, 'key', 'value'); },[m
[32m+[m				[32mTypeError,[m
[32m+[m				[32m'throws on non-object input: ' + inspect(nonObject)[m
[32m+[m			[32m);[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mforEach(v.nonPropertyKeys, function (nonPropertyKey) {[m
[32m+[m			[32mst['throws']([m
[32m+[m				[32m// @ts-expect-error[m
[32m+[m				[32mfunction () { defineDataProperty({}, nonPropertyKey, 'value'); },[m
[32m+[m				[32mTypeError,[m
[32m+[m				[32m'throws on non-PropertyKey input: ' + inspect(nonPropertyKey)[m
[32m+[m			[32m);[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mforEach(v.nonBooleans, function (nonBoolean) {[m
[32m+[m			[32mif (nonBoolean !== null) {[m
[32m+[m				[32mst['throws']([m
[32m+[m					[32m// @ts-expect-error[m
[32m+[m					[32mfunction () { defineDataProperty({}, 'key', 'value', nonBoolean); },[m
[32m+[m					[32mTypeError,[m
[32m+[m					[32m'throws on non-boolean nonEnumerable: ' + inspect(nonBoolean)[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m				[32mst['throws']([m
[32m+[m					[32m// @ts-expect-error[m
[32m+[m					[32mfunction () { defineDataProperty({}, 'key', 'value', false, nonBoolean); },[m
[32m+[m					[32mTypeError,[m
[32m+[m					[32m'throws on non-boolean nonWritable: ' + inspect(nonBoolean)[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m				[32mst['throws']([m
[32m+[m					[32m// @ts-expect-error[m
[32m+[m					[32mfunction () { defineDataProperty({}, 'key', 'value', false, false, nonBoolean); },[m
[32m+[m					[32mTypeError,[m
[32m+[m					[32m'throws on non-boolean nonConfigurable: ' + inspect(nonBoolean)[m
[32m+[m				[32m);[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.test('normal data property', function (st) {[m
[32m+[m		[32m/** @type {Record<PropertyKey, string>} */[m
[32m+[m		[32mvar obj = { existing: 'existing property' };[m
[32m+[m		[32mst.ok(hasOwn(obj, 'existing'), 'has initial own property');[m
[32m+[m		[32mst.equal(obj.existing, 'existing property', 'has expected initial value');[m
[32m+[m
[32m+[m		[32mvar res = defineDataProperty(obj, 'added', 'added property');[m
[32m+[m		[32mst.equal(res, void undefined, 'returns `undefined`');[m
[32m+[m		[32mst.ok(hasOwn(obj, 'added'), 'has expected own property');[m
[32m+[m		[32mst.equal(obj.added, 'added property', 'has expected value');[m
[32m+[m
[32m+[m		[32mdefineDataProperty(obj, 'existing', 'new value');[m
[32m+[m		[32mst.ok(hasOwn(obj, 'existing'), 'still has expected own property');[m
[32m+[m		[32mst.equal(obj.existing, 'new value', 'has new expected value');[m
[32m+[m
[32m+[m		[32mdefineDataProperty(obj, 'explicit1', 'new value', false);[m
[32m+[m		[32mst.ok(hasOwn(obj, 'explicit1'), 'has expected own property (explicit enumerable)');[m
[32m+[m		[32mst.equal(obj.explicit1, 'new value', 'has new expected value (explicit enumerable)');[m
[32m+[m
[32m+[m		[32mdefineDataProperty(obj, 'explicit2', 'new value', false, false);[m
[32m+[m		[32mst.ok(hasOwn(obj, 'explicit2'), 'has expected own property (explicit writable)');[m
[32m+[m		[32mst.equal(obj.explicit2, 'new value', 'has new expected value (explicit writable)');[m
[32m+[m
[32m+[m		[32mdefineDataProperty(obj, 'explicit3', 'new value', false, false, false);[m
[32m+[m		[32mst.ok(hasOwn(obj, 'explicit3'), 'has expected own property (explicit configurable)');[m
[32m+[m		[32mst.equal(obj.explicit3, 'new value', 'has new expected value (explicit configurable)');[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
