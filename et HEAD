[1m--- /dev/null[m
[1m+++ b/node_modules/range-parser/index.js[m
[36m@@ -0,0 +1,162 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * range-parser[m
[32m+[m[32m * Copyright(c) 2012-2014 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2015-2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = rangeParser[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse "Range" header `str` relative to the given file `size`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Number} size[m
[32m+[m[32m * @param {String} str[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction rangeParser (size, str, options) {[m
[32m+[m[32m  if (typeof str !== 'string') {[m
[32m+[m[32m    throw new TypeError('argument str must be a string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var index = str.indexOf('=')[m
[32m+[m
[32m+[m[32m  if (index === -1) {[m
[32m+[m[32m    return -2[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // split the range string[m
[32m+[m[32m  var arr = str.slice(index + 1).split(',')[m
[32m+[m[32m  var ranges = [][m
[32m+[m
[32m+[m[32m  // add ranges type[m
[32m+[m[32m  ranges.type = str.slice(0, index)[m
[32m+[m
[32m+[m[32m  // parse all ranges[m
[32m+[m[32m  for (var i = 0; i < arr.length; i++) {[m
[32m+[m[32m    var range = arr[i].split('-')[m
[32m+[m[32m    var start = parseInt(range[0], 10)[m
[32m+[m[32m    var end = parseInt(range[1], 10)[m
[32m+[m
[32m+[m[32m    // -nnn[m
[32m+[m[32m    if (isNaN(start)) {[m
[32m+[m[32m      start = size - end[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    // nnn-[m
[32m+[m[32m    } else if (isNaN(end)) {[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // limit last-byte-pos to current length[m
[32m+[m[32m    if (end > size - 1) {[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // invalid or unsatisifiable[m
[32m+[m[32m    if (isNaN(start) || isNaN(end) || start > end || start < 0) {[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // add range[m
[32m+[m[32m    ranges.push({[m
[32m+[m[32m      start: start,[m
[32m+[m[32m      end: end[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (ranges.length < 1) {[m
[32m+[m[32m    // unsatisifiable[m
[32m+[m[32m    return -1[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return options && options.combine[m
[32m+[m[32m    ? combineRanges(ranges)[m
[32m+[m[32m    : ranges[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Combine overlapping & adjacent ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction combineRanges (ranges) {[m
[32m+[m[32m  var ordered = ranges.map(mapWithIndex).sort(sortByRangeStart)[m
[32m+[m
[32m+[m[32m  for (var j = 0, i = 1; i < ordered.length; i++) {[m
[32m+[m[32m    var range = ordered[i][m
[32m+[m[32m    var current = ordered[j][m
[32m+[m
[32m+[m[32m    if (range.start > current.end + 1) {[m
[32m+[m[32m      // next range[m
[32m+[m[32m      ordered[++j] = range[m
[32m+[m[32m    } else if (range.end > current.end) {[m
[32m+[m[32m      // extend range[m
[32m+[m[32m      current.end = range.end[m
[32m+[m[32m      current.index = Math.min(current.index, range.index)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim ordered array[m
[32m+[m[32m  ordered.length = j + 1[m
[32m+[m
[32m+[m[32m  // generate combined range[m
[32m+[m[32m  var combined = ordered.sort(sortByRangeIndex).map(mapWithoutIndex)[m
[32m+[m
[32m+[m[32m  // copy ranges type[m
[32m+[m[32m  combined.type = ranges.type[m
[32m+[m
[32m+[m[32m  return combined[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map function to add index value to ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction mapWithIndex (range, index) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    start: range.start,[m
[32m+[m[32m    end: range.end,[m
[32m+[m[32m    index: index[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map function to remove index value from ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction mapWithoutIndex (range) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    start: range.start,[m
[32m+[m[32m    end: range.end[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sort function to sort ranges by index.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction sortByRangeIndex (a, b) {[m
[32m+[m[32m  return a.index - b.index[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sort function to sort ranges by start position.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction sortByRangeStart (a, b) {[m
[32m+[m[32m  return a.start - b.start[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/range-parser/package.json b/node_modules/range-parser/package.json[m
[1mnew file mode 100644[m
