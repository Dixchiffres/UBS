[32m+[m[32m# gopd <sup>[![Version Badge][npm-version-svg]][package-url]</sup>[m
[32m+[m
[32m+[m[32m[![github actions][actions-image]][actions-url][m
[32m+[m[32m[![coverage][codecov-image]][codecov-url][m
[32m+[m[32m[![License][license-image]][license-url][m
[32m+[m[32m[![Downloads][downloads-image]][downloads-url][m
[32m+[m
[32m+[m[32m[![npm badge][npm-badge-png]][package-url][m
[32m+[m
[32m+[m[32m`Object.getOwnPropertyDescriptor`, but accounts for IE's broken implementation.[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar gOPD = require('gopd');[m
[32m+[m[32mvar assert = require('assert');[m
[32m+[m
[32m+[m[32mif (gOPD) {[m
[32m+[m	[32massert.equal(typeof gOPD, 'function', 'descriptors supported');[m
[32m+[m	[32m// use gOPD like Object.getOwnPropertyDescriptor here[m
[32m+[m[32m} else {[m
[32m+[m	[32massert.ok(!gOPD, 'descriptors not supported');[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m[package-url]: https://npmjs.org/package/gopd[m
[32m+[m[32m[npm-version-svg]: https://versionbadg.es/ljharb/gopd.svg[m
[32m+[m[32m[deps-svg]: https://david-dm.org/ljharb/gopd.svg[m
[32m+[m[32m[deps-url]: https://david-dm.org/ljharb/gopd[m
[32m+[m[32m[dev-deps-svg]: https://david-dm.org/ljharb/gopd/dev-status.svg[m
[32m+[m[32m[dev-deps-url]: https://david-dm.org/ljharb/gopd#info=devDependencies[m
[32m+[m[32m[npm-badge-png]: https://nodei.co/npm/gopd.png?downloads=true&stars=true[m
[32m+[m[32m[license-image]: https://img.shields.io/npm/l/gopd.svg[m
[32m+[m[32m[license-url]: LICENSE[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/gopd.svg[m
[32m+[m[32m[downloads-url]: https://npm-stat.com/charts.html?package=gopd[m
[32m+[m[32m[codecov-image]: https://codecov.io/gh/ljharb/gopd/branch/main/graphs/badge.svg[m
[32m+[m[32m[codecov-url]: https://app.codecov.io/gh/ljharb/gopd/[m
[32m+[m[32m[actions-image]: https://img.shields.io/endpoint?url=https://github-actions-badge-u3jn4tfpocch.runkit.sh/ljharb/gopd[m
[32m+[m[32m[actions-url]: https://github.com/ljharb/gopd/actions[m
[1mdiff --git a/node_modules/gopd/index.js b/node_modules/gopd/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..fb6d3bb[m
[1m--- /dev/null[m
[1m+++ b/node_modules/gopd/index.js[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m
[32m+[m[32mvar $gOPD = GetIntrinsic('%Object.getOwnPropertyDescriptor%', true);[m
[32m+[m
[32m+[m[32mif ($gOPD) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32m$gOPD([], 'length');[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32m// IE 8 has a broken gOPD[m
[32m+[m		[32m$gOPD = null;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = $gOPD;[m
