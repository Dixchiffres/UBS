[1mnew file mode 100644[m
[1mindex 0000000..b2ca8f1[m
[1m--- /dev/null[m
[1m+++ b/node_modules/body-parser/lib/types/urlencoded.js[m
[36m@@ -0,0 +1,284 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar bytes = require('bytes')[m
[32m+[m[32mvar contentType = require('content-type')[m
[32m+[m[32mvar createError = require('http-errors')[m
[32m+[m[32mvar debug = require('debug')('body-parser:urlencoded')[m
[32m+[m[32mvar deprecate = require('depd')('body-parser')[m
[32m+[m[32mvar read = require('../read')[m
[32m+[m[32mvar typeis = require('type-is')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = urlencoded[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Cache of parser modules.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar parsers = Object.create(null)[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a middleware to parse urlencoded bodies.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction urlencoded (options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  // notice because option default will flip in next major[m
[32m+[m[32m  if (opts.extended === undefined) {[m
[32m+[m[32m    deprecate('undefined extended: provide extended option')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var extended = opts.extended !== false[m
[32m+[m[32m  var inflate = opts.inflate !== false[m
[32m+[m[32m  var limit = typeof opts.limit !== 'number'[m
[32m+[m[32m    ? bytes.parse(opts.limit || '100kb')[m
[32m+[m[32m    : opts.limit[m
[32m+[m[32m  var type = opts.type || 'application/x-www-form-urlencoded'[m
[32m+[m[32m  var verify = opts.verify || false[m
[32m+[m
[32m+[m[32m  if (verify !== false && typeof verify !== 'function') {[m
[32m+[m[32m    throw new TypeError('option verify must be function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create the appropriate query parser[m
[32m+[m[32m  var queryparse = extended[m
[32m+[m[32m    ? extendedparser(opts)[m
[32m+[m[32m    : simpleparser(opts)[m
[32m+[m
[32m+[m[32m  // create the appropriate type checking function[m
[32m+[m[32m  var shouldParse = typeof type !== 'function'[m
[32m+[m[32m    ? typeChecker(type)[m
[32m+[m[32m    : type[m
[32m+[m
[32m+[m[32m  function parse (body) {[m
[32m+[m[32m    return body.length[m
[32m+[m[32m      ? queryparse(body)[m
[32m+[m[32m      : {}[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function urlencodedParser (req, res, next) {[m
[32m+[m[32m    if (req._body) {[m
[32m+[m[32m      debug('body already parsed')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    req.body = req.body || {}[m
[32m+[m
[32m+[m[32m    // skip requests without bodies[m
[32m+[m[32m    if (!typeis.hasBody(req)) {[m
[32m+[m[32m      debug('skip empty body')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('content-type %j', req.headers['content-type'])[m
[32m+[m
[32m+[m[32m    // determine if request should be parsed[m
[32m+[m[32m    if (!shouldParse(req)) {[m
[32m+[m[32m      debug('skip parsing')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // assert charset[m
[32m+[m[32m    var charset = getCharset(req) || 'utf-8'[m
[32m+[m[32m    if (charset !== 'utf-8') {[m
[32m+[m[32m      debug('invalid charset')[m
[32m+[m[32m      next(createError(415, 'unsupported charset "' + charset.toUpperCase() + '"', {[m
[32m+[m[32m        charset: charset,[m
[32m+[m[32m        type: 'charset.unsupported'[m
[32m+[m[32m      }))[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // read[m
[32m+[m[32m    read(req, res, next, parse, debug, {[m
[32m+[m[32m      debug: debug,[m
[32m+[m[32m      encoding: charset,[m
[32m+[m[32m      inflate: inflate,[m
[32m+[m[32m      limit: limit,[m
[32m+[m[32m      verify: verify[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the extended query parser.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction extendedparser (options) {[m
[32m+[m[32m  var parameterLimit = options.parameterLimit !== undefined[m
[32m+[m[32m    ? options.parameterLimit[m
[32m+[m[32m    : 1000[m
[32m+[m[32m  var parse = parser('qs')[m
[32m+[m
[32m+[m[32m  if (isNaN(parameterLimit) || parameterLimit < 1) {[m
[32m+[m[32m    throw new TypeError('option parameterLimit must be a positive number')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isFinite(parameterLimit)) {[m
[32m+[m[32m    parameterLimit = parameterLimit | 0[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function queryparse (body) {[m
[32m+[m[32m    var paramCount = parameterCount(body, parameterLimit)[m
[32m+[m
[32m+[m[32m    if (paramCount === undefined) {[m
[32m+[m[32m      debug('too many parameters')[m
[32m+[m[32m      throw createError(413, 'too many parameters', {[m
[32m+[m[32m        type: 'parameters.too.many'[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var arrayLimit = Math.max(100, paramCount)[m
[32m+[m
[32m+[m[32m    debug('parse extended urlencoding')[m
[32m+[m[32m    return parse(body, {[m
[32m+[m[32m      allowPrototypes: true,[m
[32m+[m[32m      arrayLimit: arrayLimit,[m
[32m+[m[32m      depth: Infinity,[m
[32m+[m[32m      parameterLimit: parameterLimit[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the charset of a request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getCharset (req) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return (contentType.parse(req).parameters.charset || '').toLowerCase()[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Count the number of parameters, stopping once limit reached[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} body[m
[32m+[m[32m * @param {number} limit[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parameterCount (body, limit) {[m
[32m+[m[32m  var count = 0[m
[32m+[m[32m  var index = 0[m
[32m+[m
[32m+[m[32m  while ((index = body.indexOf('&', index)) !== -1) {[m
[32m+[m[32m    count++[m
[32m+[m[32m    index++[m
[32m+[m
[32m+[m[32m    if (count === limit) {[m
[32m+[m[32m      return undefined[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return count[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get parser for module name dynamically.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} name[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parser (name) {[m
[32m+[m[32m  var mod = parsers[name][m
[32m+[m
[32m+[m[32m  if (mod !== undefined) {[m
[32m+[m[32m    return mod.parse[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // this uses a switch for static require analysis[m
[32m+[m[32m  switch (name) {[m
[32m+[m[32m    case 'qs':[m
[32m+[m[32m      mod = require('qs')[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'querystring':[m
[32m+[m[32m      mod = require('querystring')[m
[32m+[m[32m      break[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // store to prevent invoking require()[m
[32m+[m[32m  parsers[name] = mod[m
[32m+[m
[32m+[m[32m  return mod.parse[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the simple query parser.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction simpleparser (options) {[m
[32m+[m[32m  var parameterLimit = options.parameterLimit !== undefined[m
[32m+[m[32m    ? options.parameterLimit[m
[32m+[m[32m    : 1000[m
[32m+[m[32m  var parse = parser('querystring')[m
[32m+[m
[32m+[m[32m  if (isNaN(parameterLimit) || parameterLimit < 1) {[m
[32m+[m[32m    throw new TypeError('option parameterLimit must be a positive number')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isFinite(parameterLimit)) {[m
[32m+[m[32m    parameterLimit = parameterLimit | 0[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function queryparse (body) {[m
[32m+[m[32m    var paramCount = parameterCount(body, parameterLimit)[m
[32m+[m
[32m+[m[32m    if (paramCount === undefined) {[m
[32m+[m[32m      debug('too many parameters')[m
[32m+[m[32m      throw createError(413, 'too many parameters', {[m
[32m+[m[32m        type: 'parameters.too.many'[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('parse urlencoding')[m
[32m+[m[32m    return parse(body, undefined, undefined, { maxKeys: parameterLimit })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the simple type checker.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction typeChecker (type) {[m
[32m+[m[32m  return function checkType (req) {[m
[32m+[m[32m    return Boolean(typeis(req, type))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
