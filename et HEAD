[1mnew file mode 100644[m
[1mindex 0000000..1fd4668[m
[1m--- /dev/null[m
[1m+++ b/node_modules/call-bind/test/index.js[m
[36m@@ -0,0 +1,80 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar callBind = require('../');[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m[32mvar gOPD = require('gopd');[m
[32m+[m[32mvar hasStrictMode = require('has-strict-mode')();[m
[32m+[m[32mvar forEach = require('for-each');[m
[32m+[m[32mvar inspect = require('object-inspect');[m
[32m+[m[32mvar v = require('es-value-fixtures');[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * older engines have length nonconfigurable[m
[32m+[m[32m * in io.js v3, it is configurable except on bound functions, hence the .bind()[m
[32m+[m[32m */[m
[32m+[m[32mvar functionsHaveConfigurableLengths = !!([m
[32m+[m	[32mgOPD[m
[32m+[m	[32m&& Object.getOwnPropertyDescriptor[m
[32m+[m	[32m&& Object.getOwnPropertyDescriptor(bind.call(function () {}), 'length').configurable[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mtest('callBind', function (t) {[m
[32m+[m	[32mforEach(v.nonFunctions, function (nonFunction) {[m
[32m+[m		[32mt['throws']([m
[32m+[m			[32mfunction () { callBind(nonFunction); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32minspect(nonFunction) + ' is not a function'[m
[32m+[m		[32m);[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mvar sentinel = { sentinel: true };[m
[32m+[m	[32mvar func = function (a, b) {[m
[32m+[m		[32m// eslint-disable-next-line no-invalid-this[m
[32m+[m		[32mreturn [!hasStrictMode && this === global ? undefined : this, a, b];[m
[32m+[m	[32m};[m
[32m+[m	[32mt.equal(func.length, 2, 'original function length is 2');[m
[32m+[m	[32mt.deepEqual(func(), [undefined, undefined, undefined], 'unbound func with too few args');[m
[32m+[m	[32mt.deepEqual(func(1, 2), [undefined, 1, 2], 'unbound func with right args');[m
[32m+[m	[32mt.deepEqual(func(1, 2, 3), [undefined, 1, 2], 'unbound func with too many args');[m
[32m+[m
[32m+[m	[32mvar bound = callBind(func);[m
[32m+[m	[32mt.equal(bound.length, func.length + 1, 'function length is preserved', { skip: !functionsHaveConfigurableLengths });[m
[32m+[m	[32mt.deepEqual(bound(), [undefined, undefined, undefined], 'bound func with too few args');[m
[32m+[m	[32mt.deepEqual(bound(1, 2), [hasStrictMode ? 1 : Object(1), 2, undefined], 'bound func with right args');[m
[32m+[m	[32mt.deepEqual(bound(1, 2, 3), [hasStrictMode ? 1 : Object(1), 2, 3], 'bound func with too many args');[m
[32m+[m
[32m+[m	[32mvar boundR = callBind(func, sentinel);[m
[32m+[m	[32mt.equal(boundR.length, func.length, 'function length is preserved', { skip: !functionsHaveConfigurableLengths });[m
[32m+[m	[32mt.deepEqual(boundR(), [sentinel, undefined, undefined], 'bound func with receiver, with too few args');[m
[32m+[m	[32mt.deepEqual(boundR(1, 2), [sentinel, 1, 2], 'bound func with receiver, with right args');[m
[32m+[m	[32mt.deepEqual(boundR(1, 2, 3), [sentinel, 1, 2], 'bound func with receiver, with too many args');[m
[32m+[m
[32m+[m	[32mvar boundArg = callBind(func, sentinel, 1);[m
[32m+[m	[32mt.equal(boundArg.length, func.length - 1, 'function length is preserved', { skip: !functionsHaveConfigurableLengths });[m
[32m+[m	[32mt.deepEqual(boundArg(), [sentinel, 1, undefined], 'bound func with receiver and arg, with too few args');[m
[32m+[m	[32mt.deepEqual(boundArg(2), [sentinel, 1, 2], 'bound func with receiver and arg, with right arg');[m
[32m+[m	[32mt.deepEqual(boundArg(2, 3), [sentinel, 1, 2], 'bound func with receiver and arg, with too many args');[m
[32m+[m
[32m+[m	[32mt.test('callBind.apply', function (st) {[m
[32m+[m		[32mvar aBound = callBind.apply(func);[m
[32m+[m		[32mst.deepEqual(aBound(sentinel), [sentinel, undefined, undefined], 'apply-bound func with no args');[m
[32m+[m		[32mst.deepEqual(aBound(sentinel, [1], 4), [sentinel, 1, undefined], 'apply-bound func with too few args');[m
[32m+[m		[32mst.deepEqual(aBound(sentinel, [1, 2], 4), [sentinel, 1, 2], 'apply-bound func with right args');[m
[32m+[m
[32m+[m		[32mvar aBoundArg = callBind.apply(func);[m
[32m+[m		[32mst.deepEqual(aBoundArg(sentinel, [1, 2, 3], 4), [sentinel, 1, 2], 'apply-bound func with too many args');[m
[32m+[m		[32mst.deepEqual(aBoundArg(sentinel, [1, 2], 4), [sentinel, 1, 2], 'apply-bound func with right args');[m
[32m+[m		[32mst.deepEqual(aBoundArg(sentinel, [1], 4), [sentinel, 1, undefined], 'apply-bound func with too few args');[m
[32m+[m
[32m+[m		[32mvar aBoundR = callBind.apply(func, sentinel);[m
[32m+[m		[32mst.deepEqual(aBoundR([1, 2, 3], 4), [sentinel, 1, 2], 'apply-bound func with receiver and too many args');[m
[32m+[m		[32mst.deepEqual(aBoundR([1, 2], 4), [sentinel, 1, 2], 'apply-bound func with receiver and right args');[m
[32m+[m		[32mst.deepEqual(aBoundR([1], 4), [sentinel, 1, undefined], 'apply-bound func with receiver and too few args');[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/content-disposition/HISTORY.md b/node_modules/content-disposition/HISTORY.md[m
[1mnew file mode 100644[m
[1mindex 0000000..488effa[m
[1m--- /dev/null[m
[1m+++ b/node_modules/content-disposition/HISTORY.md[m
[36m@@ -0,0 +1,60 @@[m
