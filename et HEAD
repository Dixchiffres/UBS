[1mdiff --git a/node_modules/define-data-property/README.md b/node_modules/define-data-property/README.md[m
[1mnew file mode 100644[m
[1mindex 0000000..f2304da[m
[1m--- /dev/null[m
[1m+++ b/node_modules/define-data-property/README.md[m
[36m@@ -0,0 +1,67 @@[m
[32m+[m[32m# define-data-property <sup>[![Version Badge][npm-version-svg]][package-url]</sup>[m
[32m+[m
[32m+[m[32m[![github actions][actions-image]][actions-url][m
[32m+[m[32m[![coverage][codecov-image]][codecov-url][m
[32m+[m[32m[![License][license-image]][license-url][m
[32m+[m[32m[![Downloads][downloads-image]][downloads-url][m
[32m+[m
[32m+[m[32m[![npm badge][npm-badge-png]][package-url][m
[32m+[m
[32m+[m[32mDefine a data property on an object. Will fall back to assignment in an engine without descriptors.[m
[32m+[m
[32m+[m[32mThe three `non*` argument can also be passed `null`, which will use the existing state if available.[m
[32m+[m
[32m+[m[32mThe `loose` argument will mean that if you attempt to set a non-normal data property, in an environment without descriptor support, it will fall back to normal assignment.[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar defineDataProperty = require('define-data-property');[m
[32m+[m[32mvar assert = require('assert');[m
[32m+[m
[32m+[m[32mvar obj = {};[m
[32m+[m[32mdefineDataProperty(obj, 'key', 'value');[m
[32m+[m[32mdefineDataProperty([m
[32m+[m	[32mobj,[m
[32m+[m	[32m'key2',[m
[32m+[m	[32m'value',[m
[32m+[m	[32mtrue, // nonEnumerable, optional[m
[32m+[m	[32mfalse, // nonWritable, optional[m
[32m+[m	[32mtrue, // nonConfigurable, optional[m
[32m+[m	[32mfalse // loose, optional[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32massert.deepEqual([m
[32m+[m	[32mObject.getOwnPropertyDescriptors(obj),[m
[32m+[m	[32m{[m
[32m+[m		[32mkey: {[m
[32m+[m			[32mconfigurable: true,[m
[32m+[m			[32menumerable: true,[m
[32m+[m			[32mvalue: 'value',[m
[32m+[m			[32mwritable: true,[m
[32m+[m		[32m},[m
[32m+[m		[32mkey2: {[m
[32m+[m			[32mconfigurable: false,[m
[32m+[m			[32menumerable: false,[m
[32m+[m			[32mvalue: 'value',[m
[32m+[m			[32mwritable: true,[m
[32m+[m		[32m},[m
[32m+[m	[32m}[m
[32m+[m[32m);[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m[package-url]: https://npmjs.org/package/define-data-property[m
