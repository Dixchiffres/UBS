[1m+++ b/node_modules/iconv-lite/lib/index.js[m
[36m@@ -0,0 +1,153 @@[m
[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32m// Some environments don't have global Buffer (e.g. React Native).[m
[32m+[m[32m// Solution would be installing npm modules "buffer" and "stream" explicitly.[m
[32m+[m[32mvar Buffer = require("safer-buffer").Buffer;[m
[32m+[m
[32m+[m[32mvar bomHandling = require("./bom-handling"),[m
[32m+[m[32m    iconv = module.exports;[m
[32m+[m
[32m+[m[32m// All codecs and aliases are kept here, keyed by encoding name/alias.[m
[32m+[m[32m// They are lazy loaded in `iconv.getCodec` from `encodings/index.js`.[m
[32m+[m[32miconv.encodings = null;[m
[32m+[m
[32m+[m[32m// Characters emitted in case of error.[m
[32m+[m[32miconv.defaultCharUnicode = '�';[m
[32m+[m[32miconv.defaultCharSingleByte = '?';[m
[32m+[m
[32m+[m[32m// Public API.[m
[32m+[m[32miconv.encode = function encode(str, encoding, options) {[m
[32m+[m[32m    str = "" + (str || ""); // Ensure string.[m
[32m+[m
[32m+[m[32m    var encoder = iconv.getEncoder(encoding, options);[m
[32m+[m
[32m+[m[32m    var res = encoder.write(str);[m
[32m+[m[32m    var trail = encoder.end();[m
[32m+[m[41m    [m
[32m+[m[32m    return (trail && trail.length > 0) ? Buffer.concat([res, trail]) : res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32miconv.decode = function decode(buf, encoding, options) {[m
[32m+[m[32m    if (typeof buf === 'string') {[m
[32m+[m[32m        if (!iconv.skipDecodeWarning) {[m
[32m+[m[32m            console.error('Iconv-lite warning: decode()-ing strings is deprecated. Refer to https://github.com/ashtuchkin/iconv-lite/wiki/Use-Buffers-when-decoding');[m
[32m+[m[32m            iconv.skipDecodeWarning = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        buf = Buffer.from("" + (buf || ""), "binary"); // Ensure buffer.[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var decoder = iconv.getDecoder(encoding, options);[m
[32m+[m
[32m+[m[32m    var res = decoder.write(buf);[m
[32m+[m[32m    var trail = decoder.end();[m
[32m+[m
[32m+[m[32m    return trail ? (res + trail) : res;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32miconv.encodingExists = function encodingExists(enc) {[m
[32m+[m[32m    try {[m
[32m+[m[32m        iconv.getCodec(enc);[m
[32m+[m[32m        return true;[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Legacy aliases to convert functions[m
[32m+[m[32miconv.toEncoding = iconv.encode;[m
[32m+[m[32miconv.fromEncoding = iconv.decode;[m
[32m+[m
[32m+[m[32m// Search for a codec in iconv.encodings. Cache codec data in iconv._codecDataCache.[m
[32m+[m[32miconv._codecDataCache = {};[m
[32m+[m[32miconv.getCodec = function getCodec(encoding) {[m
[32m+[m[32m    if (!iconv.encodings)[m
[32m+[m[32m        iconv.encodings = require("../encodings"); // Lazy load all encoding definitions.[m
[32m+[m[41m    [m
[32m+[m[32m    // Canonicalize encoding name: strip all non-alphanumeric chars and appended year.[m
[32m+[m[32m    var enc = iconv._canonicalizeEncoding(encoding);[m
[32m+[m
[32m+[m[32m    // Traverse iconv.encodings to find actual codec.[m
[32m+[m[32m    var codecOptions = {};[m
[32m+[m[32m    while (true) {[m
[32m+[m[32m        var codec = iconv._codecDataCache[enc];[m
[32m+[m[32m        if (codec)[m
[32m+[m[32m            return codec;[m
[32m+[m
[32m+[m[32m        var codecDef = iconv.encodings[enc];[m
[32m+[m
[32m+[m[32m        switch (typeof codecDef) {[m
[32m+[m[32m            case "string": // Direct alias to other encoding.[m
[32m+[m[32m                enc = codecDef;[m
[32m+[m[32m                break;[m
[32m+[m
[32m+[m[32m            case "object": // Alias with options. Can be layered.[m
[32m+[m[32m                for (var key in codecDef)[m
[32m+[m[32m                    codecOptions[key] = codecDef[key];[m
[32m+[m
[32m+[m[32m                if (!codecOptions.encodingName)[m
[32m+[m[32m                    codecOptions.encodingName = enc;[m
[32m+[m[41m                [m
[32m+[m[32m                enc = codecDef.type;[m
[32m+[m[32m                break;[m
[32m+[m
[32m+[m[32m            case "function": // Codec itself.[m
[32m+[m[32m                if (!codecOptions.encodingName)[m
[32m+[m[32m                    codecOptions.encodingName = enc;[m
[32m+[m
[32m+[m[32m                // The codec function must load all tables and return object with .encoder and .decoder methods.[m
[32m+[m[32m                // It'll be called only once (for each different options object).[m
[32m+[m[32m                codec = new codecDef(codecOptions, iconv);[m
[32m+[m
[32m+[m[32m                iconv._codecDataCache[codecOptions.encodingName] = codec; // Save it to be reused later.[m
[32m+[m[32m                return codec;[m
[32m+[m
[32m+[m[32m            default:[m
[32m+[m[32m                throw new Error("Encoding not recognized: '" + encoding + "' (searched as: '"+enc+"')");[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32miconv._canonicalizeEncoding = function(encoding) {[m
[32m+[m[32m    // Canonicalize encoding name: strip all non-alphanumeric chars and appended year.[m
[32m+[m[32m    return (''+encoding).toLowerCase().replace(/:\d{4}$|[^0-9a-z]/g, "");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32miconv.getEncoder = function getEncoder(encoding, options) {[m
[32m+[m[32m    var codec = iconv.getCodec(encoding),[m
[32m+[m[32m        encoder = new codec.encoder(options, codec);[m
[32m+[m
[32m+[m[32m    if (codec.bomAware && options && options.addBOM)[m
[32m+[m[32m        encoder = new bomHandling.PrependBOM(encoder, options);[m
[32m+[m
[32m+[m[32m    return encoder;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32miconv.getDecoder = function getDecoder(encoding, options) {[m
[32m+[m[32m    var codec = iconv.getCodec(encoding),[m
[32m+[m[32m        decoder = new codec.decoder(options, codec);[m
[32m+[m
[32m+[m[32m    if (codec.bomAware && !(options && options.stripBOM === false))[m
[32m+[m[32m        decoder = new bomHandling.StripBOM(decoder, options);[m
[32m+[m
[32m+[m[32m    return decoder;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Load extensions in Node. All of them are omitted in Browserify build via 'browser' field in package.json.[m
[32m+[m[32mvar nodeVer = typeof process !== 'undefined' && process.versions && process.versions.node;[m
[32m+[m[32mif (nodeVer) {[m
[32m+[m
[32m+[m[32m    // Load streaming support in Node v0.10+[m
[32m+[m[32m    var nodeVerArr = nodeVer.split(".").map(Number);[m
[32m+[m[32m    if (nodeVerArr[0] > 0 || nodeVerArr[1] >= 10) {[m
[32m+[m[32m        require("./streams")(iconv);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Load Node primitive extensions.[m
[32m+[m[32m    require("./extend-node")(iconv);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mif ("Ā" != "\u0100") {[m
[32m+[m[32m    console.error("iconv-lite warning: javascript files use encoding different from utf-8. See https://github.com/ashtuchkin/iconv-lite/wiki/Javascript-source-file-encodings for more info.");[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/iconv-lite/lib/streams.js b/node_modules/iconv-lite/lib/streams.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4409552[m
[1m--- /dev/null[m
[1m+++ b/node_modules/iconv-lite/lib/streams.js[m
[36m@@ -0,0 +1,121 @@[m
[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32mvar Buffer = require("buffer").Buffer,[m
[32m+[m[32m    Transform = require("stream").Transform;[m
[32m+[m
[32m+[m
[32m+[m[32m// == Exports ==================================================================[m
[32m+[m[32mmodule.exports = function(iconv) {[m
[32m+[m[41m    [m
[32m+[m[32m    // Additional Public API.[m
[32m+[m[32m    iconv.encodeStream = function encodeStream(encoding, options) {[m
[32m+[m[32m        return new IconvLiteEncoderStream(iconv.getEncoder(encoding, options), options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    iconv.decodeStream = function decodeStream(encoding, options) {[m
[32m+[m[32m        return new IconvLiteDecoderStream(iconv.getDecoder(encoding, options), options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    iconv.supportsStreams = true;[m
[32m+[m
[32m+[m
[32m+[m[32m    // Not published yet.[m
[32m+[m[32m    iconv.IconvLiteEncoderStream = IconvLiteEncoderStream;[m
[32m+[m[32m    iconv.IconvLiteDecoderStream = IconvLiteDecoderStream;[m
[32m+[m[32m    iconv._collect = IconvLiteDecoderStream.prototype.collect;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m// == Encoder stream =======================================================[m
[32m+[m[32mfunction IconvLiteEncoderStream(conv, options) {[m
[32m+[m[32m    this.conv = conv;[m
[32m+[m[32m    options = options || {};[m
[32m+[m[32m    options.decodeStrings = false; // We accept only strings, so we don't need to decode them.[m
[32m+[m[32m    Transform.call(this, options);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteEncoderStream.prototype = Object.create(Transform.prototype, {[m
[32m+[m[32m    constructor: { value: IconvLiteEncoderStream }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mIconvLiteEncoderStream.prototype._transform = function(chunk, encoding, done) {[m
[32m+[m[32m    if (typeof chunk != 'string')[m
[32m+[m[32m        return done(new Error("Iconv encoding stream needs strings as its input."));[m
[32m+[m[32m    try {[m
[32m+[m[32m        var res = this.conv.write(chunk);[m
[32m+[m[32m        if (res && res.length) this.push(res);[m
[32m+[m[32m        done();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (e) {[m
[32m+[m[32m        done(e);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteEncoderStream.prototype._flush = function(done) {[m
[32m+[m[32m    try {[m
[32m+[m[32m        var res = this.conv.end();[m
[32m+[m[32m        if (res && res.length) this.push(res);[m
[32m+[m[32m        done();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (e) {[m
[32m+[m[32m        done(e);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteEncoderStream.prototype.collect = function(cb) {[m
[32m+[m[32m    var chunks = [];[m
[32m+[m[32m    this.on('error', cb);[m
[32m+[m[32m    this.on('data', function(chunk) { chunks.push(chunk); });[m
[32m+[m[32m    this.on('end', function() {[m
[32m+[m[32m        cb(null, Buffer.concat(chunks));[m
[32m+[m[32m    });[m
[32m+[m[32m    return this;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// == Decoder stream =======================================================[m
[32m+[m[32mfunction IconvLiteDecoderStream(conv, options) {[m
[32m+[m[32m    this.conv = conv;[m
[32m+[m[32m    options = options || {};[m
[32m+[m[32m    options.encoding = this.encoding = 'utf8'; // We output strings.[m
[32m+[m[32m    Transform.call(this, options);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteDecoderStream.prototype = Object.create(Transform.prototype, {[m
[32m+[m[32m    constructor: { value: IconvLiteDecoderStream }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mIconvLiteDecoderStream.prototype._transform = function(chunk, encoding, done) {[m
[32m+[m[32m    if (!Buffer.isBuffer(chunk))[m
[32m+[m[32m        return done(new Error("Iconv decoding stream needs buffers as its input."));[m
[32m+[m[32m    try {[m
[32m+[m[32m        var res = this.conv.write(chunk);[m
[32m+[m[32m        if (res && res.length) this.push(res, this.encoding);[m
[32m+[m[32m        done();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (e) {[m
[32m+[m[32m        done(e);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteDecoderStream.prototype._flush = function(done) {[m
[32m+[m[32m    try {[m
[32m+[m[32m        var res = this.conv.end();[m
[32m+[m[32m        if (res && res.length) this.push(res, this.encoding);[m[41m                [m
[32m+[m[32m        done();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch (e) {[m
[32m+[m[32m        done(e);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mIconvLiteDecoderStream.prototype.collect = function(cb) {[m
[32m+[m[32m    var res = '';[m
[32m+[m[32m    this.on('error', cb);[m
[32m+[m[32m    this.on('data', function(chunk) { res += chunk; });[m
[32m+[m[32m    this.on('end', function() {[m
[32m+[m[32m        cb(null, res);[m
[32m+[m[32m    });[m
[32m+[m[32m    return this;[m
[32m+[m[32m}[m
[32m+[m
