[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mvar callBound = require('../callBound');[m
[32m+[m
[32m+[m[32mtest('callBound', function (t) {[m
[32m+[m	[32m// static primitive[m
[32m+[m	[32mt.equal(callBound('Array.length'), Array.length, 'Array.length yields itself');[m
[32m+[m	[32mt.equal(callBound('%Array.length%'), Array.length, '%Array.length% yields itself');[m
[32m+[m
[32m+[m	[32m// static non-function object[m
[32m+[m	[32mt.equal(callBound('Array.prototype'), Array.prototype, 'Array.prototype yields itself');[m
[32m+[m	[32mt.equal(callBound('%Array.prototype%'), Array.prototype, '%Array.prototype% yields itself');[m
[32m+[m	[32mt.equal(callBound('Array.constructor'), Array.constructor, 'Array.constructor yields itself');[m
[32m+[m	[32mt.equal(callBound('%Array.constructor%'), Array.constructor, '%Array.constructor% yields itself');[m
[32m+[m
[32m+[m	[32m// static function[m
[32m+[m	[32mt.equal(callBound('Date.parse'), Date.parse, 'Date.parse yields itself');[m
[32m+[m	[32mt.equal(callBound('%Date.parse%'), Date.parse, '%Date.parse% yields itself');[m
[32m+[m
[32m+[m	[32m// prototype primitive[m
[32m+[m	[32mt.equal(callBound('Error.prototype.message'), Error.prototype.message, 'Error.prototype.message yields itself');[m
[32m+[m	[32mt.equal(callBound('%Error.prototype.message%'), Error.prototype.message, '%Error.prototype.message% yields itself');[m
[32m+[m
[32m+[m	[32m// prototype function[m
[32m+[m	[32mt.notEqual(callBound('Object.prototype.toString'), Object.prototype.toString, 'Object.prototype.toString does not yield itself');[m
[32m+[m	[32mt.notEqual(callBound('%Object.prototype.toString%'), Object.prototype.toString, '%Object.prototype.toString% does not yield itself');[m
[32m+[m	[32mt.equal(callBound('Object.prototype.toString')(true), Object.prototype.toString.call(true), 'call-bound Object.prototype.toString calls into the original');[m
[32m+[m	[32mt.equal(callBound('%Object.prototype.toString%')(true), Object.prototype.toString.call(true), 'call-bound %Object.prototype.toString% calls into the original');[m
[32m+[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { callBound('does not exist'); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'nonexistent intrinsic throws'[m
[32m+[m	[32m);[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { callBound('does not exist', true); },[m
[32m+[m		[32mSyntaxError,[m
[32m+[m		[32m'allowMissing arg still throws for unknown intrinsic'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt.test('real but absent intrinsic', { skip: typeof WeakRef !== 'undefined' }, function (st) {[m
[32m+[m		[32mst['throws']([m
[32m+[m			[32mfunction () { callBound('WeakRef'); },[m
[32m+[m			[32mTypeError,[m
[32m+[m			[32m'real but absent intrinsic throws'[m
[32m+[m		[32m);[m
[32m+[m		[32mst.equal(callBound('WeakRef', true), undefined, 'allowMissing arg avoids exception');[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
