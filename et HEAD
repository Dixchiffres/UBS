[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar deprecate = require('depd')('body-parser')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Cache of loaded parsers.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar parsers = Object.create(null)[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @typedef Parsers[m
[32m+[m[32m * @type {function}[m
[32m+[m[32m * @property {function} json[m
[32m+[m[32m * @property {function} raw[m
[32m+[m[32m * @property {function} text[m
[32m+[m[32m * @property {function} urlencoded[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @type {Parsers}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = deprecate.function(bodyParser,[m
[32m+[m[32m  'bodyParser: use individual json/urlencoded middlewares')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * JSON parser.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mObject.defineProperty(exports, 'json', {[m
[32m+[m[32m  configurable: true,[m
[32m+[m[32m  enumerable: true,[m
[32m+[m[32m  get: createParserGetter('json')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Raw parser.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mObject.defineProperty(exports, 'raw', {[m
[32m+[m[32m  configurable: true,[m
[32m+[m[32m  enumerable: true,[m
[32m+[m[32m  get: createParserGetter('raw')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Text parser.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mObject.defineProperty(exports, 'text', {[m
[32m+[m[32m  configurable: true,[m
[32m+[m[32m  enumerable: true,[m
[32m+[m[32m  get: createParserGetter('text')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * URL-encoded parser.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mObject.defineProperty(exports, 'urlencoded', {[m
[32m+[m[32m  configurable: true,[m
[32m+[m[32m  enumerable: true,[m
[32m+[m[32m  get: createParserGetter('urlencoded')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a middleware to parse json and urlencoded bodies.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @deprecated[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction bodyParser (options) {[m
[32m+[m[32m  // use default type for parsers[m
[32m+[m[32m  var opts = Object.create(options || null, {[m
[32m+[m[32m    type: {[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      enumerable: true,[m
[32m+[m[32m      value: undefined,[m
[32m+[m[32m      writable: true[m
[32m+[m[32m    }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  var _urlencoded = exports.urlencoded(opts)[m
[32m+[m[32m  var _json = exports.json(opts)[m
[32m+[m
[32m+[m[32m  return function bodyParser (req, res, next) {[m
[32m+[m[32m    _json(req, res, function (err) {[m
[32m+[m[32m      if (err) return next(err)[m
[32m+[m[32m      _urlencoded(req, res, next)[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a getter for loading a parser.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createParserGetter (name) {[m
[32m+[m[32m  return function get () {[m
[32m+[m[32m    return loadParser(name)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Load a parser module.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction loadParser (parserName) {[m
[32m+[m[32m  var parser = parsers[parserName][m
[32m+[m
[32m+[m[32m  if (parser !== undefined) {[m
[32m+[m[32m    return parser[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // this uses a switch for static require analysis[m
[32m+[m[32m  switch (parserName) {[m
[32m+[m[32m    case 'json':[m
[32m+[m[32m      parser = require('./lib/types/json')[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'raw':[m
[32m+[m[32m      parser = require('./lib/types/raw')[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'text':[m
[32m+[m[32m      parser = require('./lib/types/text')[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'urlencoded':[m
[32m+[m[32m      parser = require('./lib/types/urlencoded')[m
[32m+[m[32m      break[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // store to prevent invoking require()[m
[32m+[m[32m  return (parsers[parserName] = parser)[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/body-parser/lib/read.js b/node_modules/body-parser/lib/read.js[m
[1mnew file mode 100644[m
[1mindex 0000000..fce6283[m
[1m--- /dev/null[m
[1m+++ b/node_modules/body-parser/lib/read.js[m
[36m@@ -0,0 +1,205 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar createError = require('http-errors')[m
[32m+[m[32mvar destroy = require('destroy')[m
[32m+[m[32mvar getBody = require('raw-body')[m
[32m+[m[32mvar iconv = require('iconv-lite')[m
[32m+[m[32mvar onFinished = require('on-finished')[m
[32m+[m[32mvar unpipe = require('unpipe')[m
[32m+[m[32mvar zlib = require('zlib')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = read[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Read a request into a buffer and parse.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @param {object} res[m
[32m+[m[32m * @param {function} next[m
[32m+[m[32m * @param {function} parse[m
[32m+[m[32m * @param {function} debug[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction read (req, res, next, parse, debug, options) {[m
[32m+[m[32m  var length[m
[32m+[m[32m  var opts = options[m
[32m+[m[32m  var stream[m
[32m+[m
[32m+[m[32m  // flag as parsed[m
[32m+[m[32m  req._body = true[m
[32m+[m
[32m+[m[32m  // read options[m
[32m+[m[32m  var encoding = opts.encoding !== null[m
[32m+[m[32m    ? opts.encoding[m
[32m+[m[32m    : null[m
[32m+[m[32m  var verify = opts.verify[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    // get the content stream[m
[32m+[m[32m    stream = contentstream(req, debug, opts.inflate)[m
[32m+[m[32m    length = stream.length[m
[32m+[m[32m    stream.length = undefined[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return next(err)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // set raw-body options[m
[32m+[m[32m  opts.length = length[m
[32m+[m[32m  opts.encoding = verify[m
[32m+[m[32m    ? null[m
[32m+[m[32m    : encoding[m
[32m+[m
[32m+[m[32m  // assert charset is supported[m
[32m+[m[32m  if (opts.encoding === null && encoding !== null && !iconv.encodingExists(encoding)) {[m
[32m+[m[32m    return next(createError(415, 'unsupported charset "' + encoding.toUpperCase() + '"', {[m
[32m+[m[32m      charset: encoding.toLowerCase(),[m
[32m+[m[32m      type: 'charset.unsupported'[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // read body[m
[32m+[m[32m  debug('read body')[m
[32m+[m[32m  getBody(stream, opts, function (error, body) {[m
[32m+[m[32m    if (error) {[m
[32m+[m[32m      var _error[m
[32m+[m
[32m+[m[32m      if (error.type === 'encoding.unsupported') {[m
[32m+[m[32m        // echo back charset[m
[32m+[m[32m        _error = createError(415, 'unsupported charset "' + encoding.toUpperCase() + '"', {[m
[32m+[m[32m          charset: encoding.toLowerCase(),[m
[32m+[m[32m          type: 'charset.unsupported'[m
[32m+[m[32m        })[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // set status code on error[m
[32m+[m[32m        _error = createError(400, error)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // unpipe from stream and destroy[m
[32m+[m[32m      if (stream !== req) {[m
[32m+[m[32m        unpipe(req)[m
[32m+[m[32m        destroy(stream, true)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // read off entire request[m
[32m+[m[32m      dump(req, function onfinished () {[m
[32m+[m[32m        next(createError(400, _error))[m
[32m+[m[32m      })[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // verify[m
[32m+[m[32m    if (verify) {[m
[32m+[m[32m      try {[m
[32m+[m[32m        debug('verify body')[m
[32m+[m[32m        verify(req, res, body, encoding)[m
[32m+[m[32m      } catch (err) {[m
[32m+[m[32m        next(createError(403, err, {[m
[32m+[m[32m          body: body,[m
[32m+[m[32m          type: err.type || 'entity.verify.failed'[m
[32m+[m[32m        }))[m
[32m+[m[32m        return[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // parse[m
[32m+[m[32m    var str = body[m
[32m+[m[32m    try {[m
[32m+[m[32m      debug('parse body')[m
[32m+[m[32m      str = typeof body !== 'string' && encoding !== null[m
[32m+[m[32m        ? iconv.decode(body, encoding)[m
[32m+[m[32m        : body[m
[32m+[m[32m      req.body = parse(str)[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      next(createError(400, err, {[m
[32m+[m[32m        body: str,[m
[32m+[m[32m        type: err.type || 'entity.parse.failed'[m
[32m+[m[32m      }))[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    next()[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the content stream of the request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @param {function} debug[m
[32m+[m[32m * @param {boolean} [inflate=true][m
[32m+[m[32m * @return {object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction contentstream (req, debug, inflate) {[m
[32m+[m[32m  var encoding = (req.headers['content-encoding'] || 'identity').toLowerCase()[m
[32m+[m[32m  var length = req.headers['content-length'][m
[32m+[m[32m  var stream[m
[32m+[m
[32m+[m[32m  debug('content-encoding "%s"', encoding)[m
[32m+[m
[32m+[m[32m  if (inflate === false && encoding !== 'identity') {[m
[32m+[m[32m    throw createError(415, 'content encoding unsupported', {[m
[32m+[m[32m      encoding: encoding,[m
[32m+[m[32m      type: 'encoding.unsupported'[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  switch (encoding) {[m
[32m+[m[32m    case 'deflate':[m
[32m+[m[32m      stream = zlib.createInflate()[m
[32m+[m[32m      debug('inflate body')[m
[32m+[m[32m      req.pipe(stream)[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'gzip':[m
[32m+[m[32m      stream = zlib.createGunzip()[m
[32m+[m[32m      debug('gunzip body')[m
[32m+[m[32m      req.pipe(stream)[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'identity':[m
[32m+[m[32m      stream = req[m
[32m+[m[32m      stream.length = length[m
[32m+[m[32m      break[m
[32m+[m[32m    default:[m
[32m+[m[32m      throw createError(415, 'unsupported content encoding "' + encoding + '"', {[m
[32m+[m[32m        encoding: encoding,[m
[32m+[m[32m        type: 'encoding.unsupported'[m
[32m+[m[32m      })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return stream[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Dump the contents of a request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @param {function} callback[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction dump (req, callback) {[m
[32m+[m[32m  if (onFinished.isFinished(req)) {[m
[32m+[m[32m    callback(null)[m
[32m+[m[32m  } else {[m
[32m+[m[32m    onFinished(req, callback)[m
[32m+[m[32m    req.resume()[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/body-parser/lib/types/json.js b/node_modules/body-parser/lib/types/json.js[m
[1mnew file mode 100644[m
[1mindex 0000000..59f3f7e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/body-parser/lib/types/json.js[m
[36m@@ -0,0 +1,247 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar bytes = require('bytes')[m
[32m+[m[32mvar contentType = require('content-type')[m
[32m+[m[32mvar createError = require('http-errors')[m
[32m+[m[32mvar debug = require('debug')('body-parser:json')[m
[32m+[m[32mvar read = require('../read')[m
[32m+[m[32mvar typeis = require('type-is')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = json[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match the first non-space in a string.[m
[32m+[m[32m *[m
[32m+[m[32m * Allowed whitespace is defined in RFC 7159:[m
[32m+[m[32m *[m
[32m+[m[32m *    ws = *([m
[32m+[m[32m *            %x20 /              ; Space[m
[32m+[m[32m *            %x09 /              ; Horizontal tab[m
[32m+[m[32m *            %x0A /              ; Line feed or New line[m
[32m+[m[32m *            %x0D )              ; Carriage return[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar FIRST_CHAR_REGEXP = /^[\x20\x09\x0a\x0d]*([^\x20\x09\x0a\x0d])/ // eslint-disable-line no-control-regex[m
[32m+[m
[32m+[m[32mvar JSON_SYNTAX_CHAR = '#'[m
[32m+[m[32mvar JSON_SYNTAX_REGEXP = /#+/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a middleware to parse JSON bodies.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction json (options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  var limit = typeof opts.limit !== 'number'[m
[32m+[m[32m    ? bytes.parse(opts.limit || '100kb')[m
[32m+[m[32m    : opts.limit[m
[32m+[m[32m  var inflate = opts.inflate !== false[m
[32m+[m[32m  var reviver = opts.reviver[m
[32m+[m[32m  var strict = opts.strict !== false[m
[32m+[m[32m  var type = opts.type || 'application/json'[m
[32m+[m[32m  var verify = opts.verify || false[m
[32m+[m
[32m+[m[32m  if (verify !== false && typeof verify !== 'function') {[m
[32m+[m[32m    throw new TypeError('option verify must be function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create the appropriate type checking function[m
[32m+[m[32m  var shouldParse = typeof type !== 'function'[m
[32m+[m[32m    ? typeChecker(type)[m
[32m+[m[32m    : type[m
[32m+[m
[32m+[m[32m  function parse (body) {[m
[32m+[m[32m    if (body.length === 0) {[m
[32m+[m[32m      // special-case empty json body, as it's a common client-side mistake[m
[32m+[m[32m      // TODO: maybe make this configurable or part of "strict" option[m
[32m+[m[32m      return {}[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (strict) {[m
[32m+[m[32m      var first = firstchar(body)[m
[32m+[m
[32m+[m[32m      if (first !== '{' && first !== '[') {[m
[32m+[m[32m        debug('strict violation')[m
[32m+[m[32m        throw createStrictSyntaxError(body, first)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      debug('parse json')[m
[32m+[m[32m      return JSON.parse(body, reviver)[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw normalizeJsonSyntaxError(e, {[m
[32m+[m[32m        message: e.message,[m
[32m+[m[32m        stack: e.stack[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function jsonParser (req, res, next) {[m
[32m+[m[32m    if (req._body) {[m
[32m+[m[32m      debug('body already parsed')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    req.body = req.body || {}[m
[32m+[m
[32m+[m[32m    // skip requests without bodies[m
[32m+[m[32m    if (!typeis.hasBody(req)) {[m
[32m+[m[32m      debug('skip empty body')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('content-type %j', req.headers['content-type'])[m
[32m+[m
[32m+[m[32m    // determine if request should be parsed[m
[32m+[m[32m    if (!shouldParse(req)) {[m
[32m+[m[32m      debug('skip parsing')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // assert charset per RFC 7159 sec 8.1[m
[32m+[m[32m    var charset = getCharset(req) || 'utf-8'[m
[32m+[m[32m    if (charset.slice(0, 4) !== 'utf-') {[m
[32m+[m[32m      debug('invalid charset')[m
[32m+[m[32m      next(createError(415, 'unsupported charset "' + charset.toUpperCase() + '"', {[m
[32m+[m[32m        charset: charset,[m
[32m+[m[32m        type: 'charset.unsupported'[m
[32m+[m[32m      }))[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // read[m
[32m+[m[32m    read(req, res, next, parse, debug, {[m
[32m+[m[32m      encoding: charset,[m
[32m+[m[32m      inflate: inflate,[m
[32m+[m[32m      limit: limit,[m
[32m+[m[32m      verify: verify[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create strict violation syntax error matching native error.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @param {string} char[m
[32m+[m[32m * @return {Error}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createStrictSyntaxError (str, char) {[m
[32m+[m[32m  var index = str.indexOf(char)[m
[32m+[m[32m  var partial = ''[m
[32m+[m
[32m+[m[32m  if (index !== -1) {[m
[32m+[m[32m    partial = str.substring(0, index) + JSON_SYNTAX_CHAR[m
[32m+[m
[32m+[m[32m    for (var i = index + 1; i < str.length; i++) {[m
[32m+[m[32m      partial += JSON_SYNTAX_CHAR[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    JSON.parse(partial); /* istanbul ignore next */ throw new SyntaxError('strict violation')[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return normalizeJsonSyntaxError(e, {[m
[32m+[m[32m      message: e.message.replace(JSON_SYNTAX_REGEXP, function (placeholder) {[m
[32m+[m[32m        return str.substring(index, index + placeholder.length)[m
[32m+[m[32m      }),[m
[32m+[m[32m      stack: e.stack[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the first non-whitespace character in a string.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction firstchar (str) {[m
[32m+[m[32m  var match = FIRST_CHAR_REGEXP.exec(str)[m
[32m+[m
[32m+[m[32m  return match[m
[32m+[m[32m    ? match[1][m
[32m+[m[32m    : undefined[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the charset of a request.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getCharset (req) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return (contentType.parse(req).parameters.charset || '').toLowerCase()[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Normalize a SyntaxError for JSON.parse.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {SyntaxError} error[m
[32m+[m[32m * @param {object} obj[m
[32m+[m[32m * @return {SyntaxError}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction normalizeJsonSyntaxError (error, obj) {[m
[32m+[m[32m  var keys = Object.getOwnPropertyNames(error)[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m    var key = keys[i][m
[32m+[m[32m    if (key !== 'stack' && key !== 'message') {[m
[32m+[m[32m      delete error[key][m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // replace stack before message for Node.js 0.10 and below[m
[32m+[m[32m  error.stack = obj.stack.replace(error.message, obj.message)[m
[32m+[m[32m  error.message = obj.message[m
[32m+[m
[32m+[m[32m  return error[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the simple type checker.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction typeChecker (type) {[m
[32m+[m[32m  return function checkType (req) {[m
[32m+[m[32m    return Boolean(typeis(req, type))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/body-parser/lib/types/raw.js b/node_modules/body-parser/lib/types/raw.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f5d1b67[m
[1m--- /dev/null[m
[1m+++ b/node_modules/body-parser/lib/types/raw.js[m
[36m@@ -0,0 +1,101 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * body-parser[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar bytes = require('bytes')[m
[32m+[m[32mvar debug = require('debug')('body-parser:raw')[m
[32m+[m[32mvar read = require('../read')[m
[32m+[m[32mvar typeis = require('type-is')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = raw[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a middleware to parse raw bodies.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction raw (options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  var inflate = opts.inflate !== false[m
[32m+[m[32m  var limit = typeof opts.limit !== 'number'[m
[32m+[m[32m    ? bytes.parse(opts.limit || '100kb')[m
[32m+[m[32m    : opts.limit[m
[32m+[m[32m  var type = opts.type || 'application/octet-stream'[m
[32m+[m[32m  var verify = opts.verify || false[m
[32m+[m
[32m+[m[32m  if (verify !== false && typeof verify !== 'function') {[m
[32m+[m[32m    throw new TypeError('option verify must be function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create the appropriate type checking function[m
[32m+[m[32m  var shouldParse = typeof type !== 'function'[m
[32m+[m[32m    ? typeChecker(type)[m
[32m+[m[32m    : type[m
[32m+[m
[32m+[m[32m  function parse (buf) {[m
[32m+[m[32m    return buf[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function rawParser (req, res, next) {[m
[32m+[m[32m    if (req._body) {[m
[32m+[m[32m      debug('body already parsed')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    req.body = req.body || {}[m
[32m+[m
[32m+[m[32m    // skip requests without bodies[m
[32m+[m[32m    if (!typeis.hasBody(req)) {[m
[32m+[m[32m      debug('skip empty body')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('content-type %j', req.headers['content-type'])[m
[32m+[m
[32m+[m[32m    // determine if request should be parsed[m
[32m+[m[32m    if (!shouldParse(req)) {[m
[32m+[m[32m      debug('skip parsing')[m
[32m+[m[32m      next()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // read[m
[32m+[m[32m    read(req, res, next, parse, debug, {[m
[32m+[m[32m      encoding: null,[m
[32m+[m[32m      inflate: inflate,[m
[32m+[m[32m      limit: limit,[m
[32m+[m[32m      verify: verify[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the simple type checker.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @return {function}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction typeChecker (type) {[m
[32m+[m[32m  return function checkType (req) {[m
[32m+[m[32m    return Boolean(typeis(req, type))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
