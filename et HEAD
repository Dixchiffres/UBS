[1mdiff --git a/node_modules/mime/src/build.js b/node_modules/mime/src/build.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4928e48[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/src/build.js[m
[36m@@ -0,0 +1,53 @@[m
[32m+[m[32m#!/usr/bin/env node[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst path = require('path');[m
[32m+[m[32mconst mimeScore = require('mime-score');[m
[32m+[m
[32m+[m[32mlet db = require('mime-db');[m
[32m+[m[32mlet chalk = require('chalk');[m
[32m+[m
[32m+[m[32mconst STANDARD_FACET_SCORE = 900;[m
[32m+[m
[32m+[m[32mconst byExtension = {};[m
[32m+[m
[32m+[m[32m// Clear out any conflict extensions in mime-db[m
[32m+[m[32mfor (let type in db) {[m
[32m+[m[32m  let entry = db[type];[m
[32m+[m[32m  entry.type = type;[m
[32m+[m
[32m+[m[32m  if (!entry.extensions) continue;[m
[32m+[m
[32m+[m[32m  entry.extensions.forEach(ext => {[m
[32m+[m[32m    if (ext in byExtension) {[m
[32m+[m[32m      const e0 = entry;[m
[32m+[m[32m      const e1 = byExtension[ext];[m
[32m+[m[32m      e0.pri = mimeScore(e0.type, e0.source);[m
[32m+[m[32m      e1.pri = mimeScore(e1.type, e1.source);[m
[32m+[m
[32m+[m[32m      let drop = e0.pri < e1.pri ? e0 : e1;[m
[32m+[m[32m      let keep = e0.pri >= e1.pri ? e0 : e1;[m
[32m+[m[32m      drop.extensions = drop.extensions.filter(e => e !== ext);[m
[32m+[m
[32m+[m[32m      console.log(`${ext}: Keeping ${chalk.green(keep.type)} (${keep.pri}), dropping ${chalk.red(drop.type)} (${drop.pri})`);[m
[32m+[m[32m    }[m
[32m+[m[32m    byExtension[ext] = entry;[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction writeTypesFile(types, path) {[m
[32m+[m[32m  fs.writeFileSync(path, JSON.stringify(types));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Segregate into standard and non-standard types based on facet per[m
[32m+[m[32m// https://tools.ietf.org/html/rfc6838#section-3.1[m
[32m+[m[32mconst types = {};[m
[32m+[m
[32m+[m[32mObject.keys(db).sort().forEach(k => {[m
[32m+[m[32m  const entry = db[k];[m
[32m+[m[32m  types[entry.type] = entry.extensions;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mwriteTypesFile(types, path.join(__dirname, '..', 'types.json'));[m
[1mdiff --git a/node_modules/mime/src/test.js b/node_modules/mime/src/test.js[m
[1mnew file mode 100644[m
[1mindex 0000000..42958a2[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/src/test.js[m
[36m@@ -0,0 +1,60 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Usage: node test.js[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar mime = require('../mime');[m
[32m+[m[32mvar assert = require('assert');[m
[32m+[m[32mvar path = require('path');[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test mime lookups[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('text/plain', mime.lookup('text.txt'));     // normal file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('TEXT.TXT'));     // uppercase[m
[32m+[m[32massert.equal('text/plain', mime.lookup('dir/text.txt')); // dir + file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('.text.txt'));    // hidden file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('.txt'));         // nameless[m
[32m+[m[32massert.equal('text/plain', mime.lookup('txt'));          // extension-only[m
[32m+[m[32massert.equal('text/plain', mime.lookup('/txt'));         // extension-less ()[m
[32m+[m[32massert.equal('text/plain', mime.lookup('\\txt'));        // Windows, extension-less[m
[32m+[m[32massert.equal('application/octet-stream', mime.lookup('text.nope')); // unrecognized[m
[32m+[m[32massert.equal('fallback', mime.lookup('text.fallback', 'fallback')); // alternate default[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test extensions[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('txt', mime.extension(mime.types.text));[m
[32m+[m[32massert.equal('html', mime.extension(mime.types.htm));[m
[32m+[m[32massert.equal('bin', mime.extension('application/octet-stream'));[m
[32m+[m[32massert.equal('bin', mime.extension('application/octet-stream '));[m
[32m+[m[32massert.equal('html', mime.extension(' text/html; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html; charset=UTF-8 '));[m
[32m+[m[32massert.equal('html', mime.extension('text/html; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html ; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html;charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/Html;charset=UTF-8'));[m
[32m+[m[32massert.equal(undefined, mime.extension('unrecognized'));[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test node.types lookups[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('font/woff', mime.lookup('file.woff'));[m
[32m+[m[32massert.equal('application/octet-stream', mime.lookup('file.buffer'));[m
[32m+[m[32m// TODO: Uncomment once #157 is resolved[m
[32m+[m[32m// assert.equal('audio/mp4', mime.lookup('file.m4a'));[m
[32m+[m[32massert.equal('font/otf', mime.lookup('file.otf'));[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test charsets[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('UTF-8', mime.charsets.lookup('text/plain'));[m
[32m+[m[32massert.equal('UTF-8', mime.charsets.lookup(mime.types.js));[m
[32m+[m[32massert.equal('UTF-8', mime.charsets.lookup(mime.types.json));[m
[32m+[m[32massert.equal(undefined, mime.charsets.lookup(mime.types.bin));[m
[32m+[m[32massert.equal('fallback', mime.charsets.lookup('application/octet-stream', 'fallback'));[m
[32m+[m
[32m+[m[32mconsole.log('\nAll tests passed');[m
[1mdiff --git a/node_modules/mime/types.json b/node_modules/mime/types.json[m
[1mnew file mode 100644[m
[1mindex 0000000..bec78ab[m
[1m--- /dev/null[m
