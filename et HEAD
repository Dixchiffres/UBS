[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m[32mvar hasOwn = require('has');[m
[32m+[m[32mvar $concat = bind.call(Function.call, Array.prototype.concat);[m
[32m+[m[32mvar $spliceApply = bind.call(Function.apply, Array.prototype.splice);[m
[32m+[m[32mvar $replace = bind.call(Function.call, String.prototype.replace);[m
[32m+[m[32mvar $strSlice = bind.call(Function.call, String.prototype.slice);[m
[32m+[m
[32m+[m[32m/* adapted from https://github.com/lodash/lodash/blob/4.17.15/dist/lodash.js#L6735-L6744 */[m
[32m+[m[32mvar rePropName = /[^%.[\]]+|\[(?:(-?\d+(?:\.\d+)?)|(["'])((?:(?!\2)[^\\]|\\.)*?)\2)\]|(?=(?:\.|\[\])(?:\.|\[\]|%$))/g;[m
[32m+[m[32mvar reEscapeChar = /\\(\\)?/g; /** Used to match backslashes in property paths. */[m
[32m+[m[32mvar stringToPath = function stringToPath(string) {[m
[32m+[m	[32mvar first = $strSlice(string, 0, 1);[m
[32m+[m	[32mvar last = $strSlice(string, -1);[m
[32m+[m	[32mif (first === '%' && last !== '%') {[m
[32m+[m		[32mthrow new $SyntaxError('invalid intrinsic syntax, expected closing `%`');[m
[32m+[m	[32m} else if (last === '%' && first !== '%') {[m
[32m+[m		[32mthrow new $SyntaxError('invalid intrinsic syntax, expected opening `%`');[m
[32m+[m	[32m}[m
[32m+[m	[32mvar result = [];[m
[32m+[m	[32m$replace(string, rePropName, function (match, number, quote, subString) {[m
[32m+[m		[32mresult[result.length] = quote ? $replace(subString, reEscapeChar, '$1') : number || match;[m
[32m+[m	[32m});[m
[32m+[m	[32mreturn result;[m
[32m+[m[32m};[m
[32m+[m[32m/* end adaptation */[m
[32m+[m
[32m+[m[32mvar getBaseIntrinsic = function getBaseIntrinsic(name, allowMissing) {[m
[32m+[m	[32mvar intrinsicName = name;[m
[32m+[m	[32mvar alias;[m
[32m+[m	[32mif (hasOwn(LEGACY_ALIASES, intrinsicName)) {[m
[32m+[m		[32malias = LEGACY_ALIASES[intrinsicName];[m
[32m+[m		[32mintrinsicName = '%' + alias[0] + '%';[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif (hasOwn(INTRINSICS, intrinsicName)) {[m
[32m+[m		[32mvar value = INTRINSICS[intrinsicName];[m
[32m+[m		[32mif (value === needsEval) {[m
[32m+[m			[32mvalue = doEval(intrinsicName);[m
[32m+[m		[32m}[m
[32m+[m		[32mif (typeof value === 'undefined' && !allowMissing) {[m
[32m+[m			[32mthrow new $TypeError('intrinsic ' + name + ' exists, but is not available. Please file an issue!');[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn {[m
[32m+[m			[32malias: alias,[m
[32m+[m			[32mname: intrinsicName,[m
[32m+[m			[32mvalue: value[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mthrow new $SyntaxError('intrinsic ' + name + ' does not exist!');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function GetIntrinsic(name, allowMissing) {[m
[32m+[m	[32mif (typeof name !== 'string' || name.length === 0) {[m
[32m+[m		[32mthrow new $TypeError('intrinsic name must be a non-empty string');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 1 && typeof allowMissing !== 'boolean') {[m
[32m+[m		[32mthrow new $TypeError('"allowMissing" argument must be a boolean');[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar parts = stringToPath(name);[m
[32m+[m	[32mvar intrinsicBaseName = parts.length > 0 ? parts[0] : '';[m
[32m+[m
[32m+[m	[32mvar intrinsic = getBaseIntrinsic('%' + intrinsicBaseName + '%', allowMissing);[m
[32m+[m	[32mvar intrinsicRealName = intrinsic.name;[m
[32m+[m	[32mvar value = intrinsic.value;[m
[32m+[m	[32mvar skipFurtherCaching = false;[m
[32m+[m
[32m+[m	[32mvar alias = intrinsic.alias;[m
[32m+[m	[32mif (alias) {[m
[32m+[m		[32mintrinsicBaseName = alias[0];[m
[32m+[m		[32m$spliceApply(parts, $concat([0, 1], alias));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor (var i = 1, isOwn = true; i < parts.length; i += 1) {[m
[32m+[m		[32mvar part = parts[i];[m
[32m+[m		[32mvar first = $strSlice(part, 0, 1);[m
[32m+[m		[32mvar last = $strSlice(part, -1);[m
[32m+[m		[32mif ([m
[32m+[m			[32m([m
[32m+[m				[32m(first === '"' || first === "'" || first === '`')[m
[32m+[m				[32m|| (last === '"' || last === "'" || last === '`')[m
[32m+[m			[32m)[m
[32m+[m			[32m&& first !== last[m
[32m+[m		[32m) {[m
[32m+[m			[32mthrow new $SyntaxError('property names with quotes must have matching quotes');[m
[32m+[m		[32m}[m
[32m+[m		[32mif (part === 'constructor' || !isOwn) {[m
[32m+[m			[32mskipFurtherCaching = true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mintrinsicBaseName += '.' + part;[m
[32m+[m		[32mintrinsicRealName = '%' + intrinsicBaseName + '%';[m
[32m+[m
[32m+[m		[32mif (hasOwn(INTRINSICS, intrinsicRealName)) {[m
[32m+[m			[32mvalue = INTRINSICS[intrinsicRealName];[m
[32m+[m		[32m} else if (value != null) {[m
[32m+[m			[32mif (!(part in value)) {[m
[32m+[m				[32mif (!allowMissing) {[m
[32m+[m					[32mthrow new $TypeError('base intrinsic for ' + name + ' exists, but the property is not available.');[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn void undefined;[m
[32m+[m			[32m}[m
[32m+[m			[32mif ($gOPD && (i + 1) >= parts.length) {[m
[32m+[m				[32mvar desc = $gOPD(value, part);[m
[32m+[m				[32misOwn = !!desc;[m
[32m+[m
[32m+[m				[32m// By convention, when a data property is converted to an accessor[m
[32m+[m				[32m// property to emulate a data property that does not suffer from[m
[32m+[m				[32m// the override mistake, that accessor's getter is marked with[m
[32m+[m				[32m// an `originalValue` property. Here, when we detect this, we[m
[32m+[m				[32m// uphold the illusion by pretending to see that original data[m
[32m+[m				[32m// property, i.e., returning the value rather than the getter[m
[32m+[m				[32m// itself.[m
[32m+[m				[32mif (isOwn && 'get' in desc && !('originalValue' in desc.get)) {[m
[32m+[m					[32mvalue = desc.get;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mvalue = value[part];[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32misOwn = hasOwn(value, part);[m
[32m+[m				[32mvalue = value[part];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (isOwn && !skipFurtherCaching) {[m
[32m+[m				[32mINTRINSICS[intrinsicRealName] = value;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn value;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"function-bind":10,"has":14,"has-symbols":12}],12:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
