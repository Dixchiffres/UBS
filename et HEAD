[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar hasToStringTag = require('has-tostringtag/shams')();[m
[32m+[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m
[32m+[m[32mtest('Symbol.toStringTag', { skip: !hasToStringTag }, function (t) {[m
[32m+[m[32m    t.plan(4);[m
[32m+[m
[32m+[m[32m    var obj = { a: 1 };[m
[32m+[m[32m    t.equal(inspect(obj), '{ a: 1 }', 'object, no Symbol.toStringTag');[m
[32m+[m
[32m+[m[32m    obj[Symbol.toStringTag] = 'foo';[m
[32m+[m[32m    t.equal(inspect(obj), '{ a: 1, [Symbol(Symbol.toStringTag)]: \'foo\' }', 'object with Symbol.toStringTag');[m
[32m+[m
[32m+[m[32m    t.test('null objects', { skip: 'toString' in { __proto__: null } }, function (st) {[m
[32m+[m[32m        st.plan(2);[m
[32m+[m
[32m+[m[32m        var dict = { __proto__: null, a: 1 };[m
[32m+[m[32m        st.equal(inspect(dict), '[Object: null prototype] { a: 1 }', 'null object with Symbol.toStringTag');[m
[32m+[m
[32m+[m[32m        dict[Symbol.toStringTag] = 'Dict';[m
[32m+[m[32m        st.equal(inspect(dict), '[Dict: null prototype] { a: 1, [Symbol(Symbol.toStringTag)]: \'Dict\' }', 'null object with Symbol.toStringTag');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('instances', function (st) {[m
[32m+[m[32m        st.plan(4);[m
[32m+[m
[32m+[m[32m        function C() {[m
[32m+[m[32m            this.a = 1;[m
[32m+[m[32m        }[m
[32m+[m[32m        st.equal(Object.prototype.toString.call(new C()), '[object Object]', 'instance, no toStringTag, Object.prototype.toString');[m
[32m+[m[32m        st.equal(inspect(new C()), 'C { a: 1 }', 'instance, no toStringTag');[m
[32m+[m
[32m+[m[32m        C.prototype[Symbol.toStringTag] = 'Class!';[m
[32m+[m[32m        st.equal(Object.prototype.toString.call(new C()), '[object Class!]', 'instance, with toStringTag, Object.prototype.toString');[m
[32m+[m[32m        st.equal(inspect(new C()), 'C [Class!] { a: 1 }', 'instance, with toStringTag');[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/undef.js b/node_modules/object-inspect/test/undef.js[m
[1mnew file mode 100644[m
[1mindex 0000000..e3f4961[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/test/undef.js[m
[36m@@ -0,0 +1,12 @@[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m
[32m+[m[32mvar obj = { a: 1, b: [3, 4, undefined, null], c: undefined, d: null };[m
[32m+[m
[32m+[m[32mtest('undef and null', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(obj),[m
[32m+[m[32m        '{ a: 1, b: [ 3, 4, undefined, null ], c: undefined, d: null }'[m
[32m+[m[32m    );[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/values.js b/node_modules/object-inspect/test/values.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4832b9f[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/test/values.js[m
[36m@@ -0,0 +1,211 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar mockProperty = require('mock-property');[m
[32m+[m[32mvar hasSymbols = require('has-symbols/shams')();[m
[32m+[m[32mvar hasToStringTag = require('has-tostringtag/shams')();[m
[32m+[m
[32m+[m[32mtest('values', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var obj = [{}, [], { 'a-b': 5 }];[m
[32m+[m[32m    t.equal(inspect(obj), '[ {}, [], { \'a-b\': 5 } ]');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('arrays with properties', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var arr = [3];[m
[32m+[m[32m    arr.foo = 'bar';[m
[32m+[m[32m    var obj = [1, 2, arr];[m
[32m+[m[32m    obj.baz = 'quux';[m
[32m+[m[32m    obj.index = -1;[m
[32m+[m[32m    t.equal(inspect(obj), '[ 1, 2, [ 3, foo: \'bar\' ], baz: \'quux\', index: -1 ]');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('has', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    t.teardown(mockProperty(Object.prototype, 'hasOwnProperty', { 'delete': true }));[m
[32m+[m
[32m+[m[32m    t.equal(inspect({ a: 1, b: 2 }), '{ a: 1, b: 2 }');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('indexOf seen', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var xs = [1, 2, 3, {}];[m
[32m+[m[32m    xs.push(xs);[m
[32m+[m
[32m+[m[32m    var seen = [];[m
[32m+[m[32m    seen.indexOf = undefined;[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(xs, {}, 0, seen),[m
[32m+[m[32m        '[ 1, 2, 3, {}, [Circular] ]'[m
[32m+[m[32m    );[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('seen seen', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var xs = [1, 2, 3];[m
[32m+[m
[32m+[m[32m    var seen = [xs];[m
[32m+[m[32m    seen.indexOf = undefined;[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(xs, {}, 0, seen),[m
[32m+[m[32m        '[Circular]'[m
[32m+[m[32m    );[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('seen seen seen', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var xs = [1, 2, 3];[m
[32m+[m
[32m+[m[32m    var seen = [5, xs];[m
[32m+[m[32m    seen.indexOf = undefined;[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(xs, {}, 0, seen),[m
[32m+[m[32m        '[Circular]'[m
[32m+[m[32m    );[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('symbols', { skip: !hasSymbols }, function (t) {[m
[32m+[m[32m    var sym = Symbol('foo');[m
[32m+[m[32m    t.equal(inspect(sym), 'Symbol(foo)', 'Symbol("foo") should be "Symbol(foo)"');[m
[32m+[m[32m    if (typeof sym === 'symbol') {[m
[32m+[m[32m        // Symbol shams are incapable of differentiating boxed from unboxed symbols[m
[32m+[m[32m        t.equal(inspect(Object(sym)), 'Object(Symbol(foo))', 'Object(Symbol("foo")) should be "Object(Symbol(foo))"');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    t.test('toStringTag', { skip: !hasToStringTag }, function (st) {[m
[32m+[m[32m        st.plan(1);[m
[32m+[m
[32m+[m[32m        var faker = {};[m
[32m+[m[32m        faker[Symbol.toStringTag] = 'Symbol';[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            inspect(faker),[m
[32m+[m[32m            '{ [Symbol(Symbol.toStringTag)]: \'Symbol\' }',[m
[32m+[m[32m            'object lying about being a Symbol inspects as an object'[m
[32m+[m[32m        );[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Map', { skip: typeof Map !== 'function' }, function (t) {[m
[32m+[m[32m    var map = new Map();[m
[32m+[m[32m    map.set({ a: 1 }, ['b']);[m
[32m+[m[32m    map.set(3, NaN);[m
[32m+[m[32m    var expectedString = 'Map (2) {' + inspect({ a: 1 }) + ' => ' + inspect(['b']) + ', 3 => NaN}';[m
[32m+[m[32m    t.equal(inspect(map), expectedString, 'new Map([[{ a: 1 }, ["b"]], [3, NaN]]) should show size and contents');[m
[32m+[m[32m    t.equal(inspect(new Map()), 'Map (0) {}', 'empty Map should show as empty');[m
[32m+[m
[32m+[m[32m    var nestedMap = new Map();[m
[32m+[m[32m    nestedMap.set(nestedMap, map);[m
[32m+[m[32m    t.equal(inspect(nestedMap), 'Map (1) {[Circular] => ' + expectedString + '}', 'Map containing a Map should work');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('WeakMap', { skip: typeof WeakMap !== 'function' }, function (t) {[m
[32m+[m[32m    var map = new WeakMap();[m
[32m+[m[32m    map.set({ a: 1 }, ['b']);[m
[32m+[m[32m    var expectedString = 'WeakMap { ? }';[m
[32m+[m[32m    t.equal(inspect(map), expectedString, 'new WeakMap([[{ a: 1 }, ["b"]]]) should not show size or contents');[m
[32m+[m[32m    t.equal(inspect(new WeakMap()), 'WeakMap { ? }', 'empty WeakMap should not show as empty');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Set', { skip: typeof Set !== 'function' }, function (t) {[m
[32m+[m[32m    var set = new Set();[m
[32m+[m[32m    set.add({ a: 1 });[m
[32m+[m[32m    set.add(['b']);[m
[32m+[m[32m    var expectedString = 'Set (2) {' + inspect({ a: 1 }) + ', ' + inspect(['b']) + '}';[m
[32m+[m[32m    t.equal(inspect(set), expectedString, 'new Set([{ a: 1 }, ["b"]]) should show size and contents');[m
[32m+[m[32m    t.equal(inspect(new Set()), 'Set (0) {}', 'empty Set should show as empty');[m
[32m+[m
[32m+[m[32m    var nestedSet = new Set();[m
[32m+[m[32m    nestedSet.add(set);[m
[32m+[m[32m    nestedSet.add(nestedSet);[m
[32m+[m[32m    t.equal(inspect(nestedSet), 'Set (2) {' + expectedString + ', [Circular]}', 'Set containing a Set should work');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('WeakSet', { skip: typeof WeakSet !== 'function' }, function (t) {[m
[32m+[m[32m    var map = new WeakSet();[m
[32m+[m[32m    map.add({ a: 1 });[m
[32m+[m[32m    var expectedString = 'WeakSet { ? }';[m
[32m+[m[32m    t.equal(inspect(map), expectedString, 'new WeakSet([{ a: 1 }]) should not show size or contents');[m
[32m+[m[32m    t.equal(inspect(new WeakSet()), 'WeakSet { ? }', 'empty WeakSet should not show as empty');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('WeakRef', { skip: typeof WeakRef !== 'function' }, function (t) {[m
[32m+[m[32m    var ref = new WeakRef({ a: 1 });[m
[32m+[m[32m    var expectedString = 'WeakRef { ? }';[m
[32m+[m[32m    t.equal(inspect(ref), expectedString, 'new WeakRef({ a: 1 }) should not show contents');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('FinalizationRegistry', { skip: typeof FinalizationRegistry !== 'function' }, function (t) {[m
[32m+[m[32m    var registry = new FinalizationRegistry(function () {});[m
[32m+[m[32m    var expectedString = 'FinalizationRegistry [FinalizationRegistry] {}';[m
[32m+[m[32m    t.equal(inspect(registry), expectedString, 'new FinalizationRegistry(function () {}) should work normallys');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Strings', function (t) {[m
[32m+[m[32m    var str = 'abc';[m
[32m+[m
[32m+[m[32m    t.equal(inspect(str), "'" + str + "'", 'primitive string shows as such');[m
[32m+[m[32m    t.equal(inspect(str, { quoteStyle: 'single' }), "'" + str + "'", 'primitive string shows as such, single quoted');[m
[32m+[m[32m    t.equal(inspect(str, { quoteStyle: 'double' }), '"' + str + '"', 'primitive string shows as such, double quoted');[m
[32m+[m[32m    t.equal(inspect(Object(str)), 'Object(' + inspect(str) + ')', 'String object shows as such');[m
[32m+[m[32m    t.equal(inspect(Object(str), { quoteStyle: 'single' }), 'Object(' + inspect(str, { quoteStyle: 'single' }) + ')', 'String object shows as such, single quoted');[m
[32m+[m[32m    t.equal(inspect(Object(str), { quoteStyle: 'double' }), 'Object(' + inspect(str, { quoteStyle: 'double' }) + ')', 'String object shows as such, double quoted');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Numbers', function (t) {[m
[32m+[m[32m    var num = 42;[m
[32m+[m
[32m+[m[32m    t.equal(inspect(num), String(num), 'primitive number shows as such');[m
[32m+[m[32m    t.equal(inspect(Object(num)), 'Object(' + inspect(num) + ')', 'Number object shows as such');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Booleans', function (t) {[m
[32m+[m[32m    t.equal(inspect(true), String(true), 'primitive true shows as such');[m
[32m+[m[32m    t.equal(inspect(Object(true)), 'Object(' + inspect(true) + ')', 'Boolean object true shows as such');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(false), String(false), 'primitive false shows as such');[m
[32m+[m[32m    t.equal(inspect(Object(false)), 'Object(' + inspect(false) + ')', 'Boolean false object shows as such');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Date', function (t) {[m
[32m+[m[32m    var now = new Date();[m
[32m+[m[32m    t.equal(inspect(now), String(now), 'Date shows properly');[m
[32m+[m[32m    t.equal(inspect(new Date(NaN)), 'Invalid Date', 'Invalid Date shows properly');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('RegExps', function (t) {[m
[32m+[m[32m    t.equal(inspect(/a/g), '/a/g', 'regex shows properly');[m
[32m+[m[32m    t.equal(inspect(new RegExp('abc', 'i')), '/abc/i', 'new RegExp shows properly');[m
[32m+[m
[32m+[m[32m    var match = 'abc abc'.match(/[ab]+/);[m
[32m+[m[32m    delete match.groups; // for node < 10[m
[32m+[m[32m    t.equal(inspect(match), '[ \'ab\', index: 0, input: \'abc abc\' ]', 'RegExp match object shows properly');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
