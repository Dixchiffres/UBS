[36m@@ -0,0 +1,43 @@[m
[32m+[m
[32m+[m[32m# escape-html[m
[32m+[m
[32m+[m[32m  Escape string for use in HTML[m
[32m+[m
[32m+[m[32m## Example[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar escape = require('escape-html');[m
[32m+[m[32mvar html = escape('foo & bar');[m
[32m+[m[32m// -> foo &amp; bar[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Benchmark[m
[32m+[m
[32m+[m[32m```[m
[32m+[m[32m$ npm run-script bench[m
[32m+[m
[32m+[m[32m> escape-html@1.0.3 bench nodejs-escape-html[m
[32m+[m[32m> node benchmark/index.js[m
[32m+[m
[32m+[m
[32m+[m[32m  http_parser@1.0[m
[32m+[m[32m  node@0.10.33[m
[32m+[m[32m  v8@3.14.5.9[m
[32m+[m[32m  ares@1.9.0-DEV[m
[32m+[m[32m  uv@0.10.29[m
[32m+[m[32m  zlib@1.2.3[m
[32m+[m[32m  modules@11[m
[32m+[m[32m  openssl@1.0.1j[m
[32m+[m
[32m+[m[32m  1 test completed.[m
[32m+[m[32m  2 tests completed.[m
[32m+[m[32m  3 tests completed.[m
[32m+[m
[32m+[m[32m  no special characters    x 19,435,271 ops/sec ±0.85% (187 runs sampled)[m
[32m+[m[32m  single special character x  6,132,421 ops/sec ±0.67% (194 runs sampled)[m
[32m+[m[32m  many special characters  x  3,175,826 ops/sec ±0.65% (193 runs sampled)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m  MIT[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/escape-html/index.js b/node_modules/escape-html/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..bf9e226[m
[1m--- /dev/null[m
[1m+++ b/node_modules/escape-html/index.js[m
[36m@@ -0,0 +1,78 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * escape-html[m
[32m+[m[32m * Copyright(c) 2012-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2015 Andreas Lubbe[m
[32m+[m[32m * Copyright(c) 2015 Tiancheng "Timothy" Gu[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar matchHtmlRegExp = /["'&<>]/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = escapeHtml;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Escape special characters in the given string of html.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string} string The string to escape for inserting into HTML[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction escapeHtml(string) {[m
[32m+[m[32m  var str = '' + string;[m
[32m+[m[32m  var match = matchHtmlRegExp.exec(str);[m
[32m+[m
[32m+[m[32m  if (!match) {[m
[32m+[m[32m    return str;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var escape;[m
[32m+[m[32m  var html = '';[m
[32m+[m[32m  var index = 0;[m
[32m+[m[32m  var lastIndex = 0;[m
[32m+[m
[32m+[m[32m  for (index = match.index; index < str.length; index++) {[m
[32m+[m[32m    switch (str.charCodeAt(index)) {[m
[32m+[m[32m      case 34: // "[m
[32m+[m[32m        escape = '&quot;';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 38: // &[m
[32m+[m[32m        escape = '&amp;';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 39: // '[m
[32m+[m[32m        escape = '&#39;';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 60: // <[m
[32m+[m[32m        escape = '&lt;';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 62: // >[m
[32m+[m[32m        escape = '&gt;';[m
[32m+[m[32m        break;[m
[32m+[m[32m      default:[m
[32m+[m[32m        continue;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (lastIndex !== index) {[m
[32m+[m[32m      html += str.substring(lastIndex, index);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    lastIndex = index + 1;[m
[32m+[m[32m    html += escape;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return lastIndex !== index[m
[32m+[m[32m    ? html + str.substring(lastIndex, index)[m
[32m+[m[32m    : html;[m
[32m+[m[32m}[m
