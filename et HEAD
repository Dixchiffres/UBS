[1mdiff --git a/node_modules/safer-buffer/safer.js b/node_modules/safer-buffer/safer.js[m
[1mnew file mode 100644[m
[1mindex 0000000..37c7e1a[m
[1m--- /dev/null[m
[1m+++ b/node_modules/safer-buffer/safer.js[m
[36m@@ -0,0 +1,77 @@[m
[32m+[m[32m/* eslint-disable node/no-deprecated-api */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32mvar buffer = require('buffer')[m
[32m+[m[32mvar Buffer = buffer.Buffer[m
[32m+[m
[32m+[m[32mvar safer = {}[m
[32m+[m
[32m+[m[32mvar key[m
[32m+[m
[32m+[m[32mfor (key in buffer) {[m
[32m+[m[32m  if (!buffer.hasOwnProperty(key)) continue[m
[32m+[m[32m  if (key === 'SlowBuffer' || key === 'Buffer') continue[m
[32m+[m[32m  safer[key] = buffer[key][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar Safer = safer.Buffer = {}[m
[32m+[m[32mfor (key in Buffer) {[m
[32m+[m[32m  if (!Buffer.hasOwnProperty(key)) continue[m
[32m+[m[32m  if (key === 'allocUnsafe' || key === 'allocUnsafeSlow') continue[m
[32m+[m[32m  Safer[key] = Buffer[key][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32msafer.Buffer.prototype = Buffer.prototype[m
[32m+[m
[32m+[m[32mif (!Safer.from || Safer.from === Uint8Array.from) {[m
[32m+[m[32m  Safer.from = function (value, encodingOrOffset, length) {[m
[32m+[m[32m    if (typeof value === 'number') {[m
[32m+[m[32m      throw new TypeError('The "value" argument must not be of type number. Received type ' + typeof value)[m
[32m+[m[32m    }[m
[32m+[m[32m    if (value && typeof value.length === 'undefined') {[m
[32m+[m[32m      throw new TypeError('The first argument must be one of type string, Buffer, ArrayBuffer, Array, or Array-like Object. Received type ' + typeof value)[m
[32m+[m[32m    }[m
[32m+[m[32m    return Buffer(value, encodingOrOffset, length)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mif (!Safer.alloc) {[m
[32m+[m[32m  Safer.alloc = function (size, fill, encoding) {[m
[32m+[m[32m    if (typeof size !== 'number') {[m
[32m+[m[32m      throw new TypeError('The "size" argument must be of type number. Received type ' + typeof size)[m
[32m+[m[32m    }[m
[32m+[m[32m    if (size < 0 || size >= 2 * (1 << 30)) {[m
[32m+[m[32m      throw new RangeError('The value "' + size + '" is invalid for option "size"')[m
[32m+[m[32m    }[m
[32m+[m[32m    var buf = Buffer(size)[m
[32m+[m[32m    if (!fill || fill.length === 0) {[m
[32m+[m[32m      buf.fill(0)[m
[32m+[m[32m    } else if (typeof encoding === 'string') {[m
[32m+[m[32m      buf.fill(fill, encoding)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      buf.fill(fill)[m
[32m+[m[32m    }[m
[32m+[m[32m    return buf[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mif (!safer.kStringMaxLength) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    safer.kStringMaxLength = process.binding('buffer').kStringMaxLength[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    // we can't determine kStringMaxLength in environments where process.binding[m
[32m+[m[32m    // is unsupported, so let's not set it[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mif (!safer.constants) {[m
[32m+[m[32m  safer.constants = {[m
[32m+[m[32m    MAX_LENGTH: safer.kMaxLength[m
[32m+[m[32m  }[m
[32m+[m[32m  if (safer.kStringMaxLength) {[m
[32m+[m[32m    safer.constants.MAX_STRING_LENGTH = safer.kStringMaxLength[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = safer[m
[1mdiff --git a/node_modules/safer-buffer/tests.js b/node_modules/safer-buffer/tests.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7ed2777[m
[1m--- /dev/null[m
[1m+++ b/node_modules/safer-buffer/tests.js[m
[36m@@ -0,0 +1,406 @@[m
[32m+[m[32m/* eslint-disable node/no-deprecated-api */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32mvar test = require('tape')[m
[32m+[m
[32m+[m[32mvar buffer = require('buffer')[m
[32m+[m
[32m+[m[32mvar index = require('./')[m
[32m+[m[32mvar safer = require('./safer')[m
[32m+[m[32mvar dangerous = require('./dangerous')[m
[32m+[m
[32m+[m[32m/* Inheritance tests */[m
[32m+[m
[32m+[m[32mtest('Default is Safer', function (t) {[m
[32m+[m[32m  t.equal(index, safer)[m
[32m+[m[32m  t.notEqual(safer, dangerous)[m
[32m+[m[32m  t.notEqual(index, dangerous)[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Is not a function', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(typeof impl, 'object')[m
[32m+[m[32m    t.equal(typeof impl.Buffer, 'object')[m
[32m+[m[32m  });[m
[32m+[m[32m  [buffer].forEach(function (impl) {[m
[32m+[m[32m    t.equal(typeof impl, 'object')[m
[32m+[m[32m    t.equal(typeof impl.Buffer, 'function')[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Constructor throws', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.throws(function () { impl.Buffer() })[m
[32m+[m[32m    t.throws(function () { impl.Buffer(0) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer('a') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer('a', 'utf-8') })[m
[32m+[m[32m    t.throws(function () { return new impl.Buffer() })[m
[32m+[m[32m    t.throws(function () { return new impl.Buffer(0) })[m
[32m+[m[32m    t.throws(function () { return new impl.Buffer('a') })[m
[32m+[m[32m    t.throws(function () { return new impl.Buffer('a', 'utf-8') })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Safe methods exist', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(typeof impl.Buffer.alloc, 'function', 'alloc')[m
[32m+[m[32m    t.equal(typeof impl.Buffer.from, 'function', 'from')[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Unsafe methods exist only in Dangerous', function (t) {[m
[32m+[m[32m  [index, safer].forEach(function (impl) {[m
[32m+[m[32m    t.equal(typeof impl.Buffer.allocUnsafe, 'undefined')[m
[32m+[m[32m    t.equal(typeof impl.Buffer.allocUnsafeSlow, 'undefined')[m
[32m+[m[32m  });[m
[32m+[m[32m  [dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(typeof impl.Buffer.allocUnsafe, 'function')[m
[32m+[m[32m    t.equal(typeof impl.Buffer.allocUnsafeSlow, 'function')[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Generic methods/properties are defined and equal', function (t) {[m
[32m+[m[32m  ['poolSize', 'isBuffer', 'concat', 'byteLength'].forEach(function (method) {[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl.Buffer[method], buffer.Buffer[method], method)[m
[32m+[m[32m      t.notEqual(typeof impl.Buffer[method], 'undefined', method)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Built-in buffer static methods/properties are inherited', function (t) {[m
[32m+[m[32m  Object.keys(buffer).forEach(function (method) {[m
[32m+[m[32m    if (method === 'SlowBuffer' || method === 'Buffer') return;[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl[method], buffer[method], method)[m
[32m+[m[32m      t.notEqual(typeof impl[method], 'undefined', method)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Built-in Buffer static methods/properties are inherited', function (t) {[m
[32m+[m[32m  Object.keys(buffer.Buffer).forEach(function (method) {[m
[32m+[m[32m    if (method === 'allocUnsafe' || method === 'allocUnsafeSlow') return;[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl.Buffer[method], buffer.Buffer[method], method)[m
[32m+[m[32m      t.notEqual(typeof impl.Buffer[method], 'undefined', method)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('.prototype property of Buffer is inherited', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(impl.Buffer.prototype, buffer.Buffer.prototype, 'prototype')[m
[32m+[m[32m    t.notEqual(typeof impl.Buffer.prototype, 'undefined', 'prototype')[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('All Safer methods are present in Dangerous', function (t) {[m
[32m+[m[32m  Object.keys(safer).forEach(function (method) {[m
[32m+[m[32m    if (method === 'Buffer') return;[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl[method], safer[method], method)[m
[32m+[m[32m      if (method !== 'kStringMaxLength') {[m
[32m+[m[32m        t.notEqual(typeof impl[method], 'undefined', method)[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  Object.keys(safer.Buffer).forEach(function (method) {[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl.Buffer[method], safer.Buffer[method], method)[m
[32m+[m[32m      t.notEqual(typeof impl.Buffer[method], 'undefined', method)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Safe methods from Dangerous methods are present in Safer', function (t) {[m
[32m+[m[32m  Object.keys(dangerous).forEach(function (method) {[m
[32m+[m[32m    if (method === 'Buffer') return;[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl[method], dangerous[method], method)[m
[32m+[m[32m      if (method !== 'kStringMaxLength') {[m
[32m+[m[32m        t.notEqual(typeof impl[method], 'undefined', method)[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  Object.keys(dangerous.Buffer).forEach(function (method) {[m
[32m+[m[32m    if (method === 'allocUnsafe' || method === 'allocUnsafeSlow') return;[m
[32m+[m[32m    [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m      t.equal(impl.Buffer[method], dangerous.Buffer[method], method)[m
[32m+[m[32m      t.notEqual(typeof impl.Buffer[method], 'undefined', method)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/* Behaviour tests */[m
[32m+[m
[32m+[m[32mtest('Methods return Buffers', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(0)))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(0, 10)))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(0, 'a')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(10)))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(10, 'x')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.alloc(9, 'ab')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from('')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from('string')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from('string', 'utf-8')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from('b25ldHdvdGhyZWU=', 'base64')))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from([0, 42, 3])))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from(new Uint8Array([0, 42, 3]))))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(impl.Buffer.from([])))[m
[32m+[m[32m  });[m
[32m+[m[32m  ['allocUnsafe', 'allocUnsafeSlow'].forEach(function (method) {[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(dangerous.Buffer[method](0)))[m
[32m+[m[32m    t.ok(buffer.Buffer.isBuffer(dangerous.Buffer[method](10)))[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Constructor is buffer.Buffer', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(0).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(0, 10).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(0, 'a').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(10).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(10, 'x').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(9, 'ab').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from('').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from('string').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from('string', 'utf-8').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from('b25ldHdvdGhyZWU=', 'base64').constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from([0, 42, 3]).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from(new Uint8Array([0, 42, 3])).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(impl.Buffer.from([]).constructor, buffer.Buffer)[m
[32m+[m[32m  });[m
[32m+[m[32m  [0, 10, 100].forEach(function (arg) {[m
[32m+[m[32m    t.equal(dangerous.Buffer.allocUnsafe(arg).constructor, buffer.Buffer)[m
[32m+[m[32m    t.equal(dangerous.Buffer.allocUnsafeSlow(arg).constructor, buffer.SlowBuffer(0).constructor)[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Invalid calls throw', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(0) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(10) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(10, 'utf-8') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from('string', 'invalid encoding') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(-10) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(1e90) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(Infinity) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(-Infinity) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(NaN) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(null) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from(undefined) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from() })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.from({}) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc('') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc('string') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc('string', 'utf-8') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc('b25ldHdvdGhyZWU=', 'base64') })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(-10) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(1e90) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(2 * (1 << 30)) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(Infinity) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(-Infinity) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(null) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc(undefined) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc() })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc([]) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc([0, 42, 3]) })[m
[32m+[m[32m    t.throws(function () { impl.Buffer.alloc({}) })[m
[32m+[m[32m  });[m
[32m+[m[32m  ['allocUnsafe', 'allocUnsafeSlow'].forEach(function (method) {[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]('') })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]('string') })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]('string', 'utf-8') })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method](2 * (1 << 30)) })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method](Infinity) })[m
[32m+[m[32m    if (dangerous.Buffer[method] === buffer.Buffer.allocUnsafe) {[m
[32m+[m[32m      t.skip('Skipping, older impl of allocUnsafe coerced negative sizes to 0')[m
[32m+[m[32m    } else {[m
[32m+[m[32m      t.throws(function () { dangerous.Buffer[method](-10) })[m
[32m+[m[32m      t.throws(function () { dangerous.Buffer[method](-1e90) })[m
[32m+[m[32m      t.throws(function () { dangerous.Buffer[method](-Infinity) })[m
[32m+[m[32m    }[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method](null) })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method](undefined) })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]() })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]([]) })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]([0, 42, 3]) })[m
[32m+[m[32m    t.throws(function () { dangerous.Buffer[method]({}) })[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Buffers have appropriate lengths', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(0).length, 0)[m
[32m+[m[32m    t.equal(impl.Buffer.alloc(10).length, 10)[m
[32m+[m[32m    t.equal(impl.Buffer.from('').length, 0)[m
[32m+[m[32m    t.equal(impl.Buffer.from('string').length, 6)[m
[32m+[m[32m    t.equal(impl.Buffer.from('string', 'utf-8').length, 6)[m
[32m+[m[32m    t.equal(impl.Buffer.from('b25ldHdvdGhyZWU=', 'base64').length, 11)[m
[32m+[m[32m    t.equal(impl.Buffer.from([0, 42, 3]).length, 3)[m
[32m+[m[32m    t.equal(impl.Buffer.from(new Uint8Array([0, 42, 3])).length, 3)[m
[32m+[m[32m    t.equal(impl.Buffer.from([]).length, 0)[m
[32m+[m[32m  });[m
[32m+[m[32m  ['allocUnsafe', 'allocUnsafeSlow'].forEach(function (method) {[m
[32m+[m[32m    t.equal(dangerous.Buffer[method](0).length, 0)[m
[32m+[m[32m    t.equal(dangerous.Buffer[method](10).length, 10)[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('Buffers have appropriate lengths (2)', function (t) {[m
[32m+[m[32m  t.equal(index.Buffer.alloc, safer.Buffer.alloc)[m
[32m+[m[32m  t.equal(index.Buffer.alloc, dangerous.Buffer.alloc)[m
[32m+[m[32m  var ok = true;[m
[32m+[m[32m  [ safer.Buffer.alloc,[m
[32m+[m[32m    dangerous.Buffer.allocUnsafe,[m
[32m+[m[32m    dangerous.Buffer.allocUnsafeSlow[m
[32m+[m[32m  ].forEach(function (method) {[m
[32m+[m[32m    for (var i = 0; i < 1e2; i++) {[m
[32m+[m[32m      var length = Math.round(Math.random() * 1e5)[m
[32m+[m[32m      var buf = method(length)[m
[32m+[m[32m      if (!buffer.Buffer.isBuffer(buf)) ok = false[m
[32m+[m[32m      if (buf.length !== length) ok = false[m
[32m+[m[32m    }[m
[32m+[m[32m  })[m
[32m+[m[32m  t.ok(ok)[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('.alloc(size) is zero-filled and has correct length', function (t) {[m
[32m+[m[32m  t.equal(index.Buffer.alloc, safer.Buffer.alloc)[m
[32m+[m[32m  t.equal(index.Buffer.alloc, dangerous.Buffer.alloc)[m
[32m+[m[32m  var ok = true[m
[32m+[m[32m  for (var i = 0; i < 1e2; i++) {[m
[32m+[m[32m    var length = Math.round(Math.random() * 2e6)[m
[32m+[m[32m    var buf = index.Buffer.alloc(length)[m
[32m+[m[32m    if (!buffer.Buffer.isBuffer(buf)) ok = false[m
[32m+[m[32m    if (buf.length !== length) ok = false[m
[32m+[m[32m    var j[m
[32m+[m[32m    for (j = 0; j < length; j++) {[m
[32m+[m[32m      if (buf[j] !== 0) ok = false[m
[32m+[m[32m    }[m
[32m+[m[32m    buf.fill(1)[m
[32m+[m[32m    for (j = 0; j < length; j++) {[m
[32m+[m[32m      if (buf[j] !== 1) ok = false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  t.ok(ok)[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('.allocUnsafe / .allocUnsafeSlow are fillable and have correct lengths', function (t) {[m
[32m+[m[32m  ['allocUnsafe', 'allocUnsafeSlow'].forEach(function (method) {[m
[32m+[m[32m    var ok = true[m
[32m+[m[32m    for (var i = 0; i < 1e2; i++) {[m
[32m+[m[32m      var length = Math.round(Math.random() * 2e6)[m
[32m+[m[32m      var buf = dangerous.Buffer[method](length)[m
[32m+[m[32m      if (!buffer.Buffer.isBuffer(buf)) ok = false[m
[32m+[m[32m      if (buf.length !== length) ok = false[m
[32m+[m[32m      buf.fill(0, 0, length)[m
[32m+[m[32m      var j[m
[32m+[m[32m      for (j = 0; j < length; j++) {[m
[32m+[m[32m        if (buf[j] !== 0) ok = false[m
[32m+[m[32m      }[m
[32m+[m[32m      buf.fill(1, 0, length)[m
[32m+[m[32m      for (j = 0; j < length; j++) {[m
[32m+[m[32m        if (buf[j] !== 1) ok = false[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    t.ok(ok, method)[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('.alloc(size, fill) is `fill`-filled', function (t) {[m
[32m+[m[32m  t.equal(index.Buffer.alloc, safer.Buffer.alloc)[m
[32m+[m[32m  t.equal(index.Buffer.alloc, dangerous.Buffer.alloc)[m
[32m+[m[32m  var ok = true[m
[32m+[m[32m  for (var i = 0; i < 1e2; i++) {[m
[32m+[m[32m    var length = Math.round(Math.random() * 2e6)[m
[32m+[m[32m    var fill = Math.round(Math.random() * 255)[m
[32m+[m[32m    var buf = index.Buffer.alloc(length, fill)[m
[32m+[m[32m    if (!buffer.Buffer.isBuffer(buf)) ok = false[m
[32m+[m[32m    if (buf.length !== length) ok = false[m
[32m+[m[32m    for (var j = 0; j < length; j++) {[m
[32m+[m[32m      if (buf[j] !== fill) ok = false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  t.ok(ok)[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('.alloc(size, fill) is `fill`-filled', function (t) {[m
[32m+[m[32m  t.equal(index.Buffer.alloc, safer.Buffer.alloc)[m
[32m+[m[32m  t.equal(index.Buffer.alloc, dangerous.Buffer.alloc)[m
[32m+[m[32m  var ok = true[m
[32m+[m[32m  for (var i = 0; i < 1e2; i++) {[m
[32m+[m[32m    var length = Math.round(Math.random() * 2e6)[m
[32m+[m[32m    var fill = Math.round(Math.random() * 255)[m
[32m+[m[32m    var buf = index.Buffer.alloc(length, fill)[m
[32m+[m[32m    if (!buffer.Buffer.isBuffer(buf)) ok = false[m
[32m+[m[32m    if (buf.length !== length) ok = false[m
[32m+[m[32m    for (var j = 0; j < length; j++) {[m
[32m+[m[32m      if (buf[j] !== fill) ok = false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  t.ok(ok)[m
[32m+[m[32m  t.deepEqual(index.Buffer.alloc(9, 'a'), index.Buffer.alloc(9, 97))[m
[32m+[m[32m  t.notDeepEqual(index.Buffer.alloc(9, 'a'), index.Buffer.alloc(9, 98))[m
[32m+[m
[32m+[m[32m  var tmp = new buffer.Buffer(2)[m
[32m+[m[32m  tmp.fill('ok')[m
[32m+[m[32m  if (tmp[1] === tmp[0]) {[m
[32m+[m[32m    // Outdated Node.js[m
[32m+[m[32m    t.deepEqual(index.Buffer.alloc(5, 'ok'), index.Buffer.from('ooooo'))[m
[32m+[m[32m  } else {[m
[32m+[m[32m    t.deepEqual(index.Buffer.alloc(5, 'ok'), index.Buffer.from('okoko'))[m
[32m+[m[32m  }[m
[32m+[m[32m  t.notDeepEqual(index.Buffer.alloc(5, 'ok'), index.Buffer.from('kokok'))[m
[32m+[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('safer.Buffer.from returns results same as Buffer constructor', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from(''), new buffer.Buffer(''))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('string'), new buffer.Buffer('string'))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('string', 'utf-8'), new buffer.Buffer('string', 'utf-8'))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('b25ldHdvdGhyZWU=', 'base64'), new buffer.Buffer('b25ldHdvdGhyZWU=', 'base64'))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from([0, 42, 3]), new buffer.Buffer([0, 42, 3]))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from(new Uint8Array([0, 42, 3])), new buffer.Buffer(new Uint8Array([0, 42, 3])))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from([]), new buffer.Buffer([]))[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mtest('safer.Buffer.from returns consistent results', function (t) {[m
[32m+[m[32m  [index, safer, dangerous].forEach(function (impl) {[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from(''), impl.Buffer.alloc(0))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from([]), impl.Buffer.alloc(0))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from(new Uint8Array([])), impl.Buffer.alloc(0))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('string', 'utf-8'), impl.Buffer.from('string'))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('string'), impl.Buffer.from([115, 116, 114, 105, 110, 103]))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('string'), impl.Buffer.from(impl.Buffer.from('string')))[m
[32m+[m[32m    t.deepEqual(impl.Buffer.from('b25ldHdvdGhyZWU=', 'base64'), impl.Buffer.from('onetwothree'))[m
[32m+[m[32m    t.notDeepEqual(impl.Buffer.from('b25ldHdvdGhyZWU='), impl.Buffer.from('onetwothree'))[m
[32m+[m[32m  })[m
[32m+[m[32m  t.end()[m
[32m+[m[32m})[m
