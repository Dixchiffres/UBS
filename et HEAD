[32m+[m[32mtest('simple object with indent', function (t) {[m
[32m+[m[32m    t.plan(2);[m
[32m+[m
[32m+[m[32m    var obj = { a: 1, b: 2 };[m
[32m+[m
[32m+[m[32m    var expectedSpaces = [[m
[32m+[m[32m        '{',[m
[32m+[m[32m        '  a: 1,',[m
[32m+[m[32m        '  b: 2',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedTabs = [[m
[32m+[m[32m        '{',[m
[32m+[m[32m        '	a: 1,',[m
[32m+[m[32m        '	b: 2',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(obj, { indent: 2 }), expectedSpaces, 'two');[m
[32m+[m[32m    t.equal(inspect(obj, { indent: '\t' }), expectedTabs, 'tabs');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('two deep object with indent', function (t) {[m
