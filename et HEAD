[32m+[m[32m# Security Policies and Procedures[m
[32m+[m
[32m+[m[32m## Reporting a Bug[m
[32m+[m
[32m+[m[32mThe `cookie` team and community take all security bugs seriously. Thank[m
[32m+[m[32myou for improving the security of the project. We appreciate your efforts and[m
[32m+[m[32mresponsible disclosure and will make every effort to acknowledge your[m
[32m+[m[32mcontributions.[m
[32m+[m
[32m+[m[32mReport security bugs by emailing the current owner(s) of `cookie`. This[m
[32m+[m[32minformation can be found in the npm registry using the command[m
[32m+[m[32m`npm owner ls cookie`.[m
[32m+[m[32mIf unsure or unable to get the information from the above, open an issue[m
[32m+[m[32min the [project issue tracker](https://github.com/jshttp/cookie/issues)[m
[32m+[m[32masking for the current contact information.[m
[32m+[m
[32m+[m[32mTo ensure the timely response to your report, please ensure that the entirety[m
[32m+[m[32mof the report is contained within the email body and not solely behind a web[m
[32m+[m[32mlink or an attachment.[m
[32m+[m
[32m+[m[32mAt least one owner will acknowledge your email within 48 hours, and will send a[m
[32m+[m[32mmore detailed response within 48 hours indicating the next steps in handling[m
[32m+[m[32myour report. After the initial reply to your report, the owners will[m
[32m+[m[32mendeavor to keep you informed of the progress towards a fix and full[m
[32m+[m[32mannouncement, and may ask for additional information or guidance.[m
[1mdiff --git a/node_modules/cookie/index.js b/node_modules/cookie/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..03d4c38[m
[1m--- /dev/null[m
[1m+++ b/node_modules/cookie/index.js[m
[36m@@ -0,0 +1,274 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * cookie[m
[32m+[m[32m * Copyright(c) 2012-2014 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.parse = parse;[m
[32m+[m[32mexports.serialize = serialize;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar __toString = Object.prototype.toString[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match field-content in RFC 7230 sec 3.2[m
[32m+[m[32m *[m
[32m+[m[32m * field-content = field-vchar [ 1*( SP / HTAB ) field-vchar ][m
[32m+[m[32m * field-vchar   = VCHAR / obs-text[m
[32m+[m[32m * obs-text      = %x80-FF[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar fieldContentRegExp = /^[\u0009\u0020-\u007e\u0080-\u00ff]+$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a cookie header.[m
[32m+[m[32m *[m
[32m+[m[32m * Parse the given cookie header string into an object[m
[32m+[m[32m * The object has the various cookies as keys(names) => values[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {object}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse(str, options) {[m
[32m+[m[32m  if (typeof str !== 'string') {[m
[32m+[m[32m    throw new TypeError('argument str must be a string');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var obj = {}[m
[32m+[m[32m  var opt = options || {};[m
[32m+[m[32m  var dec = opt.decode || decode;[m
[32m+[m
[32m+[m[32m  var index = 0[m
[32m+[m[32m  while (index < str.length) {[m
[32m+[m[32m    var eqIdx = str.indexOf('=', index)[m
[32m+[m
[32m+[m[32m    // no more cookie pairs[m
[32m+[m[32m    if (eqIdx === -1) {[m
[32m+[m[32m      break[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var endIdx = str.indexOf(';', index)[m
[32m+[m
[32m+[m[32m    if (endIdx === -1) {[m
[32m+[m[32m      endIdx = str.length[m
[32m+[m[32m    } else if (endIdx < eqIdx) {[m
[32m+[m[32m      // backtrack on prior semicolon[m
[32m+[m[32m      index = str.lastIndexOf(';', eqIdx - 1) + 1[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var key = str.slice(index, eqIdx).trim()[m
[32m+[m
[32m+[m[32m    // only assign once[m
[32m+[m[32m    if (undefined === obj[key]) {[m
[32m+[m[32m      var val = str.slice(eqIdx + 1, endIdx).trim()[m
[32m+[m
[32m+[m[32m      // quoted values[m
[32m+[m[32m      if (val.charCodeAt(0) === 0x22) {[m
[32m+[m[32m        val = val.slice(1, -1)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      obj[key] = tryDecode(val, dec);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    index = endIdx + 1[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return obj;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Serialize data into a cookie header.[m
[32m+[m[32m *[m
[32m+[m[32m * Serialize the a name value pair into a cookie string suitable for[m
[32m+[m[32m * http headers. An optional options object specified cookie parameters.[m
[32m+[m[32m *[m
[32m+[m[32m * serialize('foo', 'bar', { httpOnly: true })[m
[32m+[m[32m *   => "foo=bar; httpOnly"[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} name[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction serialize(name, val, options) {[m
[32m+[m[32m  var opt = options || {};[m
[32m+[m[32m  var enc = opt.encode || encode;[m
[32m+[m
[32m+[m[32m  if (typeof enc !== 'function') {[m
[32m+[m[32m    throw new TypeError('option encode is invalid');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!fieldContentRegExp.test(name)) {[m
[32m+[m[32m    throw new TypeError('argument name is invalid');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var value = enc(val);[m
[32m+[m
[32m+[m[32m  if (value && !fieldContentRegExp.test(value)) {[m
[32m+[m[32m    throw new TypeError('argument val is invalid');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var str = name + '=' + value;[m
[32m+[m
[32m+[m[32m  if (null != opt.maxAge) {[m
[32m+[m[32m    var maxAge = opt.maxAge - 0;[m
[32m+[m
[32m+[m[32m    if (isNaN(maxAge) || !isFinite(maxAge)) {[m
[32m+[m[32m      throw new TypeError('option maxAge is invalid')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    str += '; Max-Age=' + Math.floor(maxAge);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.domain) {[m
[32m+[m[32m    if (!fieldContentRegExp.test(opt.domain)) {[m
[32m+[m[32m      throw new TypeError('option domain is invalid');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    str += '; Domain=' + opt.domain;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.path) {[m
[32m+[m[32m    if (!fieldContentRegExp.test(opt.path)) {[m
[32m+[m[32m      throw new TypeError('option path is invalid');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    str += '; Path=' + opt.path;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.expires) {[m
[32m+[m[32m    var expires = opt.expires[m
[32m+[m
[32m+[m[32m    if (!isDate(expires) || isNaN(expires.valueOf())) {[m
[32m+[m[32m      throw new TypeError('option expires is invalid');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    str += '; Expires=' + expires.toUTCString()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.httpOnly) {[m
[32m+[m[32m    str += '; HttpOnly';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.secure) {[m
[32m+[m[32m    str += '; Secure';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.partitioned) {[m
[32m+[m[32m    str += '; Partitioned'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.priority) {[m
[32m+[m[32m    var priority = typeof opt.priority === 'string'[m
[32m+[m[32m      ? opt.priority.toLowerCase()[m
[32m+[m[32m      : opt.priority[m
[32m+[m
[32m+[m[32m    switch (priority) {[m
[32m+[m[32m      case 'low':[m
[32m+[m[32m        str += '; Priority=Low'[m
[32m+[m[32m        break[m
[32m+[m[32m      case 'medium':[m
[32m+[m[32m        str += '; Priority=Medium'[m
[32m+[m[32m        break[m
[32m+[m[32m      case 'high':[m
[32m+[m[32m        str += '; Priority=High'[m
[32m+[m[32m        break[m
[32m+[m[32m      default:[m
[32m+[m[32m        throw new TypeError('option priority is invalid')[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opt.sameSite) {[m
[32m+[m[32m    var sameSite = typeof opt.sameSite === 'string'[m
[32m+[m[32m      ? opt.sameSite.toLowerCase() : opt.sameSite;[m
[32m+[m
[32m+[m[32m    switch (sameSite) {[m
[32m+[m[32m      case true:[m
[32m+[m[32m        str += '; SameSite=Strict';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'lax':[m
[32m+[m[32m        str += '; SameSite=Lax';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'strict':[m
[32m+[m[32m        str += '; SameSite=Strict';[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'none':[m
[32m+[m[32m        str += '; SameSite=None';[m
[32m+[m[32m        break;[m
[32m+[m[32m      default:[m
[32m+[m[32m        throw new TypeError('option sameSite is invalid');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return str;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * URL-decode string value. Optimized to skip native call when no %.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction decode (str) {[m
[32m+[m[32m  return str.indexOf('%') !== -1[m
[32m+[m[32m    ? decodeURIComponent(str)[m
[32m+[m[32m    : str[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * URL-encode value.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction encode (val) {[m
[32m+[m[32m  return encodeURIComponent(val)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if value is a Date.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {*} val[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isDate (val) {[m
[32m+[m[32m  return __toString.call(val) === '[object Date]' ||[m
[32m+[m[32m    val instanceof Date[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Try decoding a string using a decoding function.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @param {function} decode[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction tryDecode(str, decode) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return decode(str);[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return str;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
