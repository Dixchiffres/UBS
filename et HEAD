[36m@@ -0,0 +1,50 @@[m
[32m+[m[32m# get Makefile directory name: http://stackoverflow.com/a/5982798/376773[m
[32m+[m[32mTHIS_MAKEFILE_PATH:=$(word $(words $(MAKEFILE_LIST)),$(MAKEFILE_LIST))[m
[32m+[m[32mTHIS_DIR:=$(shell cd $(dir $(THIS_MAKEFILE_PATH));pwd)[m
[32m+[m
[32m+[m[32m# BIN directory[m
[32m+[m[32mBIN := $(THIS_DIR)/node_modules/.bin[m
[32m+[m
[32m+[m[32m# Path[m
[32m+[m[32mPATH := node_modules/.bin:$(PATH)[m
[32m+[m[32mSHELL := /bin/bash[m
[32m+[m
[32m+[m[32m# applications[m
[32m+[m[32mNODE ?= $(shell which node)[m
[32m+[m[32mYARN ?= $(shell which yarn)[m
[32m+[m[32mPKG ?= $(if $(YARN),$(YARN),$(NODE) $(shell which npm))[m
[32m+[m[32mBROWSERIFY ?= $(NODE) $(BIN)/browserify[m
[32m+[m
[32m+[m[32m.FORCE:[m
[32m+[m
[32m+[m[32minstall: node_modules[m
[32m+[m
[32m+[m[32mnode_modules: package.json[m
[32m+[m	[32m@NODE_ENV= $(PKG) install[m
[32m+[m	[32m@touch node_modules[m
[32m+[m
[32m+[m[32mlint: .FORCE[m
[32m+[m	[32meslint browser.js debug.js index.js node.js[m
[32m+[m
[32m+[m[32mtest-node: .FORCE[m
[32m+[m	[32mistanbul cover node_modules/mocha/bin/_mocha -- test/**.js[m
[32m+[m
[32m+[m[32mtest-browser: .FORCE[m
[32m+[m	[32mmkdir -p dist[m
[32m+[m
[32m+[m	[32m@$(BROWSERIFY) \[m
[32m+[m		[32m--standalone debug \[m
[32m+[m		[32m. > dist/debug.js[m
[32m+[m
[32m+[m	[32mkarma start --single-run[m
[32m+[m	[32mrimraf dist[m
[32m+[m
[32m+[m[32mtest: .FORCE[m
[32m+[m	[32mconcurrently \[m
[32m+[m		[32m"make test-node" \[m
[32m+[m		[32m"make test-browser"[m
[32m+[m
[32m+[m[32mcoveralls:[m
[32m+[m	[32mcat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js[m
[32m+[m
[32m+[m[32m.PHONY: all install clean distclean[m
