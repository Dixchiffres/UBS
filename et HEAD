[36m@@ -0,0 +1,2 @@[m
[32m+[m[32m# side-channel[m
[32m+[m[32mStore information about any JS value in a side channel. Uses WeakMap if available.[m
[1mdiff --git a/node_modules/side-channel/index.d.ts b/node_modules/side-channel/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..7cb112b[m
[1m--- /dev/null[m
[1m+++ b/node_modules/side-channel/index.d.ts[m
[36m@@ -0,0 +1,27 @@[m
[32m+[m[32mdeclare namespace getSideChannel {[m
[32m+[m	[32mtype Key = unknown;[m
[32m+[m	[32mtype ListNode<T> = {[m
[32m+[m		[32mkey: Key;[m
[32m+[m		[32mnext: ListNode<T>;[m
[32m+[m		[32mvalue: T;[m
[32m+[m	[32m};[m
[32m+[m	[32mtype RootNode<T> = {[m
[32m+[m		[32mkey: object;[m
[32m+[m		[32mnext: null | ListNode<T>;[m
[32m+[m	[32m};[m
[32m+[m	[32mfunction listGetNode<T>(list: RootNode<T>, key: ListNode<T>['key']): ListNode<T> | void;[m
[32m+[m	[32mfunction listGet<T>(objects: RootNode<T>, key: ListNode<T>['key']): T | void;[m
[32m+[m	[32mfunction listSet<T>(objects: RootNode<T>, key: ListNode<T>['key'], value: T): void;[m
[32m+[m	[32mfunction listHas<T>(objects: RootNode<T>, key: ListNode<T>['key']): boolean;[m
[32m+[m
[32m+[m	[32mtype Channel = {[m
[32m+[m		[32massert: (key: Key) => void;[m
[32m+[m		[32mhas: (key: Key) => boolean;[m
[32m+[m		[32mget: <T>(key: Key) => T;[m
[32m+[m		[32mset: <T>(key: Key, value: T) => void;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdeclare function getSideChannel(): getSideChannel.Channel;[m
[32m+[m
[32m+[m[32mexport = getSideChannel;[m
[1mdiff --git a/node_modules/side-channel/index.js b/node_modules/side-channel/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6b6926e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/side-channel/index.js[m
[36m@@ -0,0 +1,129 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m[32mvar callBound = require('call-bind/callBound');[m
[32m+[m[32mvar inspect = require('object-inspect');[m
[32m+[m
[32m+[m[32mvar $TypeError = require('es-errors/type');[m
[32m+[m[32mvar $WeakMap = GetIntrinsic('%WeakMap%', true);[m
[32m+[m[32mvar $Map = GetIntrinsic('%Map%', true);[m
[32m+[m
[32m+[m[32mvar $weakMapGet = callBound('WeakMap.prototype.get', true);[m
[32m+[m[32mvar $weakMapSet = callBound('WeakMap.prototype.set', true);[m
[32m+[m[32mvar $weakMapHas = callBound('WeakMap.prototype.has', true);[m
[32m+[m[32mvar $mapGet = callBound('Map.prototype.get', true);[m
[32m+[m[32mvar $mapSet = callBound('Map.prototype.set', true);[m
[32m+[m[32mvar $mapHas = callBound('Map.prototype.has', true);[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m* This function traverses the list returning the node corresponding to the given key.[m
[32m+[m[32m*[m
[32m+[m[32m* That node is also moved to the head of the list, so that if it's accessed again we don't need to traverse the whole list. By doing so, all the recently used nodes can be accessed relatively quickly.[m
[32m+[m[32m*/[m
[32m+[m[32m/** @type {import('.').listGetNode} */[m
[32m+[m[32mvar listGetNode = function (list, key) { // eslint-disable-line consistent-return[m
[32m+[m	[32m/** @type {typeof list | NonNullable<(typeof list)['next']>} */[m
[32m+[m	[32mvar prev = list;[m
[32m+[m	[32m/** @type {(typeof list)['next']} */[m
[32m+[m	[32mvar curr;[m
[32m+[m	[32mfor (; (curr = prev.next) !== null; prev = curr) {[m
[32m+[m		[32mif (curr.key === key) {[m
[32m+[m			[32mprev.next = curr.next;[m
[32m+[m			[32m// eslint-disable-next-line no-extra-parens[m
[32m+[m			[32mcurr.next = /** @type {NonNullable<typeof list.next>} */ (list.next);[m
[32m+[m			[32mlist.next = curr; // eslint-disable-line no-param-reassign[m
[32m+[m			[32mreturn curr;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/** @type {import('.').listGet} */[m
[32m+[m[32mvar listGet = function (objects, key) {[m
[32m+[m	[32mvar node = listGetNode(objects, key);[m
[32m+[m	[32mreturn node && node.value;[m
[32m+[m[32m};[m
[32m+[m[32m/** @type {import('.').listSet} */[m
[32m+[m[32mvar listSet = function (objects, key, value) {[m
[32m+[m	[32mvar node = listGetNode(objects, key);[m
[32m+[m	[32mif (node) {[m
[32m+[m		[32mnode.value = value;[m
[32m+[m	[32m} else {[m
[32m+[m		[32m// Prepend the new node to the beginning of the list[m
[32m+[m		[32mobjects.next = /** @type {import('.').ListNode<typeof value>} */ ({ // eslint-disable-line no-param-reassign, no-extra-parens[m
[32m+[m			[32mkey: key,[m
[32m+[m			[32mnext: objects.next,[m
[32m+[m			[32mvalue: value[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m[32m/** @type {import('.').listHas} */[m
[32m+[m[32mvar listHas = function (objects, key) {[m
[32m+[m	[32mreturn !!listGetNode(objects, key);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mmodule.exports = function getSideChannel() {[m
[32m+[m	[32m/** @type {WeakMap<object, unknown>} */ var $wm;[m
[32m+[m	[32m/** @type {Map<object, unknown>} */ var $m;[m
[32m+[m	[32m/** @type {import('.').RootNode<unknown>} */ var $o;[m
[32m+[m
[32m+[m	[32m/** @type {import('.').Channel} */[m
[32m+[m	[32mvar channel = {[m
[32m+[m		[32massert: function (key) {[m
[32m+[m			[32mif (!channel.has(key)) {[m
[32m+[m				[32mthrow new $TypeError('Side channel does not contain ' + inspect(key));[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mget: function (key) { // eslint-disable-line consistent-return[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif ($wm) {[m
[32m+[m					[32mreturn $weakMapGet($wm, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif ($m) {[m
[32m+[m					[32mreturn $mapGet($m, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif ($o) { // eslint-disable-line no-lonely-if[m
[32m+[m					[32mreturn listGet($o, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mhas: function (key) {[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif ($wm) {[m
[32m+[m					[32mreturn $weakMapHas($wm, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif ($m) {[m
[32m+[m					[32mreturn $mapHas($m, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif ($o) { // eslint-disable-line no-lonely-if[m
[32m+[m					[32mreturn listHas($o, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m},[m
[32m+[m		[32mset: function (key, value) {[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif (!$wm) {[m
[32m+[m					[32m$wm = new $WeakMap();[m
[32m+[m				[32m}[m
[32m+[m				[32m$weakMapSet($wm, key, value);[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif (!$m) {[m
[32m+[m					[32m$m = new $Map();[m
[32m+[m				[32m}[m
[32m+[m				[32m$mapSet($m, key, value);[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif (!$o) {[m
[32m+[m					[32m// Initialize the linked list as an empty node, so that we don't have to special-case handling of the first node: we can always refer to it as (previous node).next, instead of something like (list).head[m
[32m+[m					[32m$o = { key: {}, next: null };[m
[32m+[m				[32m}[m
[32m+[m				[32mlistSet($o, key, value);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m	[32mreturn channel;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/side-channel/package.json b/node_modules/side-channel/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..02cffca[m
[1m--- /dev/null[m
[1m+++ b/node_modules/side-channel/package.json[m
[36m@@ -0,0 +1,84 @@[m
[32m+[m[32m{[m
[32m+[m	[32m"name": "side-channel",[m
[32m+[m	[32m"version": "1.0.6",[m
[32m+[m	[32m"description": "Store information about any JS value in a side channel. Uses WeakMap if available.",[m
[32m+[m	[32m"main": "index.js",[m
[32m+[m	[32m"exports": {[m
