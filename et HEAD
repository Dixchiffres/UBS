[32m+[m	[32mt.equal(hasSymbolsAfter, true, 'hasSymbols is true after polyfilling');[m
[32m+[m	[32m/* eslint-enable global-require */[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
