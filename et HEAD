[32m+[m[32mvar Buffer = require('safer-buffer').Buffer;[m
[32m+[m
[32m+[m[32mvar holes = ['a', 'b'];[m
[32m+[m[32mholes[4] = 'e';[m
[32m+[m[32mholes[6] = 'g';[m
