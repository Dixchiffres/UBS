[1mdiff --git a/node_modules/negotiator/README.md b/node_modules/negotiator/README.md[m
[1mnew file mode 100644[m
[1mindex 0000000..82915e5[m
[1m--- /dev/null[m
[1m+++ b/node_modules/negotiator/README.md[m
[36m@@ -0,0 +1,203 @@[m
[32m+[m[32m# negotiator[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Node.js Version][node-version-image]][node-version-url][m
[32m+[m[32m[![Build Status][github-actions-ci-image]][github-actions-ci-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mAn HTTP content negotiator for Node.js[m
[32m+[m
[32m+[m[32m## Installation[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install negotiator[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar Negotiator = require('negotiator')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Accept Negotiation[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mavailableMediaTypes = ['text/html', 'text/plain', 'application/json'][m
[32m+[m
[32m+[m[32m// The negotiator constructor receives a request object[m
[32m+[m[32mnegotiator = new Negotiator(request)[m
[32m+[m
[32m+[m[32m// Let's say Accept header is 'text/html, application/*;q=0.2, image/jpeg;q=0.8'[m
[32m+[m
[32m+[m[32mnegotiator.mediaTypes()[m
[32m+[m[32m// -> ['text/html', 'image/jpeg', 'application/*'][m
[32m+[m
[32m+[m[32mnegotiator.mediaTypes(availableMediaTypes)[m
[32m+[m[32m// -> ['text/html', 'application/json'][m
[32m+[m
[32m+[m[32mnegotiator.mediaType(availableMediaTypes)[m
[32m+[m[32m// -> 'text/html'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mYou can check a working example at `examples/accept.js`.[m
[32m+[m
[32m+[m[32m#### Methods[m
[32m+[m
[32m+[m[32m##### mediaType()[m
[32m+[m
[32m+[m[32mReturns the most preferred media type from the client.[m
[32m+[m
[32m+[m[32m##### mediaType(availableMediaType)[m
[32m+[m
[32m+[m[32mReturns the most preferred media type from a list of available media types.[m
[32m+[m
[32m+[m[32m##### mediaTypes()[m
[32m+[m
[32m+[m[32mReturns an array of preferred media types ordered by the client preference.[m
[32m+[m
[32m+[m[32m##### mediaTypes(availableMediaTypes)[m
[32m+[m
[32m+[m[32mReturns an array of preferred media types ordered by priority from a list of[m
[32m+[m[32mavailable media types.[m
[32m+[m
[32m+[m[32m### Accept-Language Negotiation[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mnegotiator = new Negotiator(request)[m
[32m+[m
[32m+[m[32mavailableLanguages = ['en', 'es', 'fr'][m
[32m+[m
[32m+[m[32m// Let's say Accept-Language header is 'en;q=0.8, es, pt'[m
[32m+[m
[32m+[m[32mnegotiator.languages()[m
[32m+[m[32m// -> ['es', 'pt', 'en'][m
[32m+[m
[32m+[m[32mnegotiator.languages(availableLanguages)[m
[32m+[m[32m// -> ['es', 'en'][m
[32m+[m
[32m+[m[32mlanguage = negotiator.language(availableLanguages)[m
[32m+[m[32m// -> 'es'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mYou can check a working example at `examples/language.js`.[m
[32m+[m
[32m+[m[32m#### Methods[m
[32m+[m
[32m+[m[32m##### language()[m
[32m+[m
[32m+[m[32mReturns the most preferred language from the client.[m
[32m+[m
[32m+[m[32m##### language(availableLanguages)[m
[32m+[m
[32m+[m[32mReturns the most preferred language from a list of available languages.[m
[32m+[m
[32m+[m[32m##### languages()[m
[32m+[m
[32m+[m[32mReturns an array of preferred languages ordered by the client preference.[m
[32m+[m
[32m+[m[32m##### languages(availableLanguages)[m
[32m+[m
[32m+[m[32mReturns an array of preferred languages ordered by priority from a list of[m
[32m+[m[32mavailable languages.[m
[32m+[m
[32m+[m[32m### Accept-Charset Negotiation[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mavailableCharsets = ['utf-8', 'iso-8859-1', 'iso-8859-5'][m
[32m+[m
[32m+[m[32mnegotiator = new Negotiator(request)[m
[32m+[m
[32m+[m[32m// Let's say Accept-Charset header is 'utf-8, iso-8859-1;q=0.8, utf-7;q=0.2'[m
[32m+[m
[32m+[m[32mnegotiator.charsets()[m
[32m+[m[32m// -> ['utf-8', 'iso-8859-1', 'utf-7'][m
[32m+[m
[32m+[m[32mnegotiator.charsets(availableCharsets)[m
[32m+[m[32m// -> ['utf-8', 'iso-8859-1'][m
[32m+[m
[32m+[m[32mnegotiator.charset(availableCharsets)[m
[32m+[m[32m// -> 'utf-8'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mYou can check a working example at `examples/charset.js`.[m
[32m+[m
[32m+[m[32m#### Methods[m
[32m+[m
[32m+[m[32m##### charset()[m
[32m+[m
[32m+[m[32mReturns the most preferred charset from the client.[m
[32m+[m
[32m+[m[32m##### charset(availableCharsets)[m
[32m+[m
[32m+[m[32mReturns the most preferred charset from a list of available charsets.[m
[32m+[m
[32m+[m[32m##### charsets()[m
[32m+[m
[32m+[m[32mReturns an array of preferred charsets ordered by the client preference.[m
[32m+[m
[32m+[m[32m##### charsets(availableCharsets)[m
[32m+[m
[32m+[m[32mReturns an array of preferred charsets ordered by priority from a list of[m
[32m+[m[32mavailable charsets.[m
[32m+[m
[32m+[m[32m### Accept-Encoding Negotiation[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mavailableEncodings = ['identity', 'gzip'][m
[32m+[m
[32m+[m[32mnegotiator = new Negotiator(request)[m
[32m+[m
[32m+[m[32m// Let's say Accept-Encoding header is 'gzip, compress;q=0.2, identity;q=0.5'[m
[32m+[m
[32m+[m[32mnegotiator.encodings()[m
[32m+[m[32m// -> ['gzip', 'identity', 'compress'][m
[32m+[m
[32m+[m[32mnegotiator.encodings(availableEncodings)[m
[32m+[m[32m// -> ['gzip', 'identity'][m
[32m+[m
[32m+[m[32mnegotiator.encoding(availableEncodings)[m
[32m+[m[32m// -> 'gzip'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mYou can check a working example at `examples/encoding.js`.[m
[32m+[m
[32m+[m[32m#### Methods[m
[32m+[m
[32m+[m[32m##### encoding()[m
[32m+[m
[32m+[m[32mReturns the most preferred encoding from the client.[m
[32m+[m
[32m+[m[32m##### encoding(availableEncodings)[m
[32m+[m
[32m+[m[32mReturns the most preferred encoding from a list of available encodings.[m
[32m+[m
[32m+[m[32m##### encodings()[m
[32m+[m
[32m+[m[32mReturns an array of preferred encodings ordered by the client preference.[m
[32m+[m
[32m+[m[32m##### encodings(availableEncodings)[m
[32m+[m
[32m+[m[32mReturns an array of preferred encodings ordered by priority from a list of[m
[32m+[m[32mavailable encodings.[m
[32m+[m
[32m+[m[32m## See Also[m
[32m+[m
[32m+[m[32mThe [accepts](https://npmjs.org/package/accepts#readme) module builds on[m
[32m+[m[32mthis module and provides an alternative interface, mime type validation,[m
[32m+[m[32mand more.[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
