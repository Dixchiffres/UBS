[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Sets', function (t) {[m
[32m+[m[32m    t.equal(inspect(new Set([[1, 2]])), 'Set (1) {[ 1, 2 ]}');[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('WeakSets', function (t) {[m
[32m+[m[32m    t.equal(inspect(new WeakSet([[1, 2]])), 'WeakSet { ? }');[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/bigint.js b/node_modules/object-inspect/test/bigint.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4ecc31d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/test/bigint.js[m
[36m@@ -0,0 +1,58 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar hasToStringTag = require('has-tostringtag/shams')();[m
[32m+[m
[32m+[m[32mtest('bigint', { skip: typeof BigInt === 'undefined' }, function (t) {[m
[32m+[m[32m    t.test('primitives', function (st) {[m
[32m+[m[32m        st.plan(3);[m
[32m+[m
[32m+[m[32m        st.equal(inspect(BigInt(-256)), '-256n');[m
[32m+[m[32m        st.equal(inspect(BigInt(0)), '0n');[m
[32m+[m[32m        st.equal(inspect(BigInt(256)), '256n');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('objects', function (st) {[m
[32m+[m[32m        st.plan(3);[m
[32m+[m
[32m+[m[32m        st.equal(inspect(Object(BigInt(-256))), 'Object(-256n)');[m
[32m+[m[32m        st.equal(inspect(Object(BigInt(0))), 'Object(0n)');[m
[32m+[m[32m        st.equal(inspect(Object(BigInt(256))), 'Object(256n)');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('syntactic primitives', function (st) {[m
[32m+[m[32m        st.plan(3);[m
[32m+[m
[32m+[m[32m        /* eslint-disable no-new-func */[m
[32m+[m[32m        st.equal(inspect(Function('return -256n')()), '-256n');[m
[32m+[m[32m        st.equal(inspect(Function('return 0n')()), '0n');[m
[32m+[m[32m        st.equal(inspect(Function('return 256n')()), '256n');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('toStringTag', { skip: !hasToStringTag }, function (st) {[m
[32m+[m[32m        st.plan(1);[m
[32m+[m
[32m+[m[32m        var faker = {};[m
[32m+[m[32m        faker[Symbol.toStringTag] = 'BigInt';[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            inspect(faker),[m
[32m+[m[32m            '{ [Symbol(Symbol.toStringTag)]: \'BigInt\' }',[m
[32m+[m[32m            'object lying about being a BigInt inspects as an object'[m
[32m+[m[32m        );[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('numericSeparator', function (st) {[m
[32m+[m[32m        st.equal(inspect(BigInt(0), { numericSeparator: false }), '0n', '0n, numericSeparator false');[m
[32m+[m[32m        st.equal(inspect(BigInt(0), { numericSeparator: true }), '0n', '0n, numericSeparator true');[m
[32m+[m
[32m+[m[32m        st.equal(inspect(BigInt(1234), { numericSeparator: false }), '1234n', '1234n, numericSeparator false');[m
[32m+[m[32m        st.equal(inspect(BigInt(1234), { numericSeparator: true }), '1_234n', '1234n, numericSeparator true');[m
[32m+[m[32m        st.equal(inspect(BigInt(-1234), { numericSeparator: false }), '-1234n', '1234n, numericSeparator false');[m
[32m+[m[32m        st.equal(inspect(BigInt(-1234), { numericSeparator: true }), '-1_234n', '1234n, numericSeparator true');[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
