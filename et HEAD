[32m+[m[32mvar forEach = require('for-each');[m
[32m+[m[32mvar inspect = require('object-inspect');[m
[32m+[m[32mvar hasOwn = require('hasown');[m
[32m+[m[32mvar hasPropertyDescriptors = require('has-property-descriptors')();[m
[32m+[m[32mvar getOwnPropertyDescriptors = require('object.getownpropertydescriptors');[m
