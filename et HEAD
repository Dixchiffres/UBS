[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32m// Update this array if you add/rename/remove files in this directory.[m
[32m+[m[32m// We support Browserify by skipping automatic module discovery and requiring modules directly.[m
[32m+[m[32mvar modules = [[m
[32m+[m[32m    require("./internal"),[m
[32m+[m[32m    require("./utf16"),[m
[32m+[m[32m    require("./utf7"),[m
[32m+[m[32m    require("./sbcs-codec"),[m
[32m+[m[32m    require("./sbcs-data"),[m
