[1mnew file mode 100644[m
[1mindex 0000000..970ea53[m
[1m--- /dev/null[m
[1m+++ b/node_modules/set-function-length/env.d.ts[m
[36m@@ -0,0 +1,9 @@[m
[32m+[m[32mdeclare const env: {[m
[32m+[m	[32m__proto__: null,[m
[32m+[m	[32mboundFnsHaveConfigurableLengths: boolean;[m
[32m+[m	[32mboundFnsHaveWritableLengths: boolean;[m
[32m+[m	[32mfunctionsHaveConfigurableLengths: boolean;[m
[32m+[m	[32mfunctionsHaveWritableLengths: boolean;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport = env;[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/set-function-length/env.js b/node_modules/set-function-length/env.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d9b0a29[m
[1m--- /dev/null[m
[1m+++ b/node_modules/set-function-length/env.js[m
[36m@@ -0,0 +1,25 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar gOPD = require('gopd');[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m
[32m+[m[32mvar unbound = gOPD && gOPD(function () {}, 'length');[m
[32m+[m[32m// @ts-expect-error ts(2555) TS is overly strict with .call[m
[32m+[m[32mvar bound = gOPD && gOPD(bind.call(function () {}), 'length');[m
[32m+[m
[32m+[m[32mvar functionsHaveConfigurableLengths = !!(unbound && unbound.configurable);[m
[32m+[m
[32m+[m[32mvar functionsHaveWritableLengths = !!(unbound && unbound.writable);[m
[32m+[m
[32m+[m[32mvar boundFnsHaveConfigurableLengths = !!(bound && bound.configurable);[m
[32m+[m
[32m+[m[32mvar boundFnsHaveWritableLengths = !!(bound && bound.writable);[m
[32m+[m
[32m+[m[32m/** @type {import('./env')} */[m
[32m+[m[32mmodule.exports = {[m
[32m+[m	[32m__proto__: null,[m
[32m+[m	[32mboundFnsHaveConfigurableLengths: boundFnsHaveConfigurableLengths,[m
[32m+[m	[32mboundFnsHaveWritableLengths: boundFnsHaveWritableLengths,[m
[32m+[m	[32mfunctionsHaveConfigurableLengths: functionsHaveConfigurableLengths,[m
[32m+[m	[32mfunctionsHaveWritableLengths: functionsHaveWritableLengths[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/set-function-length/index.d.ts b/node_modules/set-function-length/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..0451ecd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/set-function-length/index.d.ts[m
[36m@@ -0,0 +1,7 @@[m
[32m+[m[32mdeclare namespace setFunctionLength {[m
[32m+[m[32m    type Func = (...args: unknown[]) => unknown;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdeclare function setFunctionLength<T extends setFunctionLength.Func = setFunctionLength.Func>(fn: T, length: number, loose?: boolean): T;[m
[32m+[m
[32m+[m[32mexport = setFunctionLength;[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/set-function-length/index.js b/node_modules/set-function-length/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..14ce74d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/set-function-length/index.js[m
[36m@@ -0,0 +1,42 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m[32mvar define = require('define-data-property');[m
[32m+[m[32mvar hasDescriptors = require('has-property-descriptors')();[m
[32m+[m[32mvar gOPD = require('gopd');[m
[32m+[m
[32m+[m[32mvar $TypeError = require('es-errors/type');[m
[32m+[m[32mvar $floor = GetIntrinsic('%Math.floor%');[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mmodule.exports = function setFunctionLength(fn, length) {[m
[32m+[m	[32mif (typeof fn !== 'function') {[m
[32m+[m		[32mthrow new $TypeError('`fn` is not a function');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (typeof length !== 'number' || length < 0 || length > 0xFFFFFFFF || $floor(length) !== length) {[m
[32m+[m		[32mthrow new $TypeError('`length` must be a positive 32-bit integer');[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar loose = arguments.length > 2 && !!arguments[2];[m
[32m+[m
[32m+[m	[32mvar functionLengthIsConfigurable = true;[m
[32m+[m	[32mvar functionLengthIsWritable = true;[m
[32m+[m	[32mif ('length' in fn && gOPD) {[m
[32m+[m		[32mvar desc = gOPD(fn, 'length');[m
[32m+[m		[32mif (desc && !desc.configurable) {[m
[32m+[m			[32mfunctionLengthIsConfigurable = false;[m
[32m+[m		[32m}[m
[32m+[m		[32mif (desc && !desc.writable) {[m
[32m+[m			[32mfunctionLengthIsWritable = false;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif (functionLengthIsConfigurable || functionLengthIsWritable || !loose) {[m
[32m+[m		[32mif (hasDescriptors) {[m
[32m+[m			[32mdefine(/** @type {Parameters<define>[0]} */ (fn), 'length', length, true, true);[m
[32m+[m		[32m} else {[m
[32m+[m			[32mdefine(/** @type {Parameters<define>[0]} */ (fn), 'length', length);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn fn;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/set-function-length/package.json b/node_modules/set-function-length/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..f6b8881[m
[1m--- /dev/null[m
[1m+++ b/node_modules/set-function-length/package.json[m
