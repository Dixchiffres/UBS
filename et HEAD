[1mdiff --git a/node_modules/has-symbols/shams.js b/node_modules/has-symbols/shams.js[m
[1mnew file mode 100644[m
[1mindex 0000000..1285210[m
[1m--- /dev/null[m
[1m+++ b/node_modules/has-symbols/shams.js[m
[36m@@ -0,0 +1,42 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/* eslint complexity: [2, 18], max-statements: [2, 33] */[m
[32m+[m[32mmodule.exports = function hasSymbols() {[m
[32m+[m	[32mif (typeof Symbol !== 'function' || typeof Object.getOwnPropertySymbols !== 'function') { return false; }[m
[32m+[m	[32mif (typeof Symbol.iterator === 'symbol') { return true; }[m
[32m+[m
[32m+[m	[32mvar obj = {};[m
[32m+[m	[32mvar sym = Symbol('test');[m
[32m+[m	[32mvar symObj = Object(sym);[m
[32m+[m	[32mif (typeof sym === 'string') { return false; }[m
[32m+[m
[32m+[m	[32mif (Object.prototype.toString.call(sym) !== '[object Symbol]') { return false; }[m
[32m+[m	[32mif (Object.prototype.toString.call(symObj) !== '[object Symbol]') { return false; }[m
[32m+[m
[32m+[m	[32m// temp disabled per https://github.com/ljharb/object.assign/issues/17[m
[32m+[m	[32m// if (sym instanceof Symbol) { return false; }[m
[32m+[m	[32m// temp disabled per https://github.com/WebReflection/get-own-property-symbols/issues/4[m
[32m+[m	[32m// if (!(symObj instanceof Symbol)) { return false; }[m
[32m+[m
[32m+[m	[32m// if (typeof Symbol.prototype.toString !== 'function') { return false; }[m
[32m+[m	[32m// if (String(sym) !== Symbol.prototype.toString.call(sym)) { return false; }[m
[32m+[m
[32m+[m	[32mvar symVal = 42;[m
[32m+[m	[32mobj[sym] = symVal;[m
[32m+[m	[32mfor (sym in obj) { return false; } // eslint-disable-line no-restricted-syntax, no-unreachable-loop[m
[32m+[m	[32mif (typeof Object.keys === 'function' && Object.keys(obj).length !== 0) { return false; }[m
[32m+[m
[32m+[m	[32mif (typeof Object.getOwnPropertyNames === 'function' && Object.getOwnPropertyNames(obj).length !== 0) { return false; }[m
[32m+[m
[32m+[m	[32mvar syms = Object.getOwnPropertySymbols(obj);[m
[32m+[m	[32mif (syms.length !== 1 || syms[0] !== sym) { return false; }[m
[32m+[m
[32m+[m	[32mif (!Object.prototype.propertyIsEnumerable.call(obj, sym)) { return false; }[m
[32m+[m
[32m+[m	[32mif (typeof Object.getOwnPropertyDescriptor === 'function') {[m
[32m+[m		[32mvar descriptor = Object.getOwnPropertyDescriptor(obj, sym);[m
[32m+[m		[32mif (descriptor.value !== symVal || descriptor.enumerable !== true) { return false; }[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn true;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/has-symbols/test/index.js b/node_modules/has-symbols/test/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..352129c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/has-symbols/test/index.js[m
[36m@@ -0,0 +1,22 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar hasSymbols = require('../');[m
[32m+[m[32mvar runSymbolTests = require('./tests');[m
[32m+[m
[32m+[m[32mtest('interface', function (t) {[m
[32m+[m	[32mt.equal(typeof hasSymbols, 'function', 'is a function');[m
[32m+[m	[32mt.equal(typeof hasSymbols(), 'boolean', 'returns a boolean');[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Symbols are supported', { skip: !hasSymbols() }, function (t) {[m
[32m+[m	[32mrunSymbolTests(t);[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Symbols are not supported', { skip: hasSymbols() }, function (t) {[m
[32m+[m	[32mt.equal(typeof Symbol, 'undefined', 'global Symbol is undefined');[m
[32m+[m	[32mt.equal(typeof Object.getOwnPropertySymbols, 'undefined', 'Object.getOwnPropertySymbols does not exist');[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
