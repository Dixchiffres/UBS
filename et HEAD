[32m+[m
[32m+[m[32mtest('Maps', function (t) {[m
[32m+[m[32m    t.equal(inspect(new Map([[1, 2]])), 'Map (1) {1 => 2}');[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
