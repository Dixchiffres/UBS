[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar hasSymbols = require('has-symbols/shams')();[m
[32m+[m[32mvar utilInspect = require('../util.inspect');[m
[32m+[m[32mvar repeat = require('string.prototype.repeat');[m
[32m+[m
[32m+[m[32mvar inspect = require('..');[m
[32m+[m
[32m+[m[32mtest('inspect', function (t) {[m
[32m+[m[32m    t.plan(5);[m
[32m+[m
[32m+[m[32m    var obj = [{ inspect: function xyzInspect() { return '!XYZ¡'; } }, []];[m
[32m+[m[32m    var stringResult = '[ !XYZ¡, [] ]';[m
[32m+[m[32m    var falseResult = '[ { inspect: [Function: xyzInspect] }, [] ]';[m
[32m+[m
[32m+[m[32m    t.equal(inspect(obj), stringResult);[m
[32m+[m[32m    t.equal(inspect(obj, { customInspect: true }), stringResult);[m
[32m+[m[32m    t.equal(inspect(obj, { customInspect: 'symbol' }), falseResult);[m
[32m+[m[32m    t.equal(inspect(obj, { customInspect: false }), falseResult);[m
[32m+[m[32m    t['throws']([m
[32m+[m[32m        function () { inspect(obj, { customInspect: 'not a boolean or "symbol"' }); },[m
[32m+[m[32m        TypeError,[m
[32m+[m[32m        '`customInspect` must be a boolean or the string "symbol"'[m
[32m+[m[32m    );[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('inspect custom symbol', { skip: !hasSymbols || !utilInspect || !utilInspect.custom }, function (t) {[m
[32m+[m[32m    t.plan(4);[m
[32m+[m
[32m+[m[32m    var obj = { inspect: function stringInspect() { return 'string'; } };[m
[32m+[m[32m    obj[utilInspect.custom] = function custom() { return 'symbol'; };[m
[32m+[m
[32m+[m[32m    var symbolResult = '[ symbol, [] ]';[m
[32m+[m[32m    var stringResult = '[ string, [] ]';[m
[32m+[m[32m    var falseResult = '[ { inspect: [Function: stringInspect]' + (utilInspect.custom ? ', [' + inspect(utilInspect.custom) + ']: [Function: custom]' : '') + ' }, [] ]';[m
[32m+[m
[32m+[m[32m    var symbolStringFallback = utilInspect.custom ? symbolResult : stringResult;[m
[32m+[m[32m    var symbolFalseFallback = utilInspect.custom ? symbolResult : falseResult;[m
[32m+[m
[32m+[m[32m    t.equal(inspect([obj, []]), symbolStringFallback);[m
[32m+[m[32m    t.equal(inspect([obj, []], { customInspect: true }), symbolStringFallback);[m
[32m+[m[32m    t.equal(inspect([obj, []], { customInspect: 'symbol' }), symbolFalseFallback);[m
[32m+[m[32m    t.equal(inspect([obj, []], { customInspect: false }), falseResult);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('symbols', { skip: !hasSymbols }, function (t) {[m
[32m+[m[32m    t.plan(2);[m
[32m+[m
[32m+[m[32m    var obj = { a: 1 };[m
[32m+[m[32m    obj[Symbol('test')] = 2;[m
[32m+[m[32m    obj[Symbol.iterator] = 3;[m
[32m+[m[32m    Object.defineProperty(obj, Symbol('non-enum'), {[m
[32m+[m[32m        enumerable: false,[m
[32m+[m[32m        value: 4[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if (typeof Symbol.iterator === 'symbol') {[m
[32m+[m[32m        t.equal(inspect(obj), '{ a: 1, [Symbol(test)]: 2, [Symbol(Symbol.iterator)]: 3 }', 'object with symbols');[m
[32m+[m[32m        t.equal(inspect([obj, []]), '[ { a: 1, [Symbol(test)]: 2, [Symbol(Symbol.iterator)]: 3 }, [] ]', 'object with symbols in array');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // symbol sham key ordering is unreliable[m
[32m+[m[32m        t.match([m
[32m+[m[32m            inspect(obj),[m
[32m+[m[32m            /^(?:{ a: 1, \[Symbol\(test\)\]: 2, \[Symbol\(Symbol.iterator\)\]: 3 }|{ a: 1, \[Symbol\(Symbol.iterator\)\]: 3, \[Symbol\(test\)\]: 2 })$/,[m
[32m+[m[32m            'object with symbols (nondeterministic symbol sham key ordering)'[m
[32m+[m[32m        );[m
[32m+[m[32m        t.match([m
[32m+[m[32m            inspect([obj, []]),[m
[32m+[m[32m            /^\[ (?:{ a: 1, \[Symbol\(test\)\]: 2, \[Symbol\(Symbol.iterator\)\]: 3 }|{ a: 1, \[Symbol\(Symbol.iterator\)\]: 3, \[Symbol\(test\)\]: 2 }), \[\] \]$/,[m
[32m+[m[32m            'object with symbols in array (nondeterministic symbol sham key ordering)'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('maxStringLength', function (t) {[m
[32m+[m[32m    t['throws']([m
[32m+[m[32m        function () { inspect('', { maxStringLength: -1 }); },[m
[32m+[m[32m        TypeError,[m
[32m+[m[32m        'maxStringLength must be >= 0, or Infinity, not negative'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    var str = repeat('a', 1e8);[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect([str], { maxStringLength: 10 }),[m
[32m+[m[32m        '[ \'aaaaaaaaaa\'... 99999990 more characters ]',[m
[32m+[m[32m        'maxStringLength option limits output'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(['f'], { maxStringLength: null }),[m
[32m+[m[32m        '[ \'\'... 1 more character ]',[m
[32m+[m[32m        'maxStringLength option accepts `null`'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect([str], { maxStringLength: Infinity }),[m
[32m+[m[32m        '[ \'' + str + '\' ]',[m
[32m+[m[32m        'maxStringLength option accepts ∞'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('inspect options', { skip: !utilInspect.custom }, function (t) {[m
[32m+[m[32m    var obj = {};[m
[32m+[m[32m    obj[utilInspect.custom] = function () {[m
[32m+[m[32m        return JSON.stringify(arguments);[m
[32m+[m[32m    };[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(obj),[m
[32m+[m[32m        utilInspect(obj, { depth: 5 }),[m
[32m+[m[32m        'custom symbols will use node\'s inspect'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(obj, { depth: 2 }),[m
[32m+[m[32m        utilInspect(obj, { depth: 2 }),[m
[32m+[m[32m        'a reduced depth will be passed to node\'s inspect'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect({ d1: obj }, { depth: 3 }),[m
[32m+[m[32m        '{ d1: ' + utilInspect(obj, { depth: 2 }) + ' }',[m
[32m+[m[32m        'deep objects will receive a reduced depth'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect({ d1: obj }, { depth: 1 }),[m
[32m+[m[32m        '{ d1: [Object] }',[m
[32m+[m[32m        'unlike nodejs inspect, customInspect will not be used once the depth is exceeded.'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
