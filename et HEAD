[1mdiff --git a/node_modules/has-property-descriptors/test/index.js b/node_modules/has-property-descriptors/test/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7f02bd3[m
[1m--- /dev/null[m
[1m+++ b/node_modules/has-property-descriptors/test/index.js[m
[36m@@ -0,0 +1,57 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mvar hasPropertyDescriptors = require('../');[m
[32m+[m
[32m+[m[32mvar sentinel = {};[m
[32m+[m
[32m+[m[32mtest('hasPropertyDescriptors', function (t) {[m
[32m+[m	[32mt.equal(typeof hasPropertyDescriptors, 'function', 'is a function');[m
[32m+[m	[32mt.equal(typeof hasPropertyDescriptors.hasArrayLengthDefineBug, 'function', '`hasArrayLengthDefineBug` property is a function');[m
[32m+[m
[32m+[m	[32mvar yes = hasPropertyDescriptors();[m
[32m+[m	[32mt.test('property descriptors', { skip: !yes }, function (st) {[m
[32m+[m		[32mvar o = { a: sentinel };[m
[32m+[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(o, 'a'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: true,[m
[32m+[m				[32menumerable: true,[m
[32m+[m				[32mvalue: sentinel,[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'has expected property descriptor'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mObject.defineProperty(o, 'a', { enumerable: false, writable: false });[m
[32m+[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(o, 'a'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: true,[m
[32m+[m				[32menumerable: false,[m
[32m+[m				[32mvalue: sentinel,[m
[32m+[m				[32mwritable: false[m
[32m+[m			[32m},[m
[32m+[m			[32m'has expected property descriptor after [[Define]]'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mvar arrayBug = hasPropertyDescriptors.hasArrayLengthDefineBug();[m
[32m+[m	[32mt.test('defining array lengths', { skip: !yes || arrayBug }, function (st) {[m
[32m+[m		[32mvar arr = [1, , 3]; // eslint-disable-line no-sparse-arrays[m
[32m+[m		[32mst.equal(arr.length, 3, 'array starts with length 3');[m
[32m+[m
[32m+[m		[32mObject.defineProperty(arr, 'length', { value: 5 });[m
[32m+[m
[32m+[m		[32mst.equal(arr.length, 5, 'array ends with length 5');[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
