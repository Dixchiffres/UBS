[32m+[m[32m        alert('Connexion réussie ! Bienvenue.');[m
[32m+[m[32m        // Redirection ou autre action après la connexion réussie[m
[32m+[m[32m        window.location.href = 'dashboard.html'; // Exemple de redirection vers une page "dashboard"[m
[32m+[m[32m    } else {[m
[32m+[m[32m        alert('Identifiant ou mot de passe incorrect.');[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
