[1mdiff --git a/node_modules/qs/test/parse.js b/node_modules/qs/test/parse.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7d7b4dd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/test/parse.js[m
[36m@@ -0,0 +1,855 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar qs = require('../');[m
[32m+[m[32mvar utils = require('../lib/utils');[m
[32m+[m[32mvar iconv = require('iconv-lite');[m
[32m+[m[32mvar SaferBuffer = require('safer-buffer').Buffer;[m
[32m+[m
[32m+[m[32mtest('parse()', function (t) {[m
[32m+[m[32m    t.test('parses a simple string', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('0=foo'), { 0: 'foo' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=c++'), { foo: 'c  ' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[>=]=23'), { a: { '>=': '23' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[<=>]==23'), { a: { '<=>': '=23' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[==]=23'), { a: { '==': '23' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo', { strictNullHandling: true }), { foo: null });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo'), { foo: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo='), { foo: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar'), { foo: 'bar' });[m
[32m+[m[32m        st.deepEqual(qs.parse(' foo = bar = baz '), { ' foo ': ' bar = baz ' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar=baz'), { foo: 'bar=baz' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar&bar=baz'), { foo: 'bar', bar: 'baz' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo2=bar2&baz2='), { foo2: 'bar2', baz2: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar&baz', { strictNullHandling: true }), { foo: 'bar', baz: null });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar&baz'), { foo: 'bar', baz: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('cht=p3&chd=t:60,40&chs=250x100&chl=Hello|World'), {[m
[32m+[m[32m            cht: 'p3',[m
[32m+[m[32m            chd: 't:60,40',[m
[32m+[m[32m            chs: '250x100',[m
[32m+[m[32m            chl: 'Hello|World'[m
[32m+[m[32m        });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('arrayFormat: brackets allows only explicit arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c', { arrayFormat: 'brackets' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { arrayFormat: 'brackets' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b,c', { arrayFormat: 'brackets' }), { a: 'b,c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a=c', { arrayFormat: 'brackets' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('arrayFormat: indices allows only indexed arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c', { arrayFormat: 'indices' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { arrayFormat: 'indices' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b,c', { arrayFormat: 'indices' }), { a: 'b,c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a=c', { arrayFormat: 'indices' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('arrayFormat: comma allows only comma-separated arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c', { arrayFormat: 'comma' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { arrayFormat: 'comma' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b,c', { arrayFormat: 'comma' }), { a: 'b,c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a=c', { arrayFormat: 'comma' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('arrayFormat: repeat allows only repeated values', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c', { arrayFormat: 'repeat' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { arrayFormat: 'repeat' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b,c', { arrayFormat: 'repeat' }), { a: 'b,c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a=c', { arrayFormat: 'repeat' }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows enabling dot notation', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a.b=c'), { 'a.b': 'c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a.b=c', { allowDots: true }), { a: { b: 'c' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.deepEqual(qs.parse('a[b]=c'), { a: { b: 'c' } }, 'parses a single nested string');[m
[32m+[m[32m    t.deepEqual(qs.parse('a[b][c]=d'), { a: { b: { c: 'd' } } }, 'parses a double nested string');[m
[32m+[m[32m    t.deepEqual([m
[32m+[m[32m        qs.parse('a[b][c][d][e][f][g][h]=i'),[m
[32m+[m[32m        { a: { b: { c: { d: { e: { f: { '[g][h]': 'i' } } } } } } },[m
[32m+[m[32m        'defaults to a depth of 5'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.test('only parses one level when depth = 1', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b][c]=d', { depth: 1 }), { a: { b: { '[c]': 'd' } } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b][c][d]=e', { depth: 1 }), { a: { b: { '[c][d]': 'e' } } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses original key when depth = 0', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { depth: 0 }), { 'a[0]': 'b', 'a[1]': 'c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0][0]=b&a[0][1]=c&a[1]=d&e=2', { depth: 0 }), { 'a[0][0]': 'b', 'a[0][1]': 'c', 'a[1]': 'd', e: '2' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses original key when depth = false', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { depth: false }), { 'a[0]': 'b', 'a[1]': 'c' });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0][0]=b&a[0][1]=c&a[1]=d&e=2', { depth: false }), { 'a[0][0]': 'b', 'a[0][1]': 'c', 'a[1]': 'd', e: '2' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.deepEqual(qs.parse('a=b&a=c'), { a: ['b', 'c'] }, 'parses a simple array');[m
[32m+[m
[32m+[m[32m    t.test('parses an explicit array', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b'), { a: ['b'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c'), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=c&a[]=d'), { a: ['b', 'c', 'd'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses a mix of simple and explicit arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a[]=c'), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a=c'), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a=c'), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a[0]=c'), { a: ['b', 'c'] });[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=b&a=c', { arrayLimit: 20 }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a=c', { arrayLimit: 0 }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a=c'), { a: ['b', 'c'] });[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a[1]=c', { arrayLimit: 20 }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a[]=c', { arrayLimit: 0 }), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&a[]=c'), { a: ['b', 'c'] });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses a nested array', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b][]=c&a[b][]=d'), { a: { b: ['c', 'd'] } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[>=]=25'), { a: { '>=': '25' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows to specify array indices', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=c&a[0]=b&a[2]=d'), { a: ['b', 'c', 'd'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=c&a[0]=b'), { a: ['b', 'c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=c', { arrayLimit: 20 }), { a: ['c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=c', { arrayLimit: 0 }), { a: { 1: 'c' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1]=c'), { a: ['c'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('limits specific array indices to arrayLimit', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[20]=a', { arrayLimit: 20 }), { a: ['a'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[21]=a', { arrayLimit: 20 }), { a: { 21: 'a' } });[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('a[20]=a'), { a: ['a'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[21]=a'), { a: { 21: 'a' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.deepEqual(qs.parse('a[12b]=c'), { a: { '12b': 'c' } }, 'supports keys that begin with a number');[m
[32m+[m
[32m+[m[32m    t.test('supports encoded = signs', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('he%3Dllo=th%3Dere'), { 'he=llo': 'th=ere' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('is ok with url encoded strings', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b%20c]=d'), { a: { 'b c': 'd' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b]=c%20d'), { a: { b: 'c d' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows brackets in the value', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('pets=["tobi"]'), { pets: '["tobi"]' });[m
[32m+[m[32m        st.deepEqual(qs.parse('operators=[">=", "<="]'), { operators: '[">=", "<="]' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows empty values', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse(''), {});[m
[32m+[m[32m        st.deepEqual(qs.parse(null), {});[m
[32m+[m[32m        st.deepEqual(qs.parse(undefined), {});[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('transforms arrays to objects', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0]=bar&foo[bad]=baz'), { foo: { 0: 'bar', bad: 'baz' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[bad]=baz&foo[0]=bar'), { foo: { bad: 'baz', 0: 'bar' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[bad]=baz&foo[]=bar'), { foo: { bad: 'baz', 0: 'bar' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=bar&foo[bad]=baz'), { foo: { 0: 'bar', bad: 'baz' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[bad]=baz&foo[]=bar&foo[]=foo'), { foo: { bad: 'baz', 0: 'bar', 1: 'foo' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0][a]=a&foo[0][b]=b&foo[1][a]=aa&foo[1][b]=bb'), { foo: [{ a: 'a', b: 'b' }, { a: 'aa', b: 'bb' }] });[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[t]=u&a[hasOwnProperty]=c', { allowPrototypes: false }), { a: { 0: 'b', t: 'u' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[t]=u&a[hasOwnProperty]=c', { allowPrototypes: true }), { a: { 0: 'b', t: 'u', hasOwnProperty: 'c' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[hasOwnProperty]=c&a[x]=y', { allowPrototypes: false }), { a: { 0: 'b', x: 'y' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[hasOwnProperty]=c&a[x]=y', { allowPrototypes: true }), { a: { 0: 'b', hasOwnProperty: 'c', x: 'y' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('transforms arrays to objects (dot notation)', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0].baz=bar&fool.bad=baz', { allowDots: true }), { foo: [{ baz: 'bar' }], fool: { bad: 'baz' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0].baz=bar&fool.bad.boo=baz', { allowDots: true }), { foo: [{ baz: 'bar' }], fool: { bad: { boo: 'baz' } } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0][0].baz=bar&fool.bad=baz', { allowDots: true }), { foo: [[{ baz: 'bar' }]], fool: { bad: 'baz' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0].baz[0]=15&foo[0].bar=2', { allowDots: true }), { foo: [{ baz: ['15'], bar: '2' }] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0].baz[0]=15&foo[0].baz[1]=16&foo[0].bar=2', { allowDots: true }), { foo: [{ baz: ['15', '16'], bar: '2' }] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo.bad=baz&foo[0]=bar', { allowDots: true }), { foo: { bad: 'baz', 0: 'bar' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo.bad=baz&foo[]=bar', { allowDots: true }), { foo: { bad: 'baz', 0: 'bar' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=bar&foo.bad=baz', { allowDots: true }), { foo: { 0: 'bar', bad: 'baz' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo.bad=baz&foo[]=bar&foo[]=foo', { allowDots: true }), { foo: { bad: 'baz', 0: 'bar', 1: 'foo' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[0].a=a&foo[0].b=b&foo[1].a=aa&foo[1].b=bb', { allowDots: true }), { foo: [{ a: 'a', b: 'b' }, { a: 'aa', b: 'bb' }] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('correctly prunes undefined values when converting an array to an object', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[2]=b&a[99999999]=c'), { a: { 2: 'b', 99999999: 'c' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('supports malformed uri characters', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('{%:%}', { strictNullHandling: true }), { '{%:%}': null });[m
[32m+[m[32m        st.deepEqual(qs.parse('{%:%}='), { '{%:%}': '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=%:%}'), { foo: '%:%}' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('doesn\'t produce empty keys', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('_r=1&'), { _r: '1' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('cannot access Object prototype', function (st) {[m
[32m+[m[32m        qs.parse('constructor[prototype][bad]=bad');[m
[32m+[m[32m        qs.parse('bad[constructor][prototype][bad]=bad');[m
[32m+[m[32m        st.equal(typeof Object.prototype.bad, 'undefined');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses arrays of objects', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[][b]=c'), { a: [{ b: 'c' }] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0][b]=c'), { a: [{ b: 'c' }] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows for empty strings in arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[]=&a[]=c'), { a: ['b', '', 'c'] });[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[0]=b&a[1]&a[2]=c&a[19]=', { strictNullHandling: true, arrayLimit: 20 }),[m
[32m+[m[32m            { a: ['b', null, 'c', ''] },[m
[32m+[m[32m            'with arrayLimit 20 + array indices: null then empty string works'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[]=b&a[]&a[]=c&a[]=', { strictNullHandling: true, arrayLimit: 0 }),[m
[32m+[m[32m            { a: ['b', null, 'c', ''] },[m
[32m+[m[32m            'with arrayLimit 0 + array brackets: null then empty string works'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[0]=b&a[1]=&a[2]=c&a[19]', { strictNullHandling: true, arrayLimit: 20 }),[m
[32m+[m[32m            { a: ['b', '', 'c', null] },[m
[32m+[m[32m            'with arrayLimit 20 + array indices: empty string then null works'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[]=b&a[]=&a[]=c&a[]', { strictNullHandling: true, arrayLimit: 0 }),[m
[32m+[m[32m            { a: ['b', '', 'c', null] },[m
[32m+[m[32m            'with arrayLimit 0 + array brackets: empty string then null works'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[]=&a[]=b&a[]=c'),[m
[32m+[m[32m            { a: ['', 'b', 'c'] },[m
[32m+[m[32m            'array brackets: empty strings work'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('compacts sparse arrays', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[10]=1&a[2]=2', { arrayLimit: 20 }), { a: ['2', '1'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][b][2][c]=1', { arrayLimit: 20 }), { a: [{ b: [{ c: '1' }] }] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][2][3][c]=1', { arrayLimit: 20 }), { a: [[[{ c: '1' }]]] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][2][3][c][1]=1', { arrayLimit: 20 }), { a: [[[{ c: ['1'] }]]] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses sparse arrays', function (st) {[m
[32m+[m[32m        /* eslint no-sparse-arrays: 0 */[m
[32m+[m[32m        st.deepEqual(qs.parse('a[4]=1&a[1]=2', { allowSparse: true }), { a: [, '2', , , '1'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][b][2][c]=1', { allowSparse: true }), { a: [, { b: [, , { c: '1' }] }] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][2][3][c]=1', { allowSparse: true }), { a: [, [, , [, , , { c: '1' }]]] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[1][2][3][c][1]=1', { allowSparse: true }), { a: [, [, , [, , , { c: [, '1'] }]]] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses semi-parsed strings', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse({ 'a[b]': 'c' }), { a: { b: 'c' } });[m
[32m+[m[32m        st.deepEqual(qs.parse({ 'a[b]': 'c', 'a[d]': 'e' }), { a: { b: 'c', d: 'e' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses buffers correctly', function (st) {[m
[32m+[m[32m        var b = SaferBuffer.from('test');[m
[32m+[m[32m        st.deepEqual(qs.parse({ a: b }), { a: b });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses jquery-param strings', function (st) {[m
[32m+[m[32m        // readable = 'filter[0][]=int1&filter[0][]==&filter[0][]=77&filter[]=and&filter[2][]=int2&filter[2][]==&filter[2][]=8'[m
[32m+[m[32m        var encoded = 'filter%5B0%5D%5B%5D=int1&filter%5B0%5D%5B%5D=%3D&filter%5B0%5D%5B%5D=77&filter%5B%5D=and&filter%5B2%5D%5B%5D=int2&filter%5B2%5D%5B%5D=%3D&filter%5B2%5D%5B%5D=8';[m
[32m+[m[32m        var expected = { filter: [['int1', '=', '77'], 'and', ['int2', '=', '8']] };[m
[32m+[m[32m        st.deepEqual(qs.parse(encoded), expected);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('continues parsing when no parent is found', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('[]=&a=b'), { 0: '', a: 'b' });[m
[32m+[m[32m        st.deepEqual(qs.parse('[]&a=b', { strictNullHandling: true }), { 0: null, a: 'b' });[m
[32m+[m[32m        st.deepEqual(qs.parse('[foo]=bar'), { foo: 'bar' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not error when parsing a very long array', function (st) {[m
[32m+[m[32m        var str = 'a[]=a';[m
[32m+[m[32m        while (Buffer.byteLength(str) < 128 * 1024) {[m
[32m+[m[32m            str = str + '&' + str;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        st.doesNotThrow(function () {[m
[32m+[m[32m            qs.parse(str);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('should not throw when a native prototype has an enumerable property', function (st) {[m
[32m+[m[32m        Object.prototype.crash = '';[m
[32m+[m[32m        Array.prototype.crash = '';[m
[32m+[m[32m        st.doesNotThrow(qs.parse.bind(null, 'a=b'));[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b'), { a: 'b' });[m
[32m+[m[32m        st.doesNotThrow(qs.parse.bind(null, 'a[][b]=c'));[m
[32m+[m[32m        st.deepEqual(qs.parse('a[][b]=c'), { a: [{ b: 'c' }] });[m
[32m+[m[32m        delete Object.prototype.crash;[m
[32m+[m[32m        delete Array.prototype.crash;[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses a string with an alternative string delimiter', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b;c=d', { delimiter: ';' }), { a: 'b', c: 'd' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses a string with an alternative RegExp delimiter', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b; c=d', { delimiter: /[;,] */ }), { a: 'b', c: 'd' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not use non-splittable objects as delimiters', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&c=d', { delimiter: true }), { a: 'b', c: 'd' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows overriding parameter limit', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&c=d', { parameterLimit: 1 }), { a: 'b' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows setting the parameter limit to Infinity', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=b&c=d', { parameterLimit: Infinity }), { a: 'b', c: 'd' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows overriding array limit', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b', { arrayLimit: -1 }), { a: { 0: 'b' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[-1]=b', { arrayLimit: -1 }), { a: { '-1': 'b' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=b&a[1]=c', { arrayLimit: 0 }), { a: { 0: 'b', 1: 'c' } });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows disabling array parsing', function (st) {[m
[32m+[m[32m        var indices = qs.parse('a[0]=b&a[1]=c', { parseArrays: false });[m
[32m+[m[32m        st.deepEqual(indices, { a: { 0: 'b', 1: 'c' } });[m
[32m+[m[32m        st.equal(Array.isArray(indices.a), false, 'parseArrays:false, indices case is not an array');[m
[32m+[m
[32m+[m[32m        var emptyBrackets = qs.parse('a[]=b', { parseArrays: false });[m
[32m+[m[32m        st.deepEqual(emptyBrackets, { a: { 0: 'b' } });[m
[32m+[m[32m        st.equal(Array.isArray(emptyBrackets.a), false, 'parseArrays:false, empty brackets case is not an array');[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows for query string prefix', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('?foo=bar', { ignoreQueryPrefix: true }), { foo: 'bar' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar', { ignoreQueryPrefix: true }), { foo: 'bar' });[m
[32m+[m[32m        st.deepEqual(qs.parse('?foo=bar', { ignoreQueryPrefix: false }), { '?foo': 'bar' });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses an object', function (st) {[m
[32m+[m[32m        var input = {[m
[32m+[m[32m            'user[name]': { 'pop[bob]': 3 },[m
[32m+[m[32m            'user[email]': null[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var expected = {[m
[32m+[m[32m            user: {[m
[32m+[m[32m                name: { 'pop[bob]': 3 },[m
[32m+[m[32m                email: null[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var result = qs.parse(input);[m
[32m+[m
[32m+[m[32m        st.deepEqual(result, expected);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses string with comma as array divider', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=bar,tee', { comma: true }), { foo: ['bar', 'tee'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[bar]=coffee,tee', { comma: true }), { foo: { bar: ['coffee', 'tee'] } });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=', { comma: true }), { foo: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo', { comma: true }), { foo: '' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo', { comma: true, strictNullHandling: true }), { foo: null });[m
[32m+[m
[32m+[m[32m        // test cases inversed from from stringify tests[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=c'), { a: ['c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=c'), { a: ['c'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=c', { comma: true }), { a: ['c'] });[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('a[0]=c&a[1]=d'), { a: ['c', 'd'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=c&a[]=d'), { a: ['c', 'd'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('a=c,d', { comma: true }), { a: ['c', 'd'] });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses values with comma as array divider', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse({ foo: 'bar,tee' }, { comma: false }), { foo: 'bar,tee' });[m
[32m+[m[32m        st.deepEqual(qs.parse({ foo: 'bar,tee' }, { comma: true }), { foo: ['bar', 'tee'] });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('use number decoder, parses string that has one number with comma option enabled', function (st) {[m
[32m+[m[32m        var decoder = function (str, defaultDecoder, charset, type) {[m
[32m+[m[32m            if (!isNaN(Number(str))) {[m
[32m+[m[32m                return parseFloat(str);[m
[32m+[m[32m            }[m
[32m+[m[32m            return defaultDecoder(str, defaultDecoder, charset, type);[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=1', { comma: true, decoder: decoder }), { foo: 1 });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=0', { comma: true, decoder: decoder }), { foo: 0 });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses brackets holds array of arrays when having two parts of strings with comma as array divider', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=1,2,3&foo[]=4,5,6', { comma: true }), { foo: [['1', '2', '3'], ['4', '5', '6']] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=1,2,3&foo[]=', { comma: true }), { foo: [['1', '2', '3'], ''] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=1,2,3&foo[]=,', { comma: true }), { foo: [['1', '2', '3'], ['', '']] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo[]=1,2,3&foo[]=a', { comma: true }), { foo: [['1', '2', '3'], 'a'] });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses comma delimited array while having percent-encoded comma treated as normal text', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=a%2Cb', { comma: true }), { foo: 'a,b' });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=a%2C%20b,d', { comma: true }), { foo: ['a, b', 'd'] });[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=a%2C%20b,c%2C%20d', { comma: true }), { foo: ['a, b', 'c, d'] });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses an object in dot notation', function (st) {[m
[32m+[m[32m        var input = {[m
[32m+[m[32m            'user.name': { 'pop[bob]': 3 },[m
[32m+[m[32m            'user.email.': null[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var expected = {[m
[32m+[m[32m            user: {[m
[32m+[m[32m                name: { 'pop[bob]': 3 },[m
[32m+[m[32m                email: null[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var result = qs.parse(input, { allowDots: true });[m
[32m+[m
[32m+[m[32m        st.deepEqual(result, expected);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses an object and not child values', function (st) {[m
[32m+[m[32m        var input = {[m
[32m+[m[32m            'user[name]': { 'pop[bob]': { test: 3 } },[m
[32m+[m[32m            'user[email]': null[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var expected = {[m
[32m+[m[32m            user: {[m
[32m+[m[32m                name: { 'pop[bob]': { test: 3 } },[m
[32m+[m[32m                email: null[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        var result = qs.parse(input);[m
[32m+[m
[32m+[m[32m        st.deepEqual(result, expected);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not blow up when Buffer global is missing', function (st) {[m
[32m+[m[32m        var tempBuffer = global.Buffer;[m
[32m+[m[32m        delete global.Buffer;[m
[32m+[m[32m        var result = qs.parse('a=b&c=d');[m
[32m+[m[32m        global.Buffer = tempBuffer;[m
[32m+[m[32m        st.deepEqual(result, { a: 'b', c: 'd' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not crash when parsing circular references', function (st) {[m
[32m+[m[32m        var a = {};[m
[32m+[m[32m        a.b = a;[m
[32m+[m
[32m+[m[32m        var parsed;[m
[32m+[m
[32m+[m[32m        st.doesNotThrow(function () {[m
[32m+[m[32m            parsed = qs.parse({ 'foo[bar]': 'baz', 'foo[baz]': a });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.equal('foo' in parsed, true, 'parsed has "foo" property');[m
[32m+[m[32m        st.equal('bar' in parsed.foo, true);[m
[32m+[m[32m        st.equal('baz' in parsed.foo, true);[m
[32m+[m[32m        st.equal(parsed.foo.bar, 'baz');[m
[32m+[m[32m        st.deepEqual(parsed.foo.baz, a);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not crash when parsing deep objects', function (st) {[m
[32m+[m[32m        var parsed;[m
[32m+[m[32m        var str = 'foo';[m
[32m+[m
[32m+[m[32m        for (var i = 0; i < 5000; i++) {[m
[32m+[m[32m            str += '[p]';[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        str += '=bar';[m
[32m+[m
[32m+[m[32m        st.doesNotThrow(function () {[m
[32m+[m[32m            parsed = qs.parse(str, { depth: 5000 });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.equal('foo' in parsed, true, 'parsed has "foo" property');[m
[32m+[m
[32m+[m[32m        var depth = 0;[m
[32m+[m[32m        var ref = parsed.foo;[m
[32m+[m[32m        while ((ref = ref.p)) {[m
[32m+[m[32m            depth += 1;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        st.equal(depth, 5000, 'parsed is 5000 properties deep');[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses null objects correctly', { skip: !Object.create }, function (st) {[m
[32m+[m[32m        var a = Object.create(null);[m
[32m+[m[32m        a.b = 'c';[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse(a), { b: 'c' });[m
[32m+[m[32m        var result = qs.parse({ a: a });[m
[32m+[m[32m        st.equal('a' in result, true, 'result has "a" property');[m
[32m+[m[32m        st.deepEqual(result.a, a);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses dates correctly', function (st) {[m
[32m+[m[32m        var now = new Date();[m
[32m+[m[32m        st.deepEqual(qs.parse({ a: now }), { a: now });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses regular expressions correctly', function (st) {[m
[32m+[m[32m        var re = /^test$/;[m
[32m+[m[32m        st.deepEqual(qs.parse({ a: re }), { a: re });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not allow overwriting prototype properties', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[hasOwnProperty]=b', { allowPrototypes: false }), {});[m
[32m+[m[32m        st.deepEqual(qs.parse('hasOwnProperty=b', { allowPrototypes: false }), {});[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('toString', { allowPrototypes: false }),[m
[32m+[m[32m            {},[m
[32m+[m[32m            'bare "toString" results in {}'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can allow overwriting prototype properties', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a[hasOwnProperty]=b', { allowPrototypes: true }), { a: { hasOwnProperty: 'b' } });[m
[32m+[m[32m        st.deepEqual(qs.parse('hasOwnProperty=b', { allowPrototypes: true }), { hasOwnProperty: 'b' });[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('toString', { allowPrototypes: true }),[m
[32m+[m[32m            { toString: '' },[m
[32m+[m[32m            'bare "toString" results in { toString: "" }'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('params starting with a closing bracket', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse(']=toString'), { ']': 'toString' });[m
[32m+[m[32m        st.deepEqual(qs.parse(']]=toString'), { ']]': 'toString' });[m
[32m+[m[32m        st.deepEqual(qs.parse(']hello]=toString'), { ']hello]': 'toString' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('params starting with a starting bracket', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('[=toString'), { '[': 'toString' });[m
[32m+[m[32m        st.deepEqual(qs.parse('[[=toString'), { '[[': 'toString' });[m
[32m+[m[32m        st.deepEqual(qs.parse('[hello[=toString'), { '[hello[': 'toString' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('add keys to objects', function (st) {[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[b]=c&a=d'),[m
[32m+[m[32m            { a: { b: 'c', d: true } },[m
[32m+[m[32m            'can add keys to objects'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[b]=c&a=toString'),[m
[32m+[m[32m            { a: { b: 'c' } },[m
[32m+[m[32m            'can not overwrite prototype'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[b]=c&a=toString', { allowPrototypes: true }),[m
[32m+[m[32m            { a: { b: 'c', toString: true } },[m
[32m+[m[32m            'can overwrite prototype with allowPrototypes true'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('a[b]=c&a=toString', { plainObjects: true }),[m
[32m+[m[32m            { __proto__: null, a: { __proto__: null, b: 'c', toString: true } },[m
[32m+[m[32m            'can overwrite prototype with plainObjects true'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('dunder proto is ignored', function (st) {[m
[32m+[m[32m        var payload = 'categories[__proto__]=login&categories[__proto__]&categories[length]=42';[m
[32m+[m[32m        var result = qs.parse(payload, { allowPrototypes: true });[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            result,[m
[32m+[m[32m            {[m
[32m+[m[32m                categories: {[m
[32m+[m[32m                    length: '42'[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            'silent [[Prototype]] payload'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        var plainResult = qs.parse(payload, { allowPrototypes: true, plainObjects: true });[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            plainResult,[m
[32m+[m[32m            {[m
[32m+[m[32m                __proto__: null,[m
[32m+[m[32m                categories: {[m
[32m+[m[32m                    __proto__: null,[m
[32m+[m[32m                    length: '42'[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            'silent [[Prototype]] payload: plain objects'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        var query = qs.parse('categories[__proto__]=cats&categories[__proto__]=dogs&categories[some][json]=toInject', { allowPrototypes: true });[m
[32m+[m
[32m+[m[32m        st.notOk(Array.isArray(query.categories), 'is not an array');[m
[32m+[m[32m        st.notOk(query.categories instanceof Array, 'is not instanceof an array');[m
[32m+[m[32m        st.deepEqual(query.categories, { some: { json: 'toInject' } });[m
[32m+[m[32m        st.equal(JSON.stringify(query.categories), '{"some":{"json":"toInject"}}', 'stringifies as a non-array');[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('foo[__proto__][hidden]=value&foo[bar]=stuffs', { allowPrototypes: true }),[m
[32m+[m[32m            {[m
[32m+[m[32m                foo: {[m
[32m+[m[32m                    bar: 'stuffs'[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            'hidden values'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.deepEqual([m
[32m+[m[32m            qs.parse('foo[__proto__][hidden]=value&foo[bar]=stuffs', { allowPrototypes: true, plainObjects: true }),[m
[32m+[m[32m            {[m
[32m+[m[32m                __proto__: null,[m
[32m+[m[32m                foo: {[m
[32m+[m[32m                    __proto__: null,[m
[32m+[m[32m                    bar: 'stuffs'[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            'hidden values: plain objects'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can return null objects', { skip: !Object.create }, function (st) {[m
[32m+[m[32m        var expected = Object.create(null);[m
[32m+[m[32m        expected.a = Object.create(null);[m
[32m+[m[32m        expected.a.b = 'c';[m
[32m+[m[32m        expected.a.hasOwnProperty = 'd';[m
[32m+[m[32m        st.deepEqual(qs.parse('a[b]=c&a[hasOwnProperty]=d', { plainObjects: true }), expected);[m
[32m+[m[32m        st.deepEqual(qs.parse(null, { plainObjects: true }), Object.create(null));[m
[32m+[m[32m        var expectedArray = Object.create(null);[m
[32m+[m[32m        expectedArray.a = Object.create(null);[m
[32m+[m[32m        expectedArray.a[0] = 'b';[m
[32m+[m[32m        expectedArray.a.c = 'd';[m
[32m+[m[32m        st.deepEqual(qs.parse('a[]=b&a[c]=d', { plainObjects: true }), expectedArray);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can parse with custom encoding', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('%8c%a7=%91%e5%8d%e3%95%7b', {[m
[32m+[m[32m            decoder: function (str) {[m
[32m+[m[32m                var reg = /%([0-9A-F]{2})/ig;[m
[32m+[m[32m                var result = [];[m
[32m+[m[32m                var parts = reg.exec(str);[m
[32m+[m[32m                while (parts) {[m
[32m+[m[32m                    result.push(parseInt(parts[1], 16));[m
[32m+[m[32m                    parts = reg.exec(str);[m
[32m+[m[32m                }[m
[32m+[m[32m                return String(iconv.decode(SaferBuffer.from(result), 'shift_jis'));[m
[32m+[m[32m            }[m
[32m+[m[32m        }), { 県: '大阪府' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('receives the default decoder as a second argument', function (st) {[m
[32m+[m[32m        st.plan(1);[m
[32m+[m[32m        qs.parse('a', {[m
[32m+[m[32m            decoder: function (str, defaultDecoder) {[m
[32m+[m[32m                st.equal(defaultDecoder, utils.decode);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('throws error with wrong decoder', function (st) {[m
[32m+[m[32m        st['throws'](function () {[m
[32m+[m[32m            qs.parse({}, { decoder: 'string' });[m
[32m+[m[32m        }, new TypeError('Decoder has to be a function.'));[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not mutate the options argument', function (st) {[m
[32m+[m[32m        var options = {};[m
[32m+[m[32m        qs.parse('a[b]=true', options);[m
[32m+[m[32m        st.deepEqual(options, {});[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('throws if an invalid charset is specified', function (st) {[m
[32m+[m[32m        st['throws'](function () {[m
[32m+[m[32m            qs.parse('a=b', { charset: 'foobar' });[m
[32m+[m[32m        }, new TypeError('The charset option must be either utf-8, iso-8859-1, or undefined'));[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('parses an iso-8859-1 string if asked to', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('%A2=%BD', { charset: 'iso-8859-1' }), { '¢': '½' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    var urlEncodedCheckmarkInUtf8 = '%E2%9C%93';[m
[32m+[m[32m    var urlEncodedOSlashInUtf8 = '%C3%B8';[m
[32m+[m[32m    var urlEncodedNumCheckmark = '%26%2310003%3B';[m
[32m+[m[32m    var urlEncodedNumSmiley = '%26%239786%3B';[m
[32m+[m
[32m+[m[32m    t.test('prefers an utf-8 charset specified by the utf8 sentinel to a default charset of iso-8859-1', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('utf8=' + urlEncodedCheckmarkInUtf8 + '&' + urlEncodedOSlashInUtf8 + '=' + urlEncodedOSlashInUtf8, { charsetSentinel: true, charset: 'iso-8859-1' }), { ø: 'ø' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('prefers an iso-8859-1 charset specified by the utf8 sentinel to a default charset of utf-8', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('utf8=' + urlEncodedNumCheckmark + '&' + urlEncodedOSlashInUtf8 + '=' + urlEncodedOSlashInUtf8, { charsetSentinel: true, charset: 'utf-8' }), { 'Ã¸': 'Ã¸' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not require the utf8 sentinel to be defined before the parameters whose decoding it affects', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('a=' + urlEncodedOSlashInUtf8 + '&utf8=' + urlEncodedNumCheckmark, { charsetSentinel: true, charset: 'utf-8' }), { a: 'Ã¸' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('should ignore an utf8 sentinel with an unknown value', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('utf8=foo&' + urlEncodedOSlashInUtf8 + '=' + urlEncodedOSlashInUtf8, { charsetSentinel: true, charset: 'utf-8' }), { ø: 'ø' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses the utf8 sentinel to switch to utf-8 when no default charset is given', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('utf8=' + urlEncodedCheckmarkInUtf8 + '&' + urlEncodedOSlashInUtf8 + '=' + urlEncodedOSlashInUtf8, { charsetSentinel: true }), { ø: 'ø' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses the utf8 sentinel to switch to iso-8859-1 when no default charset is given', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('utf8=' + urlEncodedNumCheckmark + '&' + urlEncodedOSlashInUtf8 + '=' + urlEncodedOSlashInUtf8, { charsetSentinel: true }), { 'Ã¸': 'Ã¸' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('interprets numeric entities in iso-8859-1 when `interpretNumericEntities`', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=' + urlEncodedNumSmiley, { charset: 'iso-8859-1', interpretNumericEntities: true }), { foo: '☺' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('handles a custom decoder returning `null`, in the `iso-8859-1` charset, when `interpretNumericEntities`', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=&bar=' + urlEncodedNumSmiley, {[m
[32m+[m[32m            charset: 'iso-8859-1',[m
[32m+[m[32m            decoder: function (str, defaultDecoder, charset) {[m
[32m+[m[32m                return str ? defaultDecoder(str, defaultDecoder, charset) : null;[m
[32m+[m[32m            },[m
[32m+[m[32m            interpretNumericEntities: true[m
[32m+[m[32m        }), { foo: null, bar: '☺' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not interpret numeric entities in iso-8859-1 when `interpretNumericEntities` is absent', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=' + urlEncodedNumSmiley, { charset: 'iso-8859-1' }), { foo: '&#9786;' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not interpret numeric entities when the charset is utf-8, even when `interpretNumericEntities`', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('foo=' + urlEncodedNumSmiley, { charset: 'utf-8', interpretNumericEntities: true }), { foo: '&#9786;' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not interpret %uXXXX syntax in iso-8859-1 mode', function (st) {[m
[32m+[m[32m        st.deepEqual(qs.parse('%u263A=%u263A', { charset: 'iso-8859-1' }), { '%u263A': '%u263A' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('allows for decoding keys and values differently', function (st) {[m
[32m+[m[32m        var decoder = function (str, defaultDecoder, charset, type) {[m
[32m+[m[32m            if (type === 'key') {[m
[32m+[m[32m                return defaultDecoder(str, defaultDecoder, charset, type).toLowerCase();[m
[32m+[m[32m            }[m
[32m+[m[32m            if (type === 'value') {[m
[32m+[m[32m                return defaultDecoder(str, defaultDecoder, charset, type).toUpperCase();[m
[32m+[m[32m            }[m
[32m+[m[32m            throw 'this should never happen! type: ' + type;[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        st.deepEqual(qs.parse('KeY=vAlUe', { decoder: decoder }), { key: 'VALUE' });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
