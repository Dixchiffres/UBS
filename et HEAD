[36m@@ -0,0 +1,35 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar gOPD = require('../');[m
[32m+[m
[32m+[m[32mtest('gOPD', function (t) {[m
[32m+[m	[32mt.test('supported', { skip: !gOPD }, function (st) {[m
[32m+[m		[32mst.equal(typeof gOPD, 'function', 'is a function');[m
[32m+[m
[32m+[m		[32mvar obj = { x: 1 };[m
[32m+[m		[32mst.ok('x' in obj, 'property exists');[m
[32m+[m
[32m+[m		[32mvar desc = gOPD(obj, 'x');[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mdesc,[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: true,[m
[32m+[m				[32menumerable: true,[m
[32m+[m				[32mvalue: 1,[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'descriptor is as expected'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.test('not supported', { skip: gOPD }, function (st) {[m
[32m+[m		[32mst.notOk(gOPD, 'is falsy');[m
[32m+[m
[32m+[m		[32mst.end();[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
