[1mdiff --git a/node_modules/es-define-property/test/index.js b/node_modules/es-define-property/test/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..dbc054e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/es-define-property/test/index.js[m
[36m@@ -0,0 +1,55 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar $defineProperty = require('../');[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar gOPD = require('gopd');[m
[32m+[m
[32m+[m[32mtest('defineProperty: supported', { skip: !$defineProperty }, function (t) {[m
[32m+[m	[32mt.plan(4);[m
[32m+[m
[32m+[m	[32mt.equal(typeof $defineProperty, 'function', 'defineProperty is supported');[m
[32m+[m	[32mif ($defineProperty && gOPD) { // this `if` check is just to shut TS up[m
[32m+[m		[32mvar o = { a: 1 };[m
[32m+[m
[32m+[m		[32m$defineProperty(o, 'b', { enumerable: true, value: 2 });[m
[32m+[m		[32mt.deepEqual([m
[32m+[m			[32mgOPD(o, 'b'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: false,[m
[32m+[m				[32menumerable: true,[m
[32m+[m				[32mvalue: 2,[m
[32m+[m				[32mwritable: false[m
[32m+[m			[32m},[m
[32m+[m			[32m'property descriptor is as expected'[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32m$defineProperty(o, 'c', { enumerable: false, value: 3, writable: true });[m
[32m+[m		[32mt.deepEqual([m
[32m+[m			[32mgOPD(o, 'c'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: false,[m
[32m+[m				[32menumerable: false,[m
[32m+[m				[32mvalue: 3,[m
[32m+[m				[32mwritable: true[m
[32m+[m			[32m},[m
[32m+[m			[32m'property descriptor is as expected'[m
[32m+[m		[32m);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mt.equal($defineProperty, Object.defineProperty, 'defineProperty is Object.defineProperty');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('defineProperty: not supported', { skip: !!$defineProperty }, function (t) {[m
[32m+[m	[32mt.notOk($defineProperty, 'defineProperty is not supported');[m
[32m+[m
[32m+[m	[32mt.match([m
[32m+[m		[32mtypeof $defineProperty,[m
[32m+[m		[32m/^(?:undefined|boolean)$/,[m
[32m+[m		[32m'`typeof defineProperty` is `undefined` or `boolean`'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
