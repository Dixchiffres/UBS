[32m+[m[32m        '  {',[m
[32m+[m[32m        '    \'a-b\': 5',[m
[32m+[m[32m        '  }',[m
[32m+[m[32m        ']'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedTabs = [[m
[32m+[m[32m        '[',[m
[32m+[m[32m        '	{},',[m
[32m+[m[32m        '	[],',[m
[32m+[m[32m        '	{',[m
[32m+[m[32m        '		\'a-b\': 5',[m
[32m+[m[32m        '	}',[m
[32m+[m[32m        ']'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(obj, { indent: 2 }), expectedSpaces, 'two');[m
[32m+[m[32m    t.equal(inspect(obj, { indent: '\t' }), expectedTabs, 'tabs');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Map', { skip: typeof Map !== 'function' }, function (t) {[m
[32m+[m[32m    var map = new Map();[m
[32m+[m[32m    map.set({ a: 1 }, ['b']);[m
[32m+[m[32m    map.set(3, NaN);[m
[32m+[m
[32m+[m[32m    var expectedStringSpaces = [[m
[32m+[m[32m        'Map (2) {',[m
[32m+[m[32m        '  { a: 1 } => [ \'b\' ],',[m
[32m+[m[32m        '  3 => NaN',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedStringTabs = [[m
[32m+[m[32m        'Map (2) {',[m
[32m+[m[32m        '	{ a: 1 } => [ \'b\' ],',[m
[32m+[m[32m        '	3 => NaN',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedStringTabsDoubleQuotes = [[m
[32m+[m[32m        'Map (2) {',[m
[32m+[m[32m        '	{ a: 1 } => [ "b" ],',[m
[32m+[m[32m        '	3 => NaN',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(map, { indent: 2 }),[m
[32m+[m[32m        expectedStringSpaces,[m
[32m+[m[32m        'Map keys are not indented (two)'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(map, { indent: '\t' }),[m
[32m+[m[32m        expectedStringTabs,[m
[32m+[m[32m        'Map keys are not indented (tabs)'[m
[32m+[m[32m    );[m
[32m+[m[32m    t.equal([m
[32m+[m[32m        inspect(map, { indent: '\t', quoteStyle: 'double' }),[m
[32m+[m[32m        expectedStringTabsDoubleQuotes,[m
[32m+[m[32m        'Map keys are not indented (tabs + double quotes)'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    t.equal(inspect(new Map(), { indent: 2 }), 'Map (0) {}', 'empty Map should show as empty (two)');[m
[32m+[m[32m    t.equal(inspect(new Map(), { indent: '\t' }), 'Map (0) {}', 'empty Map should show as empty (tabs)');[m
[32m+[m
[32m+[m[32m    var nestedMap = new Map();[m
[32m+[m[32m    nestedMap.set(nestedMap, map);[m
[32m+[m[32m    var expectedNestedSpaces = [[m
[32m+[m[32m        'Map (1) {',[m
[32m+[m[32m        '  [Circular] => Map (2) {',[m
[32m+[m[32m        '    { a: 1 } => [ \'b\' ],',[m
[32m+[m[32m        '    3 => NaN',[m
[32m+[m[32m        '  }',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedNestedTabs = [[m
[32m+[m[32m        'Map (1) {',[m
[32m+[m[32m        '	[Circular] => Map (2) {',[m
[32m+[m[32m        '		{ a: 1 } => [ \'b\' ],',[m
[32m+[m[32m        '		3 => NaN',[m
[32m+[m[32m        '	}',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    t.equal(inspect(nestedMap, { indent: 2 }), expectedNestedSpaces, 'Map containing a Map should work (two)');[m
[32m+[m[32m    t.equal(inspect(nestedMap, { indent: '\t' }), expectedNestedTabs, 'Map containing a Map should work (tabs)');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('Set', { skip: typeof Set !== 'function' }, function (t) {[m
[32m+[m[32m    var set = new Set();[m
[32m+[m[32m    set.add({ a: 1 });[m
[32m+[m[32m    set.add(['b']);[m
[32m+[m[32m    var expectedStringSpaces = [[m
[32m+[m[32m        'Set (2) {',[m
[32m+[m[32m        '  {',[m
[32m+[m[32m        '    a: 1',[m
[32m+[m[32m        '  },',[m
[32m+[m[32m        '  [ \'b\' ]',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedStringTabs = [[m
[32m+[m[32m        'Set (2) {',[m
[32m+[m[32m        '	{',[m
[32m+[m[32m        '		a: 1',[m
[32m+[m[32m        '	},',[m
[32m+[m[32m        '	[ \'b\' ]',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    t.equal(inspect(set, { indent: 2 }), expectedStringSpaces, 'new Set([{ a: 1 }, ["b"]]) should show size and contents (two)');[m
[32m+[m[32m    t.equal(inspect(set, { indent: '\t' }), expectedStringTabs, 'new Set([{ a: 1 }, ["b"]]) should show size and contents (tabs)');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(new Set(), { indent: 2 }), 'Set (0) {}', 'empty Set should show as empty (two)');[m
[32m+[m[32m    t.equal(inspect(new Set(), { indent: '\t' }), 'Set (0) {}', 'empty Set should show as empty (tabs)');[m
[32m+[m
[32m+[m[32m    var nestedSet = new Set();[m
[32m+[m[32m    nestedSet.add(set);[m
[32m+[m[32m    nestedSet.add(nestedSet);[m
[32m+[m[32m    var expectedNestedSpaces = [[m
[32m+[m[32m        'Set (2) {',[m
[32m+[m[32m        '  Set (2) {',[m
[32m+[m[32m        '    {',[m
[32m+[m[32m        '      a: 1',[m
[32m+[m[32m        '    },',[m
[32m+[m[32m        '    [ \'b\' ]',[m
[32m+[m[32m        '  },',[m
[32m+[m[32m        '  [Circular]',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedNestedTabs = [[m
[32m+[m[32m        'Set (2) {',[m
[32m+[m[32m        '	Set (2) {',[m
[32m+[m[32m        '		{',[m
[32m+[m[32m        '			a: 1',[m
[32m+[m[32m        '		},',[m
[32m+[m[32m        '		[ \'b\' ]',[m
[32m+[m[32m        '	},',[m
[32m+[m[32m        '	[Circular]',[m
[32m+[m[32m        '}'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    t.equal(inspect(nestedSet, { indent: 2 }), expectedNestedSpaces, 'Set containing a Set should work (two)');[m
[32m+[m[32m    t.equal(inspect(nestedSet, { indent: '\t' }), expectedNestedTabs, 'Set containing a Set should work (tabs)');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
