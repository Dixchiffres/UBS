[32m+[m[32mfunction weakCollectionOf(type) {[m
[32m+[m[32m    return type + ' { ? }';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction collectionOf(type, size, entries, indent) {[m
[32m+[m[32m    var joinedEntries = indent ? indentedJoin(entries, indent) : $join.call(entries, ', ');[m
[32m+[m[32m    return type + ' (' + size + ') {' + joinedEntries + '}';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction singleLineValues(xs) {[m
[32m+[m[32m    for (var i = 0; i < xs.length; i++) {[m
[32m+[m[32m        if (indexOf(xs[i], '\n') >= 0) {[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getIndent(opts, depth) {[m
[32m+[m[32m    var baseIndent;[m
[32m+[m[32m    if (opts.indent === '\t') {[m
[32m+[m[32m        baseIndent = '\t';[m
[32m+[m[32m    } else if (typeof opts.indent === 'number' && opts.indent > 0) {[m
[32m+[m[32m        baseIndent = $join.call(Array(opts.indent + 1), ' ');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        return null;[m
[32m+[m[32m    }[m
[32m+[m[32m    return {[m
[32m+[m[32m        base: baseIndent,[m
[32m+[m[32m        prev: $join.call(Array(depth + 1), baseIndent)[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction indentedJoin(xs, indent) {[m
[32m+[m[32m    if (xs.length === 0) { return ''; }[m
[32m+[m[32m    var lineJoiner = '\n' + indent.prev + indent.base;[m
[32m+[m[32m    return lineJoiner + $join.call(xs, ',' + lineJoiner) + '\n' + indent.prev;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction arrObjKeys(obj, inspect) {[m
[32m+[m[32m    var isArr = isArray(obj);[m
[32m+[m[32m    var xs = [];[m
[32m+[m[32m    if (isArr) {[m
[32m+[m[32m        xs.length = obj.length;[m
[32m+[m[32m        for (var i = 0; i < obj.length; i++) {[m
[32m+[m[32m            xs[i] = has(obj, i) ? inspect(obj[i], obj) : '';[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    var syms = typeof gOPS === 'function' ? gOPS(obj) : [];[m
[32m+[m[32m    var symMap;[m
[32m+[m[32m    if (hasShammedSymbols) {[m
[32m+[m[32m        symMap = {};[m
[32m+[m[32m        for (var k = 0; k < syms.length; k++) {[m
[32m+[m[32m            symMap['$' + syms[k]] = syms[k];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var key in obj) { // eslint-disable-line no-restricted-syntax[m
[32m+[m[32m        if (!has(obj, key)) { continue; } // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        if (isArr && String(Number(key)) === key && key < obj.length) { continue; } // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        if (hasShammedSymbols && symMap['$' + key] instanceof Symbol) {[m
[32m+[m[32m            // this is to prevent shammed Symbols, which are stored as strings, from being included in the string key section[m
[32m+[m[32m            continue; // eslint-disable-line no-restricted-syntax, no-continue[m
[32m+[m[32m        } else if ($test.call(/[^\w$]/, key)) {[m
[32m+[m[32m            xs.push(inspect(key, obj) + ': ' + inspect(obj[key], obj));[m
[32m+[m[32m        } else {[m
[32m+[m[32m            xs.push(key + ': ' + inspect(obj[key], obj));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (typeof gOPS === 'function') {[m
[32m+[m[32m        for (var j = 0; j < syms.length; j++) {[m
[32m+[m[32m            if (isEnumerable.call(obj, syms[j])) {[m
[32m+[m[32m                xs.push('[' + inspect(syms[j]) + ']: ' + inspect(obj[syms[j]], obj));[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return xs;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m},{"./util.inspect":6}],16:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar GetIntrinsic = require('get-intrinsic');[m
[32m+[m[32mvar callBound = require('call-bind/callBound');[m
[32m+[m[32mvar inspect = require('object-inspect');[m
[32m+[m
[32m+[m[32mvar $TypeError = GetIntrinsic('%TypeError%');[m
[32m+[m[32mvar $WeakMap = GetIntrinsic('%WeakMap%', true);[m
[32m+[m[32mvar $Map = GetIntrinsic('%Map%', true);[m
[32m+[m
[32m+[m[32mvar $weakMapGet = callBound('WeakMap.prototype.get', true);[m
[32m+[m[32mvar $weakMapSet = callBound('WeakMap.prototype.set', true);[m
[32m+[m[32mvar $weakMapHas = callBound('WeakMap.prototype.has', true);[m
[32m+[m[32mvar $mapGet = callBound('Map.prototype.get', true);[m
[32m+[m[32mvar $mapSet = callBound('Map.prototype.set', true);[m
[32m+[m[32mvar $mapHas = callBound('Map.prototype.has', true);[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * This function traverses the list returning the node corresponding to the[m
[32m+[m[32m * given key.[m
[32m+[m[32m *[m
[32m+[m[32m * That node is also moved to the head of the list, so that if it's accessed[m
[32m+[m[32m * again we don't need to traverse the whole list. By doing so, all the recently[m
[32m+[m[32m * used nodes can be accessed relatively quickly.[m
[32m+[m[32m */[m
[32m+[m[32mvar listGetNode = function (list, key) { // eslint-disable-line consistent-return[m
[32m+[m	[32mfor (var prev = list, curr; (curr = prev.next) !== null; prev = curr) {[m
[32m+[m		[32mif (curr.key === key) {[m
[32m+[m			[32mprev.next = curr.next;[m
[32m+[m			[32mcurr.next = list.next;[m
[32m+[m			[32mlist.next = curr; // eslint-disable-line no-param-reassign[m
[32m+[m			[32mreturn curr;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar listGet = function (objects, key) {[m
[32m+[m	[32mvar node = listGetNode(objects, key);[m
[32m+[m	[32mreturn node && node.value;[m
[32m+[m[32m};[m
[32m+[m[32mvar listSet = function (objects, key, value) {[m
[32m+[m	[32mvar node = listGetNode(objects, key);[m
[32m+[m	[32mif (node) {[m
[32m+[m		[32mnode.value = value;[m
[32m+[m	[32m} else {[m
[32m+[m		[32m// Prepend the new node to the beginning of the list[m
[32m+[m		[32mobjects.next = { // eslint-disable-line no-param-reassign[m
[32m+[m			[32mkey: key,[m
[32m+[m			[32mnext: objects.next,[m
[32m+[m			[32mvalue: value[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m[32mvar listHas = function (objects, key) {[m
[32m+[m	[32mreturn !!listGetNode(objects, key);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function getSideChannel() {[m
[32m+[m	[32mvar $wm;[m
[32m+[m	[32mvar $m;[m
[32m+[m	[32mvar $o;[m
[32m+[m	[32mvar channel = {[m
[32m+[m		[32massert: function (key) {[m
[32m+[m			[32mif (!channel.has(key)) {[m
[32m+[m				[32mthrow new $TypeError('Side channel does not contain ' + inspect(key));[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mget: function (key) { // eslint-disable-line consistent-return[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif ($wm) {[m
[32m+[m					[32mreturn $weakMapGet($wm, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif ($m) {[m
[32m+[m					[32mreturn $mapGet($m, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif ($o) { // eslint-disable-line no-lonely-if[m
[32m+[m					[32mreturn listGet($o, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mhas: function (key) {[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif ($wm) {[m
[32m+[m					[32mreturn $weakMapHas($wm, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif ($m) {[m
[32m+[m					[32mreturn $mapHas($m, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif ($o) { // eslint-disable-line no-lonely-if[m
[32m+[m					[32mreturn listHas($o, key);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m},[m
[32m+[m		[32mset: function (key, value) {[m
[32m+[m			[32mif ($WeakMap && key && (typeof key === 'object' || typeof key === 'function')) {[m
[32m+[m				[32mif (!$wm) {[m
[32m+[m					[32m$wm = new $WeakMap();[m
[32m+[m				[32m}[m
[32m+[m				[32m$weakMapSet($wm, key, value);[m
[32m+[m			[32m} else if ($Map) {[m
[32m+[m				[32mif (!$m) {[m
[32m+[m					[32m$m = new $Map();[m
[32m+[m				[32m}[m
[32m+[m				[32m$mapSet($m, key, value);[m
[32m+[m			[32m} else {[m
[32m+[m				[32mif (!$o) {[m
[32m+[m					[32m/*[m
[32m+[m					[32m * Initialize the linked list as an empty node, so that we don't have[m
[32m+[m					[32m * to special-case handling of the first node: we can always refer to[m
[32m+[m					[32m * it as (previous node).next, instead of something like (list).head[m
[32m+[m					[32m */[m
[32m+[m					[32m$o = { key: {}, next: null };[m
[32m+[m				[32m}[m
[32m+[m				[32mlistSet($o, key, value);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m	[32mreturn channel;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"call-bind/callBound":7,"get-intrinsic":11,"object-inspect":15}]},{},[2])(2)[m
[32m+[m[32m});[m
