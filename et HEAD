[1mdiff --git a/node_modules/ee-first/index.js b/node_modules/ee-first/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..501287c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/ee-first/index.js[m
[36m@@ -0,0 +1,95 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * ee-first[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = first[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the first event in a set of event emitters and event pairs.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {array} stuff[m
[32m+[m[32m * @param {function} done[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction first(stuff, done) {[m
[32m+[m[32m  if (!Array.isArray(stuff))[m
[32m+[m[32m    throw new TypeError('arg must be an array of [ee, events...] arrays')[m
[32m+[m
[32m+[m[32m  var cleanups = [][m
[32m+[m
[32m+[m[32m  for (var i = 0; i < stuff.length; i++) {[m
[32m+[m[32m    var arr = stuff[i][m
[32m+[m
[32m+[m[32m    if (!Array.isArray(arr) || arr.length < 2)[m
[32m+[m[32m      throw new TypeError('each array member must be [ee, events...]')[m
[32m+[m
[32m+[m[32m    var ee = arr[0][m
[32m+[m
[32m+[m[32m    for (var j = 1; j < arr.length; j++) {[m
[32m+[m[32m      var event = arr[j][m
[32m+[m[32m      var fn = listener(event, callback)[m
[32m+[m
[32m+[m[32m      // listen to the event[m
[32m+[m[32m      ee.on(event, fn)[m
[32m+[m[32m      // push this listener to the list of cleanups[m
[32m+[m[32m      cleanups.push({[m
[32m+[m[32m        ee: ee,[m
[32m+[m[32m        event: event,[m
[32m+[m[32m        fn: fn,[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function callback() {[m
[32m+[m[32m    cleanup()[m
[32m+[m[32m    done.apply(null, arguments)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function cleanup() {[m
[32m+[m[32m    var x[m
[32m+[m[32m    for (var i = 0; i < cleanups.length; i++) {[m
[32m+[m[32m      x = cleanups[i][m
[32m+[m[32m      x.ee.removeListener(x.event, x.fn)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function thunk(fn) {[m
[32m+[m[32m    done = fn[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  thunk.cancel = cleanup[m
[32m+[m
[32m+[m[32m  return thunk[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create the event listener.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction listener(event, done) {[m
[32m+[m[32m  return function onevent(arg1) {[m
[32m+[m[32m    var args = new Array(arguments.length)[m
[32m+[m[32m    var ee = this[m
[32m+[m[32m    var err = event === 'error'[m
[32m+[m[32m      ? arg1[m
[32m+[m[32m      : null[m
[32m+[m
[32m+[m[32m    // copy args to prevent arguments escaping scope[m
[32m+[m[32m    for (var i = 0; i < args.length; i++) {[m
[32m+[m[32m      args[i] = arguments[i][m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    done(err, ee, event, args)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/ee-first/package.json b/node_modules/ee-first/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..b6d0b7d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/ee-first/package.json[m
[36m@@ -0,0 +1,29 @@[m
