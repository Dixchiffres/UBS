[32m+[m	[32mt.test('sealed object, ES5+', { skip: !hasPropertyDescriptors }, function (st) {[m
[32m+[m		[32mvar sealed = Object.seal({ existing: true });[m
[32m+[m		[32mst.deepEqual([m
[32m+[m			[32mObject.getOwnPropertyDescriptor(sealed, 'existing'),[m
[32m+[m			[32m{[m
[32m+[m				[32mconfigurable: false,[m
