[1m--- /dev/null[m
[1m+++ b/node_modules/has-proto/test/index.js[m
[36m@@ -0,0 +1,19 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar hasProto = require('../');[m
[32m+[m
[32m+[m[32mtest('hasProto', function (t) {[m
[32m+[m	[32mvar result = hasProto();[m
[32m+[m	[32mt.equal(typeof result, 'boolean', 'returns a boolean (' + result + ')');[m
[32m+[m
[32m+[m	[32mvar obj = { __proto__: null };[m
[32m+[m	[32mif (result) {[m
[32m+[m		[32mt.notOk('toString' in obj, 'null object lacks toString');[m
[32m+[m	[32m} else {[m
[32m+[m		[32mt.ok('toString' in obj, 'without proto, null object has toString');[m
[32m+[m		[32mt.equal(obj.__proto__, null); // eslint-disable-line no-proto[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
