[32m+[m[32mvar inspect = require('../');[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mtest('deep', function (t) {[m
[32m+[m[32m    t.plan(4);[m
[32m+[m[32m    var obj = [[[[[[500]]]]]];[m
[32m+[m[32m    t.equal(inspect(obj), '[ [ [ [ [ [Array] ] ] ] ] ]');[m
[32m+[m[32m    t.equal(inspect(obj, { depth: 4 }), '[ [ [ [ [Array] ] ] ] ]');[m
[32m+[m[32m    t.equal(inspect(obj, { depth: 2 }), '[ [ [Array] ] ]');[m
[32m+[m
[32m+[m[32m    t.equal(inspect([[[{ a: 1 }]]], { depth: 3 }), '[ [ [ [Object] ] ] ]');[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/element.js b/node_modules/object-inspect/test/element.js[m
[1mnew file mode 100644[m
[1mindex 0000000..47fa9e2[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/test/element.js[m
[36m@@ -0,0 +1,53 @@[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mtest('element', function (t) {[m
[32m+[m[32m    t.plan(3);[m
[32m+[m[32m    var elem = {[m
[32m+[m[32m        nodeName: 'div',[m
[32m+[m[32m        attributes: [{ name: 'class', value: 'row' }],[m
[32m+[m[32m        getAttribute: function (key) { return key; },[m
[32m+[m[32m        childNodes: [][m
[32m+[m[32m    };[m
[32m+[m[32m    var obj = [1, elem, 3];[m
[32m+[m[32m    t.deepEqual(inspect(obj), '[ 1, <div class="row"></div>, 3 ]');[m
[32m+[m[32m    t.deepEqual(inspect(obj, { quoteStyle: 'single' }), "[ 1, <div class='row'></div>, 3 ]");[m
[32m+[m[32m    t.deepEqual(inspect(obj, { quoteStyle: 'double' }), '[ 1, <div class="row"></div>, 3 ]');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('element no attr', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var elem = {[m
[32m+[m[32m        nodeName: 'div',[m
[32m+[m[32m        getAttribute: function (key) { return key; },[m
[32m+[m[32m        childNodes: [][m
[32m+[m[32m    };[m
[32m+[m[32m    var obj = [1, elem, 3];[m
[32m+[m[32m    t.deepEqual(inspect(obj), '[ 1, <div></div>, 3 ]');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('element with contents', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var elem = {[m
[32m+[m[32m        nodeName: 'div',[m
[32m+[m[32m        getAttribute: function (key) { return key; },[m
[32m+[m[32m        childNodes: [{ nodeName: 'b' }][m
[32m+[m[32m    };[m
[32m+[m[32m    var obj = [1, elem, 3];[m
[32m+[m[32m    t.deepEqual(inspect(obj), '[ 1, <div>...</div>, 3 ]');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('element instance', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var h = global.HTMLElement;[m
[32m+[m[32m    global.HTMLElement = function (name, attr) {[m
[32m+[m[32m        this.nodeName = name;[m
[32m+[m[32m        this.attributes = attr;[m
[32m+[m[32m    };[m
[32m+[m[32m    global.HTMLElement.prototype.getAttribute = function () {};[m
[32m+[m
[32m+[m[32m    var elem = new global.HTMLElement('div', []);[m
[32m+[m[32m    var obj = [1, elem, 3];[m
[32m+[m[32m    t.deepEqual(inspect(obj), '[ 1, <div></div>, 3 ]');[m
[32m+[m[32m    global.HTMLElement = h;[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/object-inspect/test/err.js b/node_modules/object-inspect/test/err.js[m
[1mnew file mode 100644[m
[1mindex 0000000..cc1d884[m
[1m--- /dev/null[m
[1m+++ b/node_modules/object-inspect/test/err.js[m
[36m@@ -0,0 +1,48 @@[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar ErrorWithCause = require('error-cause/Error');[m
[32m+[m
[32m+[m[32mvar inspect = require('../');[m
[32m+[m
[32m+[m[32mtest('type error', function (t) {[m
[32m+[m[32m    t.plan(1);[m
[32m+[m[32m    var aerr = new TypeError();[m
[32m+[m[32m    aerr.foo = 555;[m
[32m+[m[32m    aerr.bar = [1, 2, 3];[m
[32m+[m
[32m+[m[32m    var berr = new TypeError('tuv');[m
[32m+[m[32m    berr.baz = 555;[m
[32m+[m
[32m+[m[32m    var cerr = new SyntaxError();[m
[32m+[m[32m    cerr.message = 'whoa';[m
[32m+[m[32m    cerr['a-b'] = 5;[m
[32m+[m
[32m+[m[32m    var withCause = new ErrorWithCause('foo', { cause: 'bar' });[m
[32m+[m[32m    var withCausePlus = new ErrorWithCause('foo', { cause: 'bar' });[m
[32m+[m[32m    withCausePlus.foo = 'bar';[m
[32m+[m[32m    var withUndefinedCause = new ErrorWithCause('foo', { cause: undefined });[m
[32m+[m[32m    var withEnumerableCause = new Error('foo');[m
[32m+[m[32m    withEnumerableCause.cause = 'bar';[m
[32m+[m
[32m+[m[32m    var obj = [[m
[32m+[m[32m        new TypeError(),[m
[32m+[m[32m        new TypeError('xxx'),[m
[32m+[m[32m        aerr,[m
[32m+[m[32m        berr,[m
[32m+[m[32m        cerr,[m
[32m+[m[32m        withCause,[m
[32m+[m[32m        withCausePlus,[m
[32m+[m[32m        withUndefinedCause,[m
[32m+[m[32m        withEnumerableCause[m
[32m+[m[32m    ];[m
[32m+[m[32m    t.equal(inspect(obj), '[ ' + [[m
[32m+[m[32m        '[TypeError]',[m
[32m+[m[32m        '[TypeError: xxx]',[m
[32m+[m[32m        '{ [TypeError] foo: 555, bar: [ 1, 2, 3 ] }',[m
[32m+[m[32m        '{ [TypeError: tuv] baz: 555 }',[m
[32m+[m[32m        '{ [SyntaxError: whoa] message: \'whoa\', \'a-b\': 5 }',[m
[32m+[m[32m        'cause' in Error.prototype ? '[Error: foo]' : '{ [Error: foo] [cause]: \'bar\' }',[m
[32m+[m[32m        '{ [Error: foo] ' + ('cause' in Error.prototype ? '' : '[cause]: \'bar\', ') + 'foo: \'bar\' }',[m
[32m+[m[32m        'cause' in Error.prototype ? '[Error: foo]' : '{ [Error: foo] [cause]: undefined }',[m
[32m+[m[32m        '{ [Error: foo] cause: \'bar\' }'[m
[32m+[m[32m    ].join(', ') + ' ]');[m
[32m+[m[32m});[m
