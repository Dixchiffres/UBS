[1mnew file mode 100644[m
[1mindex 0000000..4dc8e86[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/router/layer.js[m
[36m@@ -0,0 +1,181 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar pathRegexp = require('path-to-regexp');[m
[32m+[m[32mvar debug = require('debug')('express:router:layer');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar hasOwnProperty = Object.prototype.hasOwnProperty;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = Layer;[m
[32m+[m
[32m+[m[32mfunction Layer(path, options, fn) {[m
[32m+[m[32m  if (!(this instanceof Layer)) {[m
[32m+[m[32m    return new Layer(path, options, fn);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  debug('new %o', path)[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m
[32m+[m[32m  this.handle = fn;[m
[32m+[m[32m  this.name = fn.name || '<anonymous>';[m
[32m+[m[32m  this.params = undefined;[m
[32m+[m[32m  this.path = undefined;[m
[32m+[m[32m  this.regexp = pathRegexp(path, this.keys = [], opts);[m
[32m+[m
[32m+[m[32m  // set fast path flags[m
[32m+[m[32m  this.regexp.fast_star = path === '*'[m
[32m+[m[32m  this.regexp.fast_slash = path === '/' && opts.end === false[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Handle the error for the layer.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} error[m
[32m+[m[32m * @param {Request} req[m
[32m+[m[32m * @param {Response} res[m
[32m+[m[32m * @param {function} next[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mLayer.prototype.handle_error = function handle_error(error, req, res, next) {[m
[32m+[m[32m  var fn = this.handle;[m
[32m+[m
[32m+[m[32m  if (fn.length !== 4) {[m
[32m+[m[32m    // not a standard error handler[m
[32m+[m[32m    return next(error);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    fn(error, req, res, next);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    next(err);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Handle the request for the layer.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Request} req[m
[32m+[m[32m * @param {Response} res[m
[32m+[m[32m * @param {function} next[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mLayer.prototype.handle_request = function handle(req, res, next) {[m
[32m+[m[32m  var fn = this.handle;[m
[32m+[m
[32m+[m[32m  if (fn.length > 3) {[m
[32m+[m[32m    // not a standard request handler[m
[32m+[m[32m    return next();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    fn(req, res, next);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    next(err);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if this route matches `path`, if so[m
[32m+[m[32m * populate `.params`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} path[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mLayer.prototype.match = function match(path) {[m
[32m+[m[32m  var match[m
[32m+[m
[32m+[m[32m  if (path != null) {[m
[32m+[m[32m    // fast path non-ending match for / (any path matches)[m
[32m+[m[32m    if (this.regexp.fast_slash) {[m
[32m+[m[32m      this.params = {}[m
[32m+[m[32m      this.path = ''[m
[32m+[m[32m      return true[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // fast path for * (everything matched in a param)[m
[32m+[m[32m    if (this.regexp.fast_star) {[m
[32m+[m[32m      this.params = {'0': decode_param(path)}[m
[32m+[m[32m      this.path = path[m
[32m+[m[32m      return true[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // match the path[m
[32m+[m[32m    match = this.regexp.exec(path)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!match) {[m
[32m+[m[32m    this.params = undefined;[m
[32m+[m[32m    this.path = undefined;[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // store values[m
[32m+[m[32m  this.params = {};[m
[32m+[m[32m  this.path = match[0][m
[32m+[m
[32m+[m[32m  var keys = this.keys;[m
[32m+[m[32m  var params = this.params;[m
[32m+[m
[32m+[m[32m  for (var i = 1; i < match.length; i++) {[m
[32m+[m[32m    var key = keys[i - 1];[m
[32m+[m[32m    var prop = key.name;[m
[32m+[m[32m    var val = decode_param(match[i])[m
[32m+[m
[32m+[m[32m    if (val !== undefined || !(hasOwnProperty.call(params, prop))) {[m
[32m+[m[32m      params[prop] = val;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return true;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Decode param value.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction decode_param(val) {[m
[32m+[m[32m  if (typeof val !== 'string' || val.length === 0) {[m
[32m+[m[32m    return val;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    return decodeURIComponent(val);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    if (err instanceof URIError) {[m
[32m+[m[32m      err.message = 'Failed to decode param \'' + val + '\'';[m
[32m+[m[32m      err.status = err.statusCode = 400;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    throw err;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
