[32m+[m[32m  "description": "Unpipe a stream from all destinations",[m
[32m+[m[32m  "version": "1.0.0",[m
[32m+[m[32m  "author": "Douglas Christopher Wilson <doug@somethingdoug.com>",[m
[32m+[m[32m  "license": "MIT",[m
[32m+[m[32m  "repository": "stream-utils/unpipe",[m
[32m+[m[32m  "devDependencies": {[m
