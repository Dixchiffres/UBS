[32m+[m	[32m/* eslint-enable global-require */[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/has-symbols/test/tests.js b/node_modules/has-symbols/test/tests.js[m
[1mnew file mode 100644[m
[1mindex 0000000..89edd12[m
[1m--- /dev/null[m
[1m+++ b/node_modules/has-symbols/test/tests.js[m
[36m@@ -0,0 +1,56 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m// eslint-disable-next-line consistent-return[m
[32m+[m[32mmodule.exports = function runSymbolTests(t) {[m
[32m+[m	[32mt.equal(typeof Symbol, 'function', 'global Symbol is a function');[m
[32m+[m
[32m+[m	[32mif (typeof Symbol !== 'function') { return false; }[m
[32m+[m
[32m+[m	[32mt.notEqual(Symbol(), Symbol(), 'two symbols are not equal');[m
[32m+[m
[32m+[m	[32m/*[m
[32m+[m	[32mt.equal([m
[32m+[m		[32mSymbol.prototype.toString.call(Symbol('foo')),[m
[32m+[m		[32mSymbol.prototype.toString.call(Symbol('foo')),[m
[32m+[m		[32m'two symbols with the same description stringify the same'[m
[32m+[m	[32m);[m
[32m+[m	[32m*/[m
[32m+[m
[32m+[m	[32m/*[m
[32m+[m	[32mvar foo = Symbol('foo');[m
[32m+[m
[32m+[m	[32mt.notEqual([m
[32m+[m		[32mString(foo),[m
[32m+[m		[32mString(Symbol('bar')),[m
[32m+[m		[32m'two symbols with different descriptions do not stringify the same'[m
[32m+[m	[32m);[m
[32m+[m	[32m*/[m
[32m+[m
[32m+[m	[32mt.equal(typeof Symbol.prototype.toString, 'function', 'Symbol#toString is a function');[m
[32m+[m	[32m// t.equal(String(foo), Symbol.prototype.toString.call(foo), 'Symbol#toString equals String of the same symbol');[m
[32m+[m
[32m+[m	[32mt.equal(typeof Object.getOwnPropertySymbols, 'function', 'Object.getOwnPropertySymbols is a function');[m
[32m+[m
[32m+[m	[32mvar obj = {};[m
[32m+[m	[32mvar sym = Symbol('test');[m
[32m+[m	[32mvar symObj = Object(sym);[m
[32m+[m	[32mt.notEqual(typeof sym, 'string', 'Symbol is not a string');[m
[32m+[m	[32mt.equal(Object.prototype.toString.call(sym), '[object Symbol]', 'symbol primitive Object#toStrings properly');[m
[32m+[m	[32mt.equal(Object.prototype.toString.call(symObj), '[object Symbol]', 'symbol primitive Object#toStrings properly');[m
[32m+[m
[32m+[m	[32mvar symVal = 42;[m
[32m+[m	[32mobj[sym] = symVal;[m
[32m+[m	[32m// eslint-disable-next-line no-restricted-syntax[m
[32m+[m	[32mfor (sym in obj) { t.fail('symbol property key was found in for..in of object'); }[m
[32m+[m
[32m+[m	[32mt.deepEqual(Object.keys(obj), [], 'no enumerable own keys on symbol-valued object');[m
[32m+[m	[32mt.deepEqual(Object.getOwnPropertyNames(obj), [], 'no own names on symbol-valued object');[m
[32m+[m	[32mt.deepEqual(Object.getOwnPropertySymbols(obj), [sym], 'one own symbol on symbol-valued object');[m
[32m+[m	[32mt.equal(Object.prototype.propertyIsEnumerable.call(obj, sym), true, 'symbol is enumerable');[m
[32m+[m	[32mt.deepEqual(Object.getOwnPropertyDescriptor(obj, sym), {[m
[32m+[m		[32mconfigurable: true,[m
[32m+[m		[32menumerable: true,[m
[32m+[m		[32mvalue: 42,[m
[32m+[m		[32mwritable: true[m
[32m+[m	[32m}, 'property descriptor is correct');[m
[32m+[m[32m};[m
