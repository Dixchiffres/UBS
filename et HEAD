[32m+[m[32mfunction updateTime() {[m
[32m+[m[32m    const now = new Date();[m
[32m+[m[32m    const date = now.toLocaleDateString('fr-FR');[m
[32m+[m[32m    const time = now.toLocaleTimeString('fr-FR');[m
[32m+[m
[32m+[m[32m    document.getElementById('date').textContent = `Date actuelle : ${date}`;[m
[32m+[m[32m    document.getElementById('time').textContent = `Heure actuelle : ${time}`;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32msetInterval(updateTime, 1000);[m
[32m+[m[32mupdateTime();[m
