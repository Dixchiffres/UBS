[32m+[m[32m"use strict";[m
[32m+[m[32mvar Buffer = require("buffer").Buffer;[m
[32m+[m[32m// Note: not polyfilled with safer-buffer on a purpose, as overrides Buffer[m
[32m+[m
[32m+[m[32m// == Extend Node primitives to use iconv-lite =================================[m
[32m+[m
[32m+[m[32mmodule.exports = function (iconv) {[m
[32m+[m[32m    var original = undefined; // Place to keep original methods.[m
[32m+[m
[32m+[m[32m    // Node authors rewrote Buffer internals to make it compatible with[m
[32m+[m[32m    // Uint8Array and we cannot patch key functions since then.[m
[32m+[m[32m    // Note: this does use older Buffer API on a purpose[m
[32m+[m[32m    iconv.supportsNodeEncodingsExtension = !(Buffer.from || new Buffer(0) instanceof Uint8Array);[m
[32m+[m
[32m+[m[32m    iconv.extendNodeEncodings = function extendNodeEncodings() {[m
[32m+[m[32m        if (original) return;[m
[32m+[m[32m        original = {};[m
[32m+[m
[32m+[m[32m        if (!iconv.supportsNodeEncodingsExtension) {[m
[32m+[m[32m            console.error("ACTION NEEDED: require('iconv-lite').extendNodeEncodings() is not supported in your version of Node");[m
[32m+[m[32m            console.error("See more info at https://github.com/ashtuchkin/iconv-lite/wiki/Node-v4-compatibility");[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var nodeNativeEncodings = {[m
[32m+[m[32m            'hex': true, 'utf8': true, 'utf-8': true, 'ascii': true, 'binary': true,[m[41m [m
[32m+[m[32m            'base64': true, 'ucs2': true, 'ucs-2': true, 'utf16le': true, 'utf-16le': true,[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        Buffer.isNativeEncoding = function(enc) {[m
[32m+[m[32m            return enc && nodeNativeEncodings[enc.toLowerCase()];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // -- SlowBuffer -----------------------------------------------------------[m
[32m+[m[32m        var SlowBuffer = require('buffer').SlowBuffer;[m
[32m+[m
[32m+[m[32m        original.SlowBufferToString = SlowBuffer.prototype.toString;[m
[32m+[m[32m        SlowBuffer.prototype.toString = function(encoding, start, end) {[m
[32m+[m[32m            encoding = String(encoding || 'utf8').toLowerCase();[m
[32m+[m
[32m+[m[32m            // Use native conversion when possible[m
[32m+[m[32m            if (Buffer.isNativeEncoding(encoding))[m
[32m+[m[32m                return original.SlowBufferToString.call(this, encoding, start, end);[m
[32m+[m
[32m+[m[32m            // Otherwise, use our decoding method.[m
[32m+[m[32m            if (typeof start == 'undefined') start = 0;[m
[32m+[m[32m            if (typeof end == 'undefined') end = this.length;[m
[32m+[m[32m            return iconv.decode(this.slice(start, end), encoding);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        original.SlowBufferWrite = SlowBuffer.prototype.write;[m
[32m+[m[32m        SlowBuffer.prototype.write = function(string, offset, length, encoding) {[m
[32m+[m[32m            // Support both (string, offset, length, encoding)[m
[32m+[m[32m            // and the legacy (string, encoding, offset, length)[m
[32m+[m[32m            if (isFinite(offset)) {[m
[32m+[m[32m                if (!isFinite(length)) {[m
[32m+[m[32m                    encoding = length;[m
[32m+[m[32m                    length = undefined;[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {  // legacy[m
[32m+[m[32m                var swap = encoding;[m
[32m+[m[32m                encoding = offset;[m
[32m+[m[32m                offset = length;[m
[32m+[m[32m                length = swap;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            offset = +offset || 0;[m
[32m+[m[32m            var remaining = this.length - offset;[m
[32m+[m[32m            if (!length) {[m
[32m+[m[32m                length = remaining;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                length = +length;[m
[32m+[m[32m                if (length > remaining) {[m
[32m+[m[32m                    length = remaining;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            encoding = String(encoding || 'utf8').toLowerCase();[m
[32m+[m
[32m+[m[32m            // Use native conversion when possible[m
[32m+[m[32m            if (Buffer.isNativeEncoding(encoding))[m
[32m+[m[32m                return original.SlowBufferWrite.call(this, string, offset, length, encoding);[m
[32m+[m
[32m+[m[32m            if (string.length > 0 && (length < 0 || offset < 0))[m
[32m+[m[32m                throw new RangeError('attempt to write beyond buffer bounds');[m
[32m+[m
[32m+[m[32m            // Otherwise, use our encoding method.[m
[32m+[m[32m            var buf = iconv.encode(string, encoding);[m
[32m+[m[32m            if (buf.length < length) length = buf.length;[m
[32m+[m[32m            buf.copy(this, offset, 0, length);[m
[32m+[m[32m            return length;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // -- Buffer ---------------------------------------------------------------[m
[32m+[m
[32m+[m[32m        original.BufferIsEncoding = Buffer.isEncoding;[m
[32m+[m[32m        Buffer.isEncoding = function(encoding) {[m
[32m+[m[32m            return Buffer.isNativeEncoding(encoding) || iconv.encodingExists(encoding);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        original.BufferByteLength = Buffer.byteLength;[m
[32m+[m[32m        Buffer.byteLength = SlowBuffer.byteLength = function(str, encoding) {[m
[32m+[m[32m            encoding = String(encoding || 'utf8').toLowerCase();[m
[32m+[m
[32m+[m[32m            // Use native conversion when possible[m
[32m+[m[32m            if (Buffer.isNativeEncoding(encoding))[m
[32m+[m[32m                return original.BufferByteLength.call(this, str, encoding);[m
[32m+[m
[32m+[m[32m            // Slow, I know, but we don't have a better way yet.[m
[32m+[m[32m            return iconv.encode(str, encoding).length;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        original.BufferToString = Buffer.prototype.toString;[m
[32m+[m[32m        Buffer.prototype.toString = function(encoding, start, end) {[m
[32m+[m[32m            encoding = String(encoding || 'utf8').toLowerCase();[m
[32m+[m
[32m+[m[32m            // Use native conversion when possible[m
[32m+[m[32m            if (Buffer.isNativeEncoding(encoding))[m
[32m+[m[32m                return original.BufferToString.call(this, encoding, start, end);[m
[32m+[m
[32m+[m[32m            // Otherwise, use our decoding method.[m
[32m+[m[32m            if (typeof start == 'undefined') start = 0;[m
[32m+[m[32m            if (typeof end == 'undefined') end = this.length;[m
[32m+[m[32m            return iconv.decode(this.slice(start, end), encoding);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        original.BufferWrite = Buffer.prototype.write;[m
[32m+[m[32m        Buffer.prototype.write = function(string, offset, length, encoding) {[m
[32m+[m[32m            var _offset = offset, _length = length, _encoding = encoding;[m
[32m+[m[32m            // Support both (string, offset, length, encoding)[m
[32m+[m[32m            // and the legacy (string, encoding, offset, length)[m
[32m+[m[32m            if (isFinite(offset)) {[m
[32m+[m[32m                if (!isFinite(length)) {[m
[32m+[m[32m                    encoding = length;[m
[32m+[m[32m                    length = undefined;[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {  // legacy[m
[32m+[m[32m                var swap = encoding;[m
[32m+[m[32m                encoding = offset;[m
[32m+[m[32m                offset = length;[m
[32m+[m[32m                length = swap;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            encoding = String(encoding || 'utf8').toLowerCase();[m
[32m+[m
[32m+[m[32m            // Use native conversion when possible[m
[32m+[m[32m            if (Buffer.isNativeEncoding(encoding))[m
[32m+[m[32m                return original.BufferWrite.call(this, string, _offset, _length, _encoding);[m
[32m+[m
[32m+[m[32m            offset = +offset || 0;[m
[32m+[m[32m            var remaining = this.length - offset;[m
[32m+[m[32m            if (!length) {[m
[32m+[m[32m                length = remaining;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                length = +length;[m
[32m+[m[32m                if (length > remaining) {[m
[32m+[m[32m                    length = remaining;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (string.length > 0 && (length < 0 || offset < 0))[m
[32m+[m[32m                throw new RangeError('attempt to write beyond buffer bounds');[m
[32m+[m
[32m+[m[32m            // Otherwise, use our encoding method.[m
[32m+[m[32m            var buf = iconv.encode(string, encoding);[m
[32m+[m[32m            if (buf.length < length) length = buf.length;[m
[32m+[m[32m            buf.copy(this, offset, 0, length);[m
[32m+[m[32m            return length;[m
[32m+[m
[32m+[m[32m            // TODO: Set _charsWritten.[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        // -- Readable -------------------------------------------------------------[m
[32m+[m[32m        if (iconv.supportsStreams) {[m
[32m+[m[32m            var Readable = require('stream').Readable;[m
[32m+[m
[32m+[m[32m            original.ReadableSetEncoding = Readable.prototype.setEncoding;[m
[32m+[m[32m            Readable.prototype.setEncoding = function setEncoding(enc, options) {[m
[32m+[m[32m                // Use our own decoder, it has the same interface.[m
[32m+[m[32m                // We cannot use original function as it doesn't handle BOM-s.[m
[32m+[m[32m                this._readableState.decoder = iconv.getDecoder(enc, options);[m
[32m+[m[32m                this._readableState.encoding = enc;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            Readable.prototype.collect = iconv._collect;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Remove iconv-lite Node primitive extensions.[m
[32m+[m[32m    iconv.undoExtendNodeEncodings = function undoExtendNodeEncodings() {[m
[32m+[m[32m        if (!iconv.supportsNodeEncodingsExtension)[m
[32m+[m[32m            return;[m
[32m+[m[32m        if (!original)[m
[32m+[m[32m            throw new Error("require('iconv-lite').undoExtendNodeEncodings(): Nothing to undo; extendNodeEncodings() is not called.")[m
[32m+[m
[32m+[m[32m        delete Buffer.isNativeEncoding;[m
[32m+[m
[32m+[m[32m        var SlowBuffer = require('buffer').SlowBuffer;[m
[32m+[m
[32m+[m[32m        SlowBuffer.prototype.toString = original.SlowBufferToString;[m
[32m+[m[32m        SlowBuffer.prototype.write = original.SlowBufferWrite;[m
[32m+[m
[32m+[m[32m        Buffer.isEncoding = original.BufferIsEncoding;[m
[32m+[m[32m        Buffer.byteLength = original.BufferByteLength;[m
[32m+[m[32m        Buffer.prototype.toString = original.BufferToString;[m
[32m+[m[32m        Buffer.prototype.write = original.BufferWrite;[m
[32m+[m
[32m+[m[32m        if (iconv.supportsStreams) {[m
[32m+[m[32m            var Readable = require('stream').Readable;[m
[32m+[m
[32m+[m[32m            Readable.prototype.setEncoding = original.ReadableSetEncoding;[m
[32m+[m[32m            delete Readable.prototype.collect;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        original = undefined;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
