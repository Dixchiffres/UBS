[32m+[m[32m[deps-svg]: https://david-dm.org/ljharb/get-intrinsic.svg[m
[32m+[m[32m[deps-url]: https://david-dm.org/ljharb/get-intrinsic[m
[32m+[m[32m[dev-deps-svg]: https://david-dm.org/ljharb/get-intrinsic/dev-status.svg[m
[32m+[m[32m[dev-deps-url]: https://david-dm.org/ljharb/get-intrinsic#info=devDependencies[m
[32m+[m[32m[npm-badge-png]: https://nodei.co/npm/get-intrinsic.png?downloads=true&stars=true[m
[32m+[m[32m[license-image]: https://img.shields.io/npm/l/get-intrinsic.svg[m
[32m+[m[32m[license-url]: LICENSE[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/get-intrinsic.svg[m
[32m+[m[32m[downloads-url]: https://npm-stat.com/charts.html?package=get-intrinsic[m
[32m+[m[32m[codecov-image]: https://codecov.io/gh/ljharb/get-intrinsic/branch/main/graphs/badge.svg[m
[32m+[m[32m[codecov-url]: https://app.codecov.io/gh/ljharb/get-intrinsic/[m
[32m+[m[32m[actions-image]: https://img.shields.io/endpoint?url=https://github-actions-badge-u3jn4tfpocch.runkit.sh/ljharb/get-intrinsic[m
[32m+[m[32m[actions-url]: https://github.com/ljharb/get-intrinsic/actions[m
[1mdiff --git a/node_modules/get-intrinsic/index.js b/node_modules/get-intrinsic/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c25e2c4[m
[1m--- /dev/null[m
[1m+++ b/node_modules/get-intrinsic/index.js[m
[36m@@ -0,0 +1,359 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar undefined;[m
[32m+[m
[32m+[m[32mvar $Error = require('es-errors');[m
[32m+[m[32mvar $EvalError = require('es-errors/eval');[m
[32m+[m[32mvar $RangeError = require('es-errors/range');[m
[32m+[m[32mvar $ReferenceError = require('es-errors/ref');[m
[32m+[m[32mvar $SyntaxError = require('es-errors/syntax');[m
[32m+[m[32mvar $TypeError = require('es-errors/type');[m
[32m+[m[32mvar $URIError = require('es-errors/uri');[m
[32m+[m
[32m+[m[32mvar $Function = Function;[m
[32m+[m
[32m+[m[32m// eslint-disable-next-line consistent-return[m
[32m+[m[32mvar getEvalledConstructor = function (expressionSyntax) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mreturn $Function('"use strict"; return (' + expressionSyntax + ').constructor;')();[m
[32m+[m	[32m} catch (e) {}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $gOPD = Object.getOwnPropertyDescriptor;[m
[32m+[m[32mif ($gOPD) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32m$gOPD({}, '');[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32m$gOPD = null; // this is IE 8, which has a broken gOPD[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar throwTypeError = function () {[m
[32m+[m	[32mthrow new $TypeError();[m
[32m+[m[32m};[m
[32m+[m[32mvar ThrowTypeError = $gOPD[m
[32m+[m	[32m? (function () {[m
[32m+[m		[32mtry {[m
[32m+[m			[32m// eslint-disable-next-line no-unused-expressions, no-caller, no-restricted-properties[m
[32m+[m			[32marguments.callee; // IE 8 does not throw here[m
[32m+[m			[32mreturn throwTypeError;[m
[32m+[m		[32m} catch (calleeThrows) {[m
[32m+[m			[32mtry {[m
[32m+[m				[32m// IE 8 throws on Object.getOwnPropertyDescriptor(arguments, '')[m
[32m+[m				[32mreturn $gOPD(arguments, 'callee').get;[m
[32m+[m			[32m} catch (gOPDthrows) {[m
[32m+[m				[32mreturn throwTypeError;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}())[m
[32m+[m	[32m: throwTypeError;[m
[32m+[m
[32m+[m[32mvar hasSymbols = require('has-symbols')();[m
[32m+[m[32mvar hasProto = require('has-proto')();[m
[32m+[m
[32m+[m[32mvar getProto = Object.getPrototypeOf || ([m
[32m+[m	[32mhasProto[m
[32m+[m		[32m? function (x) { return x.__proto__; } // eslint-disable-line no-proto[m
[32m+[m		[32m: null[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mvar needsEval = {};[m
[32m+[m
[32m+[m[32mvar TypedArray = typeof Uint8Array === 'undefined' || !getProto ? undefined : getProto(Uint8Array);[m
[32m+[m
[32m+[m[32mvar INTRINSICS = {[m
[32m+[m	[32m__proto__: null,[m
[32m+[m	[32m'%AggregateError%': typeof AggregateError === 'undefined' ? undefined : AggregateError,[m
[32m+[m	[32m'%Array%': Array,[m
[32m+[m	[32m'%ArrayBuffer%': typeof ArrayBuffer === 'undefined' ? undefined : ArrayBuffer,[m
[32m+[m	[32m'%ArrayIteratorPrototype%': hasSymbols && getProto ? getProto([][Symbol.iterator]()) : undefined,[m
[32m+[m	[32m'%AsyncFromSyncIteratorPrototype%': undefined,[m
[32m+[m	[32m'%AsyncFunction%': needsEval,[m
[32m+[m	[32m'%AsyncGenerator%': needsEval,[m
[32m+[m	[32m'%AsyncGeneratorFunction%': needsEval,[m
[32m+[m	[32m'%AsyncIteratorPrototype%': needsEval,[m
[32m+[m	[32m'%Atomics%': typeof Atomics === 'undefined' ? undefined : Atomics,[m
[32m+[m	[32m'%BigInt%': typeof BigInt === 'undefined' ? undefined : BigInt,[m
[32m+[m	[32m'%BigInt64Array%': typeof BigInt64Array === 'undefined' ? undefined : BigInt64Array,[m
[32m+[m	[32m'%BigUint64Array%': typeof BigUint64Array === 'undefined' ? undefined : BigUint64Array,[m
[32m+[m	[32m'%Boolean%': Boolean,[m
[32m+[m	[32m'%DataView%': typeof DataView === 'undefined' ? undefined : DataView,[m
[32m+[m	[32m'%Date%': Date,[m
[32m+[m	[32m'%decodeURI%': decodeURI,[m
[32m+[m	[32m'%decodeURIComponent%': decodeURIComponent,[m
[32m+[m	[32m'%encodeURI%': encodeURI,[m
[32m+[m	[32m'%encodeURIComponent%': encodeURIComponent,[m
[32m+[m	[32m'%Error%': $Error,[m
[32m+[m	[32m'%eval%': eval, // eslint-disable-line no-eval[m
[32m+[m	[32m'%EvalError%': $EvalError,[m
[32m+[m	[32m'%Float32Array%': typeof Float32Array === 'undefined' ? undefined : Float32Array,[m
[32m+[m	[32m'%Float64Array%': typeof Float64Array === 'undefined' ? undefined : Float64Array,[m
[32m+[m	[32m'%FinalizationRegistry%': typeof FinalizationRegistry === 'undefined' ? undefined : FinalizationRegistry,[m
[32m+[m	[32m'%Function%': $Function,[m
[32m+[m	[32m'%GeneratorFunction%': needsEval,[m
[32m+[m	[32m'%Int8Array%': typeof Int8Array === 'undefined' ? undefined : Int8Array,[m
[32m+[m	[32m'%Int16Array%': typeof Int16Array === 'undefined' ? undefined : Int16Array,[m
[32m+[m	[32m'%Int32Array%': typeof Int32Array === 'undefined' ? undefined : Int32Array,[m
[32m+[m	[32m'%isFinite%': isFinite,[m
[32m+[m	[32m'%isNaN%': isNaN,[m
[32m+[m	[32m'%IteratorPrototype%': hasSymbols && getProto ? getProto(getProto([][Symbol.iterator]())) : undefined,[m
[32m+[m	[32m'%JSON%': typeof JSON === 'object' ? JSON : undefined,[m
[32m+[m	[32m'%Map%': typeof Map === 'undefined' ? undefined : Map,[m
[32m+[m	[32m'%MapIteratorPrototype%': typeof Map === 'undefined' || !hasSymbols || !getProto ? undefined : getProto(new Map()[Symbol.iterator]()),[m
[32m+[m	[32m'%Math%': Math,[m
[32m+[m	[32m'%Number%': Number,[m
[32m+[m	[32m'%Object%': Object,[m
[32m+[m	[32m'%parseFloat%': parseFloat,[m
[32m+[m	[32m'%parseInt%': parseInt,[m
[32m+[m	[32m'%Promise%': typeof Promise === 'undefined' ? undefined : Promise,[m
[32m+[m	[32m'%Proxy%': typeof Proxy === 'undefined' ? undefined : Proxy,[m
[32m+[m	[32m'%RangeError%': $RangeError,[m
[32m+[m	[32m'%ReferenceError%': $ReferenceError,[m
[32m+[m	[32m'%Reflect%': typeof Reflect === 'undefined' ? undefined : Reflect,[m
[32m+[m	[32m'%RegExp%': RegExp,[m
[32m+[m	[32m'%Set%': typeof Set === 'undefined' ? undefined : Set,[m
[32m+[m	[32m'%SetIteratorPrototype%': typeof Set === 'undefined' || !hasSymbols || !getProto ? undefined : getProto(new Set()[Symbol.iterator]()),[m
[32m+[m	[32m'%SharedArrayBuffer%': typeof SharedArrayBuffer === 'undefined' ? undefined : SharedArrayBuffer,[m
[32m+[m	[32m'%String%': String,[m
[32m+[m	[32m'%StringIteratorPrototype%': hasSymbols && getProto ? getProto(''[Symbol.iterator]()) : undefined,[m
[32m+[m	[32m'%Symbol%': hasSymbols ? Symbol : undefined,[m
[32m+[m	[32m'%SyntaxError%': $SyntaxError,[m
[32m+[m	[32m'%ThrowTypeError%': ThrowTypeError,[m
[32m+[m	[32m'%TypedArray%': TypedArray,[m
[32m+[m	[32m'%TypeError%': $TypeError,[m
[32m+[m	[32m'%Uint8Array%': typeof Uint8Array === 'undefined' ? undefined : Uint8Array,[m
[32m+[m	[32m'%Uint8ClampedArray%': typeof Uint8ClampedArray === 'undefined' ? undefined : Uint8ClampedArray,[m
[32m+[m	[32m'%Uint16Array%': typeof Uint16Array === 'undefined' ? undefined : Uint16Array,[m
[32m+[m	[32m'%Uint32Array%': typeof Uint32Array === 'undefined' ? undefined : Uint32Array,[m
[32m+[m	[32m'%URIError%': $URIError,[m
[32m+[m	[32m'%WeakMap%': typeof WeakMap === 'undefined' ? undefined : WeakMap,[m
[32m+[m	[32m'%WeakRef%': typeof WeakRef === 'undefined' ? undefined : WeakRef,[m
[32m+[m	[32m'%WeakSet%': typeof WeakSet === 'undefined' ? undefined : WeakSet[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mif (getProto) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mnull.error; // eslint-disable-line no-unused-expressions[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32m// https://github.com/tc39/proposal-shadowrealm/pull/384#issuecomment-1364264229[m
[32m+[m		[32mvar errorProto = getProto(getProto(e));[m
[32m+[m		[32mINTRINSICS['%Error.prototype%'] = errorProto;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar doEval = function doEval(name) {[m
[32m+[m	[32mvar value;[m
[32m+[m	[32mif (name === '%AsyncFunction%') {[m
[32m+[m		[32mvalue = getEvalledConstructor('async function () {}');[m
[32m+[m	[32m} else if (name === '%GeneratorFunction%') {[m
[32m+[m		[32mvalue = getEvalledConstructor('function* () {}');[m
[32m+[m	[32m} else if (name === '%AsyncGeneratorFunction%') {[m
[32m+[m		[32mvalue = getEvalledConstructor('async function* () {}');[m
[32m+[m	[32m} else if (name === '%AsyncGenerator%') {[m
[32m+[m		[32mvar fn = doEval('%AsyncGeneratorFunction%');[m
[32m+[m		[32mif (fn) {[m
[32m+[m			[32mvalue = fn.prototype;[m
[32m+[m		[32m}[m
[32m+[m	[32m} else if (name === '%AsyncIteratorPrototype%') {[m
[32m+[m		[32mvar gen = doEval('%AsyncGenerator%');[m
[32m+[m		[32mif (gen && getProto) {[m
[32m+[m			[32mvalue = getProto(gen.prototype);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mINTRINSICS[name] = value;[m
[32m+[m
[32m+[m	[32mreturn value;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar LEGACY_ALIASES = {[m
[32m+[m	[32m__proto__: null,[m
[32m+[m	[32m'%ArrayBufferPrototype%': ['ArrayBuffer', 'prototype'],[m
[32m+[m	[32m'%ArrayPrototype%': ['Array', 'prototype'],[m
[32m+[m	[32m'%ArrayProto_entries%': ['Array', 'prototype', 'entries'],[m
[32m+[m	[32m'%ArrayProto_forEach%': ['Array', 'prototype', 'forEach'],[m
[32m+[m	[32m'%ArrayProto_keys%': ['Array', 'prototype', 'keys'],[m
[32m+[m	[32m'%ArrayProto_values%': ['Array', 'prototype', 'values'],[m
[32m+[m	[32m'%AsyncFunctionPrototype%': ['AsyncFunction', 'prototype'],[m
[32m+[m	[32m'%AsyncGenerator%': ['AsyncGeneratorFunction', 'prototype'],[m
[32m+[m	[32m'%AsyncGeneratorPrototype%': ['AsyncGeneratorFunction', 'prototype', 'prototype'],[m
[32m+[m	[32m'%BooleanPrototype%': ['Boolean', 'prototype'],[m
[32m+[m	[32m'%DataViewPrototype%': ['DataView', 'prototype'],[m
[32m+[m	[32m'%DatePrototype%': ['Date', 'prototype'],[m
[32m+[m	[32m'%ErrorPrototype%': ['Error', 'prototype'],[m
[32m+[m	[32m'%EvalErrorPrototype%': ['EvalError', 'prototype'],[m
[32m+[m	[32m'%Float32ArrayPrototype%': ['Float32Array', 'prototype'],[m
[32m+[m	[32m'%Float64ArrayPrototype%': ['Float64Array', 'prototype'],[m
[32m+[m	[32m'%FunctionPrototype%': ['Function', 'prototype'],[m
[32m+[m	[32m'%Generator%': ['GeneratorFunction', 'prototype'],[m
[32m+[m	[32m'%GeneratorPrototype%': ['GeneratorFunction', 'prototype', 'prototype'],[m
[32m+[m	[32m'%Int8ArrayPrototype%': ['Int8Array', 'prototype'],[m
[32m+[m	[32m'%Int16ArrayPrototype%': ['Int16Array', 'prototype'],[m
[32m+[m	[32m'%Int32ArrayPrototype%': ['Int32Array', 'prototype'],[m
[32m+[m	[32m'%JSONParse%': ['JSON', 'parse'],[m
[32m+[m	[32m'%JSONStringify%': ['JSON', 'stringify'],[m
[32m+[m	[32m'%MapPrototype%': ['Map', 'prototype'],[m
[32m+[m	[32m'%NumberPrototype%': ['Number', 'prototype'],[m
[32m+[m	[32m'%ObjectPrototype%': ['Object', 'prototype'],[m
[32m+[m	[32m'%ObjProto_toString%': ['Object', 'prototype', 'toString'],[m
[32m+[m	[32m'%ObjProto_valueOf%': ['Object', 'prototype', 'valueOf'],[m
[32m+[m	[32m'%PromisePrototype%': ['Promise', 'prototype'],[m
[32m+[m	[32m'%PromiseProto_then%': ['Promise', 'prototype', 'then'],[m
[32m+[m	[32m'%Promise_all%': ['Promise', 'all'],[m
[32m+[m	[32m'%Promise_reject%': ['Promise', 'reject'],[m
[32m+[m	[32m'%Promise_resolve%': ['Promise', 'resolve'],[m
[32m+[m	[32m'%RangeErrorPrototype%': ['RangeError', 'prototype'],[m
[32m+[m	[32m'%ReferenceErrorPrototype%': ['ReferenceError', 'prototype'],[m
[32m+[m	[32m'%RegExpPrototype%': ['RegExp', 'prototype'],[m
[32m+[m	[32m'%SetPrototype%': ['Set', 'prototype'],[m
[32m+[m	[32m'%SharedArrayBufferPrototype%': ['SharedArrayBuffer', 'prototype'],[m
[32m+[m	[32m'%StringPrototype%': ['String', 'prototype'],[m
[32m+[m	[32m'%SymbolPrototype%': ['Symbol', 'prototype'],[m
[32m+[m	[32m'%SyntaxErrorPrototype%': ['SyntaxError', 'prototype'],[m
[32m+[m	[32m'%TypedArrayPrototype%': ['TypedArray', 'prototype'],[m
[32m+[m	[32m'%TypeErrorPrototype%': ['TypeError', 'prototype'],[m
[32m+[m	[32m'%Uint8ArrayPrototype%': ['Uint8Array', 'prototype'],[m
[32m+[m	[32m'%Uint8ClampedArrayPrototype%': ['Uint8ClampedArray', 'prototype'],[m
[32m+[m	[32m'%Uint16ArrayPrototype%': ['Uint16Array', 'prototype'],[m
[32m+[m	[32m'%Uint32ArrayPrototype%': ['Uint32Array', 'prototype'],[m
[32m+[m	[32m'%URIErrorPrototype%': ['URIError', 'prototype'],[m
[32m+[m	[32m'%WeakMapPrototype%': ['WeakMap', 'prototype'],[m
[32m+[m	[32m'%WeakSetPrototype%': ['WeakSet', 'prototype'][m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m[32mvar hasOwn = require('hasown');[m
[32m+[m[32mvar $concat = bind.call(Function.call, Array.prototype.concat);[m
[32m+[m[32mvar $spliceApply = bind.call(Function.apply, Array.prototype.splice);[m
[32m+[m[32mvar $replace = bind.call(Function.call, String.prototype.replace);[m
[32m+[m[32mvar $strSlice = bind.call(Function.call, String.prototype.slice);[m
[32m+[m[32mvar $exec = bind.call(Function.call, RegExp.prototype.exec);[m
[32m+[m
[32m+[m[32m/* adapted from https://github.com/lodash/lodash/blob/4.17.15/dist/lodash.js#L6735-L6744 */[m
[32m+[m[32mvar rePropName = /[^%.[\]]+|\[(?:(-?\d+(?:\.\d+)?)|(["'])((?:(?!\2)[^\\]|\\.)*?)\2)\]|(?=(?:\.|\[\])(?:\.|\[\]|%$))/g;[m
[32m+[m[32mvar reEscapeChar = /\\(\\)?/g; /** Used to match backslashes in property paths. */[m
[32m+[m[32mvar stringToPath = function stringToPath(string) {[m
[32m+[m	[32mvar first = $strSlice(string, 0, 1);[m
[32m+[m	[32mvar last = $strSlice(string, -1);[m
[32m+[m	[32mif (first === '%' && last !== '%') {[m
[32m+[m		[32mthrow new $SyntaxError('invalid intrinsic syntax, expected closing `%`');[m
[32m+[m	[32m} else if (last === '%' && first !== '%') {[m
[32m+[m		[32mthrow new $SyntaxError('invalid intrinsic syntax, expected opening `%`');[m
[32m+[m	[32m}[m
[32m+[m	[32mvar result = [];[m
[32m+[m	[32m$replace(string, rePropName, function (match, number, quote, subString) {[m
[32m+[m		[32mresult[result.length] = quote ? $replace(subString, reEscapeChar, '$1') : number || match;[m
[32m+[m	[32m});[m
[32m+[m	[32mreturn result;[m
[32m+[m[32m};[m
[32m+[m[32m/* end adaptation */[m
[32m+[m
[32m+[m[32mvar getBaseIntrinsic = function getBaseIntrinsic(name, allowMissing) {[m
[32m+[m	[32mvar intrinsicName = name;[m
[32m+[m	[32mvar alias;[m
[32m+[m	[32mif (hasOwn(LEGACY_ALIASES, intrinsicName)) {[m
[32m+[m		[32malias = LEGACY_ALIASES[intrinsicName];[m
[32m+[m		[32mintrinsicName = '%' + alias[0] + '%';[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif (hasOwn(INTRINSICS, intrinsicName)) {[m
[32m+[m		[32mvar value = INTRINSICS[intrinsicName];[m
[32m+[m		[32mif (value === needsEval) {[m
[32m+[m			[32mvalue = doEval(intrinsicName);[m
[32m+[m		[32m}[m
[32m+[m		[32mif (typeof value === 'undefined' && !allowMissing) {[m
[32m+[m			[32mthrow new $TypeError('intrinsic ' + name + ' exists, but is not available. Please file an issue!');[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn {[m
[32m+[m			[32malias: alias,[m
[32m+[m			[32mname: intrinsicName,[m
[32m+[m			[32mvalue: value[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mthrow new $SyntaxError('intrinsic ' + name + ' does not exist!');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function GetIntrinsic(name, allowMissing) {[m
[32m+[m	[32mif (typeof name !== 'string' || name.length === 0) {[m
[32m+[m		[32mthrow new $TypeError('intrinsic name must be a non-empty string');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 1 && typeof allowMissing !== 'boolean') {[m
[32m+[m		[32mthrow new $TypeError('"allowMissing" argument must be a boolean');[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ($exec(/^%?[^%]*%?$/, name) === null) {[m
[32m+[m		[32mthrow new $SyntaxError('`%` may not be present anywhere but at the beginning and end of the intrinsic name');[m
[32m+[m	[32m}[m
[32m+[m	[32mvar parts = stringToPath(name);[m
[32m+[m	[32mvar intrinsicBaseName = parts.length > 0 ? parts[0] : '';[m
[32m+[m
[32m+[m	[32mvar intrinsic = getBaseIntrinsic('%' + intrinsicBaseName + '%', allowMissing);[m
[32m+[m	[32mvar intrinsicRealName = intrinsic.name;[m
[32m+[m	[32mvar value = intrinsic.value;[m
[32m+[m	[32mvar skipFurtherCaching = false;[m
[32m+[m
[32m+[m	[32mvar alias = intrinsic.alias;[m
[32m+[m	[32mif (alias) {[m
[32m+[m		[32mintrinsicBaseName = alias[0];[m
[32m+[m		[32m$spliceApply(parts, $concat([0, 1], alias));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor (var i = 1, isOwn = true; i < parts.length; i += 1) {[m
[32m+[m		[32mvar part = parts[i];[m
[32m+[m		[32mvar first = $strSlice(part, 0, 1);[m
[32m+[m		[32mvar last = $strSlice(part, -1);[m
[32m+[m		[32mif ([m
[32m+[m			[32m([m
[32m+[m				[32m(first === '"' || first === "'" || first === '`')[m
[32m+[m				[32m|| (last === '"' || last === "'" || last === '`')[m
[32m+[m			[32m)[m
[32m+[m			[32m&& first !== last[m
[32m+[m		[32m) {[m
[32m+[m			[32mthrow new $SyntaxError('property names with quotes must have matching quotes');[m
[32m+[m		[32m}[m
[32m+[m		[32mif (part === 'constructor' || !isOwn) {[m
[32m+[m			[32mskipFurtherCaching = true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mintrinsicBaseName += '.' + part;[m
[32m+[m		[32mintrinsicRealName = '%' + intrinsicBaseName + '%';[m
[32m+[m
[32m+[m		[32mif (hasOwn(INTRINSICS, intrinsicRealName)) {[m
[32m+[m			[32mvalue = INTRINSICS[intrinsicRealName];[m
[32m+[m		[32m} else if (value != null) {[m
[32m+[m			[32mif (!(part in value)) {[m
[32m+[m				[32mif (!allowMissing) {[m
[32m+[m					[32mthrow new $TypeError('base intrinsic for ' + name + ' exists, but the property is not available.');[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn void undefined;[m
[32m+[m			[32m}[m
[32m+[m			[32mif ($gOPD && (i + 1) >= parts.length) {[m
[32m+[m				[32mvar desc = $gOPD(value, part);[m
[32m+[m				[32misOwn = !!desc;[m
[32m+[m
[32m+[m				[32m// By convention, when a data property is converted to an accessor[m
[32m+[m				[32m// property to emulate a data property that does not suffer from[m
[32m+[m				[32m// the override mistake, that accessor's getter is marked with[m
[32m+[m				[32m// an `originalValue` property. Here, when we detect this, we[m
[32m+[m				[32m// uphold the illusion by pretending to see that original data[m
[32m+[m				[32m// property, i.e., returning the value rather than the getter[m
[32m+[m				[32m// itself.[m
[32m+[m				[32mif (isOwn && 'get' in desc && !('originalValue' in desc.get)) {[m
[32m+[m					[32mvalue = desc.get;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mvalue = value[part];[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32misOwn = hasOwn(value, part);[m
[32m+[m				[32mvalue = value[part];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif (isOwn && !skipFurtherCaching) {[m
[32m+[m				[32mINTRINSICS[intrinsicRealName] = value;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn value;[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/get-intrinsic/package.json b/node_modules/get-intrinsic/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..568dff9[m
[1m--- /dev/null[m
[1m+++ b/node_modules/get-intrinsic/package.json[m
[36m@@ -0,0 +1,93 @@[m
