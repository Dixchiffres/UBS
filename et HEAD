[32m+[m[32mdeclare function hasOwn<O, K extends PropertyKey, V = unknown>(o: O, p: K): o is O & Record<K, V>;[m
[32m+[m
[32m+[m[32mexport = hasOwn;[m
[1mdiff --git a/node_modules/hasown/index.js b/node_modules/hasown/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..34e6059[m
[1m--- /dev/null[m
[1m+++ b/node_modules/hasown/index.js[m
[36m@@ -0,0 +1,8 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar call = Function.prototype.call;[m
[32m+[m[32mvar $hasOwn = Object.prototype.hasOwnProperty;[m
[32m+[m[32mvar bind = require('function-bind');[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mmodule.exports = bind.call(call, $hasOwn);[m
