[1mdiff --git a/node_modules/define-data-property/index.d.ts b/node_modules/define-data-property/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..b56a77d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/define-data-property/index.d.ts[m
[36m@@ -0,0 +1,12 @@[m
[32m+[m
[32m+[m[32mdeclare function defineDataProperty([m
[32m+[m[32m    obj: Record<PropertyKey, unknown>,[m
[32m+[m[32m    property: keyof typeof obj,[m
[32m+[m[32m    value: typeof obj[typeof property],[m
[32m+[m[32m    nonEnumerable?: boolean | null,[m
[32m+[m[32m    nonWritable?: boolean | null,[m
[32m+[m[32m    nonConfigurable?: boolean | null,[m
[32m+[m[32m    loose?: boolean[m
[32m+[m[32m): void;[m
[32m+[m
[32m+[m[32mexport = defineDataProperty;[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/define-data-property/index.js b/node_modules/define-data-property/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..e1a38c0[m
[1m--- /dev/null[m
[1m+++ b/node_modules/define-data-property/index.js[m
[36m@@ -0,0 +1,56 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar $defineProperty = require('es-define-property');[m
[32m+[m
[32m+[m[32mvar $SyntaxError = require('es-errors/syntax');[m
[32m+[m[32mvar $TypeError = require('es-errors/type');[m
[32m+[m
[32m+[m[32mvar gopd = require('gopd');[m
[32m+[m
[32m+[m[32m/** @type {import('.')} */[m
[32m+[m[32mmodule.exports = function defineDataProperty([m
[32m+[m	[32mobj,[m
[32m+[m	[32mproperty,[m
[32m+[m	[32mvalue[m
[32m+[m[32m) {[m
[32m+[m	[32mif (!obj || (typeof obj !== 'object' && typeof obj !== 'function')) {[m
[32m+[m		[32mthrow new $TypeError('`obj` must be an object or a function`');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (typeof property !== 'string' && typeof property !== 'symbol') {[m
[32m+[m		[32mthrow new $TypeError('`property` must be a string or a symbol`');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 3 && typeof arguments[3] !== 'boolean' && arguments[3] !== null) {[m
[32m+[m		[32mthrow new $TypeError('`nonEnumerable`, if provided, must be a boolean or null');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 4 && typeof arguments[4] !== 'boolean' && arguments[4] !== null) {[m
[32m+[m		[32mthrow new $TypeError('`nonWritable`, if provided, must be a boolean or null');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 5 && typeof arguments[5] !== 'boolean' && arguments[5] !== null) {[m
[32m+[m		[32mthrow new $TypeError('`nonConfigurable`, if provided, must be a boolean or null');[m
[32m+[m	[32m}[m
[32m+[m	[32mif (arguments.length > 6 && typeof arguments[6] !== 'boolean') {[m
[32m+[m		[32mthrow new $TypeError('`loose`, if provided, must be a boolean');[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar nonEnumerable = arguments.length > 3 ? arguments[3] : null;[m
[32m+[m	[32mvar nonWritable = arguments.length > 4 ? arguments[4] : null;[m
[32m+[m	[32mvar nonConfigurable = arguments.length > 5 ? arguments[5] : null;[m
[32m+[m	[32mvar loose = arguments.length > 6 ? arguments[6] : false;[m
[32m+[m
[32m+[m	[32m/* @type {false | TypedPropertyDescriptor<unknown>} */[m
[32m+[m	[32mvar desc = !!gopd && gopd(obj, property);[m
[32m+[m
[32m+[m	[32mif ($defineProperty) {[m
[32m+[m		[32m$defineProperty(obj, property, {[m
[32m+[m			[32mconfigurable: nonConfigurable === null && desc ? desc.configurable : !nonConfigurable,[m
[32m+[m			[32menumerable: nonEnumerable === null && desc ? desc.enumerable : !nonEnumerable,[m
[32m+[m			[32mvalue: value,[m
[32m+[m			[32mwritable: nonWritable === null && desc ? desc.writable : !nonWritable[m
[32m+[m		[32m});[m
[32m+[m	[32m} else if (loose || (!nonEnumerable && !nonWritable && !nonConfigurable)) {[m
[32m+[m		[32m// must fall back to [[Set]], and was not explicitly asked to make non-enumerable, non-writable, or non-configurable[m
[32m+[m		[32mobj[property] = value; // eslint-disable-line no-param-reassign[m
[32m+[m	[32m} else {[m
[32m+[m		[32mthrow new $SyntaxError('This environment does not support defining a property as non-configurable, non-writable, or non-enumerable.');[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/define-data-property/package.json b/node_modules/define-data-property/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..eec4097[m
[1m--- /dev/null[m
[1m+++ b/node_modules/define-data-property/package.json[m
[36m@@ -0,0 +1,106 @@[m
