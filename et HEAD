[32m+[m[32m *--------------------------------------------------------------------------------------------*/[m
[32m+[m
[32m+[m[32mdeclare module 'iconv-lite' {[m
[32m+[m	[32mexport function decode(buffer: Buffer, encoding: string, options?: Options): string;[m
[32m+[m
[32m+[m	[32mexport function encode(content: string, encoding: string, options?: Options): Buffer;[m
[32m+[m
[32m+[m	[32mexport function encodingExists(encoding: string): boolean;[m
[32m+[m
[32m+[m	[32mexport function decodeStream(encoding: string, options?: Options): NodeJS.ReadWriteStream;[m
[32m+[m
[32m+[m	[32mexport function encodeStream(encoding: string, options?: Options): NodeJS.ReadWriteStream;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport interface Options {[m
[32m+[m[32m    stripBOM?: boolean;[m
[32m+[m[32m    addBOM?: boolean;[m
[32m+[m[32m    defaultEncoding?: string;[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/iconv-lite/lib/index.js b/node_modules/iconv-lite/lib/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..5391919[m
