[1mdiff --git a/node_modules/function-bind/test/index.js b/node_modules/function-bind/test/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..2edecce[m
[1m--- /dev/null[m
[1m+++ b/node_modules/function-bind/test/index.js[m
[36m@@ -0,0 +1,252 @@[m
[32m+[m[32m// jscs:disable requireUseStrict[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mvar functionBind = require('../implementation');[m
[32m+[m[32mvar getCurrentContext = function () { return this; };[m
[32m+[m
[32m+[m[32mtest('functionBind is a function', function (t) {[m
[32m+[m[32m    t.equal(typeof functionBind, 'function');[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('non-functions', function (t) {[m
[32m+[m[32m    var nonFunctions = [true, false, [], {}, 42, 'foo', NaN, /a/g];[m
[32m+[m[32m    t.plan(nonFunctions.length);[m
[32m+[m[32m    for (var i = 0; i < nonFunctions.length; ++i) {[m
[32m+[m[32m        try { functionBind.call(nonFunctions[i]); } catch (ex) {[m
[32m+[m[32m            t.ok(ex instanceof TypeError, 'throws when given ' + String(nonFunctions[i]));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('without a context', function (t) {[m
[32m+[m[32m    t.test('binds properly', function (st) {[m
[32m+[m[32m        var args, context;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                context = this;[m
[32m+[m[32m            })[m
[32m+[m[32m        };[m
[32m+[m[32m        namespace.func(1, 2, 3);[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3]);[m
[32m+[m[32m        st.equal(context, getCurrentContext.call());[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('binds properly, and still supplies bound arguments', function (st) {[m
[32m+[m[32m        var args, context;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                context = this;[m
[32m+[m[32m            }, undefined, 1, 2, 3)[m
[32m+[m[32m        };[m
[32m+[m[32m        namespace.func(4, 5, 6);[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3, 4, 5, 6]);[m
[32m+[m[32m        st.equal(context, getCurrentContext.call());[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('returns properly', function (st) {[m
[32m+[m[32m        var args;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                return this;[m
[32m+[m[32m            }, null)[m
[32m+[m[32m        };[m
[32m+[m[32m        var context = namespace.func(1, 2, 3);[m
[32m+[m[32m        st.equal(context, getCurrentContext.call(), 'returned context is namespaced context');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3], 'passed arguments are correct');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('returns properly with bound arguments', function (st) {[m
[32m+[m[32m        var args;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                return this;[m
[32m+[m[32m            }, null, 1, 2, 3)[m
[32m+[m[32m        };[m
[32m+[m[32m        var context = namespace.func(4, 5, 6);[m
[32m+[m[32m        st.equal(context, getCurrentContext.call(), 'returned context is namespaced context');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3, 4, 5, 6], 'passed arguments are correct');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('called as a constructor', function (st) {[m
[32m+[m[32m        var thunkify = function (value) {[m
[32m+[m[32m            return function () { return value; };[m
[32m+[m[32m        };[m
[32m+[m[32m        st.test('returns object value', function (sst) {[m
[32m+[m[32m            var expectedReturnValue = [1, 2, 3];[m
[32m+[m[32m            var Constructor = functionBind.call(thunkify(expectedReturnValue), null);[m
[32m+[m[32m            var result = new Constructor();[m
[32m+[m[32m            sst.equal(result, expectedReturnValue);[m
[32m+[m[32m            sst.end();[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.test('does not return primitive value', function (sst) {[m
[32m+[m[32m            var Constructor = functionBind.call(thunkify(42), null);[m
[32m+[m[32m            var result = new Constructor();[m
[32m+[m[32m            sst.notEqual(result, 42);[m
[32m+[m[32m            sst.end();[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.test('object from bound constructor is instance of original and bound constructor', function (sst) {[m
[32m+[m[32m            var A = function (x) {[m
[32m+[m[32m                this.name = x || 'A';[m
[32m+[m[32m            };[m
[32m+[m[32m            var B = functionBind.call(A, null, 'B');[m
[32m+[m
[32m+[m[32m            var result = new B();[m
[32m+[m[32m            sst.ok(result instanceof B, 'result is instance of bound constructor');[m
[32m+[m[32m            sst.ok(result instanceof A, 'result is instance of original constructor');[m
[32m+[m[32m            sst.end();[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('with a context', function (t) {[m
[32m+[m[32m    t.test('with no bound arguments', function (st) {[m
[32m+[m[32m        var args, context;[m
[32m+[m[32m        var boundContext = {};[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                context = this;[m
[32m+[m[32m            }, boundContext)[m
[32m+[m[32m        };[m
[32m+[m[32m        namespace.func(1, 2, 3);[m
[32m+[m[32m        st.equal(context, boundContext, 'binds a context properly');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3], 'supplies passed arguments');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('with bound arguments', function (st) {[m
[32m+[m[32m        var args, context;[m
[32m+[m[32m        var boundContext = {};[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                context = this;[m
[32m+[m[32m            }, boundContext, 1, 2, 3)[m
[32m+[m[32m        };[m
[32m+[m[32m        namespace.func(4, 5, 6);[m
[32m+[m[32m        st.equal(context, boundContext, 'binds a context properly');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3, 4, 5, 6], 'supplies bound and passed arguments');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('returns properly', function (st) {[m
[32m+[m[32m        var boundContext = {};[m
[32m+[m[32m        var args;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                return this;[m
[32m+[m[32m            }, boundContext)[m
[32m+[m[32m        };[m
[32m+[m[32m        var context = namespace.func(1, 2, 3);[m
[32m+[m[32m        st.equal(context, boundContext, 'returned context is bound context');[m
[32m+[m[32m        st.notEqual(context, getCurrentContext.call(), 'returned context is not lexical context');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3], 'passed arguments are correct');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('returns properly with bound arguments', function (st) {[m
[32m+[m[32m        var boundContext = {};[m
[32m+[m[32m        var args;[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            func: functionBind.call(function () {[m
[32m+[m[32m                args = Array.prototype.slice.call(arguments);[m
[32m+[m[32m                return this;[m
[32m+[m[32m            }, boundContext, 1, 2, 3)[m
[32m+[m[32m        };[m
[32m+[m[32m        var context = namespace.func(4, 5, 6);[m
[32m+[m[32m        st.equal(context, boundContext, 'returned context is bound context');[m
[32m+[m[32m        st.notEqual(context, getCurrentContext.call(), 'returned context is not lexical context');[m
[32m+[m[32m        st.deepEqual(args, [1, 2, 3, 4, 5, 6], 'passed arguments are correct');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('passes the correct arguments when called as a constructor', function (st) {[m
[32m+[m[32m        var expected = { name: 'Correct' };[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            Func: functionBind.call(function (arg) {[m
[32m+[m[32m                return arg;[m
[32m+[m[32m            }, { name: 'Incorrect' })[m
[32m+[m[32m        };[m
[32m+[m[32m        var returned = new namespace.Func(expected);[m
[32m+[m[32m        st.equal(returned, expected, 'returns the right arg when called as a constructor');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('has the new instance\'s context when called as a constructor', function (st) {[m
[32m+[m[32m        var actualContext;[m
[32m+[m[32m        var expectedContext = { foo: 'bar' };[m
[32m+[m[32m        var namespace = {[m
[32m+[m[32m            Func: functionBind.call(function () {[m
[32m+[m[32m                actualContext = this;[m
[32m+[m[32m            }, expectedContext)[m
[32m+[m[32m        };[m
[32m+[m[32m        var result = new namespace.Func();[m
[32m+[m[32m        st.equal(result instanceof namespace.Func, true);[m
[32m+[m[32m        st.notEqual(actualContext, expectedContext);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('bound function length', function (t) {[m
[32m+[m[32m    t.test('sets a correct length without thisArg', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; });[m
[32m+[m[32m        st.equal(subject.length, 3);[m
[32m+[m[32m        st.equal(subject(1, 2, 3), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('sets a correct length with thisArg', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; }, {});[m
[32m+[m[32m        st.equal(subject.length, 3);[m
[32m+[m[32m        st.equal(subject(1, 2, 3), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('sets a correct length without thisArg and first argument', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; }, undefined, 1);[m
[32m+[m[32m        st.equal(subject.length, 2);[m
[32m+[m[32m        st.equal(subject(2, 3), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('sets a correct length with thisArg and first argument', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; }, {}, 1);[m
[32m+[m[32m        st.equal(subject.length, 2);[m
[32m+[m[32m        st.equal(subject(2, 3), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('sets a correct length without thisArg and too many arguments', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; }, undefined, 1, 2, 3, 4);[m
[32m+[m[32m        st.equal(subject.length, 0);[m
[32m+[m[32m        st.equal(subject(), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('sets a correct length with thisArg and too many arguments', function (st) {[m
[32m+[m[32m        var subject = functionBind.call(function (a, b, c) { return a + b + c; }, {}, 1, 2, 3, 4);[m
[32m+[m[32m        st.equal(subject.length, 0);[m
[32m+[m[32m        st.equal(subject(), 6);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
