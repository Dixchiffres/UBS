[36m@@ -0,0 +1,25 @@[m
[32m+[m[32m# Security Policies and Procedures[m
[32m+[m
[32m+[m[32m## Reporting a Bug[m
[32m+[m
[32m+[m[32mThe `finalhandler` team and community take all security bugs seriously. Thank[m
[32m+[m[32myou for improving the security of Express. We appreciate your efforts and[m
[32m+[m[32mresponsible disclosure and will make every effort to acknowledge your[m
[32m+[m[32mcontributions.[m
[32m+[m
[32m+[m[32mReport security bugs by emailing the current owner(s) of `finalhandler`. This[m
[32m+[m[32minformation can be found in the npm registry using the command[m
[32m+[m[32m`npm owner ls finalhandler`.[m
[32m+[m[32mIf unsure or unable to get the information from the above, open an issue[m
[32m+[m[32min the [project issue tracker](https://github.com/pillarjs/finalhandler/issues)[m
[32m+[m[32masking for the current contact information.[m
[32m+[m
[32m+[m[32mTo ensure the timely response to your report, please ensure that the entirety[m
[32m+[m[32mof the report is contained within the email body and not solely behind a web[m
[32m+[m[32mlink or an attachment.[m
[32m+[m
[32m+[m[32mAt least one owner will acknowledge your email within 48 hours, and will send a[m
[32m+[m[32mmore detailed response within 48 hours indicating the next steps in handling[m
[32m+[m[32myour report. After the initial reply to your report, the owners will[m
[32m+[m[32mendeavor to keep you informed of the progress towards a fix and full[m
[32m+[m[32mannouncement, and may ask for additional information or guidance.[m
[1mdiff --git a/node_modules/finalhandler/index.js b/node_modules/finalhandler/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f628e42[m
[1m--- /dev/null[m
[1m+++ b/node_modules/finalhandler/index.js[m
[36m@@ -0,0 +1,336 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * finalhandler[m
[32m+[m[32m * Copyright(c) 2014-2022 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar debug = require('debug')('finalhandler')[m
[32m+[m[32mvar encodeUrl = require('encodeurl')[m
[32m+[m[32mvar escapeHtml = require('escape-html')[m
[32m+[m[32mvar onFinished = require('on-finished')[m
[32m+[m[32mvar parseUrl = require('parseurl')[m
[32m+[m[32mvar statuses = require('statuses')[m
[32m+[m[32mvar unpipe = require('unpipe')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar DOUBLE_SPACE_REGEXP = /\x20{2}/g[m
[32m+[m[32mvar NEWLINE_REGEXP = /\n/g[m
[32m+[m
[32m+[m[32m/* istanbul ignore next */[m
[32m+[m[32mvar defer = typeof setImmediate === 'function'[m
[32m+[m[32m  ? setImmediate[m
[32m+[m[32m  : function (fn) { process.nextTick(fn.bind.apply(fn, arguments)) }[m
[32m+[m[32mvar isFinished = onFinished.isFinished[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a minimal HTML document.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} message[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createHtmlDocument (message) {[m
[32m+[m[32m  var body = escapeHtml(message)[m
[32m+[m[32m    .replace(NEWLINE_REGEXP, '<br>')[m
[32m+[m[32m    .replace(DOUBLE_SPACE_REGEXP, ' &nbsp;')[m
[32m+[m
[32m+[m[32m  return '<!DOCTYPE html>\n' +[m
[32m+[m[32m    '<html lang="en">\n' +[m
[32m+[m[32m    '<head>\n' +[m
[32m+[m[32m    '<meta charset="utf-8">\n' +[m
[32m+[m[32m    '<title>Error</title>\n' +[m
[32m+[m[32m    '</head>\n' +[m
[32m+[m[32m    '<body>\n' +[m
[32m+[m[32m    '<pre>' + body + '</pre>\n' +[m
[32m+[m[32m    '</body>\n' +[m
[32m+[m[32m    '</html>\n'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = finalhandler[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a function to handle the final response.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Request} req[m
[32m+[m[32m * @param {Response} res[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction finalhandler (req, res, options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  // get environment[m
[32m+[m[32m  var env = opts.env || process.env.NODE_ENV || 'development'[m
[32m+[m
[32m+[m[32m  // get error callback[m
[32m+[m[32m  var onerror = opts.onerror[m
[32m+[m
[32m+[m[32m  return function (err) {[m
[32m+[m[32m    var headers[m
[32m+[m[32m    var msg[m
[32m+[m[32m    var status[m
[32m+[m
[32m+[m[32m    // ignore 404 on in-flight response[m
[32m+[m[32m    if (!err && headersSent(res)) {[m
[32m+[m[32m      debug('cannot 404 after headers sent')[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // unhandled error[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      // respect status code from error[m
[32m+[m[32m      status = getErrorStatusCode(err)[m
[32m+[m
[32m+[m[32m      if (status === undefined) {[m
[32m+[m[32m        // fallback to status code on response[m
[32m+[m[32m        status = getResponseStatusCode(res)[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // respect headers from error[m
[32m+[m[32m        headers = getErrorHeaders(err)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // get error message[m
[32m+[m[32m      msg = getErrorMessage(err, status, env)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // not found[m
[32m+[m[32m      status = 404[m
[32m+[m[32m      msg = 'Cannot ' + req.method + ' ' + encodeUrl(getResourceName(req))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('default %s', status)[m
[32m+[m
[32m+[m[32m    // schedule onerror callback[m
[32m+[m[32m    if (err && onerror) {[m
[32m+[m[32m      defer(onerror, err, req, res)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // cannot actually respond[m
[32m+[m[32m    if (headersSent(res)) {[m
[32m+[m[32m      debug('cannot %d after headers sent', status)[m
[32m+[m[32m      req.socket.destroy()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // send response[m
[32m+[m[32m    send(req, res, status, headers, msg)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get headers from Error object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} err[m
[32m+[m[32m * @return {object}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getErrorHeaders (err) {[m
[32m+[m[32m  if (!err.headers || typeof err.headers !== 'object') {[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var headers = Object.create(null)[m
[32m+[m[32m  var keys = Object.keys(err.headers)[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m    var key = keys[i][m
[32m+[m[32m    headers[key] = err.headers[key][m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return headers[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get message from Error object, fallback to status message.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} err[m
[32m+[m[32m * @param {number} status[m
[32m+[m[32m * @param {string} env[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getErrorMessage (err, status, env) {[m
[32m+[m[32m  var msg[m
[32m+[m
[32m+[m[32m  if (env !== 'production') {[m
[32m+[m[32m    // use err.stack, which typically includes err.message[m
[32m+[m[32m    msg = err.stack[m
[32m+[m
[32m+[m[32m    // fallback to err.toString() when possible[m
[32m+[m[32m    if (!msg && typeof err.toString === 'function') {[m
[32m+[m[32m      msg = err.toString()[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return msg || statuses.message[status][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get status code from Error object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} err[m
[32m+[m[32m * @return {number}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getErrorStatusCode (err) {[m
[32m+[m[32m  // check err.status[m
[32m+[m[32m  if (typeof err.status === 'number' && err.status >= 400 && err.status < 600) {[m
[32m+[m[32m    return err.status[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // check err.statusCode[m
[32m+[m[32m  if (typeof err.statusCode === 'number' && err.statusCode >= 400 && err.statusCode < 600) {[m
[32m+[m[32m    return err.statusCode[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return undefined[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get resource name for the request.[m
[32m+[m[32m *[m
[32m+[m[32m * This is typically just the original pathname of the request[m
[32m+[m[32m * but will fallback to "resource" is that cannot be determined.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {IncomingMessage} req[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getResourceName (req) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return parseUrl.original(req).pathname[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return 'resource'[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get status code from response.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {OutgoingMessage} res[m
[32m+[m[32m * @return {number}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getResponseStatusCode (res) {[m
[32m+[m[32m  var status = res.statusCode[m
[32m+[m
[32m+[m[32m  // default status code to 500 if outside valid range[m
[32m+[m[32m  if (typeof status !== 'number' || status < 400 || status > 599) {[m
[32m+[m[32m    status = 500[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return status[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if the response headers have been sent.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} res[m
[32m+[m[32m * @returns {boolean}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction headersSent (res) {[m
[32m+[m[32m  return typeof res.headersSent !== 'boolean'[m
[32m+[m[32m    ? Boolean(res._header)[m
[32m+[m[32m    : res.headersSent[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Send response.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {IncomingMessage} req[m
[32m+[m[32m * @param {OutgoingMessage} res[m
[32m+[m[32m * @param {number} status[m
[32m+[m[32m * @param {object} headers[m
[32m+[m[32m * @param {string} message[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction send (req, res, status, headers, message) {[m
[32m+[m[32m  function write () {[m
[32m+[m[32m    // response body[m
[32m+[m[32m    var body = createHtmlDocument(message)[m
[32m+[m
[32m+[m[32m    // response status[m
[32m+[m[32m    res.statusCode = status[m
[32m+[m[32m    res.statusMessage = statuses.message[status][m
[32m+[m
[32m+[m[32m    // remove any content headers[m
[32m+[m[32m    res.removeHeader('Content-Encoding')[m
[32m+[m[32m    res.removeHeader('Content-Language')[m
[32m+[m[32m    res.removeHeader('Content-Range')[m
[32m+[m
[32m+[m[32m    // response headers[m
[32m+[m[32m    setHeaders(res, headers)[m
[32m+[m
[32m+[m[32m    // security headers[m
[32m+[m[32m    res.setHeader('Content-Security-Policy', "default-src 'none'")[m
[32m+[m[32m    res.setHeader('X-Content-Type-Options', 'nosniff')[m
[32m+[m
[32m+[m[32m    // standard headers[m
[32m+[m[32m    res.setHeader('Content-Type', 'text/html; charset=utf-8')[m
[32m+[m[32m    res.setHeader('Content-Length', Buffer.byteLength(body, 'utf8'))[m
[32m+[m
[32m+[m[32m    if (req.method === 'HEAD') {[m
[32m+[m[32m      res.end()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    res.end(body, 'utf8')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isFinished(req)) {[m
[32m+[m[32m    write()[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // unpipe everything from the request[m
[32m+[m[32m  unpipe(req)[m
[32m+[m
[32m+[m[32m  // flush the request[m
[32m+[m[32m  onFinished(req, write)[m
[32m+[m[32m  req.resume()[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set response headers from an object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {OutgoingMessage} res[m
[32m+[m[32m * @param {object} headers[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction setHeaders (res, headers) {[m
[32m+[m[32m  if (!headers) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var keys = Object.keys(headers)[m
[32m+[m[32m  for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m    var key = keys[i][m
[32m+[m[32m    res.setHeader(key, headers[key])[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/finalhandler/package.json b/node_modules/finalhandler/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..16bf11e[m
