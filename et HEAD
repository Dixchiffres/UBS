[1mdiff --git a/node_modules/statuses/index.js b/node_modules/statuses/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ea351c5[m
[1m--- /dev/null[m
[1m+++ b/node_modules/statuses/index.js[m
[36m@@ -0,0 +1,146 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * statuses[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar codes = require('./codes.json')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = status[m
[32m+[m
[32m+[m[32m// status code to message map[m
[32m+[m[32mstatus.message = codes[m
[32m+[m
[32m+[m[32m// status message (lower-case) to code map[m
[32m+[m[32mstatus.code = createMessageToStatusCodeMap(codes)[m
[32m+[m
[32m+[m[32m// array of status codes[m
[32m+[m[32mstatus.codes = createStatusCodeList(codes)[m
[32m+[m
[32m+[m[32m// status codes for redirects[m
[32m+[m[32mstatus.redirect = {[m
[32m+[m[32m  300: true,[m
[32m+[m[32m  301: true,[m
[32m+[m[32m  302: true,[m
[32m+[m[32m  303: true,[m
[32m+[m[32m  305: true,[m
[32m+[m[32m  307: true,[m
[32m+[m[32m  308: true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// status codes for empty bodies[m
[32m+[m[32mstatus.empty = {[m
[32m+[m[32m  204: true,[m
[32m+[m[32m  205: true,[m
[32m+[m[32m  304: true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// status codes for when you should retry the request[m
[32m+[m[32mstatus.retry = {[m
[32m+[m[32m  502: true,[m
[32m+[m[32m  503: true,[m
[32m+[m[32m  504: true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a map of message to status code.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createMessageToStatusCodeMap (codes) {[m
[32m+[m[32m  var map = {}[m
[32m+[m
[32m+[m[32m  Object.keys(codes).forEach(function forEachCode (code) {[m
[32m+[m[32m    var message = codes[code][m
[32m+[m[32m    var status = Number(code)[m
[32m+[m
[32m+[m[32m    // populate map[m
[32m+[m[32m    map[message.toLowerCase()] = status[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  return map[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a list of all status codes.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createStatusCodeList (codes) {[m
[32m+[m[32m  return Object.keys(codes).map(function mapCode (code) {[m
[32m+[m[32m    return Number(code)[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the status code for given message.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getStatusCode (message) {[m
[32m+[m[32m  var msg = message.toLowerCase()[m
[32m+[m
[32m+[m[32m  if (!Object.prototype.hasOwnProperty.call(status.code, msg)) {[m
[32m+[m[32m    throw new Error('invalid status message: "' + message + '"')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return status.code[msg][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the status message for given code.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getStatusMessage (code) {[m
[32m+[m[32m  if (!Object.prototype.hasOwnProperty.call(status.message, code)) {[m
[32m+[m[32m    throw new Error('invalid status code: ' + code)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return status.message[code][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the status code.[m
[32m+[m[32m *[m
[32m+[m[32m * Given a number, this will throw if it is not a known status[m
[32m+[m[32m * code, otherwise the code will be returned. Given a string,[m
[32m+[m[32m * the string will be parsed for a number and return the code[m
[32m+[m[32m * if valid, otherwise will lookup the code assuming this is[m
[32m+[m[32m * the status message.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|number} code[m
[32m+[m[32m * @returns {number}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction status (code) {[m
[32m+[m[32m  if (typeof code === 'number') {[m
[32m+[m[32m    return getStatusMessage(code)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof code !== 'string') {[m
[32m+[m[32m    throw new TypeError('code must be a number or string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // '403'[m
[32m+[m[32m  var n = parseInt(code, 10)[m
[32m+[m[32m  if (!isNaN(n)) {[m
[32m+[m[32m    return getStatusMessage(n)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return getStatusCode(code)[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/statuses/package.json b/node_modules/statuses/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..8c3e719[m
[1m--- /dev/null[m
[1m+++ b/node_modules/statuses/package.json[m
[36m@@ -0,0 +1,49 @@[m
