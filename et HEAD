[32m+[m[32mdocument.querySelectorAll('.change-plan-btn, .manage-payments-btn').forEach(button => {[m
[32m+[m[32m    button.addEventListener('mouseenter', () => {[m
[32m+[m[32m        button.style.transform = 'scale(1.05)';[m
[32m+[m[32m        button.style.transition = 'transform 0.2s ease';[m
[32m+[m[32m    });[m
[32m+[m
