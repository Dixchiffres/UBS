[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Response prototype.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar res = Object.create(http.ServerResponse.prototype)[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = res[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar charsetRegExp = /;\s*charset\s*=/;[m
[32m+[m[32mvar schemaAndHostRegExp = /^(?:[a-zA-Z][a-zA-Z0-9+.-]*:)?\/\/[^\\\/\?]+/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set status `code`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Number} code[m
[32m+[m[32m * @return {ServerResponse}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.status = function status(code) {[m
[32m+[m[32m  if ((typeof code === 'string' || Math.floor(code) !== code) && code > 99 && code < 1000) {[m
[32m+[m[32m    deprecate('res.status(' + JSON.stringify(code) + '): use res.status(' + Math.floor(code) + ') instead')[m
[32m+[m[32m  }[m
[32m+[m[32m  this.statusCode = code;[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set Link header field with the given `links`.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.links({[m
[32m+[m[32m *      next: 'http://api.example.com/users?page=2',[m
[32m+[m[32m *      last: 'http://api.example.com/users?page=5'[m
[32m+[m[32m *    });[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} links[m
[32m+[m[32m * @return {ServerResponse}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.links = function(links){[m
[32m+[m[32m  var link = this.get('Link') || '';[m
[32m+[m[32m  if (link) link += ', ';[m
[32m+[m[32m  return this.set('Link', link + Object.keys(links).map(function(rel){[m
[32m+[m[32m    return '<' + links[rel] + '>; rel="' + rel + '"';[m
[32m+[m[32m  }).join(', '));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Send a response.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     res.send(Buffer.from('wahoo'));[m
[32m+[m[32m *     res.send({ some: 'json' });[m
[32m+[m[32m *     res.send('<p>some html</p>');[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|number|boolean|object|Buffer} body[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.send = function send(body) {[m
[32m+[m[32m  var chunk = body;[m
[32m+[m[32m  var encoding;[m
[32m+[m[32m  var req = this.req;[m
[32m+[m[32m  var type;[m
[32m+[m
[32m+[m[32m  // settings[m
[32m+[m[32m  var app = this.app;[m
[32m+[m
[32m+[m[32m  // allow status / body[m
[32m+[m[32m  if (arguments.length === 2) {[m
[32m+[m[32m    // res.send(body, status) backwards compat[m
[32m+[m[32m    if (typeof arguments[0] !== 'number' && typeof arguments[1] === 'number') {[m
[32m+[m[32m      deprecate('res.send(body, status): Use res.status(status).send(body) instead');[m
[32m+[m[32m      this.statusCode = arguments[1];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      deprecate('res.send(status, body): Use res.status(status).send(body) instead');[m
[32m+[m[32m      this.statusCode = arguments[0];[m
[32m+[m[32m      chunk = arguments[1];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // disambiguate res.send(status) and res.send(status, num)[m
[32m+[m[32m  if (typeof chunk === 'number' && arguments.length === 1) {[m
[32m+[m[32m    // res.send(status) will set status message as text string[m
[32m+[m[32m    if (!this.get('Content-Type')) {[m
[32m+[m[32m      this.type('txt');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    deprecate('res.send(status): Use res.sendStatus(status) instead');[m
[32m+[m[32m    this.statusCode = chunk;[m
[32m+[m[32m    chunk = statuses.message[chunk][m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  switch (typeof chunk) {[m
[32m+[m[32m    // string defaulting to html[m
[32m+[m[32m    case 'string':[m
[32m+[m[32m      if (!this.get('Content-Type')) {[m
[32m+[m[32m        this.type('html');[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'boolean':[m
[32m+[m[32m    case 'number':[m
[32m+[m[32m    case 'object':[m
[32m+[m[32m      if (chunk === null) {[m
[32m+[m[32m        chunk = '';[m
[32m+[m[32m      } else if (Buffer.isBuffer(chunk)) {[m
[32m+[m[32m        if (!this.get('Content-Type')) {[m
[32m+[m[32m          this.type('bin');[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return this.json(chunk);[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // write strings in utf-8[m
[32m+[m[32m  if (typeof chunk === 'string') {[m
[32m+[m[32m    encoding = 'utf8';[m
[32m+[m[32m    type = this.get('Content-Type');[m
[32m+[m
[32m+[m[32m    // reflect this in content-type[m
[32m+[m[32m    if (typeof type === 'string') {[m
[32m+[m[32m      this.set('Content-Type', setCharset(type, 'utf-8'));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // determine if ETag should be generated[m
[32m+[m[32m  var etagFn = app.get('etag fn')[m
[32m+[m[32m  var generateETag = !this.get('ETag') && typeof etagFn === 'function'[m
[32m+[m
[32m+[m[32m  // populate Content-Length[m
[32m+[m[32m  var len[m
[32m+[m[32m  if (chunk !== undefined) {[m
[32m+[m[32m    if (Buffer.isBuffer(chunk)) {[m
[32m+[m[32m      // get length of Buffer[m
[32m+[m[32m      len = chunk.length[m
[32m+[m[32m    } else if (!generateETag && chunk.length < 1000) {[m
[32m+[m[32m      // just calculate length when no ETag + small chunk[m
[32m+[m[32m      len = Buffer.byteLength(chunk, encoding)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // convert chunk to Buffer and calculate[m
[32m+[m[32m      chunk = Buffer.from(chunk, encoding)[m
[32m+[m[32m      encoding = undefined;[m
[32m+[m[32m      len = chunk.length[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.set('Content-Length', len);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // populate ETag[m
[32m+[m[32m  var etag;[m
[32m+[m[32m  if (generateETag && len !== undefined) {[m
[32m+[m[32m    if ((etag = etagFn(chunk, encoding))) {[m
[32m+[m[32m      this.set('ETag', etag);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // freshness[m
[32m+[m[32m  if (req.fresh) this.statusCode = 304;[m
[32m+[m
[32m+[m[32m  // strip irrelevant headers[m
[32m+[m[32m  if (204 === this.statusCode || 304 === this.statusCode) {[m
[32m+[m[32m    this.removeHeader('Content-Type');[m
[32m+[m[32m    this.removeHeader('Content-Length');[m
[32m+[m[32m    this.removeHeader('Transfer-Encoding');[m
[32m+[m[32m    chunk = '';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // alter headers for 205[m
[32m+[m[32m  if (this.statusCode === 205) {[m
[32m+[m[32m    this.set('Content-Length', '0')[m
[32m+[m[32m    this.removeHeader('Transfer-Encoding')[m
[32m+[m[32m    chunk = ''[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (req.method === 'HEAD') {[m
[32m+[m[32m    // skip body for HEAD[m
[32m+[m[32m    this.end();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // respond[m
[32m+[m[32m    this.end(chunk, encoding);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Send JSON response.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     res.json(null);[m
[32m+[m[32m *     res.json({ user: 'tj' });[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|number|boolean|object} obj[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.json = function json(obj) {[m
[32m+[m[32m  var val = obj;[m
[32m+[m
[32m+[m[32m  // allow status / body[m
[32m+[m[32m  if (arguments.length === 2) {[m
[32m+[m[32m    // res.json(body, status) backwards compat[m
[32m+[m[32m    if (typeof arguments[1] === 'number') {[m
[32m+[m[32m      deprecate('res.json(obj, status): Use res.status(status).json(obj) instead');[m
[32m+[m[32m      this.statusCode = arguments[1];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      deprecate('res.json(status, obj): Use res.status(status).json(obj) instead');[m
[32m+[m[32m      this.statusCode = arguments[0];[m
[32m+[m[32m      val = arguments[1];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // settings[m
[32m+[m[32m  var app = this.app;[m
[32m+[m[32m  var escape = app.get('json escape')[m
[32m+[m[32m  var replacer = app.get('json replacer');[m
[32m+[m[32m  var spaces = app.get('json spaces');[m
[32m+[m[32m  var body = stringify(val, replacer, spaces, escape)[m
[32m+[m
[32m+[m[32m  // content-type[m
[32m+[m[32m  if (!this.get('Content-Type')) {[m
[32m+[m[32m    this.set('Content-Type', 'application/json');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this.send(body);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Send JSON response with JSONP callback support.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     res.jsonp(null);[m
[32m+[m[32m *     res.jsonp({ user: 'tj' });[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|number|boolean|object} obj[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.jsonp = function jsonp(obj) {[m
[32m+[m[32m  var val = obj;[m
[32m+[m
[32m+[m[32m  // allow status / body[m
[32m+[m[32m  if (arguments.length === 2) {[m
[32m+[m[32m    // res.jsonp(body, status) backwards compat[m
[32m+[m[32m    if (typeof arguments[1] === 'number') {[m
[32m+[m[32m      deprecate('res.jsonp(obj, status): Use res.status(status).jsonp(obj) instead');[m
[32m+[m[32m      this.statusCode = arguments[1];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      deprecate('res.jsonp(status, obj): Use res.status(status).jsonp(obj) instead');[m
[32m+[m[32m      this.statusCode = arguments[0];[m
[32m+[m[32m      val = arguments[1];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // settings[m
[32m+[m[32m  var app = this.app;[m
[32m+[m[32m  var escape = app.get('json escape')[m
[32m+[m[32m  var replacer = app.get('json replacer');[m
[32m+[m[32m  var spaces = app.get('json spaces');[m
[32m+[m[32m  var body = stringify(val, replacer, spaces, escape)[m
[32m+[m[32m  var callback = this.req.query[app.get('jsonp callback name')];[m
[32m+[m
[32m+[m[32m  // content-type[m
[32m+[m[32m  if (!this.get('Content-Type')) {[m
[32m+[m[32m    this.set('X-Content-Type-Options', 'nosniff');[m
[32m+[m[32m    this.set('Content-Type', 'application/json');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // fixup callback[m
[32m+[m[32m  if (Array.isArray(callback)) {[m
[32m+[m[32m    callback = callback[0];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // jsonp[m
[32m+[m[32m  if (typeof callback === 'string' && callback.length !== 0) {[m
[32m+[m[32m    this.set('X-Content-Type-Options', 'nosniff');[m
[32m+[m[32m    this.set('Content-Type', 'text/javascript');[m
[32m+[m
[32m+[m[32m    // restrict callback charset[m
[32m+[m[32m    callback = callback.replace(/[^\[\]\w$.]/g, '');[m
[32m+[m
[32m+[m[32m    if (body === undefined) {[m
[32m+[m[32m      // empty argument[m
[32m+[m[32m      body = ''[m
[32m+[m[32m    } else if (typeof body === 'string') {[m
[32m+[m[32m      // replace chars not allowed in JavaScript that are in JSON[m
[32m+[m[32m      body = body[m
[32m+[m[32m        .replace(/\u2028/g, '\\u2028')[m
[32m+[m[32m        .replace(/\u2029/g, '\\u2029')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // the /**/ is a specific security mitigation for "Rosetta Flash JSONP abuse"[m
[32m+[m[32m    // the typeof check is just to reduce client error noise[m
[32m+[m[32m    body = '/**/ typeof ' + callback + ' === \'function\' && ' + callback + '(' + body + ');';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this.send(body);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Send given HTTP status code.[m
[32m+[m[32m *[m
[32m+[m[32m * Sets the response status to `statusCode` and the body of the[m
[32m+[m[32m * response to the standard description from node's http.STATUS_CODES[m
[32m+[m[32m * or the statusCode number if no description.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     res.sendStatus(200);[m
[32m+[m[32m *[m
[32m+[m[32m * @param {number} statusCode[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.sendStatus = function sendStatus(statusCode) {[m
[32m+[m[32m  var body = statuses.message[statusCode] || String(statusCode)[m
[32m+[m
[32m+[m[32m  this.statusCode = statusCode;[m
[32m+[m[32m  this.type('txt');[m
[32m+[m
[32m+[m[32m  return this.send(body);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Transfer the file at the given `path`.[m
[32m+[m[32m *[m
[32m+[m[32m * Automatically sets the _Content-Type_ response header field.[m
[32m+[m[32m * The callback `callback(err)` is invoked when the transfer is complete[m
[32m+[m[32m * or when an error occurs. Be sure to check `res.headersSent`[m
[32m+[m[32m * if you wish to attempt responding, as the header and some data[m
[32m+[m[32m * may have already been transferred.[m
[32m+[m[32m *[m
[32m+[m[32m * Options:[m
[32m+[m[32m *[m
[32m+[m[32m *   - `maxAge`   defaulting to 0 (can be string converted by `ms`)[m
[32m+[m[32m *   - `root`     root directory for relative filenames[m
[32m+[m[32m *   - `headers`  object of headers to serve with file[m
[32m+[m[32m *   - `dotfiles` serve dotfiles, defaulting to false; can be `"allow"` to send them[m
[32m+[m[32m *[m
[32m+[m[32m * Other options are passed along to `send`.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *  The following example illustrates how `res.sendFile()` may[m
[32m+[m[32m *  be used as an alternative for the `static()` middleware for[m
[32m+[m[32m *  dynamic situations. The code backing `res.sendFile()` is actually[m
[32m+[m[32m *  the same code, so HTTP cache support etc is identical.[m
[32m+[m[32m *[m
[32m+[m[32m *     app.get('/user/:uid/photos/:file', function(req, res){[m
[32m+[m[32m *       var uid = req.params.uid[m
[32m+[m[32m *         , file = req.params.file;[m
[32m+[m[32m *[m
[32m+[m[32m *       req.user.mayViewFilesFrom(uid, function(yes){[m
[32m+[m[32m *         if (yes) {[m
[32m+[m[32m *           res.sendFile('/uploads/' + uid + '/' + file);[m
[32m+[m[32m *         } else {[m
[32m+[m[32m *           res.send(403, 'Sorry! you cant see that.');[m
[32m+[m[32m *         }[m
[32m+[m[32m *       });[m
[32m+[m[32m *     });[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.sendFile = function sendFile(path, options, callback) {[m
[32m+[m[32m  var done = callback;[m
[32m+[m[32m  var req = this.req;[m
[32m+[m[32m  var res = this;[m
[32m+[m[32m  var next = req.next;[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m
[32m+[m[32m  if (!path) {[m
[32m+[m[32m    throw new TypeError('path argument is required to res.sendFile');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof path !== 'string') {[m
[32m+[m[32m    throw new TypeError('path must be a string to res.sendFile')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // support function as second arg[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    done = options;[m
[32m+[m[32m    opts = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!opts.root && !isAbsolute(path)) {[m
[32m+[m[32m    throw new TypeError('path must be absolute or specify root to res.sendFile');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create file stream[m
[32m+[m[32m  var pathname = encodeURI(path);[m
[32m+[m[32m  var file = send(req, pathname, opts);[m
[32m+[m
[32m+[m[32m  // transfer[m
[32m+[m[32m  sendfile(res, file, opts, function (err) {[m
[32m+[m[32m    if (done) return done(err);[m
[32m+[m[32m    if (err && err.code === 'EISDIR') return next();[m
[32m+[m
[32m+[m[32m    // next() all but write errors[m
[32m+[m[32m    if (err && err.code !== 'ECONNABORTED' && err.syscall !== 'write') {[m
[32m+[m[32m      next(err);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Transfer the file at the given `path`.[m
[32m+[m[32m *[m
[32m+[m[32m * Automatically sets the _Content-Type_ response header field.[m
[32m+[m[32m * The callback `callback(err)` is invoked when the transfer is complete[m
[32m+[m[32m * or when an error occurs. Be sure to check `res.headersSent`[m
[32m+[m[32m * if you wish to attempt responding, as the header and some data[m
[32m+[m[32m * may have already been transferred.[m
[32m+[m[32m *[m
[32m+[m[32m * Options:[m
[32m+[m[32m *[m
[32m+[m[32m *   - `maxAge`   defaulting to 0 (can be string converted by `ms`)[m
[32m+[m[32m *   - `root`     root directory for relative filenames[m
[32m+[m[32m *   - `headers`  object of headers to serve with file[m
[32m+[m[32m *   - `dotfiles` serve dotfiles, defaulting to false; can be `"allow"` to send them[m
[32m+[m[32m *[m
[32m+[m[32m * Other options are passed along to `send`.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *  The following example illustrates how `res.sendfile()` may[m
[32m+[m[32m *  be used as an alternative for the `static()` middleware for[m
[32m+[m[32m *  dynamic situations. The code backing `res.sendfile()` is actually[m
[32m+[m[32m *  the same code, so HTTP cache support etc is identical.[m
[32m+[m[32m *[m
[32m+[m[32m *     app.get('/user/:uid/photos/:file', function(req, res){[m
[32m+[m[32m *       var uid = req.params.uid[m
[32m+[m[32m *         , file = req.params.file;[m
[32m+[m[32m *[m
[32m+[m[32m *       req.user.mayViewFilesFrom(uid, function(yes){[m
[32m+[m[32m *         if (yes) {[m
[32m+[m[32m *           res.sendfile('/uploads/' + uid + '/' + file);[m
[32m+[m[32m *         } else {[m
[32m+[m[32m *           res.send(403, 'Sorry! you cant see that.');[m
[32m+[m[32m *         }[m
[32m+[m[32m *       });[m
[32m+[m[32m *     });[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.sendfile = function (path, options, callback) {[m
[32m+[m[32m  var done = callback;[m
[32m+[m[32m  var req = this.req;[m
[32m+[m[32m  var res = this;[m
[32m+[m[32m  var next = req.next;[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m
[32m+[m[32m  // support function as second arg[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    done = options;[m
[32m+[m[32m    opts = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // create file stream[m
[32m+[m[32m  var file = send(req, path, opts);[m
[32m+[m
[32m+[m[32m  // transfer[m
[32m+[m[32m  sendfile(res, file, opts, function (err) {[m
[32m+[m[32m    if (done) return done(err);[m
[32m+[m[32m    if (err && err.code === 'EISDIR') return next();[m
[32m+[m
[32m+[m[32m    // next() all but write errors[m
[32m+[m[32m    if (err && err.code !== 'ECONNABORTED' && err.syscall !== 'write') {[m
[32m+[m[32m      next(err);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mres.sendfile = deprecate.function(res.sendfile,[m
[32m+[m[32m  'res.sendfile: Use res.sendFile instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Transfer the file at the given `path` as an attachment.[m
[32m+[m[32m *[m
[32m+[m[32m * Optionally providing an alternate attachment `filename`,[m
[32m+[m[32m * and optional callback `callback(err)`. The callback is invoked[m
[32m+[m[32m * when the data transfer is complete, or when an error has[m
[32m+[m[32m * occurred. Be sure to check `res.headersSent` if you plan to respond.[m
[32m+[m[32m *[m
[32m+[m[32m * Optionally providing an `options` object to use with `res.sendFile()`.[m
[32m+[m[32m * This function will set the `Content-Disposition` header, overriding[m
[32m+[m[32m * any `Content-Disposition` header passed as header options in order[m
[32m+[m[32m * to set the attachment and filename.[m
[32m+[m[32m *[m
[32m+[m[32m * This method uses `res.sendFile()`.[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.download = function download (path, filename, options, callback) {[m
[32m+[m[32m  var done = callback;[m
[32m+[m[32m  var name = filename;[m
[32m+[m[32m  var opts = options || null[m
[32m+[m
[32m+[m[32m  // support function as second or third arg[m
[32m+[m[32m  if (typeof filename === 'function') {[m
[32m+[m[32m    done = filename;[m
[32m+[m[32m    name = null;[m
[32m+[m[32m    opts = null[m
[32m+[m[32m  } else if (typeof options === 'function') {[m
[32m+[m[32m    done = options[m
[32m+[m[32m    opts = null[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // support optional filename, where options may be in it's place[m
[32m+[m[32m  if (typeof filename === 'object' &&[m
[32m+[m[32m    (typeof options === 'function' || options === undefined)) {[m
[32m+[m[32m    name = null[m
[32m+[m[32m    opts = filename[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // set Content-Disposition when file is sent[m
[32m+[m[32m  var headers = {[m
[32m+[m[32m    'Content-Disposition': contentDisposition(name || path)[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // merge user-provided headers[m
[32m+[m[32m  if (opts && opts.headers) {[m
[32m+[m[32m    var keys = Object.keys(opts.headers)[m
[32m+[m[32m    for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m      var key = keys[i][m
[32m+[m[32m      if (key.toLowerCase() !== 'content-disposition') {[m
[32m+[m[32m        headers[key] = opts.headers[key][m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // merge user-provided options[m
[32m+[m[32m  opts = Object.create(opts)[m
[32m+[m[32m  opts.headers = headers[m
[32m+[m
[32m+[m[32m  // Resolve the full path for sendFile[m
[32m+[m[32m  var fullPath = !opts.root[m
[32m+[m[32m    ? resolve(path)[m
[32m+[m[32m    : path[m
[32m+[m
[32m+[m[32m  // send file[m
[32m+[m[32m  return this.sendFile(fullPath, opts, done)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set _Content-Type_ response header with `type` through `mime.lookup()`[m
[32m+[m[32m * when it does not contain "/", or set the Content-Type to `type` otherwise.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     res.type('.html');[m
[32m+[m[32m *     res.type('html');[m
[32m+[m[32m *     res.type('json');[m
[32m+[m[32m *     res.type('application/json');[m
[32m+[m[32m *     res.type('png');[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} type[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.contentType =[m
[32m+[m[32mres.type = function contentType(type) {[m
[32m+[m[32m  var ct = type.indexOf('/') === -1[m
[32m+[m[32m    ? mime.lookup(type)[m
[32m+[m[32m    : type;[m
[32m+[m
[32m+[m[32m  return this.set('Content-Type', ct);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Respond to the Acceptable formats using an `obj`[m
[32m+[m[32m * of mime-type callbacks.[m
[32m+[m[32m *[m
[32m+[m[32m * This method uses `req.accepted`, an array of[m
[32m+[m[32m * acceptable types ordered by their quality values.[m
[32m+[m[32m * When "Accept" is not present the _first_ callback[m
[32m+[m[32m * is invoked, otherwise the first match is used. When[m
[32m+[m[32m * no match is performed the server responds with[m
[32m+[m[32m * 406 "Not Acceptable".[m
[32m+[m[32m *[m
[32m+[m[32m * Content-Type is set for you, however if you choose[m
[32m+[m[32m * you may alter this within the callback using `res.type()`[m
[32m+[m[32m * or `res.set('Content-Type', ...)`.[m
[32m+[m[32m *[m
[32m+[m[32m *    res.format({[m
[32m+[m[32m *      'text/plain': function(){[m
[32m+[m[32m *        res.send('hey');[m
[32m+[m[32m *      },[m
[32m+[m[32m *[m
[32m+[m[32m *      'text/html': function(){[m
[32m+[m[32m *        res.send('<p>hey</p>');[m
[32m+[m[32m *      },[m
[32m+[m[32m *[m
[32m+[m[32m *      'application/json': function () {[m
[32m+[m[32m *        res.send({ message: 'hey' });[m
[32m+[m[32m *      }[m
[32m+[m[32m *    });[m
[32m+[m[32m *[m
[32m+[m[32m * In addition to canonicalized MIME types you may[m
[32m+[m[32m * also use extnames mapped to these types:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.format({[m
[32m+[m[32m *      text: function(){[m
[32m+[m[32m *        res.send('hey');[m
[32m+[m[32m *      },[m
[32m+[m[32m *[m
[32m+[m[32m *      html: function(){[m
[32m+[m[32m *        res.send('<p>hey</p>');[m
[32m+[m[32m *      },[m
[32m+[m[32m *[m
[32m+[m[32m *      json: function(){[m
[32m+[m[32m *        res.send({ message: 'hey' });[m
[32m+[m[32m *      }[m
[32m+[m[32m *    });[m
[32m+[m[32m *[m
[32m+[m[32m * By default Express passes an `Error`[m
[32m+[m[32m * with a `.status` of 406 to `next(err)`[m
[32m+[m[32m * if a match is not made. If you provide[m
[32m+[m[32m * a `.default` callback it will be invoked[m
[32m+[m[32m * instead.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} obj[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.format = function(obj){[m
[32m+[m[32m  var req = this.req;[m
[32m+[m[32m  var next = req.next;[m
[32m+[m
[32m+[m[32m  var keys = Object.keys(obj)[m
[32m+[m[32m    .filter(function (v) { return v !== 'default' })[m
[32m+[m
[32m+[m[32m  var key = keys.length > 0[m
[32m+[m[32m    ? req.accepts(keys)[m
[32m+[m[32m    : false;[m
[32m+[m
[32m+[m[32m  this.vary("Accept");[m
[32m+[m
[32m+[m[32m  if (key) {[m
[32m+[m[32m    this.set('Content-Type', normalizeType(key).value);[m
[32m+[m[32m    obj[key](req, this, next);[m
[32m+[m[32m  } else if (obj.default) {[m
[32m+[m[32m    obj.default(req, this, next)[m
[32m+[m[32m  } else {[m
[32m+[m[32m    next(createError(406, {[m
[32m+[m[32m      types: normalizeTypes(keys).map(function (o) { return o.value })[m
[32m+[m[32m    }))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set _Content-Disposition_ header to _attachment_ with optional `filename`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} filename[m
[32m+[m[32m * @return {ServerResponse}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.attachment = function attachment(filename) {[m
[32m+[m[32m  if (filename) {[m
[32m+[m[32m    this.type(extname(filename));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.set('Content-Disposition', contentDisposition(filename));[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Append additional header `field` with value `val`.[m
[32m+[m[32m *[m
[32m+[m[32m * Example:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.append('Link', ['<http://localhost/>', '<http://localhost:3000/>']);[m
[32m+[m[32m *    res.append('Set-Cookie', 'foo=bar; Path=/; HttpOnly');[m
[32m+[m[32m *    res.append('Warning', '199 Miscellaneous warning');[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} field[m
[32m+[m[32m * @param {String|Array} val[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.append = function append(field, val) {[m
[32m+[m[32m  var prev = this.get(field);[m
[32m+[m[32m  var value = val;[m
[32m+[m
[32m+[m[32m  if (prev) {[m
[32m+[m[32m    // concat the new and prev vals[m
[32m+[m[32m    value = Array.isArray(prev) ? prev.concat(val)[m
[32m+[m[32m      : Array.isArray(val) ? [prev].concat(val)[m
[32m+[m[32m        : [prev, val][m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this.set(field, value);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set header `field` to `val`, or pass[m
[32m+[m[32m * an object of header fields.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.set('Foo', ['bar', 'baz']);[m
[32m+[m[32m *    res.set('Accept', 'application/json');[m
[32m+[m[32m *    res.set({ Accept: 'text/plain', 'X-API-Key': 'tobi' });[m
[32m+[m[32m *[m
[32m+[m[32m * Aliased as `res.header()`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Object} field[m
[32m+[m[32m * @param {String|Array} val[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.set =[m
[32m+[m[32mres.header = function header(field, val) {[m
[32m+[m[32m  if (arguments.length === 2) {[m
[32m+[m[32m    var value = Array.isArray(val)[m
[32m+[m[32m      ? val.map(String)[m
[32m+[m[32m      : String(val);[m
[32m+[m
[32m+[m[32m    // add charset to content-type[m
[32m+[m[32m    if (field.toLowerCase() === 'content-type') {[m
[32m+[m[32m      if (Array.isArray(value)) {[m
[32m+[m[32m        throw new TypeError('Content-Type cannot be set to an Array');[m
[32m+[m[32m      }[m
[32m+[m[32m      if (!charsetRegExp.test(value)) {[m
[32m+[m[32m        var charset = mime.charsets.lookup(value.split(';')[0]);[m
[32m+[m[32m        if (charset) value += '; charset=' + charset.toLowerCase();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.setHeader(field, value);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    for (var key in field) {[m
[32m+[m[32m      this.set(key, field[key]);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get value for header `field`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} field[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.get = function(field){[m
[32m+[m[32m  return this.getHeader(field);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Clear cookie `name`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.clearCookie = function clearCookie(name, options) {[m
[32m+[m[32m  var opts = merge({ expires: new Date(1), path: '/' }, options);[m
[32m+[m
[32m+[m[32m  return this.cookie(name, '', opts);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set cookie `name` to `value`, with the given `options`.[m
[32m+[m[32m *[m
[32m+[m[32m * Options:[m
[32m+[m[32m *[m
[32m+[m[32m *    - `maxAge`   max-age in milliseconds, converted to `expires`[m
[32m+[m[32m *    - `signed`   sign the cookie[m
[32m+[m[32m *    - `path`     defaults to "/"[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *    // "Remember Me" for 15 minutes[m
[32m+[m[32m *    res.cookie('rememberme', '1', { expires: new Date(Date.now() + 900000), httpOnly: true });[m
[32m+[m[32m *[m
[32m+[m[32m *    // same as above[m
[32m+[m[32m *    res.cookie('rememberme', '1', { maxAge: 900000, httpOnly: true })[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {String|Object} value[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.cookie = function (name, value, options) {[m
[32m+[m[32m  var opts = merge({}, options);[m
[32m+[m[32m  var secret = this.req.secret;[m
[32m+[m[32m  var signed = opts.signed;[m
[32m+[m
[32m+[m[32m  if (signed && !secret) {[m
[32m+[m[32m    throw new Error('cookieParser("secret") required for signed cookies');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var val = typeof value === 'object'[m
[32m+[m[32m    ? 'j:' + JSON.stringify(value)[m
[32m+[m[32m    : String(value);[m
[32m+[m
[32m+[m[32m  if (signed) {[m
[32m+[m[32m    val = 's:' + sign(val, secret);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opts.maxAge != null) {[m
[32m+[m[32m    var maxAge = opts.maxAge - 0[m
[32m+[m
[32m+[m[32m    if (!isNaN(maxAge)) {[m
[32m+[m[32m      opts.expires = new Date(Date.now() + maxAge)[m
[32m+[m[32m      opts.maxAge = Math.floor(maxAge / 1000)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opts.path == null) {[m
[32m+[m[32m    opts.path = '/';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.append('Set-Cookie', cookie.serialize(name, String(val), opts));[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Set the location header to `url`.[m
[32m+[m[32m *[m
[32m+[m[32m * The given `url` can also be "back", which redirects[m
[32m+[m[32m * to the _Referrer_ or _Referer_ headers or "/".[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.location('/foo/bar').;[m
[32m+[m[32m *    res.location('http://example.com');[m
[32m+[m[32m *    res.location('../login');[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} url[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.location = function location(url) {[m
[32m+[m[32m  var loc;[m
[32m+[m
[32m+[m[32m  // "back" is an alias for the referrer[m
[32m+[m[32m  if (url === 'back') {[m
[32m+[m[32m    loc = this.req.get('Referrer') || '/';[m
[32m+[m[32m  } else {[m
[32m+[m[32m    loc = String(url);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var m = schemaAndHostRegExp.exec(loc);[m
[32m+[m[32m  var pos = m ? m[0].length + 1 : 0;[m
[32m+[m
[32m+[m[32m  // Only encode after host to avoid invalid encoding which can introduce[m
[32m+[m[32m  // vulnerabilities (e.g. `\\` to `%5C`).[m
[32m+[m[32m  loc = loc.slice(0, pos) + encodeUrl(loc.slice(pos));[m
[32m+[m
[32m+[m[32m  return this.set('Location', loc);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Redirect to the given `url` with optional response `status`[m
[32m+[m[32m * defaulting to 302.[m
[32m+[m[32m *[m
[32m+[m[32m * The resulting `url` is determined by `res.location()`, so[m
[32m+[m[32m * it will play nicely with mounted apps, relative paths,[m
[32m+[m[32m * `"back"` etc.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *    res.redirect('/foo/bar');[m
[32m+[m[32m *    res.redirect('http://example.com');[m
[32m+[m[32m *    res.redirect(301, 'http://example.com');[m
[32m+[m[32m *    res.redirect('../login'); // /blog/post/1 -> /blog/login[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.redirect = function redirect(url) {[m
[32m+[m[32m  var address = url;[m
[32m+[m[32m  var body;[m
[32m+[m[32m  var status = 302;[m
[32m+[m
[32m+[m[32m  // allow status / url[m
[32m+[m[32m  if (arguments.length === 2) {[m
[32m+[m[32m    if (typeof arguments[0] === 'number') {[m
[32m+[m[32m      status = arguments[0];[m
[32m+[m[32m      address = arguments[1];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      deprecate('res.redirect(url, status): Use res.redirect(status, url) instead');[m
[32m+[m[32m      status = arguments[1];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Set location header[m
[32m+[m[32m  address = this.location(address).get('Location');[m
[32m+[m
[32m+[m[32m  // Support text/{plain,html} by default[m
[32m+[m[32m  this.format({[m
[32m+[m[32m    text: function(){[m
[32m+[m[32m      body = statuses.message[status] + '. Redirecting to ' + address[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    html: function(){[m
[32m+[m[32m      var u = escapeHtml(address);[m
[32m+[m[32m      body = '<p>' + statuses.message[status] + '. Redirecting to <a href="' + u + '">' + u + '</a></p>'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    default: function(){[m
[32m+[m[32m      body = '';[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Respond[m
[32m+[m[32m  this.statusCode = status;[m
[32m+[m[32m  this.set('Content-Length', Buffer.byteLength(body));[m
[32m+[m
[32m+[m[32m  if (this.req.method === 'HEAD') {[m
[32m+[m[32m    this.end();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    this.end(body);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Add `field` to Vary. If already present in the Vary set, then[m
[32m+[m[32m * this call is simply ignored.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Array|String} field[m
[32m+[m[32m * @return {ServerResponse} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.vary = function(field){[m
[32m+[m[32m  // checks for back-compat[m
[32m+[m[32m  if (!field || (Array.isArray(field) && !field.length)) {[m
[32m+[m[32m    deprecate('res.vary(): Provide a field name');[m
[32m+[m[32m    return this;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  vary(this, field);[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Render `view` with the given `options` and optional callback `fn`.[m
[32m+[m[32m * When a callback function is given a response will _not_ be made[m
[32m+[m[32m * automatically, otherwise a response of _200_ and _text/html_ is given.[m
[32m+[m[32m *[m
[32m+[m[32m * Options:[m
[32m+[m[32m *[m
[32m+[m[32m *  - `cache`     boolean hinting to the engine it should cache[m
[32m+[m[32m *  - `filename`  filename of the view being rendered[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mres.render = function render(view, options, callback) {[m
[32m+[m[32m  var app = this.req.app;[m
[32m+[m[32m  var done = callback;[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m[32m  var req = this.req;[m
[32m+[m[32m  var self = this;[m
[32m+[m
[32m+[m[32m  // support callback function as second arg[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    done = options;[m
[32m+[m[32m    opts = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // merge res.locals[m
[32m+[m[32m  opts._locals = self.locals;[m
[32m+[m
[32m+[m[32m  // default callback to respond[m
[32m+[m[32m  done = done || function (err, str) {[m
[32m+[m[32m    if (err) return req.next(err);[m
[32m+[m[32m    self.send(str);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // render[m
[32m+[m[32m  app.render(view, opts, done);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// pipe the send file stream[m
[32m+[m[32mfunction sendfile(res, file, options, callback) {[m
[32m+[m[32m  var done = false;[m
[32m+[m[32m  var streaming;[m
[32m+[m
[32m+[m[32m  // request aborted[m
[32m+[m[32m  function onaborted() {[m
[32m+[m[32m    if (done) return;[m
[32m+[m[32m    done = true;[m
[32m+[m
[32m+[m[32m    var err = new Error('Request aborted');[m
[32m+[m[32m    err.code = 'ECONNABORTED';[m
[32m+[m[32m    callback(err);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // directory[m
[32m+[m[32m  function ondirectory() {[m
[32m+[m[32m    if (done) return;[m
[32m+[m[32m    done = true;[m
[32m+[m
[32m+[m[32m    var err = new Error('EISDIR, read');[m
[32m+[m[32m    err.code = 'EISDIR';[m
[32m+[m[32m    callback(err);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // errors[m
[32m+[m[32m  function onerror(err) {[m
[32m+[m[32m    if (done) return;[m
[32m+[m[32m    done = true;[m
[32m+[m[32m    callback(err);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ended[m
[32m+[m[32m  function onend() {[m
[32m+[m[32m    if (done) return;[m
[32m+[m[32m    done = true;[m
[32m+[m[32m    callback();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // file[m
[32m+[m[32m  function onfile() {[m
[32m+[m[32m    streaming = false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // finished[m
[32m+[m[32m  function onfinish(err) {[m
[32m+[m[32m    if (err && err.code === 'ECONNRESET') return onaborted();[m
[32m+[m[32m    if (err) return onerror(err);[m
[32m+[m[32m    if (done) return;[m
[32m+[m
[32m+[m[32m    setImmediate(function () {[m
[32m+[m[32m      if (streaming !== false && !done) {[m
[32m+[m[32m        onaborted();[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (done) return;[m
[32m+[m[32m      done = true;[m
[32m+[m[32m      callback();[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // streaming[m
[32m+[m[32m  function onstream() {[m
[32m+[m[32m    streaming = true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  file.on('directory', ondirectory);[m
[32m+[m[32m  file.on('end', onend);[m
[32m+[m[32m  file.on('error', onerror);[m
[32m+[m[32m  file.on('file', onfile);[m
[32m+[m[32m  file.on('stream', onstream);[m
[32m+[m[32m  onFinished(res, onfinish);[m
[32m+[m
[32m+[m[32m  if (options.headers) {[m
[32m+[m[32m    // set headers on successful transfer[m
[32m+[m[32m    file.on('headers', function headers(res) {[m
[32m+[m[32m      var obj = options.headers;[m
[32m+[m[32m      var keys = Object.keys(obj);[m
[32m+[m
[32m+[m[32m      for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m        var k = keys[i];[m
[32m+[m[32m        res.setHeader(k, obj[k]);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // pipe[m
[32m+[m[32m  file.pipe(res);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Stringify JSON, like JSON.stringify, but v8 optimized, with the[m
[32m+[m[32m * ability to escape characters that can trigger HTML sniffing.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {*} value[m
[32m+[m[32m * @param {function} replacer[m
[32m+[m[32m * @param {number} spaces[m
[32m+[m[32m * @param {boolean} escape[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction stringify (value, replacer, spaces, escape) {[m
[32m+[m[32m  // v8 checks arguments.length for optimizing simple call[m
[32m+[m[32m  // https://bugs.chromium.org/p/v8/issues/detail?id=4730[m
[32m+[m[32m  var json = replacer || spaces[m
[32m+[m[32m    ? JSON.stringify(value, replacer, spaces)[m
[32m+[m[32m    : JSON.stringify(value);[m
[32m+[m
[32m+[m[32m  if (escape && typeof json === 'string') {[m
[32m+[m[32m    json = json.replace(/[<>&]/g, function (c) {[m
[32m+[m[32m      switch (c.charCodeAt(0)) {[m
[32m+[m[32m        case 0x3c:[m
[32m+[m[32m          return '\\u003c'[m
[32m+[m[32m        case 0x3e:[m
[32m+[m[32m          return '\\u003e'[m
[32m+[m[32m        case 0x26:[m
[32m+[m[32m          return '\\u0026'[m
[32m+[m[32m        /* istanbul ignore next: unreachable default */[m
[32m+[m[32m        default:[m
[32m+[m[32m          return c[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return json[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/express/lib/router/index.js b/node_modules/express/lib/router/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..abb3a6f[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/router/index.js[m
[36m@@ -0,0 +1,673 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar Route = require('./route');[m
[32m+[m[32mvar Layer = require('./layer');[m
[32m+[m[32mvar methods = require('methods');[m
[32m+[m[32mvar mixin = require('utils-merge');[m
[32m+[m[32mvar debug = require('debug')('express:router');[m
[32m+[m[32mvar deprecate = require('depd')('express');[m
[32m+[m[32mvar flatten = require('array-flatten');[m
[32m+[m[32mvar parseUrl = require('parseurl');[m
[32m+[m[32mvar setPrototypeOf = require('setprototypeof')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar objectRegExp = /^\[object (\S+)\]$/;[m
[32m+[m[32mvar slice = Array.prototype.slice;[m
[32m+[m[32mvar toString = Object.prototype.toString;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialize a new `Router` with the given `options`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {Router} which is a callable function[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar proto = module.exports = function(options) {[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m
[32m+[m[32m  function router(req, res, next) {[m
[32m+[m[32m    router.handle(req, res, next);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // mixin Router class functions[m
[32m+[m[32m  setPrototypeOf(router, proto)[m
[32m+[m
[32m+[m[32m  router.params = {};[m
[32m+[m[32m  router._params = [];[m
[32m+[m[32m  router.caseSensitive = opts.caseSensitive;[m
[32m+[m[32m  router.mergeParams = opts.mergeParams;[m
[32m+[m[32m  router.strict = opts.strict;[m
[32m+[m[32m  router.stack = [];[m
[32m+[m
[32m+[m[32m  return router;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map the given param placeholder `name`(s) to the given callback.[m
[32m+[m[32m *[m
[32m+[m[32m * Parameter mapping is used to provide pre-conditions to routes[m
[32m+[m[32m * which use normalized placeholders. For example a _:user_id_ parameter[m
[32m+[m[32m * could automatically load a user's information from the database without[m
[32m+[m[32m * any additional code,[m
[32m+[m[32m *[m
[32m+[m[32m * The callback uses the same signature as middleware, the only difference[m
[32m+[m[32m * being that the value of the placeholder is passed, in this case the _id_[m
[32m+[m[32m * of the user. Once the `next()` function is invoked, just like middleware[m
[32m+[m[32m * it will continue on to execute the route, or subsequent parameter functions.[m
[32m+[m[32m *[m
[32m+[m[32m * Just like in middleware, you must either respond to the request or call next[m
[32m+[m[32m * to avoid stalling the request.[m
[32m+[m[32m *[m
[32m+[m[32m *  app.param('user_id', function(req, res, next, id){[m
[32m+[m[32m *    User.find(id, function(err, user){[m
[32m+[m[32m *      if (err) {[m
[32m+[m[32m *        return next(err);[m
[32m+[m[32m *      } else if (!user) {[m
[32m+[m[32m *        return next(new Error('failed to load user'));[m
[32m+[m[32m *      }[m
[32m+[m[32m *      req.user = user;[m
[32m+[m[32m *      next();[m
[32m+[m[32m *    });[m
[32m+[m[32m *  });[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mproto.param = function param(name, fn) {[m
[32m+[m[32m  // param logic[m
[32m+[m[32m  if (typeof name === 'function') {[m
[32m+[m[32m    deprecate('router.param(fn): Refactor to use path params');[m
[32m+[m[32m    this._params.push(name);[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // apply param functions[m
[32m+[m[32m  var params = this._params;[m
[32m+[m[32m  var len = params.length;[m
[32m+[m[32m  var ret;[m
[32m+[m
[32m+[m[32m  if (name[0] === ':') {[m
[32m+[m[32m    deprecate('router.param(' + JSON.stringify(name) + ', fn): Use router.param(' + JSON.stringify(name.slice(1)) + ', fn) instead')[m
[32m+[m[32m    name = name.slice(1)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < len; ++i) {[m
[32m+[m[32m    if (ret = params[i](name, fn)) {[m
[32m+[m[32m      fn = ret;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ensure we end up with a[m
[32m+[m[32m  // middleware function[m
[32m+[m[32m  if ('function' !== typeof fn) {[m
[32m+[m[32m    throw new Error('invalid param() call for ' + name + ', got ' + fn);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  (this.params[name] = this.params[name] || []).push(fn);[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Dispatch a req, res into the router.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mproto.handle = function handle(req, res, out) {[m
[32m+[m[32m  var self = this;[m
[32m+[m
[32m+[m[32m  debug('dispatching %s %s', req.method, req.url);[m
[32m+[m
[32m+[m[32m  var idx = 0;[m
[32m+[m[32m  var protohost = getProtohost(req.url) || ''[m
[32m+[m[32m  var removed = '';[m
[32m+[m[32m  var slashAdded = false;[m
[32m+[m[32m  var sync = 0[m
[32m+[m[32m  var paramcalled = {};[m
[32m+[m
[32m+[m[32m  // store options for OPTIONS request[m
[32m+[m[32m  // only used if OPTIONS request[m
[32m+[m[32m  var options = [];[m
[32m+[m
[32m+[m[32m  // middleware and routes[m
[32m+[m[32m  var stack = self.stack;[m
[32m+[m
[32m+[m[32m  // manage inter-router variables[m
[32m+[m[32m  var parentParams = req.params;[m
[32m+[m[32m  var parentUrl = req.baseUrl || '';[m
[32m+[m[32m  var done = restore(out, req, 'baseUrl', 'next', 'params');[m
[32m+[m
[32m+[m[32m  // setup next layer[m
[32m+[m[32m  req.next = next;[m
[32m+[m
[32m+[m[32m  // for options requests, respond with a default if nothing else responds[m
[32m+[m[32m  if (req.method === 'OPTIONS') {[m
[32m+[m[32m    done = wrap(done, function(old, err) {[m
[32m+[m[32m      if (err || options.length === 0) return old(err);[m
[32m+[m[32m      sendOptionsResponse(res, options, old);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // setup basic req values[m
[32m+[m[32m  req.baseUrl = parentUrl;[m
[32m+[m[32m  req.originalUrl = req.originalUrl || req.url;[m
[32m+[m
[32m+[m[32m  next();[m
[32m+[m
[32m+[m[32m  function next(err) {[m
[32m+[m[32m    var layerError = err === 'route'[m
[32m+[m[32m      ? null[m
[32m+[m[32m      : err;[m
[32m+[m
[32m+[m[32m    // remove added slash[m
[32m+[m[32m    if (slashAdded) {[m
[32m+[m[32m      req.url = req.url.slice(1)[m
[32m+[m[32m      slashAdded = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // restore altered req.url[m
[32m+[m[32m    if (removed.length !== 0) {[m
[32m+[m[32m      req.baseUrl = parentUrl;[m
[32m+[m[32m      req.url = protohost + removed + req.url.slice(protohost.length)[m
[32m+[m[32m      removed = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // signal to exit router[m
[32m+[m[32m    if (layerError === 'router') {[m
[32m+[m[32m      setImmediate(done, null)[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // no more matching layers[m
[32m+[m[32m    if (idx >= stack.length) {[m
[32m+[m[32m      setImmediate(done, layerError);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // max sync stack[m
[32m+[m[32m    if (++sync > 100) {[m
[32m+[m[32m      return setImmediate(next, err)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // get pathname of request[m
[32m+[m[32m    var path = getPathname(req);[m
[32m+[m
[32m+[m[32m    if (path == null) {[m
[32m+[m[32m      return done(layerError);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // find next matching layer[m
[32m+[m[32m    var layer;[m
[32m+[m[32m    var match;[m
[32m+[m[32m    var route;[m
[32m+[m
[32m+[m[32m    while (match !== true && idx < stack.length) {[m
[32m+[m[32m      layer = stack[idx++];[m
[32m+[m[32m      match = matchLayer(layer, path);[m
[32m+[m[32m      route = layer.route;[m
[32m+[m
[32m+[m[32m      if (typeof match !== 'boolean') {[m
[32m+[m[32m        // hold on to layerError[m
[32m+[m[32m        layerError = layerError || match;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (match !== true) {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (!route) {[m
[32m+[m[32m        // process non-route handlers normally[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (layerError) {[m
[32m+[m[32m        // routes do not match with a pending error[m
[32m+[m[32m        match = false;[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var method = req.method;[m
[32m+[m[32m      var has_method = route._handles_method(method);[m
[32m+[m
[32m+[m[32m      // build up automatic options response[m
[32m+[m[32m      if (!has_method && method === 'OPTIONS') {[m
[32m+[m[32m        appendMethods(options, route._options());[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // don't even bother matching route[m
[32m+[m[32m      if (!has_method && method !== 'HEAD') {[m
[32m+[m[32m        match = false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // no match[m
[32m+[m[32m    if (match !== true) {[m
[32m+[m[32m      return done(layerError);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // store route for dispatch on change[m
[32m+[m[32m    if (route) {[m
[32m+[m[32m      req.route = route;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Capture one-time layer values[m
[32m+[m[32m    req.params = self.mergeParams[m
[32m+[m[32m      ? mergeParams(layer.params, parentParams)[m
[32m+[m[32m      : layer.params;[m
[32m+[m[32m    var layerPath = layer.path;[m
[32m+[m
[32m+[m[32m    // this should be done for the layer[m
[32m+[m[32m    self.process_params(layer, paramcalled, req, res, function (err) {[m
[32m+[m[32m      if (err) {[m
[32m+[m[32m        next(layerError || err)[m
[32m+[m[32m      } else if (route) {[m
[32m+[m[32m        layer.handle_request(req, res, next)[m
[32m+[m[32m      } else {[m
[32m+[m[32m        trim_prefix(layer, layerError, layerPath, path)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      sync = 0[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function trim_prefix(layer, layerError, layerPath, path) {[m
[32m+[m[32m    if (layerPath.length !== 0) {[m
[32m+[m[32m      // Validate path is a prefix match[m
[32m+[m[32m      if (layerPath !== path.slice(0, layerPath.length)) {[m
[32m+[m[32m        next(layerError)[m
[32m+[m[32m        return[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Validate path breaks on a path separator[m
[32m+[m[32m      var c = path[layerPath.length][m
[32m+[m[32m      if (c && c !== '/' && c !== '.') return next(layerError)[m
[32m+[m
[32m+[m[32m      // Trim off the part of the url that matches the route[m
[32m+[m[32m      // middleware (.use stuff) needs to have the path stripped[m
[32m+[m[32m      debug('trim prefix (%s) from url %s', layerPath, req.url);[m
[32m+[m[32m      removed = layerPath;[m
[32m+[m[32m      req.url = protohost + req.url.slice(protohost.length + removed.length)[m
[32m+[m
[32m+[m[32m      // Ensure leading slash[m
[32m+[m[32m      if (!protohost && req.url[0] !== '/') {[m
[32m+[m[32m        req.url = '/' + req.url;[m
[32m+[m[32m        slashAdded = true;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Setup base URL (no trailing slash)[m
[32m+[m[32m      req.baseUrl = parentUrl + (removed[removed.length - 1] === '/'[m
[32m+[m[32m        ? removed.substring(0, removed.length - 1)[m
[32m+[m[32m        : removed);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('%s %s : %s', layer.name, layerPath, req.originalUrl);[m
[32m+[m
[32m+[m[32m    if (layerError) {[m
[32m+[m[32m      layer.handle_error(layerError, req, res, next);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      layer.handle_request(req, res, next);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Process any parameters for the layer.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mproto.process_params = function process_params(layer, called, req, res, done) {[m
[32m+[m[32m  var params = this.params;[m
[32m+[m
[32m+[m[32m  // captured parameters from the layer, keys and values[m
[32m+[m[32m  var keys = layer.keys;[m
[32m+[m
[32m+[m[32m  // fast track[m
[32m+[m[32m  if (!keys || keys.length === 0) {[m
[32m+[m[32m    return done();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var i = 0;[m
[32m+[m[32m  var name;[m
[32m+[m[32m  var paramIndex = 0;[m
[32m+[m[32m  var key;[m
[32m+[m[32m  var paramVal;[m
[32m+[m[32m  var paramCallbacks;[m
[32m+[m[32m  var paramCalled;[m
[32m+[m
[32m+[m[32m  // process params in order[m
[32m+[m[32m  // param callbacks can be async[m
[32m+[m[32m  function param(err) {[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      return done(err);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (i >= keys.length ) {[m
[32m+[m[32m      return done();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    paramIndex = 0;[m
[32m+[m[32m    key = keys[i++];[m
[32m+[m[32m    name = key.name;[m
[32m+[m[32m    paramVal = req.params[name];[m
[32m+[m[32m    paramCallbacks = params[name];[m
[32m+[m[32m    paramCalled = called[name];[m
[32m+[m
[32m+[m[32m    if (paramVal === undefined || !paramCallbacks) {[m
[32m+[m[32m      return param();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // param previously called with same value or error occurred[m
[32m+[m[32m    if (paramCalled && (paramCalled.match === paramVal[m
[32m+[m[32m      || (paramCalled.error && paramCalled.error !== 'route'))) {[m
[32m+[m[32m      // restore value[m
[32m+[m[32m      req.params[name] = paramCalled.value;[m
[32m+[m
[32m+[m[32m      // next param[m
[32m+[m[32m      return param(paramCalled.error);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    called[name] = paramCalled = {[m
[32m+[m[32m      error: null,[m
[32m+[m[32m      match: paramVal,[m
[32m+[m[32m      value: paramVal[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    paramCallback();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // single param callbacks[m
[32m+[m[32m  function paramCallback(err) {[m
[32m+[m[32m    var fn = paramCallbacks[paramIndex++];[m
[32m+[m
[32m+[m[32m    // store updated value[m
[32m+[m[32m    paramCalled.value = req.params[key.name];[m
[32m+[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      // store error[m
[32m+[m[32m      paramCalled.error = err;[m
[32m+[m[32m      param(err);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!fn) return param();[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      fn(req, res, paramCallback, paramVal, key.name);[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      paramCallback(e);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  param();[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Use the given middleware function, with optional path, defaulting to "/".[m
[32m+[m[32m *[m
[32m+[m[32m * Use (like `.all`) will run for any http METHOD, but it will not add[m
[32m+[m[32m * handlers for those methods so OPTIONS requests will not consider `.use`[m
[32m+[m[32m * functions even if they could respond.[m
[32m+[m[32m *[m
[32m+[m[32m * The other difference is that _route_ path is stripped and not visible[m
[32m+[m[32m * to the handler function. The main effect of this feature is that mounted[m
[32m+[m[32m * handlers can operate without any code changes regardless of the "prefix"[m
[32m+[m[32m * pathname.[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mproto.use = function use(fn) {[m
[32m+[m[32m  var offset = 0;[m
[32m+[m[32m  var path = '/';[m
[32m+[m
[32m+[m[32m  // default path to '/'[m
[32m+[m[32m  // disambiguate router.use([fn])[m
[32m+[m[32m  if (typeof fn !== 'function') {[m
[32m+[m[32m    var arg = fn;[m
[32m+[m
[32m+[m[32m    while (Array.isArray(arg) && arg.length !== 0) {[m
[32m+[m[32m      arg = arg[0];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // first arg is the path[m
[32m+[m[32m    if (typeof arg !== 'function') {[m
[32m+[m[32m      offset = 1;[m
[32m+[m[32m      path = fn;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var callbacks = flatten(slice.call(arguments, offset));[m
[32m+[m
[32m+[m[32m  if (callbacks.length === 0) {[m
[32m+[m[32m    throw new TypeError('Router.use() requires a middleware function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < callbacks.length; i++) {[m
[32m+[m[32m    var fn = callbacks[i];[m
[32m+[m
[32m+[m[32m    if (typeof fn !== 'function') {[m
[32m+[m[32m      throw new TypeError('Router.use() requires a middleware function but got a ' + gettype(fn))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // add the middleware[m
[32m+[m[32m    debug('use %o %s', path, fn.name || '<anonymous>')[m
[32m+[m
[32m+[m[32m    var layer = new Layer(path, {[m
[32m+[m[32m      sensitive: this.caseSensitive,[m
[32m+[m[32m      strict: false,[m
[32m+[m[32m      end: false[m
[32m+[m[32m    }, fn);[m
[32m+[m
[32m+[m[32m    layer.route = undefined;[m
[32m+[m
[32m+[m[32m    this.stack.push(layer);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a new Route for the given path.[m
[32m+[m[32m *[m
[32m+[m[32m * Each route contains a separate middleware stack and VERB handlers.[m
[32m+[m[32m *[m
[32m+[m[32m * See the Route api documentation for details on adding handlers[m
[32m+[m[32m * and middleware to routes.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} path[m
[32m+[m[32m * @return {Route}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mproto.route = function route(path) {[m
[32m+[m[32m  var route = new Route(path);[m
[32m+[m
[32m+[m[32m  var layer = new Layer(path, {[m
[32m+[m[32m    sensitive: this.caseSensitive,[m
[32m+[m[32m    strict: this.strict,[m
[32m+[m[32m    end: true[m
[32m+[m[32m  }, route.dispatch.bind(route));[m
[32m+[m
[32m+[m[32m  layer.route = route;[m
[32m+[m
[32m+[m[32m  this.stack.push(layer);[m
[32m+[m[32m  return route;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// create Router#VERB functions[m
[32m+[m[32mmethods.concat('all').forEach(function(method){[m
[32m+[m[32m  proto[method] = function(path){[m
[32m+[m[32m    var route = this.route(path)[m
[32m+[m[32m    route[method].apply(route, slice.call(arguments, 1));[m
[32m+[m[32m    return this;[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// append methods to a list of methods[m
[32m+[m[32mfunction appendMethods(list, addition) {[m
[32m+[m[32m  for (var i = 0; i < addition.length; i++) {[m
[32m+[m[32m    var method = addition[i];[m
[32m+[m[32m    if (list.indexOf(method) === -1) {[m
[32m+[m[32m      list.push(method);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// get pathname of request[m
[32m+[m[32mfunction getPathname(req) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return parseUrl(req).pathname;[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return undefined;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Get get protocol + host for a URL[m
[32m+[m[32mfunction getProtohost(url) {[m
[32m+[m[32m  if (typeof url !== 'string' || url.length === 0 || url[0] === '/') {[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var searchIndex = url.indexOf('?')[m
[32m+[m[32m  var pathLength = searchIndex !== -1[m
[32m+[m[32m    ? searchIndex[m
[32m+[m[32m    : url.length[m
[32m+[m[32m  var fqdnIndex = url.slice(0, pathLength).indexOf('://')[m
[32m+[m
[32m+[m[32m  return fqdnIndex !== -1[m
[32m+[m[32m    ? url.substring(0, url.indexOf('/', 3 + fqdnIndex))[m
[32m+[m[32m    : undefined[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// get type for error message[m
[32m+[m[32mfunction gettype(obj) {[m
[32m+[m[32m  var type = typeof obj;[m
[32m+[m
[32m+[m[32m  if (type !== 'object') {[m
[32m+[m[32m    return type;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // inspect [[Class]] for objects[m
[32m+[m[32m  return toString.call(obj)[m
[32m+[m[32m    .replace(objectRegExp, '$1');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Match path to a layer.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Layer} layer[m
[32m+[m[32m * @param {string} path[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction matchLayer(layer, path) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return layer.match(path);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return err;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// merge params with parent params[m
[32m+[m[32mfunction mergeParams(params, parent) {[m
[32m+[m[32m  if (typeof parent !== 'object' || !parent) {[m
[32m+[m[32m    return params;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // make copy of parent for base[m
[32m+[m[32m  var obj = mixin({}, parent);[m
[32m+[m
[32m+[m[32m  // simple non-numeric merging[m
[32m+[m[32m  if (!(0 in params) || !(0 in parent)) {[m
[32m+[m[32m    return mixin(obj, params);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var i = 0;[m
[32m+[m[32m  var o = 0;[m
[32m+[m
[32m+[m[32m  // determine numeric gaps[m
[32m+[m[32m  while (i in params) {[m
[32m+[m[32m    i++;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  while (o in parent) {[m
[32m+[m[32m    o++;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // offset numeric indices in params before merge[m
[32m+[m[32m  for (i--; i >= 0; i--) {[m
[32m+[m[32m    params[i + o] = params[i];[m
[32m+[m
[32m+[m[32m    // create holes for the merge when necessary[m
[32m+[m[32m    if (i < o) {[m
[32m+[m[32m      delete params[i];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return mixin(obj, params);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// restore obj props after function[m
[32m+[m[32mfunction restore(fn, obj) {[m
[32m+[m[32m  var props = new Array(arguments.length - 2);[m
[32m+[m[32m  var vals = new Array(arguments.length - 2);[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < props.length; i++) {[m
[32m+[m[32m    props[i] = arguments[i + 2];[m
[32m+[m[32m    vals[i] = obj[props[i]];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function () {[m
[32m+[m[32m    // restore vals[m
[32m+[m[32m    for (var i = 0; i < props.length; i++) {[m
[32m+[m[32m      obj[props[i]] = vals[i];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return fn.apply(this, arguments);[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// send an OPTIONS response[m
[32m+[m[32mfunction sendOptionsResponse(res, options, next) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    var body = options.join(',');[m
[32m+[m[32m    res.set('Allow', body);[m
[32m+[m[32m    res.send(body);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    next(err);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// wrap a function[m
[32m+[m[32mfunction wrap(old, fn) {[m
[32m+[m[32m  return function proxy() {[m
[32m+[m[32m    var args = new Array(arguments.length + 1);[m
[32m+[m
[32m+[m[32m    args[0] = old;[m
[32m+[m[32m    for (var i = 0, len = arguments.length; i < len; i++) {[m
[32m+[m[32m      args[i + 1] = arguments[i];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    fn.apply(this, args);[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
