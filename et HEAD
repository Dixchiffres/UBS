[1mnew file mode 100644[m
[1mindex 0000000..9b4c397[m
[1m--- /dev/null[m
[1m+++ b/servers.js[m
[36m@@ -0,0 +1,18 @@[m
[32m+[m[32mconst express = require('express');[m
[32m+[m[32mconst app = express();[m
[32m+[m[32mconst port = 3000;[m
[32m+[m
[32m+[m[32m// Middleware pour servir les fichiers statiques (HTML, CSS, JS)[m
[32m+[m[32mapp.use(express.static('public'));[m
[32m+[m
[32m+[m[32m// Route pour récupérer l'historique de facturation[m
[32m+[m[32mapp.get('/billing-history', (req, res) => {[m
[32m+[m[32m    res.json([[m
[32m+[m[32m        { date: '2024-09-01', service: 'Service A', invoice: '12345', recipient: 'John Doe', amount: '$50', status: 'Paid' },[m
[32m+[m[32m        // Ajouter d'autres entrées[m
[32m+[m[32m    ]);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mapp.listen(port, () => {[m
[32m+[m[32m    console.log(`Serveur en cours d'exécution sur http://localhost:${port}`);[m
[32m+[m[32m});[m
