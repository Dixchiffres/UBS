[1mdiff --git a/node_modules/setprototypeof/test/index.js b/node_modules/setprototypeof/test/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..afeb4dd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/setprototypeof/test/index.js[m
[36m@@ -0,0 +1,24 @@[m
[32m+[m[32m'use strict'[m
[32m+[m[32m/* eslint-env mocha */[m
[32m+[m[32m/* eslint no-proto: 0 */[m
[32m+[m[32mvar assert = require('assert')[m
[32m+[m[32mvar setPrototypeOf = require('..')[m
[32m+[m
[32m+[m[32mdescribe('setProtoOf(obj, proto)', function () {[m
[32m+[m[32m  it('should merge objects', function () {[m
[32m+[m[32m    var obj = { a: 1, b: 2 }[m
[32m+[m[32m    var proto = { b: 3, c: 4 }[m
[32m+[m[32m    var mergeObj = setPrototypeOf(obj, proto)[m
[32m+[m
[32m+[m[32m    if (Object.getPrototypeOf) {[m
[32m+[m[32m      assert.strictEqual(Object.getPrototypeOf(obj), proto)[m
[32m+[m[32m    } else if ({ __proto__: [] } instanceof Array) {[m
[32m+[m[32m      assert.strictEqual(obj.__proto__, proto)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      assert.strictEqual(obj.a, 1)[m
[32m+[m[32m      assert.strictEqual(obj.b, 2)[m
[32m+[m[32m      assert.strictEqual(obj.c, 4)[m
[32m+[m[32m    }[m
[32m+[m[32m    assert.strictEqual(mergeObj, obj)[m
[32m+[m[32m  })[m
[32m+[m[32m})[m
