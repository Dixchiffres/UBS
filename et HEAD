[32m+[m[32m# Merge Descriptors[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Build Status][travis-image]][travis-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mMerge objects using descriptors.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar thing = {[m
[32m+[m[32m  get name() {[m
[32m+[m[32m    return 'jon'[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar animal = {[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmerge(animal, thing)[m
[32m+[m
[32m+[m[32manimal.name === 'jon'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m### merge(destination, source)[m
[32m+[m
[32m+[m[32mRedefines `destination`'s descriptors with `source`'s.[m
[32m+[m
[32m+[m[32m### merge(destination, source, false)[m
[32m+[m
[32m+[m[32mDefines `source`'s descriptors on `destination` if `destination` does not have[m
[32m+[m[32ma descriptor by the same name.[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[MIT](LICENSE)[m
[32m+[m
[32m+[m[32m[npm-image]: https://img.shields.io/npm/v/merge-descriptors.svg[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/merge-descriptors[m
[32m+[m[32m[travis-image]: https://img.shields.io/travis/component/merge-descriptors/master.svg[m
[32m+[m[32m[travis-url]: https://travis-ci.org/component/merge-descriptors[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/component/merge-descriptors/master.svg[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/component/merge-descriptors?branch=master[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/merge-descriptors.svg[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/merge-descriptors[m
[1mdiff --git a/node_modules/merge-descriptors/index.js b/node_modules/merge-descriptors/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..573b132[m
[1m--- /dev/null[m
[1m+++ b/node_modules/merge-descriptors/index.js[m
[36m@@ -0,0 +1,60 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * merge-descriptors[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = merge[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar hasOwnProperty = Object.prototype.hasOwnProperty[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Merge the property descriptors of `src` into `dest`[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} dest Object to add descriptors to[m
[32m+[m[32m * @param {object} src Object to clone descriptors from[m
[32m+[m[32m * @param {boolean} [redefine=true] Redefine `dest` properties with `src` properties[m
[32m+[m[32m * @returns {object} Reference to dest[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction merge(dest, src, redefine) {[m
[32m+[m[32m  if (!dest) {[m
[32m+[m[32m    throw new TypeError('argument dest is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!src) {[m
[32m+[m[32m    throw new TypeError('argument src is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (redefine === undefined) {[m
[32m+[m[32m    // Default to true[m
[32m+[m[32m    redefine = true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Object.getOwnPropertyNames(src).forEach(function forEachOwnPropertyName(name) {[m
[32m+[m[32m    if (!redefine && hasOwnProperty.call(dest, name)) {[m
[32m+[m[32m      // Skip desriptor[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Copy descriptor[m
[32m+[m[32m    var descriptor = Object.getOwnPropertyDescriptor(src, name)[m
[32m+[m[32m    Object.defineProperty(dest, name, descriptor)[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  return dest[m
[32m+[m[32m}[m
