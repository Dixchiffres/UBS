[32m+[m
[32m+[m[32m// Put all encoding/alias/codec definitions to single object and export it.[m[41m [m
[32m+[m[32mfor (var i = 0; i < modules.length; i++) {[m
[32m+[m[32m    var module = modules[i];[m
[32m+[m[32m    for (var enc in module)[m
[32m+[m[32m        if (Object.prototype.hasOwnProperty.call(module, enc))[m
[32m+[m[32m            exports[enc] = module[enc];[m
[32m+[m[32m}[m
