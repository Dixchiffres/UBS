[32m+[m[32m        '  1,',[m
[32m+[m[32m        '  {',[m
[32m+[m[32m        '    a: 1,',[m
[32m+[m[32m        '    b: {',[m
[32m+[m[32m        '      c: 1',[m
[32m+[m[32m        '    }',[m
[32m+[m[32m        '  },',[m
[32m+[m[32m        '  \'asdf\\nsdf\'',[m
[32m+[m[32m        ']'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m[32m    var expectedTabs = [[m
[32m+[m[32m        '[',[m
[32m+[m[32m        '	1,',[m
[32m+[m[32m        '	{',[m
[32m+[m[32m        '		a: 1,',[m
[32m+[m[32m        '		b: {',[m
[32m+[m[32m        '			c: 1',[m
[32m+[m[32m        '		}',[m
[32m+[m[32m        '	},',[m
[32m+[m[32m        '	\'asdf\\nsdf\'',[m
[32m+[m[32m        ']'[m
[32m+[m[32m    ].join('\n');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(obj, { indent: 2 }), expectedSpaces, 'two');[m
[32m+[m[32m    t.equal(inspect(obj, { indent: '\t' }), expectedTabs, 'tabs');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('values', function (t) {[m
