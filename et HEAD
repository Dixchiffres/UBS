[1mindex 0000000..8da3200[m
[1m--- /dev/null[m
[1m+++ b/node_modules/side-channel/test/index.js[m
[36m@@ -0,0 +1,83 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m
[32m+[m[32mvar getSideChannel = require('../');[m
[32m+[m
[32m+[m[32mtest('export', function (t) {[m
[32m+[m	[32mt.equal(typeof getSideChannel, 'function', 'is a function');[m
[32m+[m	[32mt.equal(getSideChannel.length, 0, 'takes no arguments');[m
[32m+[m
[32m+[m	[32mvar channel = getSideChannel();[m
[32m+[m	[32mt.ok(channel, 'is truthy');[m
[32m+[m	[32mt.equal(typeof channel, 'object', 'is an object');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('assert', function (t) {[m
[32m+[m	[32mvar channel = getSideChannel();[m
[32m+[m	[32mt['throws']([m
[32m+[m		[32mfunction () { channel.assert({}); },[m
[32m+[m		[32mTypeError,[m
[32m+[m		[32m'nonexistent value throws'[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32mvar o = {};[m
[32m+[m	[32mchannel.set(o, 'data');[m
[32m+[m	[32mt.doesNotThrow(function () { channel.assert(o); }, 'existent value noops');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('has', function (t) {[m
[32m+[m	[32mvar channel = getSideChannel();[m
[32m+[m	[32m/** @type {unknown[]} */ var o = [];[m
[32m+[m
[32m+[m	[32mt.equal(channel.has(o), false, 'nonexistent value yields false');[m
[32m+[m
[32m+[m	[32mchannel.set(o, 'foo');[m
[32m+[m	[32mt.equal(channel.has(o), true, 'existent value yields true');[m
[32m+[m
[32m+[m	[32mt.equal(channel.has('abc'), false, 'non object value non existent yields false');[m
[32m+[m
[32m+[m	[32mchannel.set('abc', 'foo');[m
[32m+[m	[32mt.equal(channel.has('abc'), true, 'non object value that exists yields true');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('get', function (t) {[m
[32m+[m	[32mvar channel = getSideChannel();[m
[32m+[m	[32mvar o = {};[m
[32m+[m	[32mt.equal(channel.get(o), undefined, 'nonexistent value yields undefined');[m
[32m+[m
[32m+[m	[32mvar data = {};[m
[32m+[m	[32mchannel.set(o, data);[m
[32m+[m	[32mt.equal(channel.get(o), data, '"get" yields data set by "set"');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('set', function (t) {[m
[32m+[m	[32mvar channel = getSideChannel();[m
[32m+[m	[32mvar o = function () {};[m
[32m+[m	[32mt.equal(channel.get(o), undefined, 'value not set');[m
[32m+[m
[32m+[m	[32mchannel.set(o, 42);[m
[32m+[m	[32mt.equal(channel.get(o), 42, 'value was set');[m
[32m+[m
[32m+[m	[32mchannel.set(o, Infinity);[m
[32m+[m	[32mt.equal(channel.get(o), Infinity, 'value was set again');[m
[32m+[m
[32m+[m	[32mvar o2 = {};[m
[32m+[m	[32mchannel.set(o2, 17);[m
[32m+[m	[32mt.equal(channel.get(o), Infinity, 'o is not modified');[m
[32m+[m	[32mt.equal(channel.get(o2), 17, 'o2 is set');[m
[32m+[m
[32m+[m	[32mchannel.set(o, 14);[m
[32m+[m	[32mt.equal(channel.get(o), 14, 'o is modified');[m
[32m+[m	[32mt.equal(channel.get(o2), 17, 'o2 is not modified');[m
[32m+[m
[32m+[m	[32mt.end();[m
[32m+[m[32m});[m
