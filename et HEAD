[32m+[m
[32m+[m[32mtest('negative zero', function (t) {[m
[32m+[m[32m    t.equal(inspect(0), '0', 'inspect(0) === "0"');[m
[32m+[m[32m    t.equal(inspect(Object(0)), 'Object(0)', 'inspect(Object(0)) === "Object(0)"');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(-0), '-0', 'inspect(-0) === "-0"');[m
[32m+[m[32m    t.equal(inspect(Object(-0)), 'Object(-0)', 'inspect(Object(-0)) === "Object(-0)"');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('numericSeparator', function (t) {[m
[32m+[m[32m    forEach(v.nonBooleans, function (nonBoolean) {[m
[32m+[m[32m        t['throws']([m
[32m+[m[32m            function () { inspect(true, { numericSeparator: nonBoolean }); },[m
[32m+[m[32m            TypeError,[m
[32m+[m[32m            inspect(nonBoolean) + ' is not a boolean'[m
[32m+[m[32m        );[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('3 digit numbers', function (st) {[m
[32m+[m[32m        var failed = false;[m
[32m+[m[32m        for (var i = -999; i < 1000; i += 1) {[m
[32m+[m[32m            var actual = inspect(i);[m
[32m+[m[32m            var actualSepNo = inspect(i, { numericSeparator: false });[m
[32m+[m[32m            var actualSepYes = inspect(i, { numericSeparator: true });[m
[32m+[m[32m            var expected = String(i);[m
[32m+[m[32m            if (actual !== expected || actualSepNo !== expected || actualSepYes !== expected) {[m
[32m+[m[32m                failed = true;[m
[32m+[m[32m                t.equal(actual, expected);[m
[32m+[m[32m                t.equal(actualSepNo, expected);[m
[32m+[m[32m                t.equal(actualSepYes, expected);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        st.notOk(failed, 'all 3 digit numbers passed');[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.equal(inspect(1e3), '1000', '1000');[m
[32m+[m[32m    t.equal(inspect(1e3, { numericSeparator: false }), '1000', '1000, numericSeparator false');[m
[32m+[m[32m    t.equal(inspect(1e3, { numericSeparator: true }), '1_000', '1000, numericSeparator true');[m
[32m+[m[32m    t.equal(inspect(-1e3), '-1000', '-1000');[m
[32m+[m[32m    t.equal(inspect(-1e3, { numericSeparator: false }), '-1000', '-1000, numericSeparator false');[m
[32m+[m[32m    t.equal(inspect(-1e3, { numericSeparator: true }), '-1_000', '-1000, numericSeparator true');[m
[32m+[m
[32m+[m[32m    t.equal(inspect(1234.5678, { numericSeparator: true }), '1_234.567_8', 'fractional numbers get separators');[m
[32m+[m[32m    t.equal(inspect(1234.56789, { numericSeparator: true }), '1_234.567_89', 'fractional numbers get separators');[m
[32m+[m[32m    t.equal(inspect(1234.567891, { numericSeparator: true }), '1_234.567_891', 'fractional numbers get separators');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
